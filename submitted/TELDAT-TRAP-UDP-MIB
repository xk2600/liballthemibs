TELDAT-TRAP-UDP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


udpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the UDP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 13 }


udpEventsGroup	OBJECT IDENTIFIER	::= { udpTrapsMIB 1 }

udpNotifications	OBJECT IDENTIFIER	::= { udpTrapsMIB 2 }
udpNotificationsPrefix	OBJECT IDENTIFIER	::= { udpNotifications 0 }

udpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event UDP.1
        This message is generated when a packet is discarded because UDP is not
        installed in the router. The packet was for the broadcast address."
    ::= { udpEventsGroup 1 }

udp1_source_ip_address OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'source_ip_address' in event UDP.1"
    ::= { udpEventObjects1 1 }

