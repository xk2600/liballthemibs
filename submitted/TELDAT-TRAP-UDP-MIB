    ::= { udpNotificationsPrefix 2 }

udpNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              udp3_source_ip_address, 
              udp3_length 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event UDP.3

        Message: 'Discarded packet from $1, bad length $2'

        Parameters list:
              $1: udp3_source_ip_address
              $2: udp3_length

        This message is generated when a packet is discarded because it had
        an UDP length greater than its IP length."
    ::= { udpNotificationsPrefix 3 }

udpNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              udp4_calculated_checksum, 
              udp4_received_checksum 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event UDP.4

        Message: 'Bad checksum - calculated $1, received $2'

        Parameters list:
              $1: udp4_calculated_checksum
              $2: udp4_received_checksum

        This message is generated when a packet is discarded because it had
        a bad checksum."
    ::= { udpNotificationsPrefix 4 }

udpNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              udp5_incoming_interface, 
              udp5_source_ip_address,
              udp5_source_udp_port,
              udp5_destination_ip_address,
              udp5_destination_udp_port 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event UDP.5

        Message: 'Broadcast packet received on interface $1 from $2 port $3 for $4 port $5'

        Parameters list:
              $1: udp5_incoming_interface
              $2: udp5_source_ip_address
              $3: udp5_source_udp_port
              $4: udp5_destination_ip_address
              $5: udp5_destination_udp_port

        This message is generated when a broadcast UDP packet is received"
    ::= { udpNotificationsPrefix 5 }

