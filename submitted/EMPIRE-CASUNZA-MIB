-- *****************************************************************************
-- Trap Definitions
-- *****************************************************************************

-- Host Traps *********************************************************

zoneAimHostConfigChangeEvent OBJECT IDENTIFIER ::= { traps 165401 }
zoneAimHostConfigChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostStatusAlert,
		zoneAimStatHostMonitor,
		zoneAimStatHostCPULagSetting,
		zoneAimStatHostCPUAlert,
		zoneAimStatHostCPUMonitor,
		zoneAimStatHostMemoryAlert,
		zoneAimStatHostMemoryMonitor,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Configuration change trap. This trap will be sent when Alerts, Monitor and Lag Setting Values
		of a Host is changed."
	::= 165401

zoneAimHostStateChangeEvent OBJECT IDENTIFIER ::= { traps 165402 }
zoneAimHostStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostState,
		zoneAimStatHostStatus,
		zoneAimStatHostStatusAlert,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host State change trap. This trap will be sent when Host State Changes."
	::= 165402

zoneAimHostCPUStateChangeEvent OBJECT IDENTIFIER ::= { traps 165403 }
zoneAimHostCPUStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostCPUUsage,
		zoneAimStatHostCPUUsageWarnThresh,
		zoneAimStatHostCPUUsageCritThresh,
		zoneAimStatHostCPUAlert,
		zoneAimStatHostCPUAggregateStatus,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host CPU State change trap. This trap will be sent when CPU State Changes."
	::= 165403

zoneAimHostMemStateChangeEvent OBJECT IDENTIFIER ::= { traps 165404 }
zoneAimHostMemStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostMemoryUsage,
		zoneAimStatHostMemoryUsageWarnThreshold,
		zoneAimStatHostMemoryUsageCritThreshold,
		zoneAimStatHostMemoryAlert,
		zoneAimStatHostMemoryState,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Mem State change trap. This trap will be sent when Host Mem State Changes."
	::= 165404

zoneAimHostTotalZoneCPUStateChangeEvent OBJECT IDENTIFIER ::= { traps 165405 }
zoneAimHostTotalZoneCPUStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostTotalZoneCPUusage,
		zoneAimStatHostTotalZoneCPUWarnThreshold,
		zoneAimStatHostTotalZoneCPUCritThreshold,
		zoneAimStatHostTotalZoneCPUState,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Total Zone CPU State change trap. This trap will be sent when Total Zone CPU State Changes."
	::= 165405

zoneAimHostTotalZoneMemStateChangeEvent OBJECT IDENTIFIER ::= { traps 165406 }
zoneAimHostTotalZoneMemStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostTotalZoneMemoryusage,
		zoneAimStatHostTotalZoneMemoryWarnThreshold,
		zoneAimStatHostTotalZoneMemoryCritThreshold,
		zoneAimStatHostTotalZoneMemoryState,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Total Zone Mem State change trap. This trap will be sent when Total Zone Memory State Changes."
	::= 165406

zoneAimHostThresholdChangeEvent OBJECT IDENTIFIER ::= { traps 165407 }
zoneAimHostThresholdChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostCPUUsageWarnThresh,
		zoneAimStatHostCPUUsageCritThresh,
		zoneAimStatHostMemoryUsageWarnThreshold,
		zoneAimStatHostMemoryUsageCritThreshold,
		zoneAimStatHostTotalZoneCPUWarnThreshold,
		zoneAimStatHostTotalZoneCPUCritThreshold,
		zoneAimStatHostTotalZoneMemoryWarnThreshold,
		zoneAimStatHostTotalZoneMemoryCritThreshold,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Threshold change trap. This trap will be sent when Host CPU, Host Memory,
		 Total Zone CPU or Total Zone Memory Threshold Changes."
	::= 165407

zoneAimHostConnectionStateChangeEvent OBJECT IDENTIFIER ::= { traps 165408 }
zoneAimHostConnectionStateChangeTrap TRAP-TYPE
	ENTERPRISE sysmgmt
	VARIABLES {
		zoneAimStatHostName,
		zoneAimStatHostID,
		zoneAimStatHostStatus,
		zoneAimStatHostState,
		zoneAimStatHostInstanceName
	}
	DESCRIPTION
		"Zone Host Connection State change trap. This trap will be sent when Host SSH 
