EMPIRE-CASUNZA-MIB DEFINITIONS ::= BEGIN

IMPORTS
	OBJECT-TYPE
		FROM RFC-1212
	TRAP-TYPE
		FROM RFC-1215
	DisplayString
		FROM RFC1213-MIB
	enterprises, TimeTicks, Counter, Gauge
		FROM RFC1155-SMI;

empire		OBJECT IDENTIFIER ::= { enterprises 546 }
unix            OBJECT IDENTIFIER ::= { empire 1 }
sysmgmt         OBJECT IDENTIFIER ::= { unix 1 }
traps		OBJECT IDENTIFIER ::= { empire 9 }
applications	OBJECT IDENTIFIER ::= { empire 16 }
casunzaim	OBJECT IDENTIFIER ::= { applications 54 }

-- *****************************************************************************
zoneAimGroup	OBJECT IDENTIFIER ::= { casunzaim 1 }	
-- *****************************************************************************
zoneAimInstanceCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Number of Zone Agent Instances in zoneAimInstanceTable."
	::= {zoneAimGroup 1}

zoneAimInstanceTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ZoneAimInstanceRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A table containing the Zone Agent Instances."
	::= { zoneAimGroup 2 }

zoneAimInstanceEntry OBJECT-TYPE
	SYNTAX ZoneAimInstanceRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The Zone Agent Instances Table entry."
	INDEX { zoneAimInstanceUID }
	::= { zoneAimInstanceTable 1 }

ZoneAimInstanceRow ::=
	SEQUENCE {
		zoneAimInstanceUID		INTEGER,
		zoneAimInstanceName		DisplayString,
		zoneAimInstanceUserName		DisplayString,
		zoneAimInstancePassword		DisplayString,
		zoneAimInstanceRowStatus	INTEGER,
		zoneAimInstanceDataStatus	INTEGER
		}

zoneAimInstanceUID OBJECT-TYPE
	SYNTAX INTEGER (1..2147483647)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Integer UID of Zone Instance."
	::= { zoneAimInstanceEntry  1}

zoneAimInstanceName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Name of the Zone Instance"
	::= { zoneAimInstanceEntry  2}

zoneAimInstanceUserName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"User name for the Zone Instance"
	::= { zoneAimInstanceEntry  3}

zoneAimInstancePassword OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Password for the Zone Instance"
	::= { zoneAimInstanceEntry  4}

zoneAimInstanceRowStatus OBJECT-TYPE
	SYNTAX INTEGER {
		active(1),
		notInService(2),
		notReady(3),
		createAndGo(4),
		createAndWait(5),
		destroy(6)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The status of this monitor entry according to the Textual Conventions for SNMPv2 (see RFC 1443).
		active(1)        - the row is available for usage
		notInService(2)  - setting to this value disables usage of the row
		notReady(3)      - an error occured during usage of the row
		createAndGo(4)   - setting to this value creates the row with row status set to active(1)
		createAndWait(5) - setting to this value creates the row with row status set to notInService(2)
		destroy(6)       - setting to this value deletes the row"
	DEFVAL { 5 }
	::= { zoneAimInstanceEntry  5}

zoneAimInstanceDataStatus OBJECT-TYPE
	SYNTAX INTEGER {
		ready(1),
		notReady(2),
		hostUnreachable(3),
		authenticationFailed(5),
		unknownError(6)
	}
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The status of data of the Zone Instance, i.e. status of Aim caches for the Instance.
		ready(1)                - data are ready in Aim cache, caches were populated
		notReady(2)             - data are Not ready, caches were not populated yet
		hostUnreachable(3)      - Zone host unreachable, data are kept in Aim caches
		authenticationFailed(5) - authentication failed for Zone host, data are kept in Aim caches
		unknownError(6)         - unknow error, data are kept in Aim caches"
	DEFVAL { 2 }
	::= { zoneAimInstanceEntry  6}

zoneAimInstanceUnusedIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
                "An SNMP GET of this attribute returns an unused Instance UID. This
                 is normally used to expedite safe table row creation."
	::= { zoneAimGroup 3 }


