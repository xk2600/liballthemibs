zoneAimEventTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ZoneAimEventRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A Event table."
	::= {zoneAimEventGroup 1}
	
zoneAimEventEntry OBJECT-TYPE
	SYNTAX ZoneAimEventRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The Event table entry."
	INDEX { zoneAimEventInstanceUID, zoneAimEventUID }
	::= { zoneAimEventTable 1 }

ZoneAimEventRow ::=
	SEQUENCE {
		zoneAimEventInstanceUID			INTEGER,
		zoneAimEventUID 			INTEGER,
		zoneAimEventInstanceName		DisplayString,
		zoneAimEventID 				DisplayString,
		zoneAimEventplatformType		DisplayString,
		zoneAimEventplatformName		DisplayString,
		zoneAimEventObjType			DisplayString,
		zoneAimEventObjName	                DisplayString,
		zoneAimEventOldState			INTEGER,
		zoneAimEventNewState			INTEGER,
		zoneAimEventOldValue			DisplayString,
		zoneAimEventNewValue			DisplayString,
		zoneAimEventDateTime			DisplayString,
		zoneAimEventDateTimeSec			Gauge,
		zoneAimEventCfgFile			DisplayString
	}

zoneAimEventInstanceUID OBJECT-TYPE
	SYNTAX INTEGER (1..2147483647)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Integer UID of Zone Instance."
	::= { zoneAimEventEntry  1}

