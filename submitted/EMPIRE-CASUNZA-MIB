-- *****************************************************************************
-- Project Status Group
-- *****************************************************************************

zoneAimStatProjectTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ZoneAimStatProjectRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"Project Table."
	::= { zoneAimStatProjectGroup 1 }

zoneAimStatProjectEntry OBJECT-TYPE
	SYNTAX ZoneAimStatProjectRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The Project Table entry."
	INDEX { zoneAimStatProjectInstanceUID, zoneAimStatProjectUID }
	::= { zoneAimStatProjectTable 1 }

ZoneAimStatProjectRow ::=
	SEQUENCE {
		zoneAimStatProjectInstanceUID				INTEGER,
		zoneAimStatProjectUID					INTEGER,
		zoneAimStatProjectInstanceName				DisplayString,
		zoneAimStatProjectIndex					DisplayString,
		zoneAimStatProjectName					DisplayString,
		zoneAimStatProjectDesc					DisplayString,
		zoneAimStatProjectZonelId				DisplayString,
		zoneAimStatProjectZoneName				DisplayString,
		zoneAimStatProjectContainerId				DisplayString,	
		zoneAimStatProjectState					INTEGER,
		zoneAimStatProjectStatus				DisplayString,
		zoneAimStatProjectStatusAlert				INTEGER,
		zoneAimStatProjectTaskCount				INTEGER,
		zoneAimStatProjectFSSEnabled				DisplayString,
		zoneAimStatProjectCryptoMemMax				INTEGER,
		zoneAimStatProjectTaskMax				INTEGER,
		zoneAimStatProjectLWPSMax				INTEGER,
		zoneAimStatProjectSharedMemMax				INTEGER,
		zoneAimStatProjectMemCap				INTEGER,
		zoneAimStatProjectCPUCap				INTEGER,
		zoneAimStatProjectMemoryUsage				INTEGER,
		zoneAimStatProjectMemoryState				INTEGER,
		zoneAimStatProjectMemoryAlert				INTEGER,
		zoneAimStatProjectMemoryWarnThreshold			INTEGER,
		zoneAimStatProjectMemoryCritThreshold			INTEGER,
		zoneAimStatProjectCPUUsage				INTEGER,
		zoneAimStatProjectCPUState				INTEGER,
		zoneAimStatProjectCPUAlert				INTEGER,
		zoneAimStatProjectCPUWarnThreshold			INTEGER,
		zoneAimStatProjectCPUCritThreshold			INTEGER,
		zoneAimStatProjectCPULagSetting				INTEGER,
		zoneAimStatProjectCPULagValue				Counter,
		zoneAimStatProjectCPUAggregateStatus			INTEGER,
		zoneAimStatProjectCPUMonitor				INTEGER,
		zoneAimStatProjectMemoryMonitor				INTEGER,
		zoneAimStatProjectMonitor				INTEGER,
		zoneAimStatProjectMonPrevConfig				INTEGER,
		zoneAimStatProjectCPUMonPrevConfig			INTEGER,
		zoneAimStatProjectMemoryMonPrevConfig			INTEGER,
		zoneAimStatProjectHostName				DisplayString,
		zoneAimStatProjectHostIndex				DisplayString
	}

zoneAimStatProjectInstanceUID OBJECT-TYPE
	SYNTAX INTEGER (1..2147483647)
