	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Status of the Global Zone."
	::= { zoneAimStatusHostEntry 9 }

zoneAimStatHostStatusAlert OBJECT-TYPE
	SYNTAX INTEGER {
		not-running-warning(1),
		not-running-critical(2),
		not-running-normal(3)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The type of alert to be generated for Host Status- not-running-warning(1),not-running-critical(2),not-running-normal(3)"
	DEFVAL { 2 }
	::= { zoneAimStatusHostEntry 10 }

zoneAimStatHostMonitor OBJECT-TYPE
	SYNTAX INTEGER {
		do-not-monitor(1),
		monitor(2)
	}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Flag indicating whether the Host has to be monitored(2) or not(1)"
	DEFVAL { 2 }
	::= { zoneAimStatusHostEntry 11 }

