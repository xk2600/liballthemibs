zoneAimStatusHostTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ZoneAimStatusHostRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A table containing the Host Status for all Agent Instances."
	::= { zoneAimStatHostGroup 1 }

zoneAimStatusHostEntry OBJECT-TYPE
	SYNTAX ZoneAimStatusHostRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The Host Status Agent Instances Table entry."
	INDEX { zoneAimStatHostInstanceUID }
	::= { zoneAimStatusHostTable 1 }

ZoneAimStatusHostRow ::=
	SEQUENCE {
		zoneAimStatHostInstanceUID			INTEGER,
