		zoneAimStatVNICPktRcvd	 			Counter,
		zoneAimStatVNICPktTxtPS	 			INTEGER,
		zoneAimStatVNICPktRcvdPs 			INTEGER,
		zoneAimStatVNICKBTxt	 			INTEGER,
		zoneAimStatVNICKBRcvd	 			INTEGER,
		zoneAimStatVNICKBPSTxt	 			INTEGER,
		zoneAimStatVNICKBPSRcvd	 			INTEGER,
		zoneAimStatVNICIfSpeed	 			INTEGER,
