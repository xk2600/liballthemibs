zoneAimConfigGeneralAgentName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The name of the agent."
	::= { zoneAimConfigGroup 1 }

zoneAimConfigGeneralAgentVersion OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The currently installed version of the agent."
	::= { zoneAimConfigGroup 2 }

zoneAimConfigGeneralPollInterval OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The time (in seconds) between polling by the Agent. Valid values are greater than or equal to 120 seconds."
	DEFVAL { 120 }
	::= { zoneAimConfigGroup 3 }

zoneAimConfigGeneralLogLevel OBJECT-TYPE
	SYNTAX INTEGER (0..7)
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"This OID was deprecated and is always -1.
		 Use SystemEDGE Log Level setting instead."
	DEFVAL { 2 }
	::= { zoneAimConfigGroup 4 }

zoneAimConfigGeneralTable OBJECT-TYPE
	SYNTAX SEQUENCE OF ZoneAimConfigGeneralRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"A table containing the Zone Config for all Agent Instances."
	::= { zoneAimConfigGroup 5 }

zoneAimConfigGeneralEntry OBJECT-TYPE
	SYNTAX ZoneAimConfigGeneralRow
	ACCESS not-accessible
	STATUS mandatory
	DESCRIPTION
		"The Zone Config Agent Instances Table entry."
	INDEX { zoneAimConfigInstanceUID }
	::= { zoneAimConfigGeneralTable 1 }

ZoneAimConfigGeneralRow ::=
	SEQUENCE {
		zoneAimConfigInstanceUID			INTEGER,
		zoneAimConfigInstanceName			DisplayString,
		zoneAimConfigGeneralColdStartTime		TimeTicks,
		zoneAimConfigGeneralAgentHostName		DisplayString,
		zoneAimConfigGeneralZoneHostID			DisplayString,
		zoneAimConfigGeneralWRSLastTime			TimeTicks,
		zoneAimConfigEventCount				INTEGER,
		zoneAimConfigEventCountMax			INTEGER,
		zoneAimConfigEventHistory			INTEGER,
		zoneAimConfigEventClear				INTEGER
		}

zoneAimConfigInstanceUID OBJECT-TYPE
	SYNTAX INTEGER (1..2147483647)
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Integer UID of Zone Instance."
	::= { zoneAimConfigGeneralEntry  1}

zoneAimConfigInstanceName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Name of the Zone Instance"
	::= { zoneAimConfigGeneralEntry  2}

zoneAimConfigGeneralColdStartTime OBJECT-TYPE
	SYNTAX TimeTicks
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The date and time that the agent was last started."
	::= { zoneAimConfigGeneralEntry 3 }

zoneAimConfigGeneralAgentHostName OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The host name on which this agent is running."
	::= { zoneAimConfigGeneralEntry 4 }

zoneAimConfigGeneralZoneHostID OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"The host Index which is being monitored by this instance of the agent."
	::= { zoneAimConfigGeneralEntry 5 }

zoneAimConfigGeneralWRSLastTime OBJECT-TYPE
	SYNTAX TimeTicks
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The time, when WRS last fetched the event table."
	::= { zoneAimConfigGeneralEntry 6 }

zoneAimConfigEventCount OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-only
	STATUS mandatory
	DESCRIPTION
		"Number of events in event table."
	::= {zoneAimConfigGeneralEntry 7}

zoneAimConfigEventCountMax OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The maximum number of events to be stored in the event table, after which the rows start getting overwritten."
	DEFVAL { 500 }
	::= {zoneAimConfigGeneralEntry 8}

zoneAimConfigEventHistory OBJECT-TYPE
	SYNTAX INTEGER
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"The number of days the events are maintained. The value is in the number of days."
	DEFVAL {30}
	::= {zoneAimConfigGeneralEntry 9}
	
zoneAimConfigEventClear OBJECT-TYPE
	SYNTAX INTEGER {
			do-not-clear(1),
           		clear(2)
			}
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"Flag indicating whether to clear all the events in the zoneAimEventTable or not. 1)DO_NOT_CLEAR 2)CLEAR."
	DEFVAL { 1 }	
	::= {zoneAimConfigGeneralEntry 10}

zoneAimConfigGeneralCIMComputerSystem OBJECT-TYPE
	SYNTAX DisplayString
	ACCESS read-write
	STATUS mandatory
	DESCRIPTION
		"CIM Computer System Object that is running the AIM."
	::= { zoneAimConfigGroup 6 }



