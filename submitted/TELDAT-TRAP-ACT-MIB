        Message: 'IP:$1 CD:$2 Sr:$3 Bus:$4 IMSI:$5 Wst:$6 St:$7 Syt:$8 Prx:$9 Ptx:$10 Wrx:$11 Wtx:$12 Pg:$13'

        Parameters list:
              $1: act1_pppip
