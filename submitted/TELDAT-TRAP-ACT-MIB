TELDAT-TRAP-ACT-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


actTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the ACT event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 245 }


actEventsGroup	OBJECT IDENTIFIER	::= { actTrapsMIB 1 }

actNotifications	OBJECT IDENTIFIER	::= { actTrapsMIB 2 }
actNotificationsPrefix	OBJECT IDENTIFIER	::= { actNotifications 0 }

actEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ACT.1
        Sending custom trap to management station"
    ::= { actEventsGroup 1 }

