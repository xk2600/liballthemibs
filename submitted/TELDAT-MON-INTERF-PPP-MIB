    telProdNpMonIfcPPPCrtpStClear OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is readed shows the output from the last counter
        restart attempt: the value is 2 (ok) if the restart was performed
        succesfully, 3 (error) if an error ocurred and 4 (undefined) if nobody
        has tried to restart the counters yet. In set operations, the value 1
        (it is the only value that can be assigned by a writing operation)
        causes a counter restart."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 2 }

    telProdNpMonIfcPPPCrtpStOutPackets OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of outbound RTP packets."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 3 }

    telProdNpMonIfcPPPCrtpStOutComprPkts OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of outbound compressed RTP packets."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 4 }

    telProdNpMonIfcPPPCrtpStSearches OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of searchs of the connection state."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 5 }

    telProdNpMonIfcPPPCrtpStNoConnState OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Times the connection state could not be found."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 6 }

    telProdNpMonIfcPPPCrtpStInUncomPkts OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of inbound uncompressed RTP packets."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 7 }

    telProdNpMonIfcPPPCrtpStInComprPkts OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of inbound compressed RTP packets."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 8 }

    telProdNpMonIfcPPPCrtpStInUnkTypePkts OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of inbound RTP packets classified as unkown."
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 9 }

    telProdNpMonIfcPPPCrtpStInContextStPkts OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of inbound RTP Context State packets"
        ::= { telProdNpMonIfcPPPCrtpStatsEntry 10 }

-- Grupo de comandos: CLEAR y RESET-LINK.

    telProdNpMonIfcPPPCommandGroup OBJECT IDENTIFIER ::= { telProdNpMonInterfPPP 5 }

    -- Tabla que sirve para ejecutar el comando CLEAR en los distintos
    -- posibles interfaces.

    telProdNpMonIfcPPPCommClearTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPCommClearEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Table of CLEAR commands for different interfaces. The entry number is
        determined by the interface number and indicates the interface type to
        which this command is applied."
        ::= { telProdNpMonIfcPPPCommandGroup 1 }

    telProdNpMonIfcPPPCommClearEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPCommClearEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the interface number, that is the table index, and
        indicates the interface the CLEAR command is applied on, a variable to
        perform the CLEAR operation: restart interface statistics, that when is
        readed indicates the output of the last CLEAR operation."
        INDEX { telProdNpMonIfcPPPCommClearIndex }

        ::= { telProdNpMonIfcPPPCommClearTable 1 }

    TelProdNpMonIfcPPPCommClearEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPCommClearIndex
                INTEGER,
            telProdNpMonIfcPPPCommClear
                INTEGER
            }

    telProdNpMonIfcPPPCommClearIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPCommClearEntry 1 }

    telProdNpMonIfcPPPCommClear OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is readed shows the output from the last attempt to
        restart interface statistics. Possible values are: 2 (ok) if the restart
        was performed succesfully, 3 (error) if an error ocurred and
        4 (undefined) if nobody has tried to restart the table yet. In set
        operations, the value 1 (it is the only value that can be assigned by a
        writing operation) causes statistics to restart."
        ::= { telProdNpMonIfcPPPCommClearEntry 2 }

    -- Tabla que sirve para ejecutar el comando RESET-LINK en los distintos
    -- posibles interfaces.

    telProdNpMonIfcPPPCommResetTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPCommResetEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Table of RESET-LINK commands for different type of interfaces. The
        entry number is determined by the interface number, and indicates
        which interface we are considering."
        ::= { telProdNpMonIfcPPPCommandGroup 2 }

    telProdNpMonIfcPPPCommResetEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPCommResetEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the node interface number, that is the table index,
        and indicates the interface the RESET-LINK command is applied on,
        and a variable to perform the RESET-LINK operation: pull down the PPP
        link and when the variable is readed indicates the output of the last
        operation attempt."
        INDEX { telProdNpMonIfcPPPCommResetIndex }

        ::= { telProdNpMonIfcPPPCommResetTable 1 }

    TelProdNpMonIfcPPPCommResetEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPCommResetIndex
                INTEGER,
            telProdNpMonIfcPPPCommReset
                INTEGER
            }

    telProdNpMonIfcPPPCommResetIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPCommResetEntry 1 }

    telProdNpMonIfcPPPCommReset OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is readed shows the output from the last attempt
        to drop PPP link. Possible values are: 2 (ok) if the operation was
        performed succesfully, 3 (error) if an error ocurred and 4 (undefined)
        if nobody has tried to drop the link yet. In set operations, the value
        1 (it is the only value that can be assigned by a writing operation)
        implies the PPP link is going to be dropped."
        ::= { telProdNpMonIfcPPPCommResetEntry 2 }

-- Grupo de parﬂmetros relacionados con el CCP.

    telProdNpMonIfcPPPCCPGroup OBJECT IDENTIFIER ::= { telProdNpMonInterfPPP 6 }

    -- Tabla de parﬂmetros globales del CCP. Contiene informacin del estado de
    -- negociacion de parﬂmetros CCP.

    telProdNpMonIfcPPPCCPControlGloTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPCCPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains CCP global parameters. This table has an entry for
        each PPP interface. The entry number is determined by the corresponding
        interface number (table index)."
        ::= { telProdNpMonIfcPPPCCPGroup 1 }

    telProdNpMonIfcPPPCCPControlGloEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPCCPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the node interface number which is the table index,
        and variables that shows if the CCP protocol is enabled or disabled, and
        the current state of the CCP protocol."
        INDEX { telProdNpMonIfcPPPCCPCtrlGloIndex }

        ::= { telProdNpMonIfcPPPCCPControlGloTable 1 }

    TelProdNpMonIfcPPPCCPControlGloEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPCCPCtrlGloIndex
                INTEGER,
            telProdNpMonIfcPPPCCPCtrlGloCCP
                INTEGER,
            telProdNpMonIfcPPPCCPCtrlGloState
                INTEGER
            }

    telProdNpMonIfcPPPCCPCtrlGloIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPCCPControlGloEntry 1 }

    telProdNpMonIfcPPPCCPCtrlGloCCP OBJECT-TYPE
        SYNTAX INTEGER{
            enabled(1),
            disabled(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if the CCP protocol is enabled or disabled."
        ::= { telProdNpMonIfcPPPCCPControlGloEntry 2 }

    telProdNpMonIfcPPPCCPCtrlGloState OBJECT-TYPE
        SYNTAX INTEGER{
            initial(0),
            starting(1),
            closed(2),
            stopped(3),
            closing(4),
            stopping(5),
            req_sent(6),
            ack_rcvd(7),
            ack_sent(8),
            opened(9)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Current protocol state. The possible values of the variable are integer
        numbers between 0 and 9. Each number is related to a feasible state:
        (0) initial, (1) starting, (2) closed, (3) stopped, (4) closing,
        (5) stopping, (6) req_sent, (7) ack_rcvd, (8) ack_sent, (9) opened."
        ::= { telProdNpMonIfcPPPCCPControlGloEntry 3 }

