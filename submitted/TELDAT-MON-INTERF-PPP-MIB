                Counter,
            telProdNpMonIfcPPPCCPStConfRejRx
                Counter,
            telProdNpMonIfcPPPCCPStConfRejTx
                Counter,
            telProdNpMonIfcPPPCCPStTermReqRx
                Counter,
            telProdNpMonIfcPPPCCPStTermReqTx
                Counter,
            telProdNpMonIfcPPPCCPStTermAckRx
                Counter,
            telProdNpMonIfcPPPCCPStTermAckTx
                Counter,
            telProdNpMonIfcPPPCCPStProcFrsRx
                Counter,
            telProdNpMonIfcPPPCCPStProcFrsTx
                Counter,
            telProdNpMonIfcPPPCCPStProcBytRx
                Counter,
            telProdNpMonIfcPPPCCPStProcBytTx
                Counter,
            telProdNpMonIfcPPPCCPStFramesRx
                Counter,
            telProdNpMonIfcPPPCCPStFramesTx
                Counter,
