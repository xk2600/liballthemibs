 TELDAT-MON-INTERF-PPP-MIB DEFINITIONS ::= BEGIN

    IMPORTS
            IpAddress, Counter
        FROM RFC1155-SMI
            OBJECT-TYPE
        FROM RFC-1212
            DisplayString, PhysAddress
        FROM RFC1213-MIB
            telProdNpMonInterfRouter
        FROM TELDAT-SW-STRUCTURE-MIB;


    telProdNpMonInterfPPP   OBJECT IDENTIFIER ::= { telProdNpMonInterfRouter 10 }

-- Grupo telProdNpMonInterfPPP

-- Incluye los parﬂmetros referentes a los interfaces PPP.

-- Grupo de parﬂmetros relacionados con el LCP.

    telProdNpMonIfcPPPLCPGroup OBJECT IDENTIFIER ::= { telProdNpMonInterfPPP 1 }

    -- Tabla de parﬂmetros globales del LCP. Contiene informacin del estado de
    -- negociacin de parﬂmetros LCP.

    telProdNpMonIfcPPPLCPControlGloTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPLCPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains global LCP parameters. The entry number is
        determined by the PPP interface number, the lower interface number and
        the order number of the link inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPGroup 1 }

    telProdNpMonIfcPPPLCPControlGloEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPLCPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the PPP interface number of the node, the lower
        interface number, the order number of the link inside the lower
        interface, a description of the lower interface, the circuit identifier,
        the version number and the current state of the LCP protocol."
        INDEX { telProdNpMonIfcPPPLCPCtrlGloPPPIndex, telProdNpMonIfcPPPLCPCtrlGloLowIndex,
                telProdNpMonIfcPPPLCPCtrlGloOrdIndex }

        ::= { telProdNpMonIfcPPPLCPControlGloTable 1 }

    TelProdNpMonIfcPPPLCPControlGloEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPLCPCtrlGloPPPIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlGloLowIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlGloOrdIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlGloLowDescr
                DisplayString,
            telProdNpMonIfcPPPLCPCtrlGloCircId
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlGloVersion
                DisplayString,
            telProdNpMonIfcPPPLCPCtrlGloState
                INTEGER
            }

    telProdNpMonIfcPPPLCPCtrlGloPPPIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "PPP interface number of the node (first index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 1 }

    telProdNpMonIfcPPPLCPCtrlGloLowIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Lower interface number of the node (second index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 2 }

    telProdNpMonIfcPPPLCPCtrlGloOrdIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Order number of the link inside the lower interface corresponding to
        the LCP parameters of the entry (third index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 3 }

    telProdNpMonIfcPPPLCPCtrlGloLowDescr OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "A description of the lower interface corresponding to this entry."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 4 }

    telProdNpMonIfcPPPLCPCtrlGloCircId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Circuit identifier inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 5 }

    telProdNpMonIfcPPPLCPCtrlGloVersion OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Version number."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 6 }

    telProdNpMonIfcPPPLCPCtrlGloState OBJECT-TYPE
        SYNTAX INTEGER{
            initial(0),
            starting(1),
            closed(2),
            stopped(3),
            closing(4),
            stopping(5),
            req_sent(6),
            ack_rcvd(7),
            ack_sent(8),
            opened(9)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Current protocol state. The possible values of the variable are integer
        numbers between 0 and 9. Each number is related to a feasible state:
        (0) initial, (1) starting, (2) closed, (3) stopped, (4) closing,
        (5) stopping, (6) req_sent, (7) ack_rcvd, (8) ack_sent, (9) opened."
        ::= { telProdNpMonIfcPPPLCPControlGloEntry 7 }

    -- Tabla que contiene los parﬂmetros que permiten monitorizar las opciones
    -- transmitidas y recibidas en el establecimiento del enlace.

    telProdNpMonIfcPPPLCPControlOptionsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPLCPControlOptionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains variables that give information about the
        transmitted and received options during the link setup. The entry number
        is determined by the PPP interface number, the lower interface number
        and the order number of the link inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPGroup 2 }

    telProdNpMonIfcPPPLCPControlOptionsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPLCPControlOptionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the PPP interface number, the lower interface
        number, the order number of the link inside the lower interface, a
        description of the lower interface, the circuit identifier, the local
        and remote MRU, the local and remote Asynchronous Control Character Map,
        the authentication type (if it exists) in the local and remote site, the
        local and remote Magic Number, if Protocol Field Compression has been
        used in the local or remote end, if Address/Control Field Compression
        has been used in the local or remote end, and if 32 bits Checksum has
        been used in the local or remote end."
        INDEX { telProdNpMonIfcPPPLCPCtrlOptPPPIndex, telProdNpMonIfcPPPLCPCtrlOptLowIndex,
                telProdNpMonIfcPPPLCPCtrlOptOrdIndex }

        ::= { telProdNpMonIfcPPPLCPControlOptionsTable 1 }

    TelProdNpMonIfcPPPLCPControlOptionsEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPLCPCtrlOptPPPIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptLowIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptOrdIndex
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptLowDescr
                DisplayString,
            telProdNpMonIfcPPPLCPCtrlOptCircId
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptMRULoc
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptMRURem
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptAsyCharMaskLoc
                OCTET STRING,
            telProdNpMonIfcPPPLCPCtrlOptAsyCharMaskRem
                OCTET STRING,
            telProdNpMonIfcPPPLCPCtrlOptAuthLoc
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptAuthRem
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptMagNumLoc
                DisplayString,
            telProdNpMonIfcPPPLCPCtrlOptMagNumRem
                DisplayString,
            telProdNpMonIfcPPPLCPCtrlOptProtCmpLoc
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptProtCmpRem
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptAddCmpLoc
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptAddCmpRem
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptChecksumLoc
                INTEGER,
            telProdNpMonIfcPPPLCPCtrlOptChecksumRem
                INTEGER
            }

    telProdNpMonIfcPPPLCPCtrlOptPPPIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "PPP interface number of the node (first index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 1 }

    telProdNpMonIfcPPPLCPCtrlOptLowIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Lower interface number of the node (second index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 2 }

    telProdNpMonIfcPPPLCPCtrlOptOrdIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Order number of the link inside the lower interface corresponding to
        the LCP parameters of the entry (third index of the entry)."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 3 }

    telProdNpMonIfcPPPLCPCtrlOptLowDescr OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "A description of the lower interface corresponding to this entry."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 4 }

    telProdNpMonIfcPPPLCPCtrlOptCircId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Circuit identifier inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 5 }

    telProdNpMonIfcPPPLCPCtrlOptMRULoc OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "MRU (Maximum Received Unit) in the local end. It represents the PPP
        frame maximum size that is accepted in reception."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 6 }

    telProdNpMonIfcPPPLCPCtrlOptMRURem OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "MRU (Maximum Received Unit) in the remote end. It represents the PPP
        frame maximum size that is accepted in reception."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 7 }

    telProdNpMonIfcPPPLCPCtrlOptAsyCharMaskLoc OBJECT-TYPE
        SYNTAX OCTET STRING(SIZE(4))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Local Asynchronous Control Character Map. It shows the mask to use if
        transparent transmission is applied over control characters (XON, XOFF)
        for asynchronous PPP. If characters are inside the frame, they won≥t
        cause the activation of flow control process in modems or adapters."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 8 }

    telProdNpMonIfcPPPLCPCtrlOptAsyCharMaskRem OBJECT-TYPE
        SYNTAX OCTET STRING(SIZE(4))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Remote Asynchronous Control Character Map. It shows the mask to use if
        transparent transmission is applied over control characters (XON, XOFF)
        for asynchronous PPP. If characters are inside the frame, they won≥t
        cause the activation of flow control process in modems or adapters."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 9 }

    telProdNpMonIfcPPPLCPCtrlOptAuthLoc OBJECT-TYPE
        SYNTAX INTEGER{
            none(0),
            pap(1),
            chap(2),
            other(3)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Local authentication type used. Possible options are: PAP (1), CHAP (2),
        a different one (3) or no authentication used (0)."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 10 }

    telProdNpMonIfcPPPLCPCtrlOptAuthRem OBJECT-TYPE
        SYNTAX INTEGER{
            none(0),
            pap(1),
            chap(2),
            other(3)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Remote authentication type used. Possible options are: PAP (1),
        CHAP (2), a different one (3) or no authentication used (0)."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 11 }

    telProdNpMonIfcPPPLCPCtrlOptMagNumLoc OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Local magic number. It is useful to determine if there is a loop in the
        link by transmitting a random number between the ends based on the
        system clock and the times the clock has reset. The magic number has
        4 bytes. This variable is a string that shows an hexadecimal value."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 12 }

    telProdNpMonIfcPPPLCPCtrlOptMagNumRem OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Remote magic number. It is useful to determine if there is a loop in
        the link by transmitting a random number between the ends based on the
        system clock and the times the clock has reset. The magic number has
        4 bytes. This variable is a string that shows an hexadecimal value."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 13 }

    telProdNpMonIfcPPPLCPCtrlOptProtCmpLoc OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if Protocol Field Compression is used at the
        local end. Possible values are: (1) yes or (2) no. Once LCP has been
        negotiated and NCP has been established, the PPP frame protocol field
        is compressed and a byte is transmitted."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 14 }

    telProdNpMonIfcPPPLCPCtrlOptProtCmpRem OBJECT-TYPE
        SYNTAX INTEGER{
            non_negotiated(0),
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if Protocol Field Compression is used at the
        remote end. Possible values are: (0) non negotiated, (1) yes or (2) no.
        Once LCP has been negotiated and NCP has been established, the PPP frame
        protocol field is compressed and a byte is transmitted."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 15 }

    telProdNpMonIfcPPPLCPCtrlOptAddCmpLoc OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if Address/Control Field Compression is used at
        the local end. Possible values are: (1) yes or (2) no. Once LCP has been
        negotiated and NCP has been established, the control and address fields
        of the PPP frame are not transmitted."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 16 }

    telProdNpMonIfcPPPLCPCtrlOptAddCmpRem OBJECT-TYPE
        SYNTAX INTEGER{
            non_negotiated(0),
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if Address/Control Field Compression is used at
        the remote end. Possible values are: (0) non negotiated, (1) yes or
        (2) no. Once LCP has been negotiated and NCP has been established, the
        control and address fields of the PPP frame are not transmitted."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 17 }

    telProdNpMonIfcPPPLCPCtrlOptChecksumLoc OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if 32 bits Checksum is used at the local end.
        Possible values are: (1) yes or (2) no."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 18 }

    telProdNpMonIfcPPPLCPCtrlOptChecksumRem OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates if 32 bits Checksum is used at the remote end.
        Possible values are: (1) yes or (2) no."
        ::= { telProdNpMonIfcPPPLCPControlOptionsEntry 19 }

    -- Tabla que contiene los estadçsticos correspondientes al protocolo LCP.

    telProdNpMonIfcPPPLCPStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPLCPStatsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains statistics of the LCP protocol. The entry number is
        determined by the PPP interface number, the lower interface number and
        the order number of the link inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPGroup 3 }

    telProdNpMonIfcPPPLCPStatsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPLCPStatsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the PPP interface number, the lower interface
        number, the order number of the link inside the lower interface, a
        description of the lower interface, the circuit identifier, a variable
        to check variables deletion (corresponding counters reset), and
        variables that give information about the following counters values:
        sent and received frames number, sent and received bytes number, number
        of sent and received Configure Request LCP frames, number of sent and
        received Configure Ack LCP frames, number of sent and received
        Configure Nak LCP frames, number of sent and received Configure Reject
        LCP frames, number of sent and received Terminate Request LCP frames,
        number of sent and received Terminate Ack LCP frames, number of sent
        and received Echo Request LCP frames, number of sent and received
        Echo Reply LCP frames."
        INDEX { telProdNpMonIfcPPPLCPStPPPIndex, telProdNpMonIfcPPPLCPStLowIndex,
                telProdNpMonIfcPPPLCPStOrdIndex }

        ::= { telProdNpMonIfcPPPLCPStatsTable 1 }

    TelProdNpMonIfcPPPLCPStatsEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPLCPStPPPIndex
                INTEGER,
            telProdNpMonIfcPPPLCPStLowIndex
                INTEGER,
            telProdNpMonIfcPPPLCPStOrdIndex
                INTEGER,
            telProdNpMonIfcPPPLCPStLowDescr
                DisplayString,
            telProdNpMonIfcPPPLCPStCircId
                INTEGER,
            telProdNpMonIfcPPPLCPStClear
                INTEGER,
            telProdNpMonIfcPPPLCPStFramesRx
                Counter,
            telProdNpMonIfcPPPLCPStFramesTx
                Counter,
            telProdNpMonIfcPPPLCPStBytesRx
                Counter,
            telProdNpMonIfcPPPLCPStBytesTx
                Counter,
            telProdNpMonIfcPPPLCPStConfReqRx
                Counter,
            telProdNpMonIfcPPPLCPStConfReqTx
                Counter,
            telProdNpMonIfcPPPLCPStConfAckRx
                Counter,
            telProdNpMonIfcPPPLCPStConfAckTx
                Counter,
            telProdNpMonIfcPPPLCPStConfNakRx
                Counter,
            telProdNpMonIfcPPPLCPStConfNakTx
                Counter,
            telProdNpMonIfcPPPLCPStConfRejRx
                Counter,
            telProdNpMonIfcPPPLCPStConfRejTx
                Counter,
            telProdNpMonIfcPPPLCPStTermReqRx
                Counter,
            telProdNpMonIfcPPPLCPStTermReqTx
                Counter,
            telProdNpMonIfcPPPLCPStTermAckRx
                Counter,
            telProdNpMonIfcPPPLCPStTermAckTx
                Counter,
            telProdNpMonIfcPPPLCPStEchoReqRx
                Counter,
            telProdNpMonIfcPPPLCPStEchoReqTx
                Counter,
            telProdNpMonIfcPPPLCPStEchoRepRx
                Counter,
            telProdNpMonIfcPPPLCPStEchoRepTx
                Counter
            }

    telProdNpMonIfcPPPLCPStPPPIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "PPP interface number of the node (first index of the entry)."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 1 }

    telProdNpMonIfcPPPLCPStLowIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Lower interface number of the node (second index of the entry)."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 2 }

    telProdNpMonIfcPPPLCPStOrdIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Order number of the link inside the lower interface corresponding to
        the LCP parameters of the entry (third index of the entry)."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 3 }

    telProdNpMonIfcPPPLCPStLowDescr OBJECT-TYPE
        SYNTAX DisplayString
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "A description of the lower interface corresponding to this entry."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 4 }

    telProdNpMonIfcPPPLCPStCircId OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Circuit identifier inside the lower interface."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 5 }

    telProdNpMonIfcPPPLCPStClear OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is readed shows the output from the last counter
        restart attempt: the value is 2 (ok) if the restart was performed
        succesfully, 3 (error) if an error ocurred and 4 (undefined) if nobody
        has tried to restart the counters yet. In set operations, the value 1
        (it is the only value that can be assigned by a writing operation)
        causes a counter restart."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 6 }

    telProdNpMonIfcPPPLCPStFramesRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of frames received through the link shown in the indexes."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 7 }

    telProdNpMonIfcPPPLCPStFramesTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of frames sent through the link shown in the indexes."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 8 }

    telProdNpMonIfcPPPLCPStBytesRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of bytes received through the link shown in the indexes."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 9 }

    telProdNpMonIfcPPPLCPStBytesTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of bytes sent through the link shown in the indexes."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 10 }

    telProdNpMonIfcPPPLCPStConfReqRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Request LCP frames received through the link shown
        in the indexes. These frames are transmitted when a link is started and
        contains link configuration options."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 11 }

    telProdNpMonIfcPPPLCPStConfReqTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Request LCP frames sent through the link shown in
        the indexes. These frames are transmitted when a link is started and
        contains link configuration options."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 12 }

    telProdNpMonIfcPPPLCPStConfAckRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Ack LCP frames received through the link shown in
        the indexes. These frames are answers to Configure Requests and shows
        accordance with the received configuration options."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 13 }

    telProdNpMonIfcPPPLCPStConfAckTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Ack LCP frames transmitted through the link shown
        in the indexes. These frames are answers to Configure Requests and shows
        accordance with the received configuration options."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 14 }

    telProdNpMonIfcPPPLCPStConfNakRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Nak LCP frames received through the link shown in
        the indexes. These frames are answers to Configure Requests and
        indicates that some configuration options received in the identified
        frame has not been accepted, nevertheless a remote reccomended or
        accepted value is sent."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 15 }

    telProdNpMonIfcPPPLCPStConfNakTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Nak LCP frames transmitted through the link shown
        in the indexes. These frames are answers to Configure Requests and
        indicates that some configuration options received in the identified
        frame has not been accepted, nevertheless a remote reccomended or
        accepted value is sent."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 16 }

    telProdNpMonIfcPPPLCPStConfRejRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Reject LCP frames received through the link shown
        in the indexes. These frames are answers to Configure Requests and
        indicates that some configuration options received in the identified
        frame has not neither accepted nor recognized."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 17 }

    telProdNpMonIfcPPPLCPStConfRejTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Configure Reject LCP frames transmitted through the link
        shown in the indexes. These frames are answers to Configure Requests
        and indicates that some configuration options received in the
        identified frame has not neither accepted nor recognized."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 18 }

    telProdNpMonIfcPPPLCPStTermReqRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Terminate Request LCP frames received through the link shown
        in the indexes. These frames are transmitted to finish, close, the
        link."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 19 }

    telProdNpMonIfcPPPLCPStTermReqTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Terminate Request LCP frames transmitted through the link
        shown in the indexes. These frames are transmitted to finish, close,
        the link."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 20 }

    telProdNpMonIfcPPPLCPStTermAckRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Terminate Ack LCP frames received through the link shown in
        the indexes. These frames are transmitted after a Terminate Request is
        received. The reception of a non expected Terminate Ack packet indicates
        that the link has been closed."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 21 }

    telProdNpMonIfcPPPLCPStTermAckTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Terminate Ack LCP frames transmitted through the link shown
        in the indexes. These frames are transmitted after a Terminate Request
        is received. The reception of a non expected Terminate Ack packet
        indicates that the link has been closed."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 22 }

    telProdNpMonIfcPPPLCPStEchoReqRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Echo Request LCP frames received through the link shown in
        the indexes. These frames are used in link maintenance. From time to
        time a query containing Echo Request code is generated, and is sent
        back with an Echo Reply."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 23 }

    telProdNpMonIfcPPPLCPStEchoReqTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Echo Request LCP frames transmitted through the link shown in
        the indexes. These frames are used in link maintenance. From time to
        time a query containing Echo Request code is generated, and is sent
        back with an Echo Reply."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 24 }

    telProdNpMonIfcPPPLCPStEchoRepRx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Echo Reply LCP frames received through the link shown in the
        indexes. These frames are used in link maintenance. From time to time a
        query containing Echo Request code is generated, and is sent back with
        an Echo Reply."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 25 }

    telProdNpMonIfcPPPLCPStEchoRepTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of Echo Reply LCP frames transmitted through the link shown in
        the indexes. These frames are used in link maintenance. From time to
        time a query containing Echo Request code is generated, and is sent
        back with an Echo Reply."
        ::= { telProdNpMonIfcPPPLCPStatsEntry 26 }

-- Grupo de parﬂmetros relacionados con el IPCP.

    telProdNpMonIfcPPPIPCPGroup OBJECT IDENTIFIER ::= { telProdNpMonInterfPPP 2 }

    -- Tabla que contiene informacin sobre el estado actual del IPCP.

    telProdNpMonIfcPPPIPCPControlGloTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPIPCPControlGloEntry
        ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
        "This table contains IPCP global parameters. This table has an entry for
        each PPP interface of the node. The entry number is determined by the
        corresponding interface number (table index)."
        ::= { telProdNpMonIfcPPPIPCPGroup 1 }

    telProdNpMonIfcPPPIPCPControlGloEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPIPCPControlGloEntry
        ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
        "Each entry contains the node interface number which is the table index,
        and a variable that shows the current state of the IPCP protocol."
        INDEX { telProdNpMonIfcPPPIPCPCtrlGloIndex }

        ::= { telProdNpMonIfcPPPIPCPControlGloTable 1 }

    TelProdNpMonIfcPPPIPCPControlGloEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPIPCPCtrlGloIndex
                INTEGER,
            telProdNpMonIfcPPPIPCPCtrlGloState
                INTEGER
            }

    telProdNpMonIfcPPPIPCPCtrlGloIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPIPCPControlGloEntry 1 }

    telProdNpMonIfcPPPIPCPCtrlGloState OBJECT-TYPE
        SYNTAX INTEGER{
            initial(0),
            starting(1),
            closed(2),
            stopped(3),
            closing(4),
            stopping(5),
            req_sent(6),
            ack_rcvd(7),
            ack_sent(8),
            opened(9)
            }
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "Current protocol state. The possible values of the variable are integer
        numbers between 0 and 9. Each number is related to a feasible state:
        (0) initial, (1) starting, (2) closed, (3) stopped, (4) closing,
        (5) stopping, (6) req_sent, (7) ack_rcvd, (8) ack_sent, (9) opened."
        ::= { telProdNpMonIfcPPPIPCPControlGloEntry 2 }

    -- Tabla que contiene los parﬂmetros que permiten monitorizar las opciones
    -- transmitidas y recibidas en el establecimiento del protocolo de nivel
    -- de RED.

    telProdNpMonIfcPPPIPCPControlOptionsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPIPCPControlOptionsEntry
        ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
        "This table contains information about parameters used to monitor
        received and transmitted options during the network level protocol
        setup. The entry number is determined by the interface value (table
        index)."
        ::= { telProdNpMonIfcPPPIPCPGroup 2 }

    telProdNpMonIfcPPPIPCPControlOptionsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPIPCPControlOptionsEntry
        ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
        "Each entry contains the node interface number, which is the table
        index, the assigned IP address (in case of dinamic asignation), remote
        end IP address (if received) and if the Van Jacobson compression option
        has been selected at any end (local or remote)."
        INDEX { telProdNpMonIfcPPPIPCPCtrlOptIndex }

        ::= { telProdNpMonIfcPPPIPCPControlOptionsTable 1 }

    TelProdNpMonIfcPPPIPCPControlOptionsEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPIPCPCtrlOptIndex
                INTEGER,
            telProdNpMonIfcPPPIPCPCtrlOptIpAddrLoc
                IpAddress,
            telProdNpMonIfcPPPIPCPCtrlOptIpAddrRem
                IpAddress,
            telProdNpMonIfcPPPIPCPCtrlOptVJCmpLoc
                INTEGER,
            telProdNpMonIfcPPPIPCPCtrlOptVJCmpRem
                INTEGER
            }

    telProdNpMonIfcPPPIPCPCtrlOptIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPIPCPControlOptionsEntry 1 }

    telProdNpMonIfcPPPIPCPCtrlOptIpAddrLoc OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "The IP address assigned (supposing dinamic assignation has been
        selected)."
        ::= { telProdNpMonIfcPPPIPCPControlOptionsEntry 2 }

    telProdNpMonIfcPPPIPCPCtrlOptIpAddrRem OBJECT-TYPE
        SYNTAX IpAddress
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "The remote IP address (supposing it has been received)."
        ::= { telProdNpMonIfcPPPIPCPControlOptionsEntry 3 }

    telProdNpMonIfcPPPIPCPCtrlOptVJCmpLoc OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "Van Jacobson compression selected or not in the local end: (1) yes,
        (2) no."
        ::= { telProdNpMonIfcPPPIPCPControlOptionsEntry 4 }

    telProdNpMonIfcPPPIPCPCtrlOptVJCmpRem OBJECT-TYPE
        SYNTAX INTEGER{
            non_negotiated(0),
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS obsolete
        DESCRIPTION
        "Van Jacobson compression selected or not in the remote end: (0) non
        negotiated, (1) yes, (2) no."
        ::= { telProdNpMonIfcPPPIPCPControlOptionsEntry 5 }

    -- Tabla que contiene los estadçsticos correspondientes al protocolo IPCP.

    telProdNpMonIfcPPPIPCPStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPIPCPStatsEntry
        ACCESS not-accessible
        STATUS obsolete
        DESCRIPTION
