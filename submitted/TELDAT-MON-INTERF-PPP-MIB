            DESCRIPTION
            "This table contains statistics of the BCP protocol. The entry number
            is determined by the interface number (table index)."
            ::= { telProdNpMonIfcPPPNCPBCPGroup 3 }

        telProdNpMonIfcPPPNCPBCPStatsEntry OBJECT-TYPE
            SYNTAX TelProdNpMonIfcPPPNCPBCPStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
            "Each entry contains the node interface number, which is the table
            index, a variable to check variables deletion (corresponding counters
            reset), and information about the following counters values: number of
            sent and received Configure Request NCP frames, number of sent and
            received Configure Ack NCP frames, number of sent and received Configure
            Nak NCP frames, number of sent and received Configure Reject NCP frames,
            number of sent and received Terminate Request NCP frames, number of sent
            and received Terminate Ack NCP frames."
            INDEX { telProdNpMonIfcPPPNCPBCPStIndex }

            ::= { telProdNpMonIfcPPPNCPBCPStatsTable 1 }

        TelProdNpMonIfcPPPNCPBCPStatsEntry ::=
            SEQUENCE{
                telProdNpMonIfcPPPNCPBCPStIndex
                    INTEGER,
                telProdNpMonIfcPPPNCPBCPStClear
                    INTEGER,
                telProdNpMonIfcPPPNCPBCPStConfReqRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfReqTx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfAckRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfAckTx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfNakRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfNakTx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfRejRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStConfRejTx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStTermReqRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStTermReqTx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStTermAckRx
                    Counter,
                telProdNpMonIfcPPPNCPBCPStTermAckTx
                    Counter
                }

        telProdNpMonIfcPPPNCPBCPStIndex OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Interface number of the node, that indicates the corresponding table
            entry (it is the table index)."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 1 }

        telProdNpMonIfcPPPNCPBCPStClear OBJECT-TYPE
            SYNTAX INTEGER{
                delete(1),
                ok(2),
                error(3),
                undefined(4)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "This variable when is readed shows the output from the last counter
            restart attempt: the value is 2 (ok) if the restart was performed
            succesfully, 3 (error) if an error ocurred and 4 (undefined) if nobody
            has tried to restart the counters yet. In set operations, the value 1
            (it is the only value that can be assigned by a writing operation)
            causes a counter restart."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 2 }

        telProdNpMonIfcPPPNCPBCPStConfReqRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Request NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 3 }

        telProdNpMonIfcPPPNCPBCPStConfReqTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Request NCP frames transmitted through the
            interface number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 4 }

        telProdNpMonIfcPPPNCPBCPStConfAckRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Ack NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 5 }

        telProdNpMonIfcPPPNCPBCPStConfAckTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Ack NCP frames transmitted through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 6 }

        telProdNpMonIfcPPPNCPBCPStConfNakRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Nak NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 7 }

        telProdNpMonIfcPPPNCPBCPStConfNakTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Nak NCP frames transmitted through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 8 }

        telProdNpMonIfcPPPNCPBCPStConfRejRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Reject NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 9 }

        telProdNpMonIfcPPPNCPBCPStConfRejTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Reject NCP frames transmitted through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 10 }

        telProdNpMonIfcPPPNCPBCPStTermReqRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Terminate Request NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 11 }

         telProdNpMonIfcPPPNCPBCPStTermReqTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Terminate Request NCP frames transmitted through the
            interface number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 12 }

        telProdNpMonIfcPPPNCPBCPStTermAckRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Terminate Ack NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 13 }

        telProdNpMonIfcPPPNCPBCPStTermAckTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Terminate Ack NCP frames transmitted through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPBCPStatsEntry 14 }

END
