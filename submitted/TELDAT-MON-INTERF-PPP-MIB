                    INTEGER,
                telProdNpMonIfcPPPNCPIPCPCtrlOptIpAddrLoc
                    IpAddress,
                telProdNpMonIfcPPPNCPIPCPCtrlOptIpAddrRem
                    IpAddress,
                telProdNpMonIfcPPPNCPIPCPCtrlOptVJCmpLoc
                    INTEGER,
                telProdNpMonIfcPPPNCPIPCPCtrlOptVJCmpRem
                    INTEGER
                }

        telProdNpMonIfcPPPNCPIPCPCtrlOptIndex OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Interface number of the node, that indicates the corresponding table
            entry (it is the table index)."
            ::= { telProdNpMonIfcPPPNCPIPCPControlOptionsEntry 1 }

        telProdNpMonIfcPPPNCPIPCPCtrlOptIpAddrLoc OBJECT-TYPE
            SYNTAX IpAddress
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "The IP address assigned (supposing dinamic assignation has been
            selected)."
            ::= { telProdNpMonIfcPPPNCPIPCPControlOptionsEntry 2 }

        telProdNpMonIfcPPPNCPIPCPCtrlOptIpAddrRem OBJECT-TYPE
            SYNTAX IpAddress
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "The remote IP address (supposing it has been received)."
            ::= { telProdNpMonIfcPPPNCPIPCPControlOptionsEntry 3 }

        telProdNpMonIfcPPPNCPIPCPCtrlOptVJCmpLoc OBJECT-TYPE
            SYNTAX INTEGER{
                yes(1),
                no(2)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Van Jacobson compression selected or not in the local end: (1) yes,
            (2) no."
            ::= { telProdNpMonIfcPPPNCPIPCPControlOptionsEntry 4 }

        telProdNpMonIfcPPPNCPIPCPCtrlOptVJCmpRem OBJECT-TYPE
            SYNTAX INTEGER{
