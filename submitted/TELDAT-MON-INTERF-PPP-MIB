-- Grupo de parﬂmetros relacionados con el Multilink PPP (MP).

    telProdNpMonIfcPPPMPGroup OBJECT IDENTIFIER ::= { telProdNpMonInterfPPP 7 }

    -- Tabla de parﬂmetros globales del Multilink PPP.

    telProdNpMonIfcPPPMPControlGloTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPMPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains Multilink PPP global parameters. This table has an
        entry for each PPP interface. The entry number is determined by the
        corresponding interface number (table index)."
        ::= { telProdNpMonIfcPPPMPGroup 1 }

    telProdNpMonIfcPPPMPControlGloEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPMPControlGloEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the node interface number which is the table index,
        and variables that shows the remote user name, the number of active
        links in bundle and the current fragment queue length."
        INDEX { telProdNpMonIfcPPPMPCtrlGloIndex }

        ::= { telProdNpMonIfcPPPMPControlGloTable 1 }

    TelProdNpMonIfcPPPMPControlGloEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPMPCtrlGloIndex
                INTEGER,
            telProdNpMonIfcPPPMPCtrlGloRemUserName
                DisplayString,
            telProdNpMonIfcPPPMPCtrlGloBundleLinks
                INTEGER,
            telProdNpMonIfcPPPMPCtrlGloFragments
                INTEGER,
            telProdNpMonIfcPPPMPCtrlGloRTFragments
                INTEGER
            }

    telProdNpMonIfcPPPMPCtrlGloIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPMPControlGloEntry 1 }

    telProdNpMonIfcPPPMPCtrlGloRemUserName OBJECT-TYPE
        SYNTAX DisplayString(SIZE(0..30))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "When the authentication is enabled, this variable indicates the remote
        user name. In other case, it contains a empty string."
        ::= { telProdNpMonIfcPPPMPControlGloEntry 2 }

    telProdNpMonIfcPPPMPCtrlGloBundleLinks OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows the number of active links in bundle."
        ::= { telProdNpMonIfcPPPMPControlGloEntry 3 }

    telProdNpMonIfcPPPMPCtrlGloFragments OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows the current fragment queue length."
        ::= { telProdNpMonIfcPPPMPControlGloEntry 4 }

    telProdNpMonIfcPPPMPCtrlGloRTFragments OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows the current fragment queue length for real time
        traffic."
        ::= { telProdNpMonIfcPPPMPControlGloEntry 5 }

    -- Tabla que contiene los parﬂmetros que permiten monitorizar las opciones
    -- del Multilink PPP.

    telProdNpMonIfcPPPMPControlOptionsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPMPControlOptionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains the variables that informs about the Multilink PPP
        options. The entry number is determined by the interface value (table
        index)."
        ::= { telProdNpMonIfcPPPMPGroup 2 }

    telProdNpMonIfcPPPMPControlOptionsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPMPControlOptionsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the node interface number, which is the table
        index, the local and remote maximum packet size accepted in reception,
        if short sequence number header format is enabled in the local and
        remote end, the local and remote associated class and the local and
        remote address."
        INDEX { telProdNpMonIfcPPPMPCtrlOptIndex }

        ::= { telProdNpMonIfcPPPMPControlOptionsTable 1 }

    TelProdNpMonIfcPPPMPControlOptionsEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPMPCtrlOptIndex
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptMRRULoc
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptMRRURem
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptSSNLoc
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptSSNRem
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptClassLoc
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptClassRem
                INTEGER,
            telProdNpMonIfcPPPMPCtrlOptAddressLoc
                DisplayString,
            telProdNpMonIfcPPPMPCtrlOptAddressRem
                DisplayString,
            telprodNpMonIfcPPPMPCtrlOptMHFNeg
                INTEGER
            }

    telProdNpMonIfcPPPMPCtrlOptIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 1 }

    telProdNpMonIfcPPPMPCtrlOptMRRULoc OBJECT-TYPE
        SYNTAX INTEGER(1..65535)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum packet size accepted in reception associated to the local end."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 2 }

    telProdNpMonIfcPPPMPCtrlOptMRRURem OBJECT-TYPE
        SYNTAX INTEGER(1..65535)
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Maximum packet size accepted in reception associated to the remote
        end."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 3 }

    telProdNpMonIfcPPPMPCtrlOptSSNLoc OBJECT-TYPE
        SYNTAX INTEGER{
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows if the short sequence number header format in the
        local end is enabled or disabled."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 4 }

    telProdNpMonIfcPPPMPCtrlOptSSNRem OBJECT-TYPE
        SYNTAX INTEGER{
            non_negotiated(0),
            yes(1),
            no(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows if the short sequence number header format in the
        remote end is enabled or disabled."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 5 }

    telProdNpMonIfcPPPMPCtrlOptClassLoc OBJECT-TYPE
        SYNTAX INTEGER{
            null_class(0),
            locally_assigned_address(1),
            ip_address(2),
            ethernet_mac_address(3),
            magic_number(4),
            public_switched_network_directory_number(5)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows the class associated to the local end."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 6 }

    telProdNpMonIfcPPPMPCtrlOptClassRem OBJECT-TYPE
        SYNTAX INTEGER{
            null_class(0),
            locally_assigned_address(1),
            ip_address(2),
            ethernet_mac_address(3),
            magic_number(4),
            public_switched_network_directory_number(5)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows the class associated to the remote end."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 7 }

    telProdNpMonIfcPPPMPCtrlOptAddressLoc OBJECT-TYPE
        SYNTAX DisplayString(SIZE(0..19))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates the address associated to the local end. It is
        a string that must be interpreted according to the local class."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 8 }

    telProdNpMonIfcPPPMPCtrlOptAddressRem OBJECT-TYPE
        SYNTAX DisplayString(SIZE(0..19))
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable indicates the address associated to the remote end. It is
        a string that must be interpreted according to the remote class."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 9 }

    telprodNpMonIfcPPPMPCtrlOptMHFNeg OBJECT-TYPE
        SYNTAX INTEGER{
            no(1),
            yes(2)
            }
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "This variable shows if the multiclass multilink option has been
        negotiated."
        ::= { telProdNpMonIfcPPPMPControlOptionsEntry 10 }

    -- Tabla que contiene los estadçsticos correspondientes al Multilink PPP.

    telProdNpMonIfcPPPMPStatsTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonIfcPPPMPStatsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "This table contains statistics of the Multilink PPP. The entry number
        is determined by the interface number (table index)."
        ::= { telProdNpMonIfcPPPMPGroup 3 }

    telProdNpMonIfcPPPMPStatsEntry OBJECT-TYPE
        SYNTAX TelProdNpMonIfcPPPMPStatsEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry contains the node interface number, which is the table
        index, a variable to check variables deletion (corresponding counters
        reset), and information about the following counters values: number of
        sent and received frames, number of sent and received bytes, number of
        queued frames, number of error frames and number of sequence loss in
        reception."
        INDEX { telProdNpMonIfcPPPMPStIndex }

        ::= { telProdNpMonIfcPPPMPStatsTable 1 }

    TelProdNpMonIfcPPPMPStatsEntry ::=
        SEQUENCE{
            telProdNpMonIfcPPPMPStIndex
                INTEGER,
            telProdNpMonIfcPPPMPStClear
                INTEGER,
            telProdNpMonIfcPPPMPStFramesTx
                Counter,
            telProdNpMonIfcPPPMPStFramesRx
                Counter,
            telProdNpMonIfcPPPMPStBytesTx
                Counter,
            telProdNpMonIfcPPPMPStBytesRx
                Counter,
            telProdNpMonIfcPPPMPStQueuedFrm
                Counter,
            telProdNpMonIfcPPPMPStErrorFrm
                Counter,
            telProdNpMonIfcPPPMPStSeqLoss
                Counter,
            telProdNpMonIfcPPPMPStRTFramesTx
                Counter,
            telProdNpMonIfcPPPMPStRTFramesRx
                Counter,
            telProdNpMonIfcPPPMPStRTBytesTx
                Counter,
            telProdNpMonIfcPPPMPStRTBytesRx
                Counter,
            telProdNpMonIfcPPPMPStRTQueuedFrm
                Counter,
            telProdNpMonIfcPPPMPStRTErrorFrm
                Counter,
            telProdNpMonIfcPPPMPStRTSeqLoss
                Counter
            }

    telProdNpMonIfcPPPMPStIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Interface number of the node, that indicates the corresponding table
        entry (it is the table index)."
        ::= { telProdNpMonIfcPPPMPStatsEntry 1 }

    telProdNpMonIfcPPPMPStClear OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is readed shows the output from the last counter
        restart attempt: the value is 2 (ok) if the restart was performed
        succesfully, 3 (error) if an error ocurred and 4 (undefined) if nobody
        has tried to restart the counters yet. In set operations, the value 1
        (it is the only value that can be assigned by a writing operation)
        causes a counter restart."
        ::= { telProdNpMonIfcPPPMPStatsEntry 2 }

    telProdNpMonIfcPPPMPStFramesTx OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of sent frames."
        ::= { telProdNpMonIfcPPPMPStatsEntry 3 }

    telProdNpMonIfcPPPMPStFramesRx OBJECT-TYPE
