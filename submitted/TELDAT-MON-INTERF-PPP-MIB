                INTEGER,
            telProdNpMonIfcPPPVJStOutPackets
                Counter,
            telProdNpMonIfcPPPVJStOutComprPkts
                Counter,
            telProdNpMonIfcPPPVJStSearches
                Counter,
            telProdNpMonIfcPPPVJStNoConnState
                Counter,
            telProdNpMonIfcPPPVJStInUncomPkts
                Counter,
            telProdNpMonIfcPPPVJStInComprPkts
                Counter,
            telProdNpMonIfcPPPVJStInUnkTypePkts
                Counter,
            telProdNpMonIfcPPPVJStInPktsTossErr
                Counter
            }

