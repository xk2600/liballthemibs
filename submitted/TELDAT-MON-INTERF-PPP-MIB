            ::= { telProdNpMonIfcPPPNCPIPCPControlOptionsEntry 5 }

    -- Tabla que contiene los estadçsticos correspondientes al protocolo IPCP.

        telProdNpMonIfcPPPNCPIPCPStatsTable OBJECT-TYPE
            SYNTAX SEQUENCE OF TelProdNpMonIfcPPPNCPIPCPStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
            "This table contains statistics of the IPCP protocol. The entry number
            is determined by the interface number (table index)."
            ::= { telProdNpMonIfcPPPNCPIPCPGroup 3 }

        telProdNpMonIfcPPPNCPIPCPStatsEntry OBJECT-TYPE
            SYNTAX TelProdNpMonIfcPPPNCPIPCPStatsEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
            "Each entry contains the node interface number, which is the table
            index, a variable to check variables deletion (corresponding counters
            reset), and information about the following counters values: number of
            sent and received Configure Request NCP frames, number of sent and
            received Configure Ack NCP frames, number of sent and received Configure
            Nak NCP frames, number of sent and received Configure Reject NCP frames,
            number of sent and received Terminate Request NCP frames, number of sent
            and received Terminate Ack NCP frames."
            INDEX { telProdNpMonIfcPPPNCPIPCPStIndex }

            ::= { telProdNpMonIfcPPPNCPIPCPStatsTable 1 }

        TelProdNpMonIfcPPPNCPIPCPStatsEntry ::=
            SEQUENCE{
                telProdNpMonIfcPPPNCPIPCPStIndex
                    INTEGER,
                telProdNpMonIfcPPPNCPIPCPStClear
                    INTEGER,
                telProdNpMonIfcPPPNCPIPCPStConfReqRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfReqTx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfAckRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfAckTx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfNakRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfNakTx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfRejRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStConfRejTx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStTermReqRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStTermReqTx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStTermAckRx
                    Counter,
                telProdNpMonIfcPPPNCPIPCPStTermAckTx
                    Counter
                }

        telProdNpMonIfcPPPNCPIPCPStIndex OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Interface number of the node, that indicates the corresponding table
            entry (it is the table index)."
            ::= { telProdNpMonIfcPPPNCPIPCPStatsEntry 1 }

        telProdNpMonIfcPPPNCPIPCPStClear OBJECT-TYPE
            SYNTAX INTEGER{
                delete(1),
                ok(2),
                error(3),
                undefined(4)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
            "This variable when is readed shows the output from the last counter
            restart attempt: the value is 2 (ok) if the restart was performed
            succesfully, 3 (error) if an error ocurred and 4 (undefined) if nobody
            has tried to restart the counters yet. In set operations, the value 1
            (it is the only value that can be assigned by a writing operation)
            causes a counter restart."
            ::= { telProdNpMonIfcPPPNCPIPCPStatsEntry 2 }

        telProdNpMonIfcPPPNCPIPCPStConfReqRx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Request NCP frames received through the interface
            number shown in the index."
            ::= { telProdNpMonIfcPPPNCPIPCPStatsEntry 3 }

        telProdNpMonIfcPPPNCPIPCPStConfReqTx OBJECT-TYPE
            SYNTAX Counter
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
            "Number of Configure Request NCP frames transmitted through the
