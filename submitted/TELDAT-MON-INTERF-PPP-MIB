                Counter,
            telProdNpMonIfcPPPCCPStBytesTx
                Counter,
            telProdNpMonIfcPPPCCPStUncompFrsRx
                Counter,
            telProdNpMonIfcPPPCCPStUncompFrsTx
                Counter,
            telProdNpMonIfcPPPCCPStErrorFrsRx
                Counter,
            telProdNpMonIfcPPPCCPStErrorFrsTx
                Counter,
            telProdNpMonIfcPPPCCPStResetReqRx
                Counter,
            telProdNpMonIfcPPPCCPStResetReqTx
                Counter,
            telProdNpMonIfcPPPCCPStResetAckRx
                Counter,
            telProdNpMonIfcPPPCCPStResetAckTx
                Counter,
            telProdNpMonIfcPPPCCPStSeqErrorsRx
                Counter,
            telProdNpMonIfcPPPCCPStSeqErrorsTx
                Counter,
            telProdNpMonIfcPPPCCPStLCBErrorsRx
                Counter,
            telProdNpMonIfcPPPCCPStLCBErrorsTx
                Counter,
            telProdNpMonIfcPPPCCPStHdrErrorsRx
                Counter,
            telProdNpMonIfcPPPCCPStHdrErrorsTx
                Counter
            }

