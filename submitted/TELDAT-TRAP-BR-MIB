    ::= { brEventObjects2 2 }

br2_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event BR.2"
    ::= { brEventObjects2 3 }

brEventObjects3 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event BR.3
        A frame has been received over a WAN interface in the Ethernet bridge
        encapsulation, but the IPX protocol (in 802.3 encapsulation) is routed
        by this node."
    ::= { brEventsGroup 3 }

