TELDAT-TRAP-BR-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


brTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the BR event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 74 }


brEventsGroup	OBJECT IDENTIFIER	::= { brTrapsMIB 1 }

brNotifications	OBJECT IDENTIFIER	::= { brTrapsMIB 2 }
brNotificationsPrefix	OBJECT IDENTIFIER	::= { brNotifications 0 }

brEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event BR.1
        A MAC frame has been received by the hardware, but is being dropped
        because the port it was received on is in the 'blocking' or 'listening'
        state. Frames are only processed when the port is in the 'learning' or
        'forwarding' state."
    ::= { brEventsGroup 1 }

br1_source_mac OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'source_mac' in event BR.1"
    ::= { brEventObjects1 1 }

br1_dest_mac OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'dest_mac' in event BR.1"
    ::= { brEventObjects1 2 }

br1_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event BR.1"
    ::= { brEventObjects1 3 }

brEventObjects2 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event BR.2
        A MAC frame has been received whose destination address is known to be
        on the same side of the bridge as the packet came from. It is dropped
        by the filtering logic since it does not need to be bridged. Note that
        this event is not counted by ELS for performance reasons. A counter
        is kept in ASRT, it is the 'Dropped, dest addr filtering' entry in
        the ASRT>LIST SOURCE-ROUTE COUNTERS and ASRT>LIST TRANSPARENT COUNTERS
        commands."
    ::= { brEventsGroup 2 }

br2_source_mac OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'source_mac' in event BR.2"
    ::= { brEventObjects2 1 }

