    STATUS      current
    DESCRIPTION
                "Load average over the last 15 minutes."
    ::= { load 3 }

cpudetail	OBJECT IDENTIFIER ::= { kernelReader 5 }

cpuUtilTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF CpuUtilEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "MIB tree branch"
    ::= { cpudetail 1 }

cpuUtilEntry OBJECT-TYPE
    SYNTAX      CpuUtilEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
                "MIB tree branch"
    INDEX  { cpuinstance }
    ::= { cpuUtilTable 1 }

CpuUtilEntry ::=
    SEQUENCE {
        cpuinstance
            Integer32,
        cpudelta
            DisplayString,
        cpuidle
            DisplayString,
        cpuuser
            DisplayString,
        cpukernel
            DisplayString,
        cpuwait
            DisplayString,
        waitio
            DisplayString,
        waitswap
            DisplayString,
        waitpio
            DisplayString,
        idlethread
            Counter64,
        rknrd403
            DisplayString
    }

