-- *****************************************************************************
-- Ucs Chassis Fabric Extender
--
-- Ucs Chassis Fabric Extender Table
-- *****************************************************************************
caUcsChassisFabricExtenderTableLastChange OBJECT-TYPE
	SYNTAX TimeTicks
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The value of sysUpTime at the time of the last creation or deletion of an entry in the caUcsChassisFabricExtenderTable.  If the number of entries has been unchanged since the last re-initialization of the local network management subsystem, then this object contains a zero value."
	::= { caUcsChassisFabricExtender 1 }

caUcsChassisFabricExtenderTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsChassisFabricExtenderRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUcsChassisFabricExtender table."
	::= { caUcsChassisFabricExtender 2 }

caUcsChassisFabricExtenderEntry OBJECT-TYPE
	SYNTAX CaUcsChassisFabricExtenderRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsChassisFabricExtenderUcsManagerId,
		caUcsChassisFabricExtenderChassisId,
		caUcsChassisFabricExtenderSlotId }
	::= { caUcsChassisFabricExtenderTable 1 }

CaUcsChassisFabricExtenderRow ::=
	SEQUENCE {
		caUcsChassisFabricExtenderUcsManagerId		Integer32,	
		caUcsChassisFabricExtenderChassisId		Integer32,
		caUcsChassisFabricExtenderSlotId 		Integer32,
		caUcsChassisFabricExtenderModel			DisplayString,
		caUcsChassisFabricExtenderSerialNumber		DisplayString,
		caUcsChassisFabricExtenderVendor		DisplayString,
		caUcsChassisFabricExtenderRevision		DisplayString,
		caUcsChassisFabricExtenderSide			INTEGER,
		caUcsChassisFabricExtenderAmbientTemp		Float,
		caUcsChassisFabricExtenderOperState		INTEGER,
		caUcsChassisFabricExtenderBootLoaderVersion	DisplayString,
		caUcsChassisFabricExtenderFirmwareVersion	DisplayString,
		caUcsChassisFabricExtenderStartupFirmwareVersion DisplayString,
		caUcsChassisFabricExtenderBackupFirmwareVersion	DisplayString,
		caUcsChassisFabricExtenderOperability		INTEGER,
		caUcsChassisFabricExtenderProductName           DisplayString,
		caUcsChassisFabricExtenderPid			DisplayString
		}

caUcsChassisFabricExtenderUcsManagerId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"UCS Manager id"
	::= { caUcsChassisFabricExtenderEntry 1  }

caUcsChassisFabricExtenderChassisId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Instance of a chassis/enclosure; corresponds to caChassisId."
	::= { caUcsChassisFabricExtenderEntry 2 }

