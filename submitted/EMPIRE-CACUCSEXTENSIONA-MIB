-- *****************************************************************************
-- Ucs Chassis Fabric Extender Port Table
-- *****************************************************************************
caUcsChassisFabricExtenderPortTableLastChange OBJECT-TYPE
	SYNTAX TimeTicks
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The value of sysUpTime at the time of the last creation or deletion of an entry in the caUcsChassisFabricExtenderPortTable.  If the number of entries has been unchanged since the last re-initialization of the local network management subsystem, then this object contains a zero value."
	::= { caUcsChassisFabricExtender 3 }

caUcsChassisFabricExtenderPortTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsChassisFabricExtenderPortRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUcsChassisFabricExtenderPort table."
	::= { caUcsChassisFabricExtender 4 }

caUcsChassisFabricExtenderPortEntry OBJECT-TYPE
	SYNTAX CaUcsChassisFabricExtenderPortRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsChassisFabricExtenderPortUcsManagerId,
		caUcsChassisFabricExtenderPortChassisId,
		caUcsChassisFabricExtenderPortIndex }
	::= { caUcsChassisFabricExtenderPortTable 1 }

CaUcsChassisFabricExtenderPortRow ::=
	SEQUENCE {
		caUcsChassisFabricExtenderPortUcsManagerId		Integer32,
		caUcsChassisFabricExtenderPortChassisId			Integer32,
		caUcsChassisFabricExtenderPortIndex 			Integer32,
		caUcsChassisFabricExtenderPortSlotId 			Integer32,
		caUcsChassisFabricExtenderPortModel			DisplayString,
		caUcsChassisFabricExtenderPortSerialNumber		DisplayString,
		caUcsChassisFabricExtenderPortVendor			DisplayString,
		caUcsChassisFabricExtenderPortRevision			DisplayString,
		caUcsChassisFabricExtenderPortType			INTEGER,
		caUcsChassisFabricExtenderDeviceIndex			Integer32,
		caUcsChassisFabricExtenderConnectedPortIndex		Integer32,
		caUcsChassisFabricExtenderPortOperState			DisplayString,
		caUcsChassisFabricExtenderPortPeerSlotId		Integer32,
		caUcsChassisFabricExtenderPortAcknowledge		INTEGER,
		caUcsChassisFabricExtenderPortPeerDn			DisplayString,		
		caUcsChassisFabricExtenderPortProductName		DisplayString,
		caUcsChassisFabricExtenderPortPid			DisplayString,
		caUcsChassisFabricExtenderPortPeerFullyQualifiedDn	DisplayString,
		caUcsChassisFabricExtenderPortSwitchId			Integer32
		}

caUcsChassisFabricExtenderPortUcsManagerId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"UCS Manager id"
	::= { caUcsChassisFabricExtenderPortEntry 1 }

