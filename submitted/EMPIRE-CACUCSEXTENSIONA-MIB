EMPIRE-CACUCSEXTENSIONA-MIB DEFINITIONS ::= BEGIN


IMPORTS
--	enterprises
--		FROM RFC1155-SMI
--	empire, applications, traps
--		FROM EMPIRE
	MODULE-IDENTITY, NOTIFICATION-TYPE, enterprises, 
	Integer32, Gauge32, Unsigned32, Counter32, Counter64, 
	TimeTicks, IpAddress
		FROM SNMPv2-SMI
	InetAddressType, InetAddress
		FROM INET-ADDRESS-MIB
	OBJECT-TYPE
		FROM RFC-1212
	PhysAddress
		FROM RFC1213-MIB
	IANAifType
		FROM IANAifType-MIB
	TruthValue, DisplayString, TEXTUAL-CONVENTION
		FROM SNMPv2-TC;

caUcsExtensions MODULE-IDENTITY
	LAST-UPDATED "201007131200Z"  
	ORGANIZATION "CA, Inc."
	CONTACT-INFO "http://www.ca.com"
	DESCRIPTION  "This MIB module defines the 
                      Cisco Universal Computing System Aim extension tables."
--	REVISION     "201001131200Z" 
--	DESCRIPTION  "mib name change from cacucsext to cacucsextensiona"
	REVISION     "201007131200Z" 
	DESCRIPTION  "InetAddress usage for future IPv6 support capability"
	::= { cadatacentera 3 }


cadatacentera			OBJECT IDENTIFIER ::= { applications 58 }
empire				OBJECT IDENTIFIER ::= { enterprises 546 }
unix            		      OBJECT IDENTIFIER ::= { empire 1 }
sysmgmt         		      OBJECT IDENTIFIER ::= { unix 1 }

traps		                 OBJECT IDENTIFIER ::= { empire 9 }
applications			OBJECT IDENTIFIER ::= { empire 16 }

-- caUcsExtensions			OBJECT IDENTIFIER ::= { cadatacentera 3 }
	


-- *****************************************************************************
-- caUcsExtensions Groups
-- *****************************************************************************
caUcsChassisExt			OBJECT IDENTIFIER ::= { caUcsExtensions 1 }
caUcsBladeExt			OBJECT IDENTIFIER ::= { caUcsExtensions 2 }
caUcsSwitchExt			OBJECT IDENTIFIER ::= { caUcsExtensions 3 }
caUcsManager			OBJECT IDENTIFIER ::= { caUcsExtensions 4 }
caUcsServiceProfile		OBJECT IDENTIFIER ::= { caUcsExtensions 5 }

-- *****************************************************************************
-- caUcsChassisExt Groups
-- *****************************************************************************
caUcsChassis			OBJECT IDENTIFIER ::= { caUcsChassisExt 1 }
caUcsChassisFan			OBJECT IDENTIFIER ::= { caUcsChassisExt 2 }
caUcsChassisPower		      OBJECT IDENTIFIER ::= { caUcsChassisExt 3 }
caUcsChassisFabricExtender	OBJECT IDENTIFIER ::= { caUcsChassisExt 4 }

-- *****************************************************************************
-- caUcsBladeExt Groups
-- *****************************************************************************
caUcsBlade			      OBJECT IDENTIFIER ::= { caUcsBladeExt 1 }
caUcsBladeInterfaces		OBJECT IDENTIFIER ::= { caUcsBladeExt 2 }
caUcsBladeServer		      OBJECT IDENTIFIER ::= { caUcsBladeExt 3 }

-- *****************************************************************************
-- caUcsBladeServer Groups
-- *****************************************************************************
caUcsBladeServerCpu		OBJECT IDENTIFIER ::= { caUcsBladeServer 1 }
caUcsBladeServerMotherboard	OBJECT IDENTIFIER ::= { caUcsBladeServer 2 }
caUcsBladeServerMemory		OBJECT IDENTIFIER ::= { caUcsBladeServer 3 }
caUcsBladeServerStorage	OBJECT IDENTIFIER ::= { caUcsBladeServer 4 }

-- *****************************************************************************
-- caUcsSwitchExt Groups
-- *****************************************************************************
caUcsSwitch			OBJECT IDENTIFIER ::= { caUcsSwitchExt 1 }
caUcsSwitchFan		OBJECT IDENTIFIER ::= { caUcsSwitchExt 2 }
caUcsSwitchPower		OBJECT IDENTIFIER ::= { caUcsSwitchExt 3 }
caUcsSwitchInterfaces	OBJECT IDENTIFIER ::= { caUcsSwitchExt 4 }




Float ::= TEXTUAL-CONVENTION
	DISPLAY-HINT "d-2"
	STATUS current
	DESCRIPTION "This data type represents a Float.
		     9456 represents 94.56%..."
	SYNTAX Integer32

Uint64 ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION "This data type represents an unsigned 64 bit
		     Limited to use in OBJECT-TYPE where the 
		     ACCESS clause is 'read-only' or 'accessible-for-notify'."
	SYNTAX Counter64

-- *****************************************************************************
-- Ucs Chassis Table
-- *****************************************************************************
caUcsChassisTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsChassisRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUcsChassis table."
	::= { caUcsChassis 1 }

caUcsChassisTableLastChange OBJECT-TYPE
	SYNTAX TimeTicks
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Table tast refresh time"
	::= { caUcsChassis 2 }
	
caUcsChassisEntry OBJECT-TYPE
	SYNTAX CaUcsChassisRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsChassisUcsManagerId,
		caUcsChassisId }
	::= { caUcsChassisTable 1 }

CaUcsChassisRow ::=
	SEQUENCE {
		caUcsChassisUcsManagerId		Integer32,
		caUcsChassisId				Integer32,
		caUcsChassisRedundancyCondition		INTEGER,
		caUcsChassisInputPowerState		Integer32,
		caUcsChassisOutputPowerState		Integer32,
		caUcsChassisAdminState			INTEGER,
		caUcsChassisOperState			INTEGER,
		caUcsChassisConfigState			INTEGER,
		caUcsChassisConnPath			INTEGER,
		caUcsChassisConnStatus			INTEGER,
		caUcsChassisManagingInst		INTEGER,
		caUcsChassisOperability			INTEGER
		}

caUcsChassisUcsManagerId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"UCS Manager id"
	::= { caUcsChassisEntry 1 }
	
caUcsChassisId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Instance of a chassis/enclosure; corresponds to caChassisId."
	::= { caUcsChassisEntry 2 }

caUcsChassisRedundancyCondition OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		ok(2),
		failed(3),
		degraded(4)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Overall condition of the redundancy of the powersubsystem in the chassis."
	::= { caUcsChassisEntry 3 }

caUcsChassisInputPowerState OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		ok(2),
		upperNonRecoverable(3),
		upperNonCritical(4),		
		lowerNonCritical(5),
		lowerCritical(6),		
		lowerNonRecoverable(7),
		notSupported(8)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Overall condition of input power of the PSUs in this chassis."
	::= { caUcsChassisEntry 4 }

caUcsChassisOutputPowerState OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		ok(2),
		upperNonRecoverable(3),
		upperNonCritical(4),		
		lowerNonCritical(5),
		lowerCritical(6),		
		lowerNonRecoverable(7),
		notSupported(8)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Overall condition of output power of the PSUs in this chassis."
	::= { caUcsChassisEntry 5 }

caUcsChassisAdminState OBJECT-TYPE
	SYNTAX INTEGER {
		acknowledged(1),  --default
		reacknowledge(2),
		remove(3)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Questioning Cisco on the meaning of the object mapped."
	::= { caUcsChassisEntry 6 }

caUcsChassisOperState OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		operable(2),
		inoperable(3),
		degraded(4),
		poweredOff(5),
		powerProblem(6),
		removed(7),
		voltageProblem(8),
 		hermalProblem(9),
		performanceProblem(10),
		accessibilityProblem(11),
		identityUnestablishable(12),
		biosPostTimeout(13),
		fabricConnProblem(51),
		fabricUnsupportedConn(52),
		config(81),
		equipmentProblem(82),
		decommissioning(83),
		notSupported(100),
		discovery(101),
		discoveryFailed(102),
		identify(103),
		postFailure(104),
		upgradeProblem(105),
		peerCommProblem(106),
		autoUpgrade(107)

	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Operational state of the chassis."
	::= { caUcsChassisEntry 7 }

caUcsChassisConfigState OBJECT-TYPE
	SYNTAX INTEGER {
		uninitialized(1),
		unacknowledged(2),  -- default
		unsupportedConnectivity(3),
		ok(4),
		removing(5),
		ackInProgress(6)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Questioning Cisco on the meaning of the object mapped."
	::= { caUcsChassisEntry 8 }

caUcsChassisConnPath OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		a(2),
		b(3),
		ab(4)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The configured switch to use for connectivity."
	::= { caUcsChassisEntry 9 }

caUcsChassisConnStatus OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		a(2),
		b(3)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The actual switch being used currently for connectivity."
	::= { caUcsChassisEntry 10 }

caUcsChassisManagingInst OBJECT-TYPE
	SYNTAX INTEGER {
		a(1), --default
		b(2),
		none(3)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"The switch managing the chassis (actively running the UCS Manager)."
	::= { caUcsChassisEntry 11 }
	

caUcsChassisOperability OBJECT-TYPE
	SYNTAX INTEGER {
		unknown(1), --default
		operable(2),
		inoperable(3),
		degraded(4),
		poweredOff(5),
