	SYNTAX TimeTicks
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Last table refresh time"
	::= { caUcsSwitch 2 }
	
caUcsSwitchEntry OBJECT-TYPE
	SYNTAX CaUcsSwitchRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsSwitchUcsManagerId,
		caUcsSwitchIndex }
	::= { caUcsSwitchTable 1 }

CaUcsSwitchRow ::=
	SEQUENCE {
		caUcsSwitchUcsManagerId			Integer32,	
		caUcsSwitchIndex			Integer32,
		caUcsSwitchInbandIfIpType		InetAddressType,
		caUcsSwitchInbandIfIp			InetAddress,
		caUcsSwitchInbandIfMask			InetAddress,
		caUcsSwitchInbandIfGateway		InetAddress,
		caUcsSwitchInbandIfVnet			Unsigned32,
		caUcsSwitchOutOfBandIfIpType	InetAddressType,
		caUcsSwitchOutOfBandIfIp		InetAddress,
		caUcsSwitchOutOfBandIfMask		InetAddress,
		caUcsSwitchOutOfBandIfGateway		InetAddress,
		caUcsSwitchAdminInbandIfState		INTEGER,
		caUcsSwitchEnvFanControllerInlet1	Float,
		caUcsSwitchEnvFanControllerInlet2	Float,
		caUcsSwitchEnvFanControllerInlet3	Float,
		caUcsSwitchEnvFanControllerInlet4	Float,
		caUcsSwitchEnvMainBoardOutlet1		Float,
		caUcsSwitchEnvMainBoardOutlet2		Float,
		caUcsSwitchEnvMezzBoardOutlet1		Float,
		caUcsSwitchEnvMezzBoardOutlet2		Float,
		caUcsSwitchCpuLoad			Float,
		caUcsSwitchAvailableMemory		Unsigned32,
		caUcsSwitchMemoryCacheSize		Unsigned32,
		caUcsSwitchBootLoaderVersion		DisplayString,
		caUcsSwitchKernelVersion		DisplayString,
		caUcsSwitchSystemVersion		DisplayString,
		caUcsSwitchStartupKernelVersion		DisplayString,
		caUcsSwitchStartupSystemVersion		DisplayString,
		caUcsSwitchOperability			INTEGER
		}

