caUcsBladeTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsBladeRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUCSBlade table."
	::= { caUcsBlade 1 }

caUcsBladeTableLastChange OBJECT-TYPE
	SYNTAX TimeTicks
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Table tast refresh time"
	::= { caUcsBlade 2 }
	
caUcsBladeEntry OBJECT-TYPE
	SYNTAX CaUcsBladeRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsBladeUcsManagerId,
		caUcsBladeIndex }
	::= { caUcsBladeTable 1 }

CaUcsBladeRow ::=
	SEQUENCE {
		caUcsBladeUcsManagerId			Integer32,
		caUcsBladeIndex				Integer32,
		caUcsBladeOperState			INTEGER,
		caUcsBladeBootLoaderFirmwareVersion	DisplayString,
		caUcsBladeSystemFirmwareVersion		DisplayString,
		caUcsBladeBackupFirmwareVersion		DisplayString,
		caUcsBladeBiosVersion			DisplayString,
		caUcsBladeRestartBiosVersion		DisplayString,
		caUcsBladeBiosBootSequence 		DisplayString,
		caUcsBladeNumOfAdaptors 		Integer32,
		caUcsBladeNumOfCpus 			Integer32,
		caUcsBladeNumOfCores 			Integer32,
		caUcsBladeNumOfEthernetIfs 		Integer32,
		caUcsBladeNumOfFcIfs	 		Integer32,
		caUcsBladeAvailableMemory 		Unsigned32,
		caUcsBladeCachedMemory 			Unsigned32,
		caUcsBladeTotalMemory 			Unsigned32,
		caUcsBladeAvailableSwapMemory 		Unsigned32,
		caUcsBladeCachedSwapMemory 		Unsigned32,
		caUcsBladeTotalSwapMemory 		Unsigned32,
		caUcsBladeAssignedToDn			DisplayString,
		caUcsBladeOperability			INTEGER,
		caUcsBladeDiscovery			DisplayString,
		caUcsBladeNumOfThreads			Integer32,
		caUcsBladeOperPower			DisplayString,
		caUcsBladeOperQualifier			DisplayString,
		caUcsBladeAdminState			DisplayString,
		caUcsBladeAvailableState		DisplayString,
		caUcsBladeAssociateState		DisplayString,
		caUcsBladeSlotStatus			DisplayString,
		caUcsBladeCheckPoint			DisplayString,
		caUcsBladeAssignedToFullyQualifiedDn	DisplayString,
		caUcsBladeServiceProfileOid		OBJECT IDENTIFIER	
		}

caUcsBladeUcsManagerId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"UCS Manager id"
	::= { caUcsBladeEntry  1  }
	
