	::= { caUcsServiceProfileEntry 4 }
	
	
caUcsServiceProfileBladeIndex OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Index of a blade in a chassis for referencing caBladeTable and caUcsBladeTable.  Note that this info won't be available if no blades are assigned to the service profile."
	::= { caUcsServiceProfileEntry 5 }

	
caUcsServiceProfileBladeDn OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Dn of the blade that is associated with the service profile. Note that this info won't be available if no blades are assigned to the service profile."
	::= { caUcsServiceProfileEntry 6 }



caUcsServiceProfileBladeFullyQualifiedDn OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Concatenation of UCS Manager name with the Dn of the blade. Note that this info won't be available if no blades are assigned to the service profile."
	::= { caUcsServiceProfileEntry 7 }
	

caUcsServiceProfileAssignedState OBJECT-TYPE
	SYNTAX INTEGER {
		unassigned(1), -- default
		assigned(2),
		failed(3)
	}

	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Service profile assigned state"
	::= { caUcsServiceProfileEntry 8 }

caUcsServiceProfileAssociationState OBJECT-TYPE
	SYNTAX INTEGER {
		unassociated(1), -- default
		associating(2),
		associated(3),
		disassociating(4),
		failed(5)
	}

	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Service profile association state"
	::= { caUcsServiceProfileEntry 9 }


caUcsServiceProfileConfigState OBJECT-TYPE
	SYNTAX INTEGER {
		notApplied(1), -- default
		applying(2),
		failedToApply(3),
		applied(4)
	}

	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Service profile configuration state"
	::= { caUcsServiceProfileEntry 10 }
	

caUcsServiceProfileOperState OBJECT-TYPE
	SYNTAX INTEGER {
		indeterminate(1), -- default
		unassociated(2),
		ok(10),
		discovery(11),
		config(12),
		unconfig(13),
		powerOff(14),
		restart(15),
		maintenance(20),
		test(21),
		computeMismatch(29),
		computeFailed(30),
		degraded(31),
		discoveryFailed(32),
		configFailed(33),
		unconfigFailed(34),
		testFailed(35),
		maintenanceFailed(36),
		removed(40),
		disabled(41),
		inaccessible(50),
		thermalProblem(60),
		powerProblem(61),
		voltageProblem(62),
		inoperable(63),
		decomissioning(101),
		biosRestore(201),
		cmosReset(202),
		diagnostics(203),
		diagnosticsFailed(204),
		pendingReboot(210),
		pendingReassociation(211)
	}

	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Service profile operating state"
	::= { caUcsServiceProfileEntry 11 }

	
caUcsServiceProfileDn OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Dn of the service profile."
	::= { caUcsServiceProfileEntry 12 }



caUcsServiceProfileFullyQualifiedDn OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Concatenation of UCS Manager name with the Dn of the service profile."
	::= { caUcsServiceProfileEntry 13 }
		

caUcsServiceProfileBladeOid OBJECT-TYPE
	SYNTAX  OBJECT IDENTIFIER
	MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
      		"Reference blade index <UCS Manager Id>,<blade index> with full OID path"
	::= { caUcsServiceProfileEntry 14 }


-- *****************************************
--Traps
-- ******************************************
caUcsChassisFabricExtenderRemovedEvent OBJECT IDENTIFIER ::= { traps 165526 }
caUcsChassisFabricExtenderRemovedTrap NOTIFICATION-TYPE
	OBJECTS {
		caUcsChassisFabricExtenderPortUcsManagerId,
		caUcsChassisFabricExtenderPortChassisId,
		caUcsChassisFabricExtenderPortSlotId
	}
	STATUS current
	DESCRIPTION
		"A fabric extender module has been removed from a chassis."
	::= { sysmgmt 165526 }

caUcsChassisFabricExtenderPortFailureEvent OBJECT IDENTIFIER ::= { traps 165527 }
caUcsChassisFabricExtenderPortFailureTrap NOTIFICATION-TYPE
	OBJECTS {
		caUcsChassisFabricExtenderPortUcsManagerId,
		caUcsChassisFabricExtenderPortChassisId,
		caUcsChassisFabricExtenderPortSlotId,
		caUcsChassisFabricExtenderPortIndex
	}
	STATUS current
	DESCRIPTION
		"A port on a fabric extender has failed due to hardware failure."
	::= { sysmgmt 165527 }

caUcsChassisFabricExtenderPortLinkDownEvent OBJECT IDENTIFIER ::= { traps 165528 }
caUcsChassisFabricExtenderPortLinkDownTrap NOTIFICATION-TYPE
	OBJECTS {
		caUcsChassisFabricExtenderPortUcsManagerId,
		caUcsChassisFabricExtenderPortChassisId,
		caUcsChassisFabricExtenderPortSlotId,
		caUcsChassisFabricExtenderPortIndex
	}
	STATUS current
	DESCRIPTION
		"A port on a fabric extender has lost its link."
	::= { sysmgmt 165528 }
		

END
