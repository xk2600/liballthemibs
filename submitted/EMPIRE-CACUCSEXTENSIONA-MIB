-- Ucs Chassis Psu Table
-- *****************************************************************************
caUcsChassisPsuTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsChassisPsuRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUcsChassisPsu table."
	::= { caUcsChassisPower 1 }

caUcsChassisPsuEntry OBJECT-TYPE
	SYNTAX CaUcsChassisPsuRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
