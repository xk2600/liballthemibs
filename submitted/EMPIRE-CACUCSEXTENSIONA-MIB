	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Peer slot id of the fabric extender port"
	::= { caUcsChassisFabricExtenderPortEntry 13 }

caUcsChassisFabricExtenderPortAcknowledge OBJECT-TYPE
	SYNTAX INTEGER{
		unInitialized(1),  --default
		unAcknowledged(2),
		unsupportedConnectivity(3),
		ok(4),
		removing(5),
		ackInProgress(6),
		notAvailable(20)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Only available for fabric not for host port "
	::= { caUcsChassisFabricExtenderPortEntry 14 }


caUcsChassisFabricExtenderPortPeerDn OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Peer Dn"
	::= { caUcsChassisFabricExtenderPortEntry 15 }
	
