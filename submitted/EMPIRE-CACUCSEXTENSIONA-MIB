-- *****************************************************************************
caUcsBladeIfFcStatsTable OBJECT-TYPE
	SYNTAX SEQUENCE OF CaUcsBladeIfFcStatsRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"caUcsBladeIfFcStats table."
	::= { caUcsBladeInterfaces 3 }

caUcsBladeIfFcStatsEntry OBJECT-TYPE
	SYNTAX CaUcsBladeIfFcStatsRow
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"The table entry."
	INDEX { caUcsBladeIfFcStatsUcsManagerId,
		caUcsBladeIfFcStatsBladeIndex,
		caUcsBladeIfFcStatsIfIndex }
	::= { caUcsBladeIfFcStatsTable 1 }

CaUcsBladeIfFcStatsRow ::=
	SEQUENCE {
	caUcsBladeIfFcStatsUcsManagerId			Integer32,	
	caUcsBladeIfFcStatsBladeIndex			Integer32,
	caUcsBladeIfFcStatsIfIndex			Integer32,
	caUcsBladeIfFcStatsInvalidCRCCount		Counter64,
	caUcsBladeIfFcStatsLinkFailureCount		Counter64,
	caUcsBladeIfFcStatsLipCount			Counter64,
	caUcsBladeIfFcStatsLossOfSignalCount		Counter64,
	caUcsBladeIfFcStatsLossOfSyncCount		Counter64,
	caUcsBladeIfFcStatsNOSCount			Counter64,
	caUcsBladeIfFcStatsControlRequests		Counter64,
	caUcsBladeIfFcStatsInputMegabytes		Counter64,
	caUcsBladeIfFcStatsInputRequests		Counter64,
	caUcsBladeIfFcStatsOutputMegabytes		Counter64,
	caUcsBladeIfFcStatsOutputRequests		Counter64,
	caUcsBladeIfFcStatsDumpedFrames			Counter64,
	caUcsBladeIfFcStatsErrorFrames			Counter64,
	caUcsBladeIfFcStatsRxFrames			Counter64,
	caUcsBladeIfFcStatsTxFrames			Counter64,
	caUcsBladeIfFcStatsRxBadFrames			Counter64,
	caUcsBladeIfFcStatsTotalRxFrames		Counter64,
	caUcsBladeIfFcStatsTxBadFrames			Counter64,
	caUcsBladeIfFcStatsTotalTxFrames		Counter64
	}

caUcsBladeIfFcStatsUcsManagerId OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"UCS Manager id"
	::= { caUcsBladeIfFcStatsEntry 1 }	
	
caUcsBladeIfFcStatsBladeIndex OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Index of the blade from the caBladeTable."
	::= { caUcsBladeIfFcStatsEntry 2 }

caUcsBladeIfFcStatsIfIndex OBJECT-TYPE
	SYNTAX Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Index of an interface on a blade; corresponds to caBladeIfIndex."
	::= { caUcsBladeIfFcStatsEntry 3 }

caUcsBladeIfFcStatsInvalidCRCCount OBJECT-TYPE
	SYNTAX Counter64
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Number of frames received with invalid CRC."
	::= { caUcsBladeIfFcStatsEntry 4 }

caUcsBladeIfFcStatsLinkFailureCount OBJECT-TYPE
	SYNTAX Counter64
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Number of times a link error has occurred."
	::= { caUcsBladeIfFcStatsEntry 5 }

caUcsBladeIfFcStatsLipCount OBJECT-TYPE
	SYNTAX Counter64
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Number of LIP events that have occurred on an arbitrated loop."
	::= { caUcsBladeIfFcStatsEntry 6 }

