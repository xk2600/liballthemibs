        (table index)."
        ::= { telProdNpMonInterfFRLmiEntry 1 }

    telProdNpMonInterfFRLmiClearStat OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is read shows the output from the last table delete
