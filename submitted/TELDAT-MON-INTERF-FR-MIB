-- Tabla de los estadçsticos de compresin CRTP. Incluye, para un dlci de un
-- interfaz Frame Relay o FR sobre RDSI, una variable que indica el resultado de
-- la îltima operacin de borrado si se accede a ella con un get, y mediante
-- un set de esta variable se procede al borrado de los estadçsticos de
-- esta tabla, y tambiân contadores de paquetes RTP salientes detectados,
-- paquetes RTP salientes comprimidos, nîmero de bîsquedas del estado de la
-- conexin, nîmero de veces que no se encontr el estado de la conexin,
-- nîmero de paquetes RTP entrantes no comprimidos, nîmero de paquetes RTP
-- entrantes comprimidos, nîmero de paquetes RTP entrantes desconocidos y
-- nîmero de paquetes RTP tipo Context State.

    telProdNpMonInterfFRCrtpTable OBJECT-TYPE
        SYNTAX SEQUENCE OF TelProdNpMonInterfFRCrtpEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Table of statistics of CRTP compression of Frame Relay, FR-Dial and
        FR over RDSI interfaces. The entry number is determined by
        the interface value (first table index) and the dlci number for
        the interface of which we want to obtain the statistics (second index)."
        ::= { telProdNpMonInterfFR 5 }

    telProdNpMonInterfFRCrtpEntry OBJECT-TYPE
        SYNTAX TelProdNpMonInterfFRCrtpEntry
        ACCESS not-accessible
        STATUS mandatory
        DESCRIPTION
        "Each entry consists of the node interface number, which is
        the table index, the dlci number for the interface of which we want
        to obtain the statistics (second index),a variable that shows
        the output of the last table delete attempt (get operation) or performs
        the delete operation (set operation), and the current output from
        the counters of outgoing detected RTP packets, outgoing compressed
        RTP packets, number of searchs of the connection status, number of times
        the connection status was not found, number of incoming non compressed
        RTP packets, number of incoming compressed RTP packets, number of
        incoming unknown RTP packets and number of Context State RTP packets."
        INDEX { telProdNpMonInterfFRCrtpIfcIndex, telProdNpMonInterfFRCrtpDlciIndex }
        ::= { telProdNpMonInterfFRCrtpTable 1 }

    TelProdNpMonInterfFRCrtpEntry ::=
        SEQUENCE {
            telProdNpMonInterfFRCrtpIfcIndex
                INTEGER,
            telProdNpMonInterfFRCrtpDlciIndex
                INTEGER,
            telProdNpMonInterfFRCrtpClear
                INTEGER,
            telProdNpMonInterfFRCrtpOutRTPPack
                Counter,
            telProdNpMonInterfFRCrtpOutRTPComp
                Counter,
            telProdNpMonInterfFRCrtpSearchConn
                Counter,
            telProdNpMonInterfFRCrtpNoFindConn
                Counter,
            telProdNpMonInterfFRCrtpInRTPunCom
                Counter,
            telProdNpMonInterfFRCrtpInRTPComp
                Counter,
            telProdNpMonInterfFRCrtpInUnknownType
                Counter,
            telProdNpMonInterfFRCrtpContexStatePack
                Counter
            }

    telProdNpMonInterfFRCrtpIfcIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Node interface number. It is the first table index."
        ::= { telProdNpMonInterfFRCrtpEntry 1 }

    telProdNpMonInterfFRCrtpDlciIndex OBJECT-TYPE
        SYNTAX INTEGER
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Dlci number of the interface. It is the second table index."
        ::= { telProdNpMonInterfFRCrtpEntry 2 }

    telProdNpMonInterfFRCrtpClear OBJECT-TYPE
        SYNTAX INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
        "This variable when is read shows the output from the last table delete
        attempt: the value is 2 if the deletion was performed succesfully,
        3 if an error ocurred and 4 for an undefined result, that means
        that nobody has tried to delete the table yet. In set operations,
        the value 1 (it is the only value that can be assigned by a
        writing operation) causes a table deletion: frame counters are set to 0."
        ::= { telProdNpMonInterfFRCrtpEntry 3 }

    telProdNpMonInterfFRCrtpOutRTPPack OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of detected outgoing RTP packets."
        ::= { telProdNpMonInterfFRCrtpEntry 4 }

    telProdNpMonInterfFRCrtpOutRTPComp OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of compressed outgoing RTP packets"
        ::= { telProdNpMonInterfFRCrtpEntry 5 }

    telProdNpMonInterfFRCrtpSearchConn OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of searchs of the connection status."
        ::= { telProdNpMonInterfFRCrtpEntry 6 }

    telProdNpMonInterfFRCrtpNoFindConn OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of times the connection status was not found."
        ::= { telProdNpMonInterfFRCrtpEntry 7 }

    telProdNpMonInterfFRCrtpInRTPunCom OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of non compressed incoming RTP packets."
        ::= { telProdNpMonInterfFRCrtpEntry 8 }

    telProdNpMonInterfFRCrtpInRTPComp OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of compressed incoming RTP packets."
        ::= { telProdNpMonInterfFRCrtpEntry 9 }

    telProdNpMonInterfFRCrtpInUnknownType OBJECT-TYPE
        SYNTAX Counter
        ACCESS read-only
        STATUS mandatory
        DESCRIPTION
        "Number of unknown incoming RTP packets."
        ::= { telProdNpMonInterfFRCrtpEntry 10 }

