dhcpNotification1 NOTIFICATION-TYPE
    STATUS current
    DESCRIPTION
        "Notification of Event DHCP.1

        Message: 'BOOTREQUEST packet received'

        The router has received a packet from a DHCP client requesting a
        response to a DHCP server."
    ::= { dhcpNotificationsPrefix 1 }

dhcpNotification2 NOTIFICATION-TYPE
    STATUS current
    DESCRIPTION
        "Notification of Event DHCP.2

        Message: 'BOOTREPLY packet received'

        The router has received a packet from a DHCP server in response to a
        BOOTREQUEST."
    ::= { dhcpNotificationsPrefix 2 }

dhcpNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              dhcp3_source_ip_address, 
              dhcp3_destination_ip_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event DHCP.3

        Message: 'Relay BOOTREQUEST from $1 to DHCP server $2'

        Parameters list:
              $1: dhcp3_source_ip_address
              $2: dhcp3_destination_ip_address

        Message generated when the router relays packets received from
        DHCP clients and addressed to DHCP servers."
    ::= { dhcpNotificationsPrefix 3 }

