TELDAT-TRAP-DHCP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


dhcpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the DHCP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 133 }


dhcpEventsGroup	OBJECT IDENTIFIER	::= { dhcpTrapsMIB 1 }

dhcpNotifications	OBJECT IDENTIFIER	::= { dhcpTrapsMIB 2 }
dhcpNotificationsPrefix	OBJECT IDENTIFIER	::= { dhcpNotifications 0 }

dhcpEventObjects3 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event DHCP.3
        Message generated when the router relays packets received from
        DHCP clients and addressed to DHCP servers."
    ::= { dhcpEventsGroup 3 }

