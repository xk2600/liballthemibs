        Message: 'T$1:UA port=$2 len=$3 $4->$5 $6->$7 $8'

        Parameters list:
              $1: ban21_direction
              $2: ban21_bridge_port
              $3: ban21_len
              $4: ban21_source_mac_address
              $5: ban21_dest_mac_address
              $6: ban21_source_sap
              $7: ban21_dest_sap
              $8: ban21_rif_data

        UA packet."
    ::= { banNotificationsPrefix 21 }

