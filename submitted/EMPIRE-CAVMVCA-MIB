-- *****************************************************************************
-- Status Host Disk Group 
-- *****************************************************************************
vmvcAimStatHostDiskCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Disks in vmvcAimStatHostDiskTable."
    ::= { vmvcAimStatHostDiskGroup 1 }

vmvcAimStatHostDiskTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatHostDiskRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Disk table."
    ::= { vmvcAimStatHostDiskGroup 2 }

vmvcAimStatHostDiskEntry OBJECT-TYPE
    SYNTAX VmvcAimStatHostDiskRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatHostDiskUID  }
    ::= { vmvcAimStatHostDiskTable 1 }

VmvcAimStatHostDiskRow ::=
    SEQUENCE {
        vmvcAimStatHostDiskUID                      INTEGER,
        vmvcAimStatHostDiskHostID                   SnmpAdminString,
        vmvcAimStatHostDiskName                     SnmpAdminString,
        vmvcAimStatHostDiskBusResets                Gauge,
        vmvcAimStatHostDiskCmdAborted               Gauge,
        vmvcAimStatHostDiskRead                     Gauge,
        vmvcAimStatHostDiskWrite                    Gauge,
        vmvcAimStatHostDiskKBPSRead                 Gauge,
        vmvcAimStatHostDiskKBPSWrite                Gauge,
        vmvcAimStatHostDiskDatastoreName            SnmpAdminString,
        vmvcAimStatHostDiskReadPerSec               Gauge,
        vmvcAimStatHostDiskWritePerSec              Gauge,
        vmvcAimStatHostDiskKBPSReadPerSec           Gauge,
        vmvcAimStatHostDiskKBPSWritePerSec          Gauge,
        vmvcAimStatHostDiskCommandsAbortedPerSec    Gauge,
        vmvcAimStatHostDiskBusResetsPerSec          Gauge,
        vmvcAimStatHostDiskTotalReadLatency         Gauge,
        vmvcAimStatHostDiskTotalWriteLatency        Gauge,
        vmvcAimStatHostDiskTotalLatency             Gauge,
        vmvcAimStatHostDiskDeviceReadLatency        Gauge,
        vmvcAimStatHostDiskDeviceWriteLatency       Gauge,
        vmvcAimStatHostDiskDeviceLatency            Gauge,
        vmvcAimStatHostDiskQueueReadLatency         Gauge,
        vmvcAimStatHostDiskQueueWriteLatency        Gauge,
        vmvcAimStatHostDiskQueueLatency             Gauge,
        vmvcAimStatHostDiskKernelReadLatency        Gauge,
        vmvcAimStatHostDiskKernelWriteLatency       Gauge,
        vmvcAimStatHostDiskKernelLatency            Gauge,
        vmvcAimStatHostDiskMaxTotalLatency          Gauge,
        vmvcAimStatHostDiskVCUID                    INTEGER,
        vmvcAimStatHostDiskVCName                   SnmpAdminString,
        vmvcAimStatHostDiskVCUUID                   SnmpAdminString
    }

vmvcAimStatHostDiskUID  OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Disk device UID"
    ::= { vmvcAimStatHostDiskEntry  1}

