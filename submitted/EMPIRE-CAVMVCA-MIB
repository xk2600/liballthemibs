    ::= { vmvcAimStatVMTable 1 }

VmvcAimStatVMRow ::=
    SEQUENCE {
        vmvcAimStatVMUID                        INTEGER,
        vmvcAimStatVMIndex                      SnmpAdminString,
        vmvcAimStatVMName                       SnmpAdminString,
        vmvcAimStatVMDesc                       SnmpAdminString,
        vmvcAimStatVMConfigFileName             SnmpAdminString,
