    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance UID where this VC event occurred."
    ::= { vmvcAimVCEventEntry 8 }

vmvcAimVCEventVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this VC event occurred."
    ::= { vmvcAimVCEventEntry 9 }


