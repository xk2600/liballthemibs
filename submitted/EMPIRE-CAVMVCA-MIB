    DESCRIPTION
        "Flag whether to monitor the Sensor or not.   The value of this 
         attribute is used as a default value for created objects."
    DEFVAL { 2 }
    ::= { vmvcAimConfigMonitorMemSensorGroup  4 }


-- *****************************************************************************
-- Fan Sensor Monitor Configuration Group
-- *****************************************************************************
vmvcAimConfigMonFanSensorUpdate OBJECT-TYPE
    SYNTAX INTEGER{
        done(1),
        update(2)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "One-shot trigger to update all Fan Sensor objects with 
         Monitoring configuration for all monitoring parameters.
         When completed, Agent will set this attribute to done(1)."
    DEFVAL { 1 }
    ::= { vmvcAimConfigMonitorFanSensorGroup  1 }

vmvcAimConfigMonFanSensorVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This attribute will scope the UPDATE request to the objects 
         within a single matching VC Instance UID 
         (see vmvcAimConfigVCTable).  If this attribute
         is set to zero(0), all objects will be updated."
    DEFVAL { 0 }
    ::= { vmvcAimConfigMonitorFanSensorGroup  2 }

vmvcAimConfigMonFanSensorAlert OBJECT-TYPE
    SYNTAX INTEGER {
        not-green-warn(1),
        not-green-critical(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Level for the alert.   The value of this attribute is
                 used as a default value for created objects."
    DEFVAL { 2 }
    ::= { vmvcAimConfigMonitorFanSensorGroup  3 }

