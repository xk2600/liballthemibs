-- *****************************************************************************

vmvcAimStatCustSpecCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Customization Specification in vmvcAimStatCustSpecTable."
    ::= { vmvcAimStatCustSpecGroup 1}

vmvcAimStatCustSpecTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatCustSpecRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Customization Specification table."
    ::= { vmvcAimStatCustSpecGroup 2 }

vmvcAimStatCustSpecEntry OBJECT-TYPE
    SYNTAX VmvcAimStatCustSpecRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatCustomizationSpecUID }
    ::= { vmvcAimStatCustSpecTable 1 }

VmvcAimStatCustSpecRow ::=
    SEQUENCE {
        vmvcAimStatCustomizationSpecUID                 INTEGER,
        vmvcAimStatCustomizationSpecName                SnmpAdminString,
        vmvcAimStatCustomizationSpecType                SnmpAdminString,
        vmvcAimStatCustomizationSpecLastUpdated         SnmpAdminString,
        vmvcAimStatCustomizationSpecChgVer              SnmpAdminString,
        vmvcAimStatCustomizationSpecHostName            SnmpAdminString,
        vmvcAimStatCustomizationSpecPrimaryDNSServer    SnmpAdminString,
        vmvcAimStatCustomizationSpecSecondaryDNSServer  SnmpAdminString,
        vmvcAimStatCustomizationSpecTertiaryDNSServer   SnmpAdminString,
        vmvcAimStatCustomizationSpecDomainName          SnmpAdminString,
        vmvcAimStatCustomizationSpecDNSSearchSuffixes   SnmpAdminString,
        vmvcAimStatCustomizationSpecVCUID               INTEGER,
        vmvcAimStatCustomizationSpecVCName              SnmpAdminString,
        vmvcAimStatCustomizationSpecLastUpdateTimeSec   INTEGER,
        vmvcAimStatCustomizationSpecVCUUID              SnmpAdminString
    }
    
vmvcAimStatCustomizationSpecUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer UID of the customization specification."
    ::= { vmvcAimStatCustSpecEntry  1}

vmvcAimStatCustomizationSpecName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Name of the customization specification."
    ::= { vmvcAimStatCustSpecEntry  2}

vmvcAimStatCustomizationSpecType OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Guest operating system for this specification (Linux or Windows)."
    ::= { vmvcAimStatCustSpecEntry  3}

vmvcAimStatCustomizationSpecLastUpdated OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Time when the specification was last modified.
         DEPRECATED!  This attribute no longer returns any data, due to
         Internationalization standards involving Data/Time Formatting.
         Refer to vmvcAimStatCustomizationSpecLastUpdateTimeSec for new value."
    ::= { vmvcAimStatCustSpecEntry  4}

vmvcAimStatCustomizationSpecChgVer OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The changeVersion is a unique identifier for a given version of the configuration."
    ::= { vmvcAimStatCustSpecEntry  5}

