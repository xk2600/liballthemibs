        vmvcAimStatVMVDiskThinProvisioned       INTEGER,
        vmvcAimStatVMVDiskWriteThrough          INTEGER,
        vmvcAimStatVMVDiskDatastore             SnmpAdminString,
        vmvcAimStatVMVDiskBusResets             Gauge,
        vmvcAimStatVMVDiskCmdAborted            Gauge,
        vmvcAimStatVMVDiskRead                  Gauge,
        vmvcAimStatVMVDiskWrite                 Gauge,
        vmvcAimStatVMVDiskKBPSRead              Gauge,
        vmvcAimStatVMVDiskKBPSWrite             Gauge,
        vmvcAimStatVMVDiskMonitor               INTEGER,
        vmvcAimStatVMVDiskKBReadAlert           INTEGER,
        vmvcAimStatVMVDiskKBReadState           INTEGER,
        vmvcAimStatVMVDiskKBWriteAlert          INTEGER,
        vmvcAimStatVMVDiskKBWriteState          INTEGER,
        vmvcAimStatVMVDiskState                 INTEGER,
        vmvcAimStatVMVDiskDatastoreName         SnmpAdminString,
        vmvcAimStatVMVDiskReadPerSec            Gauge,
        vmvcAimStatVMVDiskWritePerSec           Gauge,
        vmvcAimStatVMVDiskKBPSReadPerSec        Gauge,
        vmvcAimStatVMVDiskKBPSWritePerSec       Gauge,
        vmvcAimStatVMVDiskCommandsAbortedPerSec Gauge,
        vmvcAimStatVMVDiskBusResetsPerSec       Gauge,
        vmvcAimStatVMVDiskControllerKey         INTEGER,
        vmvcAimStatVMVDiskUnitNumber            INTEGER,
        vmvcAimStatVMVDiskVCUID                 INTEGER,
        vmvcAimStatVMVDiskVCName                SnmpAdminString,
        vmvcAimStatVMVDiskHostName              SnmpAdminString,
        vmvcAimStatVMVDiskVCUUID                SnmpAdminString
    }

