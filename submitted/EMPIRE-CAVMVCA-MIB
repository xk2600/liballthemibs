    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Host Virtual NIC count in vmvcAimStatVNICTable."
    ::= { vmvcAimStatHostVNICGroup 1}

vmvcAimStatHostVNICTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatHostVNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Port Group."
    ::= { vmvcAimStatHostVNICGroup 2 }

vmvcAimStatHostVNICEntry OBJECT-TYPE
    SYNTAX VmvcAimStatHostVNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatHostVNICUID }
    ::= { vmvcAimStatHostVNICTable 1 }

VmvcAimStatHostVNICRow ::=
    SEQUENCE {
        vmvcAimStatHostVNICUID              INTEGER,
        vmvcAimStatHostVNICName             SnmpAdminString,
        vmvcAimStatHostVNICHostIndex        SnmpAdminString,
        vmvcAimStatHostVNICPortgroup        SnmpAdminString,
        vmvcAimStatHostVNICMAC              SnmpAdminString,
        vmvcAimStatHostVNICIp               SnmpAdminString,
        vmvcAimStatHostVNICHostName         SnmpAdminString,
        vmvcAimStatHostVNICType             INTEGER,
        vmvcAimStatHostVNICPortGroupUID     INTEGER,
        vmvcAimStatHostVNICIpV6             SnmpAdminString,
        vmvcAimStatHostVNICVCUID            INTEGER,
        vmvcAimStatHostVNICVCName           SnmpAdminString,
        vmvcAimStatHostVNICDVPortGroupIndex SnmpAdminString,
        vmvcAimStatHostVNICDVPortGroupUID   INTEGER,
        vmvcAimStatHostVNICDVPortKey        SnmpAdminString,
        vmvcAimStatHostVNICVCUUID           SnmpAdminString
    }

