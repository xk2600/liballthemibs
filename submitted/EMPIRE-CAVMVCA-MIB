vmvcAimStatCustomSpecNICCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "CustomSpec NIC count in vmvcAimStatCustomSpecNICTable."
    ::= { vmvcAimStatCustomSpecNICGroup 1}

vmvcAimStatCustomSpecNICTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatCustomSpecNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "CustomSpec NIC table."
    ::= { vmvcAimStatCustomSpecNICGroup 2 }

vmvcAimStatCustomSpecNICEntry OBJECT-TYPE
    SYNTAX VmvcAimStatCustomSpecNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatCustomSpecNICUID }
    ::= { vmvcAimStatCustomSpecNICTable 1 }

VmvcAimStatCustomSpecNICRow ::=
    SEQUENCE {
        vmvcAimStatCustomSpecNICUID                 INTEGER,
        vmvcAimStatCustomSpecName                   SnmpAdminString,
        vmvcAimStatCustomSpecNICOrder               INTEGER,
        vmvcAimStatCustomSpecNICType                INTEGER,
        vmvcAimStatCustomSpecNICIPAddress           SnmpAdminString,
        vmvcAimStatCustomSpecNICSubnetMask          SnmpAdminString,
        vmvcAimStatCustomSpecNICGateway             SnmpAdminString,
        vmvcAimStatCustomSpecNICAlternateGateway    SnmpAdminString,
        vmvcAimStatCustomSpecNICDNS                 SnmpAdminString,
        vmvcAimStatCustomSpecNICAlternateDNS        SnmpAdminString,
        vmvcAimStatCustomSpecNICPrimaryWINSServer   SnmpAdminString,
        vmvcAimStatCustomSpecNICSecondaryWINSServer SnmpAdminString,
        vmvcAimStatCustomSpecNICVCUID               INTEGER,
        vmvcAimStatCustomSpecNICVCName              SnmpAdminString,
        vmvcAimStatCustomSpecUID                    INTEGER,
        vmvcAimStatCustomSpecNICVCUUID              SnmpAdminString
        }

vmvcAimStatCustomSpecNICUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "CustomSpec NIC device UID."
    ::= { vmvcAimStatCustomSpecNICEntry 1 }

vmvcAimStatCustomSpecName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Name of the CustomSpecification this NIC is associated with."
    ::= { vmvcAimStatCustomSpecNICEntry 2 }

vmvcAimStatCustomSpecNICOrder OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The order of the network interface cards for the operating 
         system."
    ::= { vmvcAimStatCustomSpecNICEntry 3 }
    
vmvcAimStatCustomSpecNICType OBJECT-TYPE
    SYNTAX INTEGER {
        static(1),
        dhcp(2)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The type of the NIC card."
    ::= { vmvcAimStatCustomSpecNICEntry  4}
    
vmvcAimStatCustomSpecNICIPAddress OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The IP address that is to be assigned to the NIC."
    ::= { vmvcAimStatCustomSpecNICEntry 5 }
    
