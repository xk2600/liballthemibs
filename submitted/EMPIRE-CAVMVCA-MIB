    INDEX { vmvcAimStatVMVDiskUID  }
    ::= { vmvcAimStatVMVDiskTable 1 }

VmvcAimStatVMVDiskRow ::=
    SEQUENCE {
        vmvcAimStatVMVDiskUID                   INTEGER,
        vmvcAimStatVMVDiskDeviceId              SnmpAdminString,
        vmvcAimStatVMDiskVMID                   SnmpAdminString,
