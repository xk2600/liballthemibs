    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of VM Guest Disks in vmvcAimStatVMGuestDiskTable."
    ::= { vmvcAimStatVMGuestDiskGroup 1 }

vmvcAimStatVMGuestDiskTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatVMGuestDiskRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "VM Guest Disk table."
    ::= { vmvcAimStatVMGuestDiskGroup 2 }

vmvcAimStatVMGuestDiskEntry OBJECT-TYPE
    SYNTAX VmvcAimStatVMGuestDiskRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatVMGuestDiskUID  }
    ::= { vmvcAimStatVMGuestDiskTable 1 }

VmvcAimStatVMGuestDiskRow ::=
    SEQUENCE {
            vmvcAimStatVMGuestDiskUID               INTEGER,
            vmvcAimStatVMGuestDiskVMID              SnmpAdminString,
            vmvcAimStatVMGuestDiskVMName            SnmpAdminString,
            vmvcAimStatVMGuestDiskPath              SnmpAdminString,
            vmvcAimStatVMGuestDiskCapacity          INTEGER,
            vmvcAimStatVMGuestDiskFreeSpace         INTEGER,
            vmvcAimStatVMGuestDiskUsagePct          Gauge,
            vmvcAimStatVMGuestDiskMonitor           INTEGER,
            vmvcAimStatVMGuestDiskAlert             INTEGER,
            vmvcAimStatVMGuestDiskUsageWarnThresh   INTEGER,
            vmvcAimStatVMGuestDiskUsageCritThresh   INTEGER,
            vmvcAimStatVMGuestDiskState             INTEGER,
            vmvcAimStatVMGuestDiskVCUID             INTEGER,
            vmvcAimStatVMGuestDiskVCName            SnmpAdminString,
            vmvcAimStatVMGuestDiskVCUUID            SnmpAdminString
    }

vmvcAimStatVMGuestDiskUID  OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VM Guest Disk device UID"
    ::= { vmvcAimStatVMGuestDiskEntry  1}

vmvcAimStatVMGuestDiskVMID    OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Moid of the virtual machine this Device is connected to."
    ::= { vmvcAimStatVMGuestDiskEntry  2}

