-- *****************************************************************************
-- Status Virtual Switch  Group
-- *****************************************************************************
vmvcAimStatVSwitchCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Virtual Switch Group."
    ::= { vmvcAimStatVSwitchGroup 1}

vmvcAimStatVSwitchTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatVSwitchRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Port Group Group."
    ::= { vmvcAimStatVSwitchGroup 2 }

vmvcAimStatVSwitchEntry OBJECT-TYPE
    SYNTAX VmvcAimStatVSwitchRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatSwitchUID }
    ::= { vmvcAimStatVSwitchTable 1 }

VmvcAimStatVSwitchRow ::=
    SEQUENCE {
        vmvcAimStatSwitchUID                            INTEGER,
        vmvcAimStatSwitchIndex                          SnmpAdminString,
        vmvcAimStatSwitchHost                           SnmpAdminString,
        vmvcAimStatSwitchName                           SnmpAdminString,
        vmvcAimStatSwitchNumPorts                       INTEGER,
        vmvcAimStatSwitchNumAvailPorts                  INTEGER,
        vmvcAimStatSwitchPortgroup                      SnmpAdminString,
        vmvcAimStatSwitchBridgeType                     SnmpAdminString,
        vmvcAimStatSwitchBridgePNic                     SnmpAdminString,
        vmvcAimStatSwitchPolicyOffloadCkhSum            INTEGER,
        vmvcAimStatSwitchPolicyOffloadtcpSegmt          INTEGER,
        vmvcAimStatSwitchPolicyOffloadZeroCpyX          INTEGER,
        vmvcAimStatSwitchPolicySecurityAllowPromis      INTEGER,
        vmvcAimStatSwitchPolicySecurityForgedTransmit   INTEGER,
        vmvcAimStatSwitchPolicySecurityMACChanges       INTEGER,
        vmvcAimStatSwitchPolicyShapingAvgBandWidth      INTEGER,
        vmvcAimStatSwitchPolicyShapingBurstSize         INTEGER,
        vmvcAimStatSwitchPolicyShapingEnabled           INTEGER,
        vmvcAimStatSwitchPolicyShapingPeakBandWidth     INTEGER,
        vmvcAimStatSwitchState                          INTEGER,
        vmvcAimStatSwitchNicTeamActiveNic               SnmpAdminString,
        vmvcAimStatSwitchNicTeamStandByNic              SnmpAdminString,
        vmvcAimStatSwitchNicTeamNotifySwitch            INTEGER,
        vmvcAimStatSwitchNicTeamPolicy                  SnmpAdminString,
        vmvcAimStatSwitchNicTeamReversePolicy           INTEGER,
        vmvcAimStatSwitchNicTeamRollingOrder            INTEGER,
        vmvcAimStatSwitchNicTeamFailureChkBeacon        INTEGER,
        vmvcAimStatSwitchNicTeamFailureChkDuplex        INTEGER,
        vmvcAimStatSwitchNicTeamFailureChkErrPct        INTEGER,
        vmvcAimStatSwitchNicTeamFailureChkSpeed         SnmpAdminString,
        vmvcAimStatSwitchNicTeamFailureFullDuplex       INTEGER,
        vmvcAimStatSwitchNicTeamFailurePercentage       INTEGER,
        vmvcAimStatSwitchNicTeamFailureSpeed            INTEGER,
        vmvcAimStatSwitchMonitor                        INTEGER,
        vmvcAimStatSwitchAlert                          INTEGER,
        vmvcAimStatSwitchHostName                       SnmpAdminString,
        vmvcAimStatSwitchVCUID                          INTEGER,
        vmvcAimStatSwitchVCName                         SnmpAdminString,
        vmvcAimStatSwitchVCUUID                         SnmpAdminString
    }

vmvcAimStatSwitchUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
