vmvcAimStatVAppStartAction OBJECT-TYPE
    SYNTAX INTEGER{
        none(1),
        powerOn(2)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Start action for this vApp. Valid settings are none or powerOn. If set to none, then the
         entity does not participate in auto-start."
    ::= { vmvcAimStatVAppEntry 19}

vmvcAimStatVAppStartOrder OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Specifies the start order for this vApp. Entities are started from lower numbers to
         higher-numbers and reverse on shutdown. Multiple entities with the same start-order can
         be started in parallel and the order is unspecified. This value must be 0 or higher."
    ::= { vmvcAimStatVAppEntry 20}

vmvcAimStatVAppStopAction OBJECT-TYPE
    SYNTAX INTEGER{
        none(1),
        powerOff(2)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Stop action for the vApp. Can be set to none or powerOff. If set to none,
         then the entity does not participate in auto-stop."
    ::= { vmvcAimStatVAppEntry 21}
    
vmvcAimStatVAppVCUUID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The vCenter-specific UUID where this vApp resides."
    ::= { vmvcAimStatVAppEntry  22}


