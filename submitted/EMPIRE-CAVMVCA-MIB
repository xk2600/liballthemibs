-- *****************************************************************************
-- Status Distributed Virtual Port Group
-- *****************************************************************************
vmvcAimStatDVPortGrpCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Distributed Virtual Port Group count in vmvcAimStatDVPortGrpTable."
    ::= { vmvcAimStatDVPortGrpGroup 1}

vmvcAimStatDVPortGrpTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatDVPortGrpRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Disctributed Virtual Port Group Table."
    ::= { vmvcAimStatDVPortGrpGroup 2 }

vmvcAimStatDVPortGrpEntry OBJECT-TYPE
    SYNTAX VmvcAimStatDVPortGrpRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatDVPGUID }
    ::= { vmvcAimStatDVPortGrpTable 1 }

VmvcAimStatDVPortGrpRow ::=
    SEQUENCE {
        vmvcAimStatDVPGUID                              INTEGER,
        vmvcAimStatDVPGVCUID                            INTEGER,
        vmvcAimStatDVPGVCName                           SnmpAdminString,
        vmvcAimStatDVPGIndex                            SnmpAdminString,
        vmvcAimStatDVPGName                             SnmpAdminString,
        vmvcAimStatDVPGDVSwitchName                     SnmpAdminString,
        vmvcAimStatDVPGDVSwitchIndex                    SnmpAdminString,
        vmvcAimStatDVPGDVSwitchUID                      INTEGER,
        vmvcAimStatDVPGDCName                           SnmpAdminString,
        vmvcAimStatDVPGDCIndex                          SnmpAdminString,
        vmvcAimStatDVPGHost                             SnmpAdminString,
        vmvcAimStatDVPGNumPorts                         INTEGER,
        vmvcAimStatDVPGPortKeys                         SnmpAdminString,
        vmvcAimStatDVPGUplink                           INTEGER,
        vmvcAimStatDVPGOverallStatus                    INTEGER,
        vmvcAimStatDVPGPolicySecurityAllowPromis        INTEGER,
        vmvcAimStatDVPGPolicySecurityForgedTransmit     INTEGER,
        vmvcAimStatDVPGPolicySecurityMACChanges         INTEGER,
        vmvcAimStatDVPGPolicyShapingInAvgBandWidth      INTEGER,
        vmvcAimStatDVPGPolicyShapingInBurstSize         INTEGER,
        vmvcAimStatDVPGPolicyShapingInEnabled           INTEGER,
        vmvcAimStatDVPGPolicyShapingInPeakBandWidth     INTEGER,
        vmvcAimStatDVPGPolicyShapingOutAvgBandWidth     INTEGER,
        vmvcAimStatDVPGPolicyShapingOutBurstSize        INTEGER,
        vmvcAimStatDVPGPolicyShapingOutEnabled          INTEGER,
        vmvcAimStatDVPGPolicyShapingOutPeakBandWidth    INTEGER,
        vmvcAimStatDVPGUplinkTeamActivePort             SnmpAdminString,
        vmvcAimStatDVPGUplinkTeamStandByPort            SnmpAdminString,
        vmvcAimStatDVPGUplinkTeamNotifySwitch           INTEGER,
        vmvcAimStatDVPGUplinkTeamPolicy                 SnmpAdminString,
        vmvcAimStatDVPGUplinkTeamReversePolicy          INTEGER,
        vmvcAimStatDVPGUplinkTeamRollingOrder           INTEGER,
        vmvcAimStatDVPGUplinkTeamFailureChkBeacon       INTEGER,
        vmvcAimStatDVPGUplinkTeamFailureChkDuplex       INTEGER,
        vmvcAimStatDVPGUplinkTeamFailureChkErrPct       INTEGER,
        vmvcAimStatDVPGUplinkTeamFailureChkSpeed        SnmpAdminString,
        vmvcAimStatDVPGUplinkTeamFailureFullDuplex      INTEGER,
        vmvcAimStatDVPGUplinkTeamFailurePercentage      INTEGER,
        vmvcAimStatDVPGUplinkTeamFailureSpeed           INTEGER,
        vmvcAimStatDVPGVLanID                           INTEGER,
        vmvcAimStatDVPGPrivateVLanID                    INTEGER,
        vmvcAimStatDVPGVLanTrunkRange                   SnmpAdminString,
        vmvcAimStatDVPGVCUUID                           SnmpAdminString
    }

