    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The DNS server for IP address that is to be assigned to the NIC."
    ::= { vmvcAimStatCustomSpecNICEntry 9 }
    
