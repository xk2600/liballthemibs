    SYNTAX SnmpAdminString (SIZE(0..1024))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Distributed Virtual Uplink Port Groups configured for this virtual switch."
    ::= { vmvcAimStatDistribVSwitchEntry 18 }

vmvcAimStatDVSwitchOverallStatus OBJECT-TYPE
    SYNTAX INTEGER {
        gray(1),
        green(2),
        yellow(3),
        red(4)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Overall Distributed Virtual Switch status from vCenter.  
         It can be grey,green,red and yellow."
    ::= { vmvcAimStatDistribVSwitchEntry 19 }

vmvcAimStatDVSwitchMonitor OBJECT-TYPE
    SYNTAX INTEGER {
        do-not-monitor(1),
        monitor(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Flag whether to monitor this Distributed Virtual Switch or not."
    DEFVAL { 2 }
    ::= { vmvcAimStatDistribVSwitchEntry 20 }

vmvcAimStatDVSwitchAlert OBJECT-TYPE
    SYNTAX INTEGER {
        not-green-warn(1),
        not-green-critical(2),
        follow-overallStatus(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Level choices for the alert, as follows:
            not-green-warn       -  Set the State=warning(3) if OverallStatus!=green
            not-green-crit       -  Set the State=critical(4) if OverallStatus!=green
            follow-overallStatus -  Use the OverallStatus to set the State, as follows:
               OverallStatus=gray    ->  State=unknown(1)
               OverallStatus=green   ->  State=ok(2)
               OverallStatus=yellow  ->  State=warning(3)
               OverallStatus=red     ->  State=critical(4)"
    DEFVAL { 3 }
    ::= { vmvcAimStatDistribVSwitchEntry 21 }

vmvcAimStatDVSwitchState OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(1),
        ok(2),
        warning(3),
        critical(4)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The state of the Distributed Virtual Switch, as calculated by the combination of OverallStatus,
         Monitor flag, and Alert level."
    ::= { vmvcAimStatDistribVSwitchEntry 22 }

vmvcAimStatDVSwitchPolicySecurityAllowPromis OBJECT-TYPE
    SYNTAX INTEGER{
        false(1),
        true(2),
        notAvail(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The flag to indicate whether or not all traffic is seen on the port."
    ::= { vmvcAimStatDistribVSwitchEntry 23 }

vmvcAimStatDVSwitchPolicySecurityForgedTransmit OBJECT-TYPE
    SYNTAX INTEGER{
        false(1),
        true(2),
        notAvail(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The flag to indicate if different MAC address than that of the virtual network adapter."
    ::= { vmvcAimStatDistribVSwitchEntry 24 }

vmvcAimStatDVSwitchPolicySecurityMACChanges OBJECT-TYPE
    SYNTAX INTEGER{
        false(1),
        true(2),
        notAvail(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The flag to indicate whether or not the Media Access Control (MAC) address can be changed"
    ::= { vmvcAimStatDistribVSwitchEntry 25 }

