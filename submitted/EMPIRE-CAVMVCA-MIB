-- *****************************************************************************
-- Status Distributed Virtual Switch Group
-- *****************************************************************************
vmvcAimStatDistribVSwitchCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Distributed Virtual Switch count in vmvcAimStatDistribVSwitchTable."
    ::= { vmvcAimStatDistribVSwitchGroup 1}

vmvcAimStatDistribVSwitchTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatDistribVSwitchRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Distributed Virtual Switch Table."
    ::= { vmvcAimStatDistribVSwitchGroup 2 }

vmvcAimStatDistribVSwitchEntry OBJECT-TYPE
    SYNTAX VmvcAimStatDistribVSwitchRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatDVSwitchUID }
    ::= { vmvcAimStatDistribVSwitchTable 1 }

VmvcAimStatDistribVSwitchRow ::=
    SEQUENCE {
        vmvcAimStatDVSwitchUID                              INTEGER,
        vmvcAimStatDVSwitchVCUID                            INTEGER,
        vmvcAimStatDVSwitchVCName                           SnmpAdminString,
        vmvcAimStatDVSwitchIndex                            SnmpAdminString,
        vmvcAimStatDVSwitchName                             SnmpAdminString,
        vmvcAimStatDVSwitchUUID                             SnmpAdminString,
        vmvcAimStatDVSwitchDCName                           SnmpAdminString,
        vmvcAimStatDVSwitchDCIndex                          SnmpAdminString,
        vmvcAimStatDVSwitchParentPath                       SnmpAdminString,
        vmvcAimStatDVSwitchProductClass                     SnmpAdminString,
        vmvcAimStatDVSwitchProductName                      SnmpAdminString,
        vmvcAimStatDVSwitchProductVendor                    SnmpAdminString,
        vmvcAimStatDVSwitchProductVersion                   SnmpAdminString,
        vmvcAimStatDVSwitchHost                             SnmpAdminString,
        vmvcAimStatDVSwitchNumPorts                         INTEGER,
        vmvcAimStatDVSwitchMaxPorts                         INTEGER,
        vmvcAimStatDVSwitchDVPortGroup                      SnmpAdminString,
        vmvcAimStatDVSwitchDVUplinkPortGroup                SnmpAdminString,
        vmvcAimStatDVSwitchOverallStatus                    INTEGER,
        vmvcAimStatDVSwitchMonitor                          INTEGER,
        vmvcAimStatDVSwitchAlert                            INTEGER,
        vmvcAimStatDVSwitchState                            INTEGER,
        vmvcAimStatDVSwitchPolicySecurityAllowPromis        INTEGER,
        vmvcAimStatDVSwitchPolicySecurityForgedTransmit     INTEGER,
        vmvcAimStatDVSwitchPolicySecurityMACChanges         INTEGER,
        vmvcAimStatDVSwitchPolicyShapingInAvgBandWidth      INTEGER,
        vmvcAimStatDVSwitchPolicyShapingInBurstSize         INTEGER,
        vmvcAimStatDVSwitchPolicyShapingInEnabled           INTEGER,
        vmvcAimStatDVSwitchPolicyShapingInPeakBandWidth     INTEGER,
        vmvcAimStatDVSwitchPolicyShapingOutAvgBandWidth     INTEGER,
        vmvcAimStatDVSwitchPolicyShapingOutBurstSize        INTEGER,
        vmvcAimStatDVSwitchPolicyShapingOutEnabled          INTEGER,
        vmvcAimStatDVSwitchPolicyShapingOutPeakBandWidth    INTEGER,
        vmvcAimStatDVSwitchUplinkTeamActivePort             SnmpAdminString,
        vmvcAimStatDVSwitchUplinkTeamStandByPort            SnmpAdminString,
        vmvcAimStatDVSwitchUplinkTeamNotifySwitch           INTEGER,
        vmvcAimStatDVSwitchUplinkTeamPolicy                 SnmpAdminString,
        vmvcAimStatDVSwitchUplinkTeamReversePolicy          INTEGER,
        vmvcAimStatDVSwitchUplinkTeamRollingOrder           INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailureChkBeacon       INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailureChkDuplex       INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailureChkErrPct       INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailureChkSpeed        SnmpAdminString,
        vmvcAimStatDVSwitchUplinkTeamFailureFullDuplex      INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailurePercentage      INTEGER,
        vmvcAimStatDVSwitchUplinkTeamFailureSpeed           INTEGER,
        vmvcAimStatDVSwitchVLanID                           INTEGER,
        vmvcAimStatDVSwitchPrivateVLanID                    INTEGER,
        vmvcAimStatDVSwitchVCUUID                           SnmpAdminString
    }

