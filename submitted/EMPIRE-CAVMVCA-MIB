vmvcAimStatDatastoreCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Datastores in vmvcAimStatDatastoreTable."
    ::= { vmvcAimStatDatastoreGroup 1}

vmvcAimStatDatastoreTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatDatastoreRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Datastore table."
    ::= { vmvcAimStatDatastoreGroup 2 }

vmvcAimStatDatastoreEntry OBJECT-TYPE
    SYNTAX VmvcAimStatDatastoreRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatDatastoreUID  }
    ::= { vmvcAimStatDatastoreTable 1 }

VmvcAimStatDatastoreRow ::=
    SEQUENCE {
        vmvcAimStatDatastoreUID                 INTEGER,
        vmvcAimStatDatastoreIndex               SnmpAdminString,
        vmvcAimStatDatastoreName                SnmpAdminString,
        vmvcAimStatDatastoreURL                 SnmpAdminString,
        vmvcAimStatDatastoreCapacity            INTEGER,
        vmvcAimStatDatastoreFreeSpace           Gauge,
        vmvcAimStatDatastoreFreeSpacePct        Gauge,
        vmvcAimStatDatastoreMaxFileSize         INTEGER,
        vmvcAimStatDatastoreType                SnmpAdminString,
        vmvcAimStatDatastoreMultiHostAccess     INTEGER,
        vmvcAimStatDatastoreAccessible          INTEGER,
        vmvcAimStatDatastoreMonitor             INTEGER,
        vmvcAimStatDatastoreFreeSpaceAlert      INTEGER,
        vmvcAimStatDatastoreFreeSpaceWarnThresh INTEGER,
        vmvcAimStatDatastoreFreeSpaceCritThresh INTEGER,
        vmvcAimStatDatastoreFreeSpaceState      INTEGER,
        vmvcAimStatDatastoreAccessibleAlert     INTEGER,
        vmvcAimStatDatastoreAccessState         INTEGER,
        vmvcAimStatDatastoreHostList            SnmpAdminString,
        vmvcAimStatDatastoreHostNameList        SnmpAdminString,
        vmvcAimStatDatastoreUsedSpace           Gauge,
        vmvcAimStatDatastoreUsedSpacePct        Gauge,
        vmvcAimStatDatastoreVCUID               INTEGER,
        vmvcAimStatDatastoreVCName              SnmpAdminString,
        vmvcAimStatDatastoreVCUUID              SnmpAdminString
    }

