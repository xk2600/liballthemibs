    ::= { vmvcAimStatVMEntry 101 }


-- *****************************************************************************
-- Status VM vApp Group
-- *****************************************************************************
vmvcAimStatVMvAppCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of VMs in vmvcAimStatVMvAppTable."
    ::= { vmvcAimStatVMGroup  3}

vmvcAimStatVMvAppTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatVMvAppRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "VM vApp extension table.  This table will have a subset of the VM Table
         within it, as it only shows the vApp extension information for those VMs that reside within a
         vApp, or has 'vApp configuration' enabled.  Correlation data between these two tables are:
            UID             ->  Consistent between both tables
            Index + VCName  ->  Consistent & Unique"
    ::= { vmvcAimStatVMGroup  4}

vmvcAimStatVMvAppEntry OBJECT-TYPE
    SYNTAX VmvcAimStatVMvAppRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatVMvAppUID }
    ::= { vmvcAimStatVMvAppTable 1 }

VmvcAimStatVMvAppRow ::=
    SEQUENCE {
        vmvcAimStatVMvAppUID                    INTEGER,
        vmvcAimStatVMvAppIndex                  SnmpAdminString,
        vmvcAimStatVMvAppName                   SnmpAdminString,
        vmvcAimStatVMvAppVCUID                  INTEGER,
        vmvcAimStatVMvAppVCName                 SnmpAdminString,
        vmvcAimStatVMvAppOptions  	            INTEGER,
        vmvcAimStatVMvAppParentVApp	            SnmpAdminString,
        vmvcAimStatVMvAppInstallBootRequired    INTEGER,
        vmvcAimStatVMvAppInstallBootStopDelay   INTEGER,
        vmvcAimStatVMvAppIpAllocationPolicy     INTEGER,
        vmvcAimStatVMvAppSupportedIpProtocol    SnmpAdminString,
        vmvcAimStatVMvAppIpProtocol             SnmpAdminString,
        vmvcAimStatVMvAppProductAppURL          SnmpAdminString,
        vmvcAimStatVMvAppProductVersion         SnmpAdminString,
        vmvcAimStatVMvAppProductFullVersion     SnmpAdminString,
        vmvcAimStatVMvAppProductName            SnmpAdminString,
        vmvcAimStatVMvAppProductURL             SnmpAdminString,
        vmvcAimStatVMvAppProductVendor          SnmpAdminString,
        vmvcAimStatVMvAppProductVendorURL       SnmpAdminString,
        vmvcAimStatVMvAppStartAction            INTEGER,
        vmvcAimStatVMvAppStartOrder             INTEGER,
        vmvcAimStatVMvAppStartDelay             INTEGER,
        vmvcAimStatVMvAppStartWaitForGuest      INTEGER,
        vmvcAimStatVMvAppStopAction             INTEGER,
        vmvcAimStatVMvAppStopDelay              INTEGER,
        vmvcAimStatVMvAppVCUUID                 SnmpAdminString
    }

vmvcAimStatVMvAppUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "UID of the VM"
    ::= { vmvcAimStatVMvAppEntry  1}

vmvcAimStatVMvAppIndex OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Moid of the VM"
    ::= { vmvcAimStatVMvAppEntry  2}

