    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "vApp (MOID) that this vApp is a primary member of."
    ::= { vmvcAimStatVAppEntry  6}

vmvcAimStatVAppOperatingStatus OBJECT-TYPE
    SYNTAX INTEGER{
        started(1),
        stopped(2),
        starting(3),
        stopping(4),
        notApplicable(5)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Inidicates the current execution status of this vApp. notApplicable indicates that
         this vApp has no children, so the operating status cannot be validly specified."
    ::= { vmvcAimStatVAppEntry  7}

vmvcAimStatVAppInstallBootRequired OBJECT-TYPE
    SYNTAX INTEGER{
        false(1),
        true(2)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Indicates whether one or more VMs in this vApp require a reboot to finish installation. "
    ::= { vmvcAimStatVAppEntry  8}

vmvcAimStatVAppIpAllocationPolicy OBJECT-TYPE
    SYNTAX INTEGER{
        fixed(1),
        transient(2),
        dhcp(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Specifies how IP allocation should be managed by the VI platform.
            fixed     - IP addresses are manually configured. No automatic allocation is performed.
            transient - IP addresses are automatically allocated from the vCenter managed IP network range
                        at power-on, and released at power-off.
            dhcp      - A DHCP server is used for IP allocation."
    ::= { vmvcAimStatVAppEntry  9}

vmvcAimStatVAppSupportedIpProtocol OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Specifies the IP protocols supported by the guest software."
    ::= { vmvcAimStatVAppEntry 10}

