    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of monitored objects in a warning state."
    ::= { vmvcAimStatGeneralGroup 2 }

vmvcAimStatGenTotalCrit OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of monitored objects in a critical state."
    ::= { vmvcAimStatGeneralGroup 3 }

