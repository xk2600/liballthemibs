-- *****************************************************************************
-- Status PNIC Group
-- *****************************************************************************
vmvcAimStatPNICCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Physical NIC count in vmvcAimStatPNICTable."
    ::= { vmvcAimStatPNICGroup 1}

vmvcAimStatPNICTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatPNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Physical NIC table."
    ::= { vmvcAimStatPNICGroup 2 }

vmvcAimStatPNICEntry OBJECT-TYPE
    SYNTAX VmvcAimStatPNICRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatPNICUID }
    ::= { vmvcAimStatPNICTable 1 }

VmvcAimStatPNICRow ::=
    SEQUENCE {
        vmvcAimStatPNICUID              INTEGER,
        vmvcAimStatPNICIndex            SnmpAdminString,
        vmvcAimStatPNICHost             SnmpAdminString,
        vmvcAimStatPNICName             SnmpAdminString,
        vmvcAimStatPNICPCI              SnmpAdminString,
        vmvcAimStatPNICVendor           SnmpAdminString,
        vmvcAimStatPNICDriver           SnmpAdminString,
        vmvcAimStatPNICIPIsDHCP         INTEGER,
        vmvcAimStatPNICIP               SnmpAdminString,
        vmvcAimStatPNICSubnetmask       SnmpAdminString,
        vmvcAimStatPNICLinkSpeedDuplex  INTEGER,
        vmvcAimStatPNICLinkSpeedSpeedMb INTEGER,
        vmvcAimStatPNICVirtualSwitch    SnmpAdminString,
        vmvcAimStatPNICLinkState        INTEGER,
        vmvcAimStatPNICMonitor          INTEGER,
        vmvcAimStatPNICAlert            INTEGER,
        vmvcAimStatPNICState            INTEGER,
        vmvcAimStatPNICHostName         SnmpAdminString,
        vmvcAimStatPNICPktTxt           INTEGER,
        vmvcAimStatPNICPktTxtPerSec     INTEGER,
        vmvcAimStatPNICPktRcvd          INTEGER,
        vmvcAimStatPNICPktRcvdPerSec    INTEGER,
        vmvcAimStatPNICKBPSTxt          INTEGER,
        vmvcAimStatPNICKBPSTxtPerSec    INTEGER,
        vmvcAimStatPNICKBPSRcvd         INTEGER,
        vmvcAimStatPNICKBPSRcvdPerSec   INTEGER,
        vmvcAimStatPNICMAC              SnmpAdminString,
        vmvcAimStatPNICVSwitchUID       INTEGER,
        vmvcAimStatPNICVCUID            INTEGER,
        vmvcAimStatPNICVCName           SnmpAdminString,
        vmvcAimStatPNICDVUplinkPGIndex  SnmpAdminString,
        vmvcAimStatPNICDVPortGroupUID   INTEGER,
        vmvcAimStatPNICDVPortKey        SnmpAdminString,
        vmvcAimStatPNICDVUplink         SnmpAdminString,
        vmvcAimStatPNICVCUUID           SnmpAdminString
    }

vmvcAimStatPNICUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "UID of PNIC"
    ::= { vmvcAimStatPNICEntry  1}

