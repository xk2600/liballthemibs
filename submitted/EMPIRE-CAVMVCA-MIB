-- *****************************************************************************
-- Status ResPool vApp Group
-- *****************************************************************************
vmvcAimStatVAppCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of vApps in vmvcAimStatVAppTable."
    ::= { vmvcAimStatResPoolGroup 3}

vmvcAimStatVAppTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatVAppRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "vApp extension table.  This table will have a subset of the Resource Pool table
         within it, as it only shows the vApp extension information for those Resource
         Pools which are also Vapps.  Correlation data between these two tables are:
            UID             ->  Consistent between both tables
            Index + VCName  ->  Consistent & Unique"
    ::= { vmvcAimStatResPoolGroup 4 }

vmvcAimStatVAppEntry OBJECT-TYPE
    SYNTAX VmvcAimStatVAppRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatVAppUID  }
    ::= { vmvcAimStatVAppTable 1 }

VmvcAimStatVAppRow ::=
    SEQUENCE {
        vmvcAimStatVAppUID                   INTEGER,
        vmvcAimStatVAppIndex                 SnmpAdminString,
        vmvcAimStatVAppName                  SnmpAdminString,
        vmvcAimStatVAppVCUID                 INTEGER,
        vmvcAimStatVAppVCName                SnmpAdminString,
        vmvcAimStatVAppParentVApp            SnmpAdminString,
        vmvcAimStatVAppOperatingStatus       INTEGER,
        vmvcAimStatVAppInstallBootRequired   INTEGER,
        vmvcAimStatVAppIpAllocationPolicy    INTEGER,
        vmvcAimStatVAppSupportedIpProtocol   SnmpAdminString,
        vmvcAimStatVAppIpProtocol            SnmpAdminString,
        vmvcAimStatVAppProductAppURL         SnmpAdminString,
        vmvcAimStatVAppProductVersion        SnmpAdminString,
        vmvcAimStatVAppProductFullVersion    SnmpAdminString,
        vmvcAimStatVAppProductName           SnmpAdminString,
        vmvcAimStatVAppProductURL            SnmpAdminString,
        vmvcAimStatVAppProductVendor         SnmpAdminString,
        vmvcAimStatVAppProductVendorURL      SnmpAdminString,
        vmvcAimStatVAppStartAction           INTEGER,
        vmvcAimStatVAppStartOrder            INTEGER,
        vmvcAimStatVAppStopAction            INTEGER,
        vmvcAimStatVAppVCUUID                SnmpAdminString
    }

vmvcAimStatVAppUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "UID of the vApp"
    ::= { vmvcAimStatVAppEntry  1}

