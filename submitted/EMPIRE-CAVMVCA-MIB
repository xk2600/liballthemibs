-- *****************************************************************************
-- Status Port Group
-- *****************************************************************************
vmvcAimStatPortGrpCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Port Group count in vmvcAimStatPortGrpTable."
    ::= { vmvcAimStatPortGrpGroup 1}

vmvcAimStatPortGrpTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatPortGrpRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Port Group."
    ::= { vmvcAimStatPortGrpGroup 2 }

vmvcAimStatPortGrpEntry OBJECT-TYPE
    SYNTAX VmvcAimStatPortGrpRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatPGUID }
    ::= { vmvcAimStatPortGrpTable 1 }

VmvcAimStatPortGrpRow ::=
    SEQUENCE {
        vmvcAimStatPGUID                            INTEGER,
        vmvcAimStatPGName                           SnmpAdminString,
        vmvcAimStatPGHostIndex                      SnmpAdminString,
        vmvcAimStatPGVlanId                         SnmpAdminString,
        vmvcAimStatPGVSwitchName                    SnmpAdminString,
        vmvcAimStatPGPorts                          SnmpAdminString,
        vmvcAimStatPGPolicyOffloadCkhSum            INTEGER,
        vmvcAimStatPGPolicyOffloadtcpSegmt          INTEGER,
        vmvcAimStatPGPolicyOffloadZeroCpyX          INTEGER,
        vmvcAimStatPGPolicySecurityAllowPromis      INTEGER,
        vmvcAimStatPGPolicySecurityForgedTransmit   INTEGER,
        vmvcAimStatPGPolicySecurityMACChanges       INTEGER,
        vmvcAimStatPGHostName                       SnmpAdminString,
        vmvcAimStatPGVSwitchUID                     INTEGER,
        vmvcAimStatPGPolicyShapingAvgBandWidth      INTEGER,
        vmvcAimStatPGPolicyShapingBurstSize         INTEGER,
        vmvcAimStatPGPolicyShapingEnabled           INTEGER,
        vmvcAimStatPGPolicyShapingPeakBandWidth     INTEGER,
        vmvcAimStatPGNicTeamAvailableNic            SnmpAdminString,
        vmvcAimStatPGNicTeamActiveNic               SnmpAdminString,
        vmvcAimStatPGNicTeamStandByNic              SnmpAdminString,
        vmvcAimStatPGNicTeamNotifySwitch            INTEGER,
        vmvcAimStatPGNicTeamPolicy                  SnmpAdminString,
        vmvcAimStatPGNicTeamReversePolicy           INTEGER,
        vmvcAimStatPGNicTeamRollingOrder            INTEGER,
        vmvcAimStatPGNicTeamFailureChkBeacon        INTEGER,
        vmvcAimStatPGNicTeamFailureChkDuplex        INTEGER,
        vmvcAimStatPGNicTeamFailureChkErrPct        INTEGER,
        vmvcAimStatPGNicTeamFailureChkSpeed         SnmpAdminString,
        vmvcAimStatPGNicTeamFailureFullDuplex       INTEGER,
        vmvcAimStatPGNicTeamFailurePercentage       INTEGER,
        vmvcAimStatPGNicTeamFailureSpeed            INTEGER,    
        vmvcAimStatPGVCUID                          INTEGER,
        vmvcAimStatPGVCName                         SnmpAdminString,
        vmvcAimStatPGVCUUID                         SnmpAdminString
    }

vmvcAimStatPGUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "UID of the port group"
    ::= { vmvcAimStatPortGrpEntry  1}

