EMPIRE-CAVMVCA-MIB DEFINITIONS ::= BEGIN


IMPORTS
    OBJECT-TYPE
        FROM RFC-1212
    TRAP-TYPE
        FROM RFC-1215
    SnmpAdminString
        FROM SNMP-FRAMEWORK-MIB
    enterprises, TimeTicks, Gauge
        FROM RFC1155-SMI;

empire          OBJECT IDENTIFIER ::= { enterprises 546 }
unix            OBJECT IDENTIFIER ::= { empire 1 }
sysmgmt         OBJECT IDENTIFIER ::= { unix 1 }

traps           OBJECT IDENTIFIER ::= { empire 9 }
applications    OBJECT IDENTIFIER ::= { empire 16 }

cavmvcaim       OBJECT IDENTIFIER ::= { applications 52 }

--cavmvcaim MODULE-IDENTITY
--  LAST-UPDATED "201201240000Z"  
--  ORGANIZATION "CA Technologies, Inc."
--  CONTACT-INFO "http://www.ca.com"
--  DESCRIPTION  "This MIB module defines the 
--                CA Vmware vCenter Aim."

--  REVISION     "201201240000Z"
--  DESCRIPTION  "Added vCenter UUID to all status tables and appropriate traps."

--  REVISION     "201109200000Z"
--  DESCRIPTION  "Added vmvcAimStatNetFolderTable and supporting items.
--                Added vmvcAimStatVAppTable and supporting items.
--                Added vmvcAimStatVMvAppTable and supporting items."

--  REVISION     "201106300000Z"
--  DESCRIPTION  "Added vmvcAimStatHostCPUAvailableForVM.
--                Added vmvcAimStatHostTotalVMCPUUsage.
--                Added vmvcAimStatVMTotalHostMemoryUsage.
--                Widened size of vmvcAimStatHostDatastore.
--                Expanded varbind list of traps to support Multi-Instance VC AIM mode."

--  REVISION     "201105100000Z"
--  DESCRIPTION  "Deprecated vmvcAimStatCustomizationSpecLastUpdated.
--                Added vmvcAimStatCustomizationSpecLastUpdateTimeSec.
--                Fault Tolerance attributes added to vmvcAimStatHostTable and vmvcAimStatVMTable.
--                Added vmvcAimStatVMVDiskHostName.
--                Added vmvcAimStatVMVNICHostName.
--                Added vmvcAimStatCustomSpecUID.
--                Corrected varbind list for Datacenter State Change traps."

--  REVISION     "201102180000Z"
--  DESCRIPTION  "Added Distributed Virtual Switch and Port Group support.
--                Corrected enumeration for vmvcAimStatComputeResourceOverallState.
--                Added vmvcAimStatHostHardwareUUID.
--                Widened size of vmvcAimStatSnapshotDescription.

--  REVISION     "201010150000Z"
--  DESCRIPTION  "Added vmvcAimStatHostMaxSupportedVCPUs.
--                Added vmvcAimStatVMVDiskUnitNumber.
--                Added Disk Latency attributes to vmvcAimStatHostDiskTable.
--                Added vmvcAimStatHostVNICIpV6."

--  REVISION     "201009220000Z"
--  DESCRIPTION  "Added Multi-Instance Configuration support.
--                Widened enumeration of vmvcAimStatVMAlertHeartbeatLoss.
--                Added Port Group policy attributes to vmvcAimStatPortGrpTable.

--  REVISION     "201008200000Z"
--  DESCRIPTION  "Finalization of Single-Instance-Compatibility mode updates.
--                Addition of Local CPU and Memory attributes to vmvcAimStatVCTable.
--                Added vmvcAimStatVMSyncTimeWithHost."

--  REVISION     "201008122200Z"
--  DESCRIPTION  "Additional Single-Instance Compatibility mode updates."

--  REVISION     "201007292200Z" 
--  DESCRIPTION  "Supply VMKernelIP, StaticSwitch, StorageSensor, ResourcePool Type, and GuestDisk Enhancemements.  Single-Instance Compatibility mode updates."

--  REVISION     "201007071300Z" 
--  DESCRIPTION  "convert DisplayString elements to SnmpAdminString"

--  ::= { applications 52 }


vmvcAimConfigGroup      OBJECT IDENTIFIER ::= { cavmvcaim 1 }
