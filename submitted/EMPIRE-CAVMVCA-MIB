-- *****************************************************************************
-- Status Snapshot Specification Group
-- *****************************************************************************

vmvcAimStatSnapshotCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Snapshot in vmvcAimStatSnapshotTable."
    ::= { vmvcAimStatSnapshotGroup 1}

vmvcAimStatSnapshotTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatSnapshotRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "Customization Specification table."
    ::= { vmvcAimStatSnapshotGroup 2 }

vmvcAimStatSnapshotEntry OBJECT-TYPE
    SYNTAX VmvcAimStatSnapshotRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatSnapshotUID }
    ::= { vmvcAimStatSnapshotTable 1 }

VmvcAimStatSnapshotRow ::=
    SEQUENCE {
        vmvcAimStatSnapshotUID          INTEGER,
        vmvcAimStatSnapshotParent       SnmpAdminString,
        vmvcAimStatSnapshotName         SnmpAdminString,
        vmvcAimStatSnapshotDescription  SnmpAdminString,
        vmvcAimStatSnapshotID           SnmpAdminString,
        vmvcAimStatSnapshotTime         INTEGER,
        vmvcAimStatSnapshotMemoryState  INTEGER,
        vmvcAimStatSnapshotQuiesce      INTEGER,
        vmvcAimStatSnapshotVMIndex      SnmpAdminString,
        vmvcAimStatSnapshotCurrent      INTEGER,
        vmvcAimStatSnapshotVCUID        INTEGER,
        vmvcAimStatSnapshotVCName       SnmpAdminString,
        vmvcAimStatSnapshotVCUUID       SnmpAdminString
    }

vmvcAimStatSnapshotUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer UID of the snapshot."
    ::= { vmvcAimStatSnapshotEntry  1}

vmvcAimStatSnapshotParent OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "ID of the parent snapshot or ID of the VM if there is 
         no parent snapshot."
    ::= { vmvcAimStatSnapshotEntry  2}

vmvcAimStatSnapshotName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Name of the snapshot."
    ::= { vmvcAimStatSnapshotEntry  3}

vmvcAimStatSnapshotDescription OBJECT-TYPE
    SYNTAX SnmpAdminString (SIZE(0..1024))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Description of the snapshot."
    ::= { vmvcAimStatSnapshotEntry  4}

vmvcAimStatSnapshotID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Id of the snapshot."
    ::= { vmvcAimStatSnapshotEntry  5}

vmvcAimStatSnapshotTime OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of seconds elapsed since midnight (00:00:00), January 1, 1970, 
         coordinated universal time (UTC) when the snapshot was taken."
    ::= { vmvcAimStatSnapshotEntry  6}

vmvcAimStatSnapshotMemoryState OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "A boolean Value used to represent the internal state of a virtual machine.
         If TRUE, a dump of the internal state of the virtual machine (basically a memory dump) is included in the snapshot.
         If FALSE, the power state of the snapshot is set to powered off. "
    ::= { vmvcAimStatSnapshotEntry  7}

vmvcAimStatSnapshotQuiesce OBJECT-TYPE
    SYNTAX INTEGER 
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "A boolean Value used for the quiesce the file system in the virtual machine.
         If TRUE and the virtual machine is powered on when the snapshot is taken, VMware Tools is used to quiesce the file
         system in the virtual machine. 
         If the    virutal machine is powered off or VMware Tools are not available, the quiesce flag is ignored."
    ::= { vmvcAimStatSnapshotEntry  8}

vmvcAimStatSnapshotVMIndex OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The index (VMIndex) of the VM the snapshot is associated with."
    ::= { vmvcAimStatSnapshotEntry 9 }

vmvcAimStatSnapshotCurrent OBJECT-TYPE
    SYNTAX INTEGER {
        true(1),
        false(2)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "A boolean value indicating whether this snapshot is the current snapshot the VM is running."
    ::= { vmvcAimStatSnapshotEntry  10}

vmvcAimStatSnapshotVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance SNMP UID where this Snapshot resides."
    ::= { vmvcAimStatSnapshotEntry 11 }

vmvcAimStatSnapshotVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this Snapshot resides."
    ::= { vmvcAimStatSnapshotEntry 12 }

vmvcAimStatSnapshotVCUUID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The vCenter-specific UUID where this Snapshot resides."
    ::= { vmvcAimStatSnapshotEntry 13 }


-- *****************************************************************************
-- vmvcAimEventGroup 
-- *****************************************************************************
vmvcAimEventCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of events in event table."
    ::= {vmvcAimEventGroup 1}

vmvcAimEventCountMax OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The maximum number of events to be stored in the event table, after which the rows start getting overwritten."
    DEFVAL { 500 }
    ::= {vmvcAimEventGroup 2}

vmvcAimEventHistory OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The number of days the events are maintained. The value is in the number of days."
    DEFVAL {30}
    ::= {vmvcAimEventGroup 3}

vmvcAimEventClear OBJECT-TYPE
    SYNTAX INTEGER {
            do-not-clear(1),
                   clear(2)
            }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Flag indicating whether to clear all the events in the aimstatuseventtable or not. 1)DO_NOT_CLEAR 2)CLEAR."
    DEFVAL { 1 }    
    ::= {vmvcAimEventGroup 4}

vmvcAimEventTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimEventRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "A Event table."
    ::= {vmvcAimEventGroup 5}

vmvcAimEventEntry OBJECT-TYPE
    SYNTAX VmvcAimEventRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimEventUID }
    ::= { vmvcAimEventTable 1 }

VmvcAimEventRow ::=
    SEQUENCE {
        vmvcAimEventUID             INTEGER,
        vmvcAimEventID              SnmpAdminString,
        vmvcAimEventplatformType    SnmpAdminString,
        vmvcAimEventplatformName    SnmpAdminString,
        vmvcAimEventObjType         SnmpAdminString,
        vmvcAimEventObjName         SnmpAdminString,
        vmvcAimEventOldState        INTEGER,
        vmvcAimEventNewState        INTEGER,
        vmvcAimEventOldValue        SnmpAdminString,
        vmvcAimEventNewValue        SnmpAdminString,
        vmvcAimEventDateTime        SnmpAdminString,
        vmvcAimEventDateTimeSec     INTEGER,
        vmvcAimEventCfgFile         SnmpAdminString,
        vmvcAimEventVCUID           INTEGER,
        vmvcAimEventVCName          SnmpAdminString
    }

vmvcAimEventUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        " Event UID."
    ::= { vmvcAimEventEntry 1 }

vmvcAimEventID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Event ID."
    ::= { vmvcAimEventEntry 2 }

vmvcAimEventplatformType OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Platform Type."
    ::= { vmvcAimEventEntry 3 }

vmvcAimEventplatformName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Platform Name."
    ::= { vmvcAimEventEntry 4 }

vmvcAimEventObjType OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Object Type."
    ::= { vmvcAimEventEntry 5 }

vmvcAimEventObjName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Object Name."
    ::= { vmvcAimEventEntry 6 }

vmvcAimEventOldState OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(1),
        ok(2),
        warning(3),
        critical(4)
    }    
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Old State."
    ::= { vmvcAimEventEntry 7 }

vmvcAimEventNewState OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(1),
        ok(2),
        warning(3),
        critical(4)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The New state."
    ::= { vmvcAimEventEntry 8 }

vmvcAimEventOldValue OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The Old Value."
    ::= { vmvcAimEventEntry 9 }

vmvcAimEventNewValue OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The New Value."
    ::= { vmvcAimEventEntry 10 }

vmvcAimEventDateTime OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Date and Time.  This attribute is now DEPRECATED and will return empty data, 
         as it does not support I18N conventions."
    ::= { vmvcAimEventEntry 11 }

vmvcAimEventDateTimeSec     OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The time the event occured in seconds."
    ::= { vmvcAimEventEntry 12 }

vmvcAimEventCfgFile OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The name of the configuration file."
    ::= { vmvcAimEventEntry 13 }

vmvcAimEventVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance UID where this event occurred."
    ::= { vmvcAimEventEntry 14 }

vmvcAimEventVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this event occurred."
    ::= { vmvcAimEventEntry 15 }


