        vmvcAimStatVMResourcePoolName           SnmpAdminString,
        vmvcAimStatVMFullPath                   SnmpAdminString,
        vmvcAimStatVMHostName                   SnmpAdminString,
        vmvcAimStatVMHostID                     SnmpAdminString,
        vmvcAimStatVMClusterName                SnmpAdminString,
        vmvcAimStatVMParentID                   SnmpAdminString,
        vmvcAimStatVMDatacenterName             SnmpAdminString,
        vmvcAimStatVMVMFolderName               SnmpAdminString,
        vmvcAimStatVMIPv4Address                SnmpAdminString,
        vmvcAimStatVMIPv6Address                SnmpAdminString,
        vmvcAimStatVMGuestOS                    SnmpAdminString,
        vmvcAimStatVMUpTime                     TimeTicks,
        vmvcAimStatVMMonitor                    INTEGER,
        vmvcAimStatVMPowerStatus                INTEGER,
        vmvcAimStatVMAlertPowerOffStatus        INTEGER,
        vmvcAimStatVMPowerState                 INTEGER,
        vmvcAimStatVMHeartbeat                  INTEGER,
        vmvcAimStatVMAlertHeartbeatLoss         INTEGER,
        vmvcAimStatVMHeartbeatState             INTEGER,
        vmvcAimStatVMCPUSharesAssigned          INTEGER, 
        vmvcAimStatVMCPUSharesActive            INTEGER,
        vmvcAimStatVMCPUSharesPercentage        INTEGER,
        vmvcAimStatVMCPUSharesWarnThreshold     INTEGER,
        vmvcAimStatVMCPUSharesCritThreshold     INTEGER,
        vmvcAimStatVMCPUAlert                   INTEGER,
        vmvcAimStatVMCPUState                   INTEGER,
        vmvcAimStatVMCPUAffinity                SnmpAdminString,
        vmvcAimStatVMMemorySharesAssigned       INTEGER,
        vmvcAimStatVMMemoryMin                  INTEGER,
        vmvcAimStatVMMemoryMax                  INTEGER,
        vmvcAimStatVMMemoryActual               INTEGER,
        vmvcAimStatVMMemoryActive               INTEGER,
        vmvcAimStatVMMemoryPercentage           INTEGER,
        vmvcAimStatVMMemoryWarnThreshold        INTEGER,
        vmvcAimStatVMMemoryCritThreshold        INTEGER, 
        vmvcAimStatVMMemoryAlert                INTEGER,
        vmvcAimStatVMMemoryState                INTEGER,
        vmvcAimStatVMMemoryCtl                  INTEGER,
        vmvcAimStatVMMemorySwapped              INTEGER,
        vmvcAimStatVMMemoryShared               INTEGER,
