    ::= { vmvcAimStatVCGroup  45}

vmvcAimStatVCTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatVCRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "A table containing the Virtual Center Instance 
         status information."
    ::= { vmvcAimStatVCGroup  46}

vmvcAimStatVCEntry OBJECT-TYPE
    SYNTAX VmvcAimStatVCRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The Virtual Center Instance Status Table entry."
    INDEX { vmvcAimStatVCUID }
    ::= { vmvcAimStatVCTable 1 }

VmvcAimStatVCRow ::=
    SEQUENCE {
        vmvcAimStatVCUID                            INTEGER,
        vmvcAimStatVCName                           SnmpAdminString,
        vmvcAimStatVCProduct                        SnmpAdminString,
        vmvcAimStatVCOSType                         SnmpAdminString,
        vmvcAimStatVCVer                            SnmpAdminString,
        vmvcAimStatVCAPIVer                         SnmpAdminString,
        vmvcAimStatVCBuild                          SnmpAdminString,
        vmvcAimStatVCProdLine                       SnmpAdminString,
        vmvcAimStatVCStatusAlert                    INTEGER,
        vmvcAimStatVCStatusMonitor                  INTEGER,
        vmvcAimStatVCState                          INTEGER,
        vmvcAimStatVCLocalCPUUsage                  INTEGER,
        vmvcAimStatVCLocalCPUUsageWarn              INTEGER,
        vmvcAimStatVCLocalCPUUsageCrit              INTEGER,
        vmvcAimStatVCLocalCPUUsageAlert             INTEGER,
        vmvcAimStatVCLocalCPUState                  INTEGER,
        vmvcAimStatVCLocalMemUsage                  INTEGER,
        vmvcAimStatVCLocalTotalPhysMem              INTEGER,
        vmvcAimStatVCLocalUsedPhysMem               INTEGER,
        vmvcAimStatVCLocalMemWarn                   INTEGER,
        vmvcAimStatVCLocalMemCrit                   INTEGER,
        vmvcAimStatVCLocalMemAlert                  INTEGER,
        vmvcAimStatVCLocalMemState                  INTEGER,
        vmvcAimStatVCTotalCPUUsage                  INTEGER,
        vmvcAimStatVCTotalCPUUsageWarn              INTEGER,
        vmvcAimStatVCTotalCPUUsageCrit              INTEGER,
        vmvcAimStatVCTotalCPUState                  INTEGER,
        vmvcAimStatVCTotalMemUsage                  INTEGER,
        vmvcAimStatVCTotalMemWarn                   INTEGER,
        vmvcAimStatVCTotalMemCrit                   INTEGER,
        vmvcAimStatVCTotalMemState                  INTEGER,
        vmvcAimStatVCTotalDatastoreCapacity         INTEGER,
        vmvcAimStatVCTotalDatastoreFreeSpace        INTEGER,
        vmvcAimStatVCTotalDatastoreFreeSpacePct     INTEGER,
        vmvcAimStatVCTotalDatastoreFreeSpaceWarn    INTEGER,
        vmvcAimStatVCTotalDatastoreFreeSpaceCrit    INTEGER,
        vmvcAimStatVCTotalDatastoreFreeSpaceState   INTEGER,
        vmvcAimStatVCLocalCPUMonitor                INTEGER,
        vmvcAimStatVCLocalCPULagSetting             INTEGER,
        vmvcAimStatVCLocalCPULagValue               INTEGER,
        vmvcAimStatVCLocalCPUAggregateState         INTEGER,
        vmvcAimStatVCLocalMemoryMonitor             INTEGER,
        vmvcAimStatVCIPAddress                      SnmpAdminString,
        vmvcAimStatVCMAC                            SnmpAdminString,
        vmvcAimStatVCUUID                           SnmpAdminString
    }

