        vmvcAimStatVMvAppStartDelay,
        vmvcAimStatVMvAppStartWaitForGuest,
        vmvcAimStatVMvAppStopAction,
        vmvcAimStatVMvAppStopDelay,
        vmvcAimStatVMvAppVCUUID
    }
    DESCRIPTION
        "The specified VM has had a vApp configuration change within vCenter applied to it.  This 
         trap is a notification of this detected change.  Current values for the above attributes are sent.
         Enabled with Trap Enable Flag 0x1."
    ::= 165968

vmvcAimNetFolderAddedEvent OBJECT IDENTIFIER ::= { traps 165970 }
vmvcAimNetFolderAddedTrap TRAP-TYPE
    ENTERPRISE sysmgmt
    VARIABLES {
        vmvcAimStatNetworkFolderIndex,
        vmvcAimStatNetworkFolderName,
        vmvcAimStatNetworkFolderVCName,
        vmvcAimStatNetworkFolderParentId,
        vmvcAimStatNetworkFolderParentPath,
        vmvcAimStatNetworkFolderVCUUID
    }
    DESCRIPTION
        "A new Network Folder has been added to VC.
         Enabled with Trap Enable Flag 0x1."
    ::= 165970

vmvcAimNetFolderRemovedEvent OBJECT IDENTIFIER ::= { traps 165971 }
vmvcAimNetFolderRemovedTrap TRAP-TYPE
    ENTERPRISE sysmgmt
    VARIABLES {
        vmvcAimStatNetworkFolderIndex,
        vmvcAimStatNetworkFolderName,
        vmvcAimStatNetworkFolderVCName,
        vmvcAimStatNetworkFolderParentId,
        vmvcAimStatNetworkFolderParentPath,
        vmvcAimStatNetworkFolderVCUUID
    }
    DESCRIPTION
        "The specified Network Folder has been removed from VC.
         Enabled with Trap Enable Flag 0x1."
    ::= 165971

vmvcAimNetFolderConfigChangeEvent OBJECT IDENTIFIER ::= { traps 165972 }
vmvcAimNetFolderConfigChangeTrap TRAP-TYPE
    ENTERPRISE sysmgmt
    VARIABLES {
        vmvcAimStatNetworkFolderIndex,
        vmvcAimStatNetworkFolderName,
        vmvcAimStatNetworkFolderVCName,
        vmvcAimStatNetworkFolderParentId,
        vmvcAimStatNetworkFolderParentPath,
        vmvcAimStatNetworkFolderVCUUID
    }
    DESCRIPTION
        "The configuration information for the specified Network Folder has been modified.
         Enabled with Trap Enable Flag 0x1."
    ::= 165972

END
