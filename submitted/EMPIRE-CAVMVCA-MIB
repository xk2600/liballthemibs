    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The gateway for IP address that is to be assigned to the NIC."
    ::= { vmvcAimStatCustomSpecNICEntry 7 }
    
