    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Indicates whether or not the tools program will sync time with the host time."
    ::= { vmvcAimStatVMEntry 88 }

vmvcAimStatVMVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance SNMP UID where this VM resides."
    ::= { vmvcAimStatVMEntry 89 }

vmvcAimStatVMVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this VM resides."
    ::= { vmvcAimStatVMEntry 90 }

vmvcAimStatVMFaultToleranceStatus OBJECT-TYPE
    SYNTAX INTEGER{
        notConfigured(1),
        disabled(2),
        enabled(3),
        needSecondary(4),
        starting(5),
        running(6)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Enumeration indicating the current Fault Tolerance State of this VM.
            notConfigured - Not configured for FT
            disabled      - FT enabled, not protected (disabled mode)
            enabled       - FT enabled, not protected (not running)
            needSecondary - FT enabled, not protected (in need of Secondary VM)
            starting      - FT enabled, not protected (starting)
            running       - FT enabled, protected"
    ::= { vmvcAimStatVMEntry 91 }

vmvcAimStatVMFTActivityState OBJECT-TYPE
    SYNTAX INTEGER{
        recording(1),
        replaying(2),
        inactive(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Enumeration indicating the current Fault Tolerance Activity State of this VM.
            recording - This FT VM is primary and is caching its activities for the secondary.
            replaying - This FT VM is secondary and is currently receiving the recorded activities of the primary.
            inactive  - This VM is not actively Fault Tolerant."
    ::= { vmvcAimStatVMEntry 92 }

vmvcAimStatVMFaultToleranceRole OBJECT-TYPE
    SYNTAX INTEGER{
        notConfigured(1),
        primary(2),
        secondary(3)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Enumeration indicating the current Fault Tolerance Role of this VM.
            notConfigured - This VM is not configured for Fault Tolerance.
            primary   - This VM is the current primary Fault Tolerant VM.
            secondary - This VM is the current secondary Fault Tolerant VM."
    ::= { vmvcAimStatVMEntry 93 }

vmvcAimStatVMFTSiblingIndex OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VM Index (MOID) of this Fault Tolerant VM's Sibling.  This is empty if this VM does 
         not have a Fault Tolerant sibling."
    ::= { vmvcAimStatVMEntry 94 }

vmvcAimStatVMFTSiblingHostIndex OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Host Index (MOID) of this Fault Tolerant VM's Sibling.  This is empty if this VM does 
         not have a Fault Tolerant sibling."
    ::= { vmvcAimStatVMEntry 95 }

vmvcAimStatVMFTSiblingHostName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Host Name of this Fault Tolerant VM's Sibling.  This is empty if this VM does 
         not have a Fault Tolerant sibling."
    ::= { vmvcAimStatVMEntry 96 }

vmvcAimStatVMFTSiblingHostUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Host UID of this Fault Tolerant VM's Sibling.  This is zero(0) if this VM does 
         not have a Fault Tolerant sibling."
    ::= { vmvcAimStatVMEntry 97 }

vmvcAimStatVMFTTotalSiblingCPU OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
