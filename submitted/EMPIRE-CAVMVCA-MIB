        vmvcAimStatVMVDiskLabel                 SnmpAdminString,
        vmvcAimStatVMVDiskCapacity              INTEGER,
        vmvcAimStatVMVDiskShares                INTEGER,
        vmvcAimStatVMVDiskMode                  SnmpAdminString,
        vmvcAimStatVMVDiskFileName              SnmpAdminString,
