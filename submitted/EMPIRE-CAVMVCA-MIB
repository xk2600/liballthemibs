-- *****************************************************************************
-- General Config Group
-- *****************************************************************************
vmvcAimConfigGeneralAgentName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The name of the agent."
    ::= { vmvcAimConfigGeneralGroup 1 }

vmvcAimConfigGeneralAgentVersion OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The currently installed version of the agent."
    ::= { vmvcAimConfigGeneralGroup 2 }

vmvcAimConfigGeneralColdStartTime OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of time-ticks the AIM has been running."
    ::= { vmvcAimConfigGeneralGroup 3 }

vmvcAimConfigGeneralPollInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The time (in seconds) between the polling by the Agent. Valid values are greater than or equal to 30.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 120 }
    ::= { vmvcAimConfigGeneralGroup 4 }

vmvcAimConfigGeneralAgentHostName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The host name on which this agent is running."
    ::= { vmvcAimConfigGeneralGroup 5 }

vmvcAimConfigGeneralWRSLastTime OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of time-ticks since the AIM has fetched the event table.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 6 }

vmvcAimConfigGeneralVCEventPoll OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The time (in seconds) between the polling of VC Events by the Agent. Valid values are greater than 
         or equal to 120.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 120 }
    ::= { vmvcAimConfigGeneralGroup 7 }

vmvcAimConfigGeneralVCEventEnable OBJECT-TYPE
    SYNTAX INTEGER {
        collectAndTrap(1),
        collect(2),
        disable(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events capturing is enabled or disabled.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 3 }
    ::= { vmvcAimConfigGeneralGroup 8 }

vmvcAimConfigGeneralVCEventMonitorInfo OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Info messages capturing is enabled or disabled.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 2 }
    ::= { vmvcAimConfigGeneralGroup 9 }

vmvcAimConfigGeneralVCEventMonitorUser OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events User messages capturing is enabled or disabled.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 2 }
    ::= { vmvcAimConfigGeneralGroup 10 }

vmvcAimConfigGeneralVCEventMonitorError OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Error messages capturing is enabled or disabled.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 2 }
    ::= { vmvcAimConfigGeneralGroup 11 }

vmvcAimConfigGeneralVCEventMonitorWarning OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Warning messages capturing is enabled or disabled.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 2 }
    ::= { vmvcAimConfigGeneralGroup 12 }

vmvcAimConfigGeneralTrapEnableMask OBJECT-TYPE
    SYNTAX INTEGER (0..7)
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "A set of flags to enable classes of traps to be 
         sent by the AIM.  See the individual AIM traps for specifics
             as to which flag enables it.
         The flags are:
         0x1    Send Detected VC Change Traps
         0x2    Send Detected AIM State Change Traps
         0x4    Send AIM Configuration Change Traps"
    DEFVAL { 7 }
    ::= { vmvcAimConfigGeneralGroup 13 }

vmvcAimConfigGeneralLogLevel OBJECT-TYPE
    SYNTAX INTEGER (0..7)
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The Log Level of the AIM.  This impacts how much logging
         will be written to the AIM log file, and is cumulative (i.e.
         Log Level 4 will get all messages at levels 0 through 4).
         Values are:
         0    FATAL
         1    CRITICAL
         2    WARNING
         3    INFO
         4    DEBUG
         5    DEBUG_LOW
         6    DEBUG_LOWER
         7    DEBUG_LOWEST
         Specifying a debug level greater than 4 is discouraged."
    DEFVAL { 2 }
    ::= { vmvcAimConfigGeneralGroup 14 }

vmvcAimConfigGeneralVCHostName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The (remote of local) vCenter host name.  This is used for
         connecting to the vCenter Web Service.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 15 }

vmvcAimConfigGeneralVCUserName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The user name for connecting to the vCenter Web Service.
         Please note that this entry can be seen correctly using SNMP GET, but MUST BE encrypted prior 
         to SNMP SET, meaning that only compliant SNMP managers can configure this entry properly.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 16 }

vmvcAimConfigGeneralVCPassword OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The encrypted password for connecting to the vCenter Web Service.
         Please note that this entry must be encrypted prior to setting, meaning
         that only compliant SNMP managers can configure this entry properly.
         This parameter can NEVER been seen correctly using SNMP GET.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 17 }

vmvcAimConfigGeneralVCProtocol OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The protocol (https or http) for connecting to the vCenter Web Service.
         This agent will use https as the default protocol unless changed here.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 18 }

vmvcAimConfigGeneralVCPort OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The port number for connecting to the vCenter Web Service.
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    DEFVAL { 443 }
    ::= { vmvcAimConfigGeneralGroup 19 }

vmvcAimConfigGeneralPollerStatus OBJECT-TYPE
    SYNTAX INTEGER {
        active(1),
        notInService(2),
        notReady(3),
        createAndGo(4)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The status of this Virtual Center AIM according to the Textual Conventions for SNMPv2 (see RFC 1443).  Specifics for each status:
         active(1) - The AIM is currently communicating successfully to the Virtual Center. (ReadOnly value)
         notInService(2) - Setting to this vlaue disables polling on the AIM (and allows for reconfiguration). (Writable value)
         notReady(3) - The AIM has identified a problem communicating to the Virtual Center. (ReadOnly value)
         createAndGo(4) - Setting to this value will reactivate AIM and validate Virtual Center credentials as needed. (Writable value)
         NOTE:  This parameter is deprecated, and is only accessible when the AIM is in single-instance
                compatibility mode."
    ::= { vmvcAimConfigGeneralGroup 20 }


-- *****************************************************************************
-- VC Config Group
-- *****************************************************************************
vmvcAimConfigVCCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Virtual Center Instances in vmvcAimConfigVCTable."
    ::= {vmvcAimConfigVCGroup 1}

vmvcAimConfigVCUnusedIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "An SNMP GET of this attribute returns an unused VC UID.  This
         is normally used to expedite safe table row creation."
    ::= {vmvcAimConfigVCGroup  2 }

vmvcAimConfigVCTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimConfigVCRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "A table containing the Virtual Center Instances."
    ::= { vmvcAimConfigVCGroup 3 }

vmvcAimConfigVCEntry OBJECT-TYPE
    SYNTAX VmvcAimConfigVCRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The Virtual Center Instances Table entry."
    INDEX { vmvcAimConfigVCUID }
    ::= { vmvcAimConfigVCTable 1 }

VmvcAimConfigVCRow ::=
    SEQUENCE {
        vmvcAimConfigVCUID                  INTEGER,
        vmvcAimConfigVCHostName             SnmpAdminString,
        vmvcAimConfigVCUserName             SnmpAdminString,
        vmvcAimConfigVCPassword             SnmpAdminString,
        vmvcAimConfigVCProtocol             SnmpAdminString,
        vmvcAimConfigVCPort                 INTEGER,
        vmvcAimConfigVCPollInterval         INTEGER,
        vmvcAimConfigVCLastPollTime         TimeTicks,
        vmvcAimConfigVCRefreshInterval      INTEGER,
        vmvcAimConfigVCLastRefreshTime      TimeTicks,
        vmvcAimConfigVCEventPollInterval    INTEGER,
        vmvcAimConfigVCEventLastPollTime    TimeTicks,
        vmvcAimConfigVCEventEnable          INTEGER,
        vmvcAimConfigVCEventMonitorInfo     INTEGER,
        vmvcAimConfigVCEventMonitorUser     INTEGER,
        vmvcAimConfigVCEventMonitorWarning  INTEGER,
        vmvcAimConfigVCEventMonitorError    INTEGER,
        vmvcAimConfigVCRowStatus            INTEGER,
        vmvcAimConfigVCDataStatus           INTEGER
    }

vmvcAimConfigVCUID OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Integer UID of Virtual Center Instance."
    ::= { vmvcAimConfigVCEntry  1}

vmvcAimConfigVCHostName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Host Name of the Virtual Center Instance.  Once a VC instance 
         row is added in this table, the VCName is no longer writeable."
    ::= { vmvcAimConfigVCEntry  2}

vmvcAimConfigVCUserName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The user name for connecting to this Virtual Center Instance.
         Please note that this entry can be seen correctly using SNMP GET, but MUST BE encrypted prior 
         to SNMP SET, meaning that only compliant SNMP managers can configure this entry properly."
    ::= { vmvcAimConfigVCEntry  3 }

vmvcAimConfigVCPassword OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The encrypted password for connecting to this Virtual Center Instance.
         Please note that this entry must be encrypted prior to setting, meaning
         that only compliant SNMP managers can configure this entry properly.
         This parameter can NEVER been seen correctly using SNMP GET."
    ::= { vmvcAimConfigVCEntry  4 }

vmvcAimConfigVCProtocol OBJECT-TYPE
    SYNTAX SnmpAdminString
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Protocol for connecting to this Virtual Center Instance. Valid
         choices are 'http' or 'https'. Default is 'https'."
    ::= { vmvcAimConfigVCEntry  5 }

vmvcAimConfigVCPort OBJECT-TYPE
    SYNTAX INTEGER
        ACCESS read-write
        STATUS mandatory
        DESCRIPTION
            "Port designation for connecting to this Virtual Center Instance."
    DEFVAL { 443 }
    ::= { vmvcAimConfigVCEntry  6 }

vmvcAimConfigVCPollInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The wait time (in SECONDS) between the standard polling 
         requests by the AIM (timed from the end of the previous poll 
         to the beginning of the next poll). 
         Valid values are 30-3600 sec.  This polling involves 
         retrieving performance metrics and event-driven VC 
         occurrences.  Objects which cannot be retrieved using VC 
         event-driven mechanisms will be polled normally.  These are:
            Datastores
            Customization Specifications
            Sensors (ALL)"
    DEFVAL { 120 }
    ::= { vmvcAimConfigVCEntry  7 }

vmvcAimConfigVCLastPollTime OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The amount of time since the last successful standard poll."
    ::= { vmvcAimConfigVCEntry  8 }

vmvcAimConfigVCRefreshInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The time (in SECONDS) between full VC refresh of the AIM.
         This refresh is very CPU-intensive, and is normally done on a
         daily basis (i.e. 86400 secs).
         Valid values are 30-604800 seconds (1 week), and must be 
         greater than or equal to vmvcAimConfigVCPollInterval.
         [Strongly suggested to perform this refresh very infrequently, 
          especially for large VC instances.]"
    DEFVAL { 14400 }
    ::= { vmvcAimConfigVCEntry  9 }

vmvcAimConfigVCLastRefreshTime OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The amount of time since the last successful full VC refresh."
    ::= { vmvcAimConfigVCEntry  10 }

vmvcAimConfigVCEventPollInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The wait time (in SECONDS) between the standard polling 
         requests by the AIM (timed from the end of the previous poll 
         to the beginning of the next poll). Valid values are 30-86400 sec.
         This polling only applies when Event retrieval is enabled (via
         vmvcAimConfigVCEventEnable)."
    DEFVAL { 120 }
    ::= { vmvcAimConfigVCEntry  11 }

vmvcAimConfigVCEventLastPollTime OBJECT-TYPE
    SYNTAX TimeTicks
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The number of time-ticks since the AIM has fetched 
         the VC Event table."
    ::= { vmvcAimConfigVCEntry  12 }

vmvcAimConfigVCEventEnable OBJECT-TYPE
    SYNTAX INTEGER {
        collectAndTrap(1),
        collect(2),
        disable(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events capturing is enabled or disabled"
    DEFVAL { 3 }
    ::= { vmvcAimConfigVCEntry  13 }

vmvcAimConfigVCEventMonitorInfo OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Info messages capturing is enabled or disabled"
    DEFVAL { 2 }
    ::= { vmvcAimConfigVCEntry  14 }

vmvcAimConfigVCEventMonitorUser OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events User messages capturing is enabled or disabled"
    DEFVAL { 2 }
    ::= { vmvcAimConfigVCEntry  15 }

vmvcAimConfigVCEventMonitorWarning OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Warning messages capturing is enabled or disabled"
    DEFVAL { 2 }
    ::= { vmvcAimConfigVCEntry  16 }

vmvcAimConfigVCEventMonitorError OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Error messages capturing is enabled or disabled"
    DEFVAL { 2 }
    ::= { vmvcAimConfigVCEntry  17 }

vmvcAimConfigVCRowStatus OBJECT-TYPE
    SYNTAX INTEGER {
        active(1),
        notInService(2),
        notReady(3),
        createAndGo(4),
        createAndWait(5),
        destroy(6)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The status of this Virtual Center Instance entry according 
         to the Textual Conventions for SNMPv2 (see RFC 1443).
         active(1)        - The AIM is currently communicating with this VC instance.
         notInService(2)  - Setting to this value disables polling of this VC instance.
         notReady(3)      - The AIM has identified a problem polling this VC instance.  Refer to vmvcAimConfigVCDataStatus for additional info.
         createAndGo(4)   - Setting to this value creates the VC Instance row with row status set to active(1).
         createAndWait(5) - Setting to this value creates the VC Instance row with row status set to notInService(2).
         destroy(6)       - Setting to this value deletes the VC Instance row."
    DEFVAL { 4 }
    ::= { vmvcAimConfigVCEntry  18 }

vmvcAimConfigVCDataStatus OBJECT-TYPE
    SYNTAX INTEGER {
        ready(1),
        notReady(2),
        connectionFailed(3),
        unknownError(4)
    }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The status of data retrieval processing for this Virtual
         Center Instance.
         ready(1)            - VC Instance ready, AIM cache populated
         notReady(2)         - VC Instance not ready, AIM cache not yet populated
         connectionFailed(3) - VC Instance connection failed, perhaps credential issue or unavailable VC Instance.  AIM cache retained if populated.
         unknownError(4)     - Unknown Error, AIM cache retained if populated."
    ::= { vmvcAimConfigVCEntry 19 }


-- *****************************************************************************
-- VC Monitor Configuration Group
-- *****************************************************************************
vmvcAimConfigMonServerUpdate OBJECT-TYPE
    SYNTAX INTEGER{
        done(1),
        update(2)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "One-shot trigger to update all configured VC Servers with 
         Monitoring configuration for all monitoring parameters.
         When completed, Agent will set this attribute to done(1).
         NOTE:  Only monitoring parameters will be updated using this trigger.
         Communication credentials and polling interval parameters will not be 
         updated."
    DEFVAL { 1 }
    ::= { vmvcAimConfigMonitorVCGroup 1 }

vmvcAimConfigMonVCProtocol OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Protocol for connecting to this Virtual Center Instance. Valid
         choices are 'http' or 'https'. Default is 'https'.  The value of this
         attribute is used as a default value for created objects."
     ::= { vmvcAimConfigMonitorVCGroup 2 }

vmvcAimConfigMonVCPort OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Port designation for connecting to this Virtual Center Instance.
         The value of this attribute is used as a default value for created 
         objects."
    DEFVAL { 443 }
    ::= { vmvcAimConfigMonitorVCGroup 3 }

vmvcAimConfigMonVCPollInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The wait time (in SECONDS) between the standard polling 
         requests by the AIM (timed from the end of the previous poll 
         to the beginning of the next poll). 
         Valid values are 30-3600 sec.  This polling involves 
         retrieving performance metrics and event-driven VC 
         occurrences.  Objects which cannot be retrieved using VC 
         event-driven mechanisms will be polled normally.  These are:
            Datastores
            Customization Specifications
            Sensors (ALL)
         The value of this attribute is used as a default value for created
         objects."
    DEFVAL { 120 }
    ::= { vmvcAimConfigMonitorVCGroup 4 }

vmvcAimConfigMonVCRefreshInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The time (in SECONDS) between full VC refresh of the AIM.
         This refresh is very CPU-intensive, and is normally done on a
         daily basis (i.e. 86400 secs).
         Valid values are 30-604800 seconds (1 week), and must be 
         greater than or equal to vmvcAimConfigVCPollInterval.
         [Strongly suggested to perform this refresh very infrequently, 
          especially for large VC instances.]
         The value of this attribute is used as a default value for created
         objects."
    DEFVAL { 14400 }
    ::= { vmvcAimConfigMonitorVCGroup 5 }

vmvcAimConfigMonVCEventPollInterval OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "The wait time (in SECONDS) between the standard polling 
         requests by the AIM (timed from the end of the previous poll 
         to the beginning of the next poll). Valid values are 30-86400 sec.
         This polling only applies when Event retrieval is enabled (via
         vmvcAimConfigVCEventEnable).  The value of this attribute is used 
         as a default value for created objects."
    DEFVAL { 120 }
    ::= { vmvcAimConfigMonitorVCGroup 6 }

vmvcAimConfigMonVCEventEnable OBJECT-TYPE
    SYNTAX INTEGER {
        collectAndTrap(1),
        collect(2),
        disable(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events capturing is enabled or disabled.  The value of this 
         attribute is used as a default value for created objects."
    DEFVAL { 3 }
    ::= { vmvcAimConfigMonitorVCGroup 7 }

vmvcAimConfigMonVCEventMonitorInfo OBJECT-TYPE
    SYNTAX INTEGER {
        enable(1),
        disable(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "VC Events Info messages capturing is enabled or disabled.  The value 
         of this attribute is used as a default value for created objects."
    DEFVAL { 2 }
    ::= { vmvcAimConfigMonitorVCGroup 8 }

