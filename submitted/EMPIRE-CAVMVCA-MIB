-- *****************************************************************************
-- Status Resource Pool Group
-- *****************************************************************************
vmvcAimStatResourcePoolCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of Resource Pools in vmvcAimStatResourcePoolTable."
    ::= { vmvcAimStatResPoolGroup 1}

vmvcAimStatResourcePoolTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatResourcePoolRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "ResourcePool table."
    ::= { vmvcAimStatResPoolGroup 2 }

vmvcAimStatResourcePoolEntry OBJECT-TYPE
    SYNTAX VmvcAimStatResourcePoolRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatResourcePoolUID  }
    ::= { vmvcAimStatResourcePoolTable 1 }

VmvcAimStatResourcePoolRow ::=
    SEQUENCE {
        vmvcAimStatResourcePoolUID                  INTEGER,
        vmvcAimStatResourcePoolIndex                SnmpAdminString,
        vmvcAimStatResourcePoolName                 SnmpAdminString,
        vmvcAimStatResourcePoolParentId             SnmpAdminString,
        vmvcAimStatResourcePoolParentPath           SnmpAdminString,
        vmvcAimStatResourcePoolIndexPath            SnmpAdminString,
        vmvcAimStatResourcePoolComputeResourceId    SnmpAdminString,
        vmvcAimStatResourcePoolComputeResourceName  SnmpAdminString,
        vmvcAimStatResourcePoolComputeResourcePath  SnmpAdminString,
        vmvcAimStatResourcePoolCPUExpandReservation INTEGER,
        vmvcAimStatResourcePoolCPUMax               INTEGER,
        vmvcAimStatResourcePoolCPUMin               INTEGER,
        vmvcAimStatResourcePoolCPUShares            INTEGER,
        vmvcAimStatResourcePoolMemExpandReservation INTEGER,
        vmvcAimStatResourcePoolMemMax               INTEGER,
        vmvcAimStatResourcePoolMemMin               INTEGER,
        vmvcAimStatResourcePoolMemShares            INTEGER,
        vmvcAimStatResourcePoolCfgChgVersion        SnmpAdminString,
        vmvcAimStatResourcePoolCPUMaxUsage          INTEGER,
        vmvcAimStatResourcePoolCPUOverAllUsage      INTEGER,
        vmvcAimStatResourcePoolCPUUsagePct          INTEGER,
        vmvcAimStatResourcePoolCPUUsageWarn         INTEGER,
        vmvcAimStatResourcePoolCPUUsageCrit         INTEGER,
        vmvcAimStatResourcePoolCPUAlert             INTEGER,
        vmvcAimStatResourcePoolCPUState             INTEGER,
        vmvcAimStatResourcePoolCPUReserveUsed       INTEGER,
        vmvcAimStatResourcePoolCPUReserveUsedForVM  INTEGER,
        vmvcAimStatResourcePoolCPUUnReserveForPool  INTEGER,
        vmvcAimStatResourcePoolCPUUnReserveForVM    INTEGER,
        vmvcAimStatResourcePoolMemMaxUsage          INTEGER,
        vmvcAimStatResourcePoolMemOverAllUsage      INTEGER, 
        vmvcAimStatResourcePoolMemUsagePct          INTEGER, 
        vmvcAimStatResourcePoolMemUsageWarn         INTEGER, 
        vmvcAimStatResourcePoolMemUsageCrit         INTEGER, 
        vmvcAimStatResourcePoolMemAlert             INTEGER, 
        vmvcAimStatResourcePoolMemState             INTEGER, 
        vmvcAimStatResourcePoolMemReserveUsed       INTEGER,
        vmvcAimStatResourcePoolMemReserveUsedForVM  INTEGER,
        vmvcAimStatResourcePoolMemUnReserveForPool  INTEGER,
        vmvcAimStatResourcePoolMemUnReserveForVM    INTEGER,
        vmvcAimStatResourcePoolStatus               INTEGER,
        vmvcAimStatResourcePoolMonitor              INTEGER,
        vmvcAimStatResourcePoolAlert                INTEGER,
        vmvcAimStatResourcePoolState                INTEGER,
        vmvcAimStatResourcePoolType                 INTEGER,
        vmvcAimStatResourcePoolVCUID                INTEGER,
        vmvcAimStatResourcePoolVCName               SnmpAdminString,
        vmvcAimStatResourcePoolVCUUID               SnmpAdminString
    }

