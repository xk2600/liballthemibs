-- *****************************************************************************
-- Distributed Virtual Switch Monitor Configuration Group
-- *****************************************************************************
vmvcAimConfigMonitorDVSwitchUpdate OBJECT-TYPE
    SYNTAX INTEGER{
        done(1),
        update(2)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "One-shot trigger to update all Distributed Virtual Switch objects with 
         Monitoring configuration for all monitoring parameters.
         When completed, Agent will set this attribute to done(1)."
    DEFVAL { 1 }
    ::= { vmvcAimConfigMonitorDistribVSwitchGroup 1 }

vmvcAimConfigMonitorDVSwitchVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "This attribute will scope the UPDATE request to the objects 
         within a single matching VC Instance UID 
         (see vmvcAimConfigVCTable).  If this attribute
         is set to zero(0), all objects will be updated."
    DEFVAL { 0 }
    ::= { vmvcAimConfigMonitorDistribVSwitchGroup 2 }

vmvcAimConfigMonDVSwitchMonitor OBJECT-TYPE
    SYNTAX INTEGER {
        do-not-monitor(1),
        monitor(2)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Flag whether to monitor this Distributed Virtual Switch or not.   The 
         value of this attribute is used as a default value for 
         created objects."
    DEFVAL { 2 }
    ::= { vmvcAimConfigMonitorDistribVSwitchGroup 3 }

vmvcAimConfigMonDVSwitchAlert OBJECT-TYPE
    SYNTAX INTEGER {
        not-green-warn(1),
        not-green-critical(2),
        follow-overallStatus(3)
    }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
        "Level choices for the alert, as follows:
            not-green-warn       -  Set the State=warning(3) if OverallStatus!=green
            not-green-crit       -  Set the State=critical(4) if OverallStatus!=green
            follow-overallStatus -  Use the OverallStatus to set the State, as follows:
               OverallStatus=gray    ->  State=unknown(1)
               OverallStatus=green   ->  State=ok(2)
               OverallStatus=yellow  ->  State=warning(3)
               OverallStatus=red     ->  State=critical(4)
         The value of this attribute is used as a default value for created objects."
    DEFVAL { 3 }
    ::= { vmvcAimConfigMonitorDistribVSwitchGroup 4 }


