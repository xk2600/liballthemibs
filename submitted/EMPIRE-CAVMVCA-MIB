    ::= { vmvcAimStatHostDiskEntry  16}

vmvcAimStatHostDiskTotalReadLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time taken during the collection interval to process a SCSI read command issued from the Guest OS to the virtual machine. The sum of kernelReadLatency and deviceReadLatency."
    ::= { vmvcAimStatHostDiskEntry  17}

vmvcAimStatHostDiskTotalWriteLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, taken during the collection interval to process a SCSI write command issued by the Guest OS to the virtual machine. The sum of kernelWriteLatency and deviceWriteLatency."
    ::= { vmvcAimStatHostDiskEntry  18}

vmvcAimStatHostDiskTotalLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, taken during the collection interval to process a SCSI command issued by the Guest OS to the virtual machine. The sum of kernelLatency and deviceLatency."
    ::= { vmvcAimStatHostDiskEntry  19}

vmvcAimStatHostDiskDeviceReadLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average time, in milliseconds, taken to complete a read from the physical device"
    ::= { vmvcAimStatHostDiskEntry  20}

vmvcAimStatHostDiskDeviceWriteLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, to write to the physical device (LUN)."
    ::= { vmvcAimStatHostDiskEntry  21}

vmvcAimStatHostDiskDeviceLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, to complete a SCSI command from the physical device."
    ::= { vmvcAimStatHostDiskEntry  22}

vmvcAimStatHostDiskQueueReadLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average time, in milliseconds, spent in the ESX Server VMKernel queue per read."
    ::= { vmvcAimStatHostDiskEntry  23}

vmvcAimStatHostDiskQueueWriteLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount time, in milliseconds, taken during the collection interval per SCSI write command in the VMKernel queue."
    ::= { vmvcAimStatHostDiskEntry  24}

vmvcAimStatHostDiskQueueLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, spent in the VMkernel queue, per SCSI command, during the collection interval."
    ::= { vmvcAimStatHostDiskEntry  25}

vmvcAimStatHostDiskKernelReadLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average time, in milliseconds, spent in ESX Server VMKernel per read"
    ::= { vmvcAimStatHostDiskEntry  26}

vmvcAimStatHostDiskKernelWriteLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, spent by VMKernel processing each SCSI write command."
    ::= { vmvcAimStatHostDiskEntry  27}

vmvcAimStatHostDiskKernelLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The average amount of time, in milliseconds, spent by VMkernel processing each SCSI command."
    ::= { vmvcAimStatHostDiskEntry  28}

vmvcAimStatHostDiskMaxTotalLatency  OBJECT-TYPE
    SYNTAX Gauge
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Highest latency value across all disks used by the host, in milliseconds. Latency measures the time taken to process a SCSI command issued by the guest OS to the virtual machine. The kernel latency is the time VMkernel takes to process an IO request. The device latency is the time it takes the hardware to handle the request."
    ::= { vmvcAimStatHostDiskEntry  29}

vmvcAimStatHostDiskVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance SNMP UID where this Host Disk resides."
    ::= { vmvcAimStatHostDiskEntry  30}

vmvcAimStatHostDiskVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this Host Disk resides."
    ::= { vmvcAimStatHostDiskEntry  31}

vmvcAimStatHostDiskVCUUID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The vCenter-specific UUID where this Host Disk resides."
    ::= { vmvcAimStatHostDiskEntry  32}


-- *****************************************************************************
-- Status Host SCSI Controller Group 
-- *****************************************************************************
vmvcAimStatSCSIControllerCount OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Number of SCSI Controller in vmvcAimStatSCSIControllerTable."
    ::= { vmvcAimStatSCSIControllerGroup 1 }

vmvcAimStatSCSIControllerTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VmvcAimStatSCSIControllerRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "SCSI Controller table."
    ::= { vmvcAimStatSCSIControllerGroup 2 }

vmvcAimStatSCSIControllerEntry OBJECT-TYPE
    SYNTAX VmvcAimStatSCSIControllerRow
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
        "The table entry."
    INDEX { vmvcAimStatSCSIControllerUID  }
    ::= { vmvcAimStatSCSIControllerTable 1 }

VmvcAimStatSCSIControllerRow ::=
    SEQUENCE {
        vmvcAimStatSCSIControllerUID            INTEGER,
        vmvcAimStatSCSIControllerVMID           SnmpAdminString,
        vmvcAimStatSCSIControllerName           SnmpAdminString,
        vmvcAimStatSCSIControllerSummary        SnmpAdminString,
        vmvcAimStatSCSIControllerKey            INTEGER,
        vmvcAimStatSCSIControllerControllerKey  INTEGER,
        vmvcAimStatSCSIControllerBusNumber      INTEGER,
        vmvcAimStatSCSIControllerUnitNumber     INTEGER,
        vmvcAimStatSCSIControllerVCUID          INTEGER,
        vmvcAimStatSCSIControllerVCName         SnmpAdminString,
        vmvcAimStatSCSIControllerVCUUID         SnmpAdminString
    }

vmvcAimStatSCSIControllerUID  OBJECT-TYPE
    SYNTAX INTEGER (1..2147483647)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "SCSI Controller UID"
    ::= { vmvcAimStatSCSIControllerEntry  1}

vmvcAimStatSCSIControllerVMID    OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The ID of the VM that the SCSI Controller is associated with."
    ::= { vmvcAimStatSCSIControllerEntry  2}

vmvcAimStatSCSIControllerName  OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The name of the SCSI Controller."
    ::= { vmvcAimStatSCSIControllerEntry  3}
            
vmvcAimStatSCSIControllerSummary  OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The SCSI Controller summary (type)."
    ::= { vmvcAimStatSCSIControllerEntry  4}

vmvcAimStatSCSIControllerKey  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The identifier for the SCSI interface."
    ::= { vmvcAimStatSCSIControllerEntry  5}

vmvcAimStatSCSIControllerControllerKey  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Object key that denotes the controller object for this device. This property contains 
         the key property value of the controller device object."
    ::= { vmvcAimStatSCSIControllerEntry  6}

vmvcAimStatSCSIControllerBusNumber  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Bus number associated with this controller."
    ::= { vmvcAimStatSCSIControllerEntry  7}

vmvcAimStatSCSIControllerUnitNumber  OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "Unit number of this device on its controller. This property is null if the controller property is null 
         (for example, when the device is not attached to a specific controller object). 
         Normally, two devices on the same controller may not be assigned the same unit number. "
    ::= { vmvcAimStatSCSIControllerEntry  8}

vmvcAimStatSCSIControllerVCUID OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance SNMP UID where this SCSI Controller resides."
    ::= { vmvcAimStatSCSIControllerEntry  9 }

vmvcAimStatSCSIControllerVCName OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "VC Instance where this SCSI Controller resides."
    ::= { vmvcAimStatSCSIControllerEntry  10 }

vmvcAimStatSCSIControllerVCUUID OBJECT-TYPE
    SYNTAX SnmpAdminString
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
        "The vCenter-specific UUID where this SCSI Controller resides."
    ::= { vmvcAimStatSCSIControllerEntry  11 }


