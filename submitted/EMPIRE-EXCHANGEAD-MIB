   exchAdHubTransDatabaseInstTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdHubTransDatabaseInstEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "hub transport database instance table"
      ::= { exchAdPerformanceHubTransportRole 2 }

    exchAdHubTransDatabaseInstEntry OBJECT-TYPE
       SYNTAX      ExchAdHubTransDatabaseInstEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdHubTransDatabaseInstTable"

       INDEX {
                exchAdHubTransDatabaseInstHostIndex,
                exchAdHubTransDatabaseInstIndex
             }
       ::= { exchAdHubTransDatabaseInstTable 1 }

    ExchAdHubTransDatabaseInstEntry ::=
       SEQUENCE {
           exchAdHubTransDatabaseInstHostIndex              INTEGER,
           exchAdHubTransDatabaseInstIndex                  INTEGER,
           exchAdHubTransDatabaseInstId                     SnmpAdminString,
           exchAdHubTransDatabaseInstDatabaseWritesPerSec   Gauge,
           exchAdHubTransDatabaseInstDatabaseReadsPerSec    Gauge,
           exchAdHubTransDatabaseInstLogFileWritesPerSec    Gauge,
           exchAdHubTransDatabaseInstLogFileReadsPerSec     Gauge,
           exchAdHubTransDatabaseInstLogGenCkptDepth        Gauge,
           exchAdHubTransDatabaseInstLogRecordStallsPerSec  Gauge,
           exchAdHubTransDatabaseInstLogThreadsWaiting      Gauge,
           exchAdHubTransDatabaseInstVersionBucketsAlloc    Gauge
         }

   exchAdHubTransDatabaseInstHostIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "unique index for each hub transport host, same as exchAdUniversalHostIndex"
       ::= { exchAdHubTransDatabaseInstEntry 1 }

    exchAdHubTransDatabaseInstIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "unique index for each database instance on the host"
       ::= { exchAdHubTransDatabaseInstEntry 2 }

    exchAdHubTransDatabaseInstId OBJECT-TYPE
       SYNTAX      SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "unique identifier for each database instance"
       ::= { exchAdHubTransDatabaseInstEntry 3 }

    exchAdHubTransDatabaseInstDatabaseWritesPerSec OBJECT-TYPE
       SYNTAX      Gauge
