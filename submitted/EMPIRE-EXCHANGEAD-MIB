--**************************************************************************
--**************************************************************************
-- 
-- Synthetic Tests Parameters
--
--**************************************************************************
--**************************************************************************

--**************************************************************************  
--
-- Test-ServiceHealth
--
-- returns whether all the Microsoft Windows services that Exchange requires on a
-- server have started for each Role installed.
--
--**************************************************************************

exchAdTestServiceHealthMaxTests  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "maximum number of entries in exchAdTestServiceHealthControlTable"
       ::= { exchAdSyntheticTests 1 }


--**************************************************************************  
--
-- exchAdTestServiceHealthControlTable
--
--**************************************************************************

exchAdTestServiceHealthControlTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdTestServiceHealthControlEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "Test-ServiceHealth control table"
      ::= { exchAdSyntheticTests 2 }

exchAdTestServiceHealthControlEntry OBJECT-TYPE
       SYNTAX      ExchAdTestServiceHealthControlEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdTestServiceHealthControlTable"

       INDEX {
                exchAdTestServiceHealthOwnerIndex,
                exchAdTestServiceHealthTestIndex
             }
       ::= { exchAdTestServiceHealthControlTable 1 }

    ExchAdTestServiceHealthControlEntry ::=
       SEQUENCE {
          exchAdTestServiceHealthOwnerIndex             SnmpAdminString,
          exchAdTestServiceHealthTestIndex              SnmpAdminString,
          exchAdTestServiceHealthRowStatus              RowStatus,
          exchAdTestServiceHealthAdminStatus            INTEGER,
          exchAdTestServiceHealthHostIndex              INTEGER,
          exchAdTestServiceHealthActiveDirectoryTimeout INTEGER,
          exchAdTestServiceHealthDomainControllerIndex  INTEGER
            }

    exchAdTestServiceHealthOwnerIndex OBJECT-TYPE
       SYNTAX      SnmpAdminString (SIZE(1..56))
