-- Notifications
--
--**************************************************************************
--**************************************************************************

  exchAdForestAddedTrap TRAP-TYPE
	ENTERPRISE sysmgmt
    VARIABLES {
      exchAdForestIndex,
      exchAdForestName }
    DESCRIPTION
        "Forest added to exchAdForestTable"
    ::= 166100

  exchAdForestRemovedTrap TRAP-TYPE
	ENTERPRISE sysmgmt
    VARIABLES {
      exchAdForestIndex,
