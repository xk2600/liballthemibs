       INDEX {
                exchAdHubTransHostIndex
             }
       ::= { exchAdHubTransGeneralPerformanceTable 1 }

    ExchAdHubTransGeneralPerformanceEntry ::=
       SEQUENCE {
           exchAdHubTransHostIndex                                  INTEGER,
           exchAdHubTransStoreDriverTotMsgDeliveryAttInPerSec       Gauge,
           exchAdHubTransStoreDriverTotLocalDeliveryCallsInPerSec   Gauge,
           exchAdHubTransStoreDriverTotRecipientsDeliveredInPerSec  Gauge,
           exchAdHubTransStoreDriverTotSubmMailItemsOutPerSec       Gauge,
           exchAdHubTransDumpsterDeletesPerSec                      Gauge,
           exchAdHubTransDumpsterInsertsPerSec                      Gauge,
           exchAdHubTransDumpsterItems                              Gauge,
           exchAdHubTransDumpsterSize                               Gauge,
           exchAdHubTransQueuesTotActiveMbxDeliveryQueueLength      Gauge,
           exchAdHubTransQueuesTotActiveNonSmtpDeliveryQueueLength  Gauge,
           exchAdHubTransQueuesTotActiveRemoteDeliveryQueueLength   Gauge,
           exchAdHubTransQueuesTotAggDeliveryQueueLength            Gauge,
           exchAdHubTransQueuesTotLargestDeliveryQueueLength        Gauge,
           exchAdHubTransQueuesTotMsgsDeliveredPerSec               Gauge,
           exchAdHubTransQueuesTotMsgsDeliveryQueuedPerSec          Gauge,
