--             "CA Technologies, Inc.
--             273 Corporate Dr.
--             Portsmouth, NH 03801"
--     DESCRIPTION
--         "The MIB is used to manage Active Directory and Exchange."
-- 
--     REVISION        "201210150000Z"
--     DESCRIPTION
--         "Updated varbinds for several traps;  added exchAdDagFailOverTrap;  fixed some descriptions"
-- 
--     REVISION        "201206120000Z"
--     DESCRIPTION
--         "Made exchAdMailboxDatabaseInstInventoryIndex and exchAdMailboxDatabaseInvIndex obsolete"
-- 
--     REVISION        "201204240000Z"
--     DESCRIPTION
--         "DAG database copy table object name changes"
-- 
--     REVISION        "201204190000Z"
--     DESCRIPTION
--         "Corrected OID branch"
-- 
--     REVISION        "201204090000Z"
--     DESCRIPTION
--         "Multiple changes including removal of SNMPv2-SMI import & addition of IsOperationsMaster"
-- 
--     REVISION        "201203260000Z"
--     DESCRIPTION
--         "Ongoing changes towards MIB version 1.1"
-- 
--     REVISION        "201112190000Z"
--     DESCRIPTION
--         "MIB version 1.0"
-- 
--     REVISION        "201106290000Z"
--     DESCRIPTION
--         "commented out unused branches; fixed exchAdDomainTechnology"
-- 
--     REVISION        "201106170000Z"
--     DESCRIPTION
--         "Additional Compile errors addressed"
-- 
--     REVISION        "201106150000Z"
--     DESCRIPTION
--         "Reworked for SimpleWeb validation"
-- 
--     REVISION        "201105240000Z"
--     DESCRIPTION
--         "Traps added"
-- 
--     REVISION        "201105200000Z"
--     DESCRIPTION
--          "Initial Draft"
-- 
--     ::= { applications 61 }


   empire               OBJECT IDENTIFIER ::= { enterprises 546 }
   unix                 OBJECT IDENTIFIER ::= { empire 1 }
   sysmgmt              OBJECT IDENTIFIER ::= { unix 1 }

   applications         OBJECT IDENTIFIER ::= { empire 16 }
   empireExchangeAdMIB  OBJECT IDENTIFIER ::= { applications 61 }

-- ****************************************************************************
-- ****************************************************************************

   exchAdConfiguration                  OBJECT IDENTIFIER ::= { empireExchangeAdMIB 1 }
   exchAdInventory                      OBJECT IDENTIFIER ::= { empireExchangeAdMIB 2 }
   exchAdPerformance                    OBJECT IDENTIFIER ::= { empireExchangeAdMIB 3 }
   exchAdWindowsEvents                  OBJECT IDENTIFIER ::= { empireExchangeAdMIB 4 }
   exchAdDag                            OBJECT IDENTIFIER ::= { empireExchangeAdMIB 5 }
   exchAdSyntheticTests                 OBJECT IDENTIFIER ::= { empireExchangeAdMIB 6 }

-- { exchAdPerformance 1 } currently unused and available
   exchAdPerformanceHost                OBJECT IDENTIFIER ::= { exchAdPerformance 2 }
   exchAdPerformanceActiveDirectory     OBJECT IDENTIFIER ::= { exchAdPerformance 3 }
   exchAdPerformanceMailboxRole         OBJECT IDENTIFIER ::= { exchAdPerformance 4 }
   exchAdPerformanceHubTransportRole    OBJECT IDENTIFIER ::= { exchAdPerformance 5 }
   exchAdPerformanceClientAccessRole    OBJECT IDENTIFIER ::= { exchAdPerformance 6 }

   exchAdInventoryGen                   OBJECT IDENTIFIER ::= { exchAdInventory 1 }

--
-- Empty branches commented out but retained for future use
--
   exchAdInventoryMailboxRole           OBJECT IDENTIFIER ::= { exchAdInventory 4 }
   exchAdInventoryHubTransportRole      OBJECT IDENTIFIER ::= { exchAdInventory 5 }

--**************************************************************************
--
-- Configuration Parameters
--
--**************************************************************************

   exchAdPollInterval  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "host poll interval in seconds - valid values are non-zero multiples of 30"
       DEFVAL   { 300 }
       ::= { exchAdConfiguration 1 }    

   exchAdWindowsEventTableSize  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "configurable size of windows event table"
       DEFVAL   { 4000 }
       ::= { exchAdConfiguration 2 }  

   exchAdPollWindowsEvents  OBJECT-TYPE
       SYNTAX       INTEGER  {poll(1), doNotPoll(2)}
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "windows events poll switch"
       DEFVAL   { 1 }
       ::= { exchAdConfiguration 3 }

   exchAdEnableTraps  OBJECT-TYPE
       SYNTAX       INTEGER  {enable(1), disable(2)}
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "trap enable switch"
       DEFVAL   { 1 }
       ::= { exchAdConfiguration 4 }    

   exchAdInitiateDiscovery  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "setting this object to the value 1 initiates discovery"
       ::= { exchAdConfiguration 5 }  

   exchAdWindowsEventCriticalityThreshold  OBJECT-TYPE
       SYNTAX       INTEGER  { information(1), warning(2), critical(3) }
       ACCESS       read-write
       STATUS       mandatory
       DESCRIPTION "threshold for retaining events in windows event table"
       DEFVAL   { 3 }
       ::= { exchAdConfiguration 6 }

--**************************************************************************  
--
-- Inventory Parameters
--
--**************************************************************************

   exchAdAgentName  OBJECT-TYPE
       SYNTAX       SnmpAdminString
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION "name of the agent"
       ::= { exchAdInventoryGen 1 }

   exchAdAgentVersion  OBJECT-TYPE
       SYNTAX       SnmpAdminString
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION "agent version"
       ::= { exchAdInventoryGen 2 }

   exchAdAgentStatus  OBJECT-TYPE
       SYNTAX       INTEGER {notReady(1), ready(2)}
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION "agent status: not ready until AIM is fully initialized - this will revert to notReady when a discovery is initiated"
       ::= { exchAdInventoryGen 3 }

   exchAdSysUpTime  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION 
            "time (in hundredths of a second) since the network management portion of the system was last re-initialized"
       ::= { exchAdInventoryGen 4 }

   exchAdLastDiscovery  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at the completion of the last discovery"
       ::= { exchAdInventoryGen 5 }

   exchAdInventoryLastChange  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at the completion of the last discovery or at the last change of the exchAdForestTableLastChange, exchAdDomainTableLastChange or exchAdManagedHostTableLastChange objects"
       ::= { exchAdInventoryGen 6 }

   exchAdHostName  OBJECT-TYPE
       SYNTAX       SnmpAdminString
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION "name of the machine hosting the sysEDGE agent and AD/Ex AIM"
       ::= { exchAdInventoryGen 7 }

--**************************************************************************  
--
-- exchAdForestTable
--
--**************************************************************************

   exchAdForestTableSize  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "number of entries in exchAdForestTable"
       ::= { exchAdInventoryGen 8 }

   exchAdForestTableLastChange  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at last change"
       ::= { exchAdInventoryGen 9 }

   exchAdForestTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdForestEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "Forest table"
      ::= { exchAdInventoryGen 10 }

   exchAdForestEntry OBJECT-TYPE
       SYNTAX      ExchAdForestEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdForestTable"

       INDEX {
                exchAdForestIndex
             }
       ::= { exchAdForestTable 1 }

   ExchAdForestEntry ::=
       SEQUENCE {
           exchAdForestIndex               INTEGER,
           exchAdForestName                SnmpAdminString,
           exchAdForestNamingRoleOwner     SnmpAdminString,
           exchAdForestSchemaRoleOwner     SnmpAdminString,
           exchAdForestRootDomain          SnmpAdminString
       }

   exchAdForestIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "unique index for each Forest"
       ::= { exchAdForestEntry 1 }

   exchAdForestName OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "name of Forest"
       ::= { exchAdForestEntry 2 }

   exchAdForestNamingRoleOwner OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "name of domain controller which is the naming master of this forest"
       ::= { exchAdForestEntry 3 }

   exchAdForestSchemaRoleOwner OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "name of domain controller which is the schema master of this forest"
       ::= { exchAdForestEntry 4 }

   exchAdForestRootDomain OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "name of root domain of this Forest"
       ::= { exchAdForestEntry 5 }

--**************************************************************************  
--
-- exchAdDomainTable
--
--**************************************************************************

   exchAdDomainTableSize  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "number of entries in exchAdDomainTable"
       ::= { exchAdInventoryGen 11 }

   exchAdDomainTableLastChange  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at last change"
       ::= { exchAdInventoryGen 12 }

   exchAdDomainTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdDomainEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "Domain table"
      ::= { exchAdInventoryGen 13 }

   exchAdDomainEntry OBJECT-TYPE
       SYNTAX      ExchAdDomainEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdDomainTable"

       INDEX {
                exchAdDomainIndex
             }
       ::= { exchAdDomainTable 1 }

   ExchAdDomainEntry ::=
       SEQUENCE {
           exchAdDomainIndex                  INTEGER,
           exchAdDomainName                   SnmpAdminString,
           exchAdDomainMgmtMode               INTEGER, 
           exchAdDomainTechnology             INTEGER, 
           exchAdDomainForestIndex            INTEGER,
           exchAdDomainDomainControllerIndex  INTEGER,
           exchAdDomainParentDomainIndex      INTEGER,
           exchAdDomainParentDomainName       SnmpAdminString,
           exchAdDomainInfrastructureMaster   SnmpAdminString,
           exchAdDomainPrimaryDCEmulator      SnmpAdminString,
           exchAdDomainRelativeIDMaster       SnmpAdminString
       }

   exchAdDomainIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "unique index for each Domain"
       ::= { exchAdDomainEntry 1 }

   exchAdDomainName OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "name of Domain"
       ::= { exchAdDomainEntry 2 }

   exchAdDomainMgmtMode OBJECT-TYPE
       SYNTAX     INTEGER {domainBased(1), hostBased(2)}
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "management mode of Domain"
       ::= { exchAdDomainEntry 3 }

   exchAdDomainTechnology OBJECT-TYPE
       SYNTAX     INTEGER {adAndEx(1), adOnly(2), exOnly(3)}
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "domain supported technology"
       ::= { exchAdDomainEntry 4 }

   exchAdDomainForestIndex OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "pointer to forest in exchAdForestTable"
       ::= { exchAdDomainEntry 5 }

   exchAdDomainDomainControllerIndex OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      obsolete
       DESCRIPTION "pointer to domain controller in exchAdDomainControllerTable - this object is obsolete"
       ::= { exchAdDomainEntry 6 }

   exchAdDomainParentDomainIndex OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "pointer to parent Domain in this exchAdDomainTable - 0 if no parent"
       ::= { exchAdDomainEntry 7 }

   exchAdDomainParentDomainName OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "parent Domain name"
       ::= { exchAdDomainEntry 8 }

   exchAdDomainInfrastructureMaster OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "infrastructure master name of Domain"
       ::= { exchAdDomainEntry 9 }

   exchAdDomainPrimaryDCEmulator OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "primary domain controller emulator name of Domain"
       ::= { exchAdDomainEntry 10 }

   exchAdDomainRelativeIDMaster OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION "relative ID master name of Domain"
       ::= { exchAdDomainEntry 11 }

--**************************************************************************  
--
-- exchAdUniversalHostTable
--
--**************************************************************************

   exchAdUniversalHostTableSize  OBJECT-TYPE
       SYNTAX       INTEGER
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "number of entries in exchAdUniversalHostTable"
       ::= { exchAdInventoryGen 14 }

   exchAdUniversalHostTableLastChange  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at last change"
       ::= { exchAdInventoryGen 15 }

   exchAdUniversalHostTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdUniversalHostEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "table of hosts managed by all AIMs"
      ::= { exchAdInventoryGen 16 }

   exchAdUniversalHostEntry OBJECT-TYPE
       SYNTAX      ExchAdUniversalHostEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdUniversalHostTable"

       INDEX {
                exchAdUniversalHostIndex
             }
       ::= { exchAdUniversalHostTable 1 }

   ExchAdUniversalHostEntry ::=
       SEQUENCE {
           exchAdUniversalHostIndex          INTEGER,
           exchAdUniversalHostName           SnmpAdminString,
           exchAdUniversalHostTechnology     INTEGER,
           exchAdUniversalHostExRoleMask     INTEGER,
           exchAdUniversalHostAdRoleMask     INTEGER,
           exchAdUniversalHostManagedByAgent INTEGER,
           exchAdUniversalHostIpAddressType  InetAddressType,
           exchAdUniversalHostIpAddress      InetAddress,
           exchAdUniversalHostMacAddress     MacAddress,
           exchAdUniversalHostDomainName     SnmpAdminString,
           exchAdUniversalHostForestName     SnmpAdminString
       }

   exchAdUniversalHostIndex OBJECT-TYPE
          SYNTAX      INTEGER (0..2147483647)
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "unique index for each host"
          ::= { exchAdUniversalHostEntry 1 }

   exchAdUniversalHostName OBJECT-TYPE
          SYNTAX      SnmpAdminString
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "name of host"
          ::= { exchAdUniversalHostEntry 2 }

   exchAdUniversalHostTechnology OBJECT-TYPE
          SYNTAX      INTEGER {exchange(1), activeDirectory(2), both(3)}
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host supported technology"
          ::= { exchAdUniversalHostEntry 3 }

   exchAdUniversalHostExRoleMask OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "This object is a bit field. Each bit represents the following Exchange roles:
            Hex: 0000 0001, Decimal:  1 - Edge Transport
            Hex: 0000 0002, Decimal:  2 - Hub Transport
            Hex: 0000 0004, Decimal:  4 - Client Access
            Hex: 0000 0008, Decimal:  8 - Mailbox
            Hex: 0000 0010, Decimal: 16 - Unified Messaging"
          ::= { exchAdUniversalHostEntry 4 }

   exchAdUniversalHostAdRoleMask OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "This object is a bit field. Each bit represents the following Active Directory roles:
            Hex: 0000 0001, Decimal:  1 - Domain Services
            Hex: 0000 0002, Decimal:  2 - ADAM
            Hex: 0000 0004, Decimal:  4 - Federation Services
            Hex: 0000 0008, Decimal:  8 - Rights Management Services
            Hex: 0000 0010, Decimal: 16 - Certification Server"
          ::= { exchAdUniversalHostEntry 5 }

   exchAdUniversalHostManagedByAgent OBJECT-TYPE
          SYNTAX      INTEGER {manage(1), doNotManage(2)}
          ACCESS      read-write
          STATUS      mandatory
          DESCRIPTION 
                      "host management flag:  Initialized to 1 if associated domain management mode is Domain-Based; 2 otherwise"
          ::= { exchAdUniversalHostEntry 6 }

   exchAdUniversalHostIpAddressType OBJECT-TYPE
          SYNTAX      InetAddressType
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host IP address type"
          ::= { exchAdUniversalHostEntry 7 }

   exchAdUniversalHostIpAddress OBJECT-TYPE
          SYNTAX      InetAddress
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host IP address"
          ::= { exchAdUniversalHostEntry 8 }

   exchAdUniversalHostMacAddress OBJECT-TYPE
          SYNTAX      MacAddress
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host MAC address"
          ::= { exchAdUniversalHostEntry 9 }

   exchAdUniversalHostDomainName OBJECT-TYPE
          SYNTAX      SnmpAdminString
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "associated domain name"
          ::= { exchAdUniversalHostEntry 10 }

   exchAdUniversalHostForestName OBJECT-TYPE
          SYNTAX      SnmpAdminString
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "associated Forest name"
          ::= { exchAdUniversalHostEntry 11 }

