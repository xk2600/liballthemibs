--**************************************************************************  
--**************************************************************************  
--
-- Windows Event Table
--
--**************************************************************************
--**************************************************************************  

   exchAdWindowsEventTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdWindowsEventEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "Windows event table"
       ::= { exchAdWindowsEvents 1 }

   exchAdWindowsEventEntry OBJECT-TYPE
       SYNTAX      ExchAdWindowsEventEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdWindowsEventTable"
       INDEX { 
                exchAdWindowsEventHostIndex,
                exchAdWindowsEventIndex
             }
       ::= { exchAdWindowsEventTable 1 }

   ExchAdWindowsEventEntry ::=
       SEQUENCE {
           exchAdWindowsEventHostIndex                  INTEGER,                        
           exchAdWindowsEventTimeCreated                SnmpAdminString,
           exchAdWindowsEventIndex                      INTEGER,
           exchAdWindowsEventHost                       SnmpAdminString,
           exchAdWindowsEventProviderName               SnmpAdminString,
           exchAdWindowsEventId                         INTEGER,
           exchAdWindowsEventLevel                      INTEGER,
           exchAdWindowsEventData                       SnmpAdminString
       }

   exchAdWindowsEventHostIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "index of host that generated the event, same as exchAdUniversalHostIndex"
       ::= { exchAdWindowsEventEntry 1 }

   exchAdWindowsEventTimeCreated OBJECT-TYPE
       SYNTAX      SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "event creation time"
       ::= { exchAdWindowsEventEntry 2 }

   exchAdWindowsEventIndex OBJECT-TYPE
       SYNTAX      INTEGER (0..2147483647)
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "unique index for each event for a given host and creation time - this value ascends for newer events"
       ::= { exchAdWindowsEventEntry 3 }

   exchAdWindowsEventHost OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "name of host that generated the event"
       ::= { exchAdWindowsEventEntry 4 }

   exchAdWindowsEventProviderName OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "generating application"
       ::= { exchAdWindowsEventEntry 5 }

   exchAdWindowsEventId OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "unique index for each event for a given host and creation time"
       ::= { exchAdWindowsEventEntry 6 }

   exchAdWindowsEventLevel OBJECT-TYPE
       SYNTAX      INTEGER { information(1), warning(2), critical(3) }
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "event severity"
       ::= { exchAdWindowsEventEntry 7 }

   exchAdWindowsEventData OBJECT-TYPE
       SYNTAX     SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "event specific data, maximum 255 characters"
       ::= { exchAdWindowsEventEntry 8 }


