          "DAG IP V4 addresses: string with multiple IP addresses"
       ::= { exchAdDagEntry 10 }

   exchAdDagServers  OBJECT-TYPE
       SYNTAX      SnmpAdminString
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "DAG servers: string with multiple server names"
       ::= { exchAdDagEntry 11 }

   exchAdPrimaryActiveManagerIndex  OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
