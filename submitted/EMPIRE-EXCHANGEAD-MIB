       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
          "completion status of test"
       ::= { exchAdTestServiceHealthResultsEntry 2 }

    exchAdTestServiceHealthManagedHostExRoleMask  OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
            "This object is a bit field. Each bit represents the following Exchange roles:
            Hex: 0000 0001, Decimal:  1 - Edge Transport
            Hex: 0000 0002, Decimal:  2 - Hub Transport
            Hex: 0000 0004, Decimal:  4 - Client Access
            Hex: 0000 0008, Decimal:  8 - Mailbox
            Hex: 0000 0010, Decimal: 16 - Unified Messaging"
       ::= { exchAdTestServiceHealthResultsEntry 3 }

    exchAdTestServiceHealthManagedHostExRoleRunningMask  OBJECT-TYPE
       SYNTAX      INTEGER
       ACCESS      read-only
       STATUS      mandatory
       DESCRIPTION
            "This object is a bit field. Each bit represents the following Exchange roles:
            Hex: 0000 0001, Decimal:  1 - Edge Transport
            Hex: 0000 0002, Decimal:  2 - Hub Transport
            Hex: 0000 0004, Decimal:  4 - Client Access
            Hex: 0000 0008, Decimal:  8 - Mailbox
            Hex: 0000 0010, Decimal: 16 - Unified Messaging"
       ::= { exchAdTestServiceHealthResultsEntry 4 }

