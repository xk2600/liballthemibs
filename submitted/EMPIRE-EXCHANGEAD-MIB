           exchAdDagAltWitnessServerIndex    INTEGER,
           exchAdDagAltWitnessDirectory      SnmpAdminString,
           exchAdDagNetworkCompression       INTEGER,    
           exchAdDagNetworkEncryptionStatus  INTEGER,           
           exchAdDagDataCenterActivationMode INTEGER,
           exchAdDagIpv4Addresses            SnmpAdminString,
           exchAdDagServers                  SnmpAdminString,
           exchAdPrimaryActiveManagerIndex   INTEGER,
           exchAdDagThirdPartyReplStatus     INTEGER,
           exchAdDagReplicaPort              INTEGER,
           exchAdDagOrigServerIndex          INTEGER,
           exchAdDagIdentity                 SnmpAdminString
       }

