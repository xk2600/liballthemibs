       STATUS       mandatory
       DESCRIPTION  "number of entries in exchAdManagedHostTable"
       ::= { exchAdInventoryGen 17 }

   exchAdManagedHostTableLastChange  OBJECT-TYPE
       SYNTAX       TimeTicks
       ACCESS       read-only
       STATUS       mandatory
       DESCRIPTION  "exchAdSysUpTime value at last change"
       ::= { exchAdInventoryGen 18 }

   exchAdManagedHostTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdManagedHostEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "table of hosts managed by this AIM"
      ::= { exchAdInventoryGen 19 }

   exchAdManagedHostEntry OBJECT-TYPE
       SYNTAX      ExchAdManagedHostEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "defines an entry in the exchAdManagedHostTable"

       INDEX {
                exchAdManagedHostIndex
             }
       ::= { exchAdManagedHostTable 1 }

   ExchAdManagedHostEntry ::=
       SEQUENCE {
           exchAdManagedHostIndex               INTEGER,
           exchAdManagedHostName                SnmpAdminString,
           exchAdManagedHostTechnology          INTEGER,
           exchAdManagedHostExRoleMask          INTEGER,
           exchAdManagedHostAdRoleMask          INTEGER,
           exchAdManagedHostDagIndex            INTEGER,
           exchAdManagedHostIpAddressType       InetAddressType,
           exchAdManagedHostIpAddress           InetAddress,
           exchAdManagedHostMacAddress          MacAddress,
           exchAdManagedHostLastPollTime        TimeTicks,
           exchAdManagedHostPollStatus          INTEGER,
           exchAdManagedHostPreviousPollStatus  INTEGER,
           exchAdManagedHostDomainIndex         INTEGER,
           exchAdManagedHostForestIndex         INTEGER
       }

   exchAdManagedHostIndex OBJECT-TYPE
          SYNTAX      INTEGER (0..2147483647)
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION 
             "unique index for each host, same as exchAdUniversalHostIndex"
          ::= { exchAdManagedHostEntry 1 }

   exchAdManagedHostName OBJECT-TYPE
          SYNTAX      SnmpAdminString
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "name of host"
          ::= { exchAdManagedHostEntry 2 }

   exchAdManagedHostTechnology OBJECT-TYPE
          SYNTAX      INTEGER {exchange(1), activeDirectory(2), both(3)}
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host supported technology"
          ::= { exchAdManagedHostEntry 3 }

   exchAdManagedHostExRoleMask OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "This object is a bit field. Each bit represents the following Exchange roles:
            Hex: 0000 0001, Decimal:  1 - Edge Transport
            Hex: 0000 0002, Decimal:  2 - Hub Transport
            Hex: 0000 0004, Decimal:  4 - Client Access
            Hex: 0000 0008, Decimal:  8 - Mailbox
            Hex: 0000 0010, Decimal: 16 - Unified Messaging"
          ::= { exchAdManagedHostEntry 4 }

   exchAdManagedHostAdRoleMask OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION
            "This object is a bit field. Each bit represents the following Active Directory roles:
            Hex: 0000 0001, Decimal:  1 - Domain Services
            Hex: 0000 0002, Decimal:  2 - ADAM
            Hex: 0000 0004, Decimal:  4 - Federation Services
            Hex: 0000 0008, Decimal:  8 - Rights Management Services
            Hex: 0000 0010, Decimal: 16 - Certification Server"
          ::= { exchAdManagedHostEntry 5 }

   exchAdManagedHostDagIndex OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION 
            "pointer to DAG in exchAdDagTable - 0 if not in a DAG"
          ::= { exchAdManagedHostEntry 6 }

   exchAdManagedHostIpAddressType OBJECT-TYPE
          SYNTAX      InetAddressType
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host IP address type"
          ::= { exchAdManagedHostEntry 7 }

   exchAdManagedHostIpAddress OBJECT-TYPE
          SYNTAX      InetAddress
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host IP address"
          ::= { exchAdManagedHostEntry 8 }

   exchAdManagedHostMacAddress OBJECT-TYPE
          SYNTAX      MacAddress
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "host MAC address"
          ::= { exchAdManagedHostEntry 9 }

   exchAdManagedHostLastPollTime OBJECT-TYPE
          SYNTAX      TimeTicks
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "exchAdSysUpTime value at last successful poll"
          ::= { exchAdManagedHostEntry 10 }

   exchAdManagedHostPollStatus OBJECT-TYPE
          SYNTAX      INTEGER {initial(1), established(2), failed(3)}
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "status of AIM's ability to successfully communicate with the host on its most recent poll"
          ::= { exchAdManagedHostEntry 11 }

   exchAdManagedHostPreviousPollStatus OBJECT-TYPE
          SYNTAX      INTEGER {initial(1), established(2), failed(3)}
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "value of exchAdManagedHostPollStatus from the previous poll"
          ::= { exchAdManagedHostEntry 12 }

   exchAdManagedHostDomainIndex OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "pointer to domain in exchAdDomainTable"
          ::= { exchAdManagedHostEntry 13 }

   exchAdManagedHostForestIndex OBJECT-TYPE
          SYNTAX      INTEGER
          ACCESS      read-only
          STATUS      mandatory
          DESCRIPTION "pointer to forest in exchAdForestTable"
          ::= { exchAdManagedHostEntry 14 }

--**************************************************************************  
--
-- exchAdDomainControllerTable
--
-- This table's implementation was changed from version 1.0 to version 1.1. The
-- 1.0 version of this MIB was never implemented by the ADES AIM agent.
--
--**************************************************************************

   exchAdDomainControllerTable OBJECT-TYPE
       SYNTAX      SEQUENCE OF ExchAdDomainControllerEntry
       ACCESS      not-accessible
       STATUS      mandatory
       DESCRIPTION
           "Domain Controller table"
      ::= { exchAdInventoryGen 20 }

   exchAdDomainControllerEntry OBJECT-TYPE
