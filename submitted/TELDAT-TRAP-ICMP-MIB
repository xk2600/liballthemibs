icmpNotification13 NOTIFICATION-TYPE
    OBJECTS { 
              icmp13_problem_offset, 
              icmp13_source_IP_address,
              icmp13_destination_IP_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.13

        Message: 'sending parameter problem message problem offset $1 for packet from $2 to $3'

        Parameters list:
              $1: icmp13_problem_offset
              $2: icmp13_source_IP_address
              $3: icmp13_destination_IP_address

        The router is sending an ICMP Parameter Problem message, for an
        unspecified problem at the given offset."
    ::= { icmpNotificationsPrefix 13 }

icmpNotification14 NOTIFICATION-TYPE
    OBJECTS { 
              icmp14_ICMP_type, 
              icmp14_ICMP_code,
              icmp14_source_IP_address,
              icmp14_destination_IP_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.14

        Message: 'sending packet type $1 code $2 for packet from $3 to $4'

        Parameters list:
              $1: icmp14_ICMP_type
              $2: icmp14_ICMP_code
              $3: icmp14_source_IP_address
              $4: icmp14_destination_IP_address

        The router is sending an ICMP packet of the specified type about
        a packet from the source host to the destination."
    ::= { icmpNotificationsPrefix 14 }

icmpNotification15 NOTIFICATION-TYPE
    OBJECTS { 
              icmp15_header_length, 
              icmp15_source_ip_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.15

        Message: 'short ICMP packet $1 received in packet from $2'

        Parameters list:
              $1: icmp15_header_length
              $2: icmp15_source_ip_address

        This message is generated when an ICMP packet's indicated header length
        is below the minimum possible length for an ICMP packet."
    ::= { icmpNotificationsPrefix 15 }

icmpNotification16 NOTIFICATION-TYPE
    OBJECTS { 
              icmp16_current_next_hop, 
              icmp16_IP_destination,
              icmp16_better_next_hop 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.16

        Message: '$1 has redirected traffic for $2 to $3'

        Parameters list:
              $1: icmp16_current_next_hop
              $2: icmp16_IP_destination
              $3: icmp16_better_next_hop

        A redirect has been received, changing the next hop for the given
        destination. Redirects are only processed when running in host mode."
    ::= { icmpNotificationsPrefix 16 }

icmpNotification17 NOTIFICATION-TYPE
    OBJECTS { 
              icmp17_gateway_address, 
              icmp17_reason 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.17

        Message: 'Redirect received from $1 was bad for the reason: $2'

        Parameters list:
              $1: icmp17_gateway_address
              $2: icmp17_reason

        A redirect was received from a router, but rejected for the
        specified reason."
    ::= { icmpNotificationsPrefix 17 }

icmpNotification18 NOTIFICATION-TYPE
    OBJECTS { 
              icmp18_router_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ICMP.18

        Message: 'Router advertisement received from $1'

        Parameters list:
              $1: icmp18_router_address

        An ICMP Router Advertisement (Gateway Discovery) message has
        been received from the specified router."
    ::= { icmpNotificationsPrefix 18 }

