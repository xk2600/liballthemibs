TELDAT-TRAP-ICMP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


icmpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the ICMP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 11 }


icmpEventsGroup	OBJECT IDENTIFIER	::= { icmpTrapsMIB 1 }

icmpNotifications	OBJECT IDENTIFIER	::= { icmpTrapsMIB 2 }
icmpNotificationsPrefix	OBJECT IDENTIFIER	::= { icmpNotifications 0 }

icmpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ICMP.1
        A bad ICMP checksum was detected in an incoming ICMP message. The
        received checksum is displayed, together with the value that the checkum
        should have had. The received packet is discarded."
    ::= { icmpEventsGroup 1 }

