    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The Type of the System. P4/P5/P6."
::= { lparAimStatSysEntry 4 }

lparAimStatSysStatus OBJECT-TYPE
    SYNTAX INTEGER {
        unknown(1),
        initializing(2),
        ready(3),
