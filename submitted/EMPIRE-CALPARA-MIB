lparAimStatvDiskCount OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of physical disk on Virtual I/O Server. Only those disks are displayed,
        which has PhysicalVolume parameter specified."
::= { lparAimStatvDiskGroup 1 }

lparAimStatvDiskTable OBJECT-TYPE
    SYNTAX SEQUENCE OF LparAimStatvDiskRow
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table containing the physical disks."
::= { lparAimStatvDiskGroup 2 }

lparAimStatvDiskEntry OBJECT-TYPE
    SYNTAX LparAimStatvDiskRow 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The table entry."
    INDEX { lparAimStatvDiskInstanceUID, lparAimStatvDiskUID }
::= { lparAimStatvDiskTable 1 }

LparAimStatvDiskRow ::= SEQUENCE {
    lparAimStatvDiskInstanceUID   Integer32,
    lparAimStatvDiskInstanceName  DisplayString,
    lparAimStatvDiskUID           Integer32,
    lparAimStatvDiskLPARID        DisplayString,
    lparAimStatvDiskLPAR          SnmpAdminString,
    lparAimStatvDiskName          DisplayString,
    lparAimStatvDiskActivePercent Integer32,
    lparAimStatvDiskKBPS          Integer32,
    lparAimStatvDiskTPS           Integer32,
    lparAimStatvDiskKBRead        Counter64,
    lparAimStatvDiskKBWritten     Counter64
}

lparAimStatvDiskInstanceUID  OBJECT-TYPE
    SYNTAX Integer32 (1..2147483647)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "LPAR Instance unique identifier."
::= { lparAimStatvDiskEntry 1 }

lparAimStatvDiskInstanceName  OBJECT-TYPE
    SYNTAX DisplayString (SIZE(20))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Name of LPAR Instance."
::= { lparAimStatvDiskEntry 2 }

lparAimStatvDiskUID  OBJECT-TYPE
    SYNTAX Integer32 (1..2147483647)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The unique identifier of physical disk."
::= { lparAimStatvDiskEntry 3 }

lparAimStatvDiskLPARID  OBJECT-TYPE
    SYNTAX DisplayString (SIZE(20))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "LPAR identification number to which this disk belongs to."
::= { lparAimStatvDiskEntry 4 }

lparAimStatvDiskLPAR  OBJECT-TYPE
    SYNTAX SnmpAdminString 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "LPAR name to which this disk belongs to."
::= { lparAimStatvDiskEntry 5 }

lparAimStatvDiskName  OBJECT-TYPE
    SYNTAX DisplayString (SIZE(20))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The name of physical disk."
::= { lparAimStatvDiskEntry 6 }

lparAimStatvDiskActivePercent  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The percentage of time the physical disk was active(bandwidth utilization for the drive)."
::= { lparAimStatvDiskEntry 7 }

lparAimStatvDiskKBPS  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The amount of data transferred (read or written) to the drive in KB per second."
::= { lparAimStatvDiskEntry 8 }

lparAimStatvDiskTPS  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The number of transfers per second that were issued to the physical disk.
        A transfer is an I/O request to the physical disk. Multiple logical requests can be
        combined into a single I/O request to the disk. A transfer is of indeterminate size."
::= { lparAimStatvDiskEntry 9 }

lparAimStatvDiskKBRead  OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Total number of KB read."
::= { lparAimStatvDiskEntry 10 }

lparAimStatvDiskKBWritten  OBJECT-TYPE
    SYNTAX Counter64
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Total number of KB written."
::= { lparAimStatvDiskEntry 11 }


------------------------------------------------------------
--  LPAR Deprecated TRAP-TYPE Definitions 
------------------------------------------------------------

--  The following traps are deprecated.
--  These trap numbers should no more be used.

--  lparAimSysStateChangeTrap    TRAP-TYPE ::= 165310
--  lparAimSysCfgMonitorChange   TRAP-TYPE ::= 165311
--  lparAimSysCfgAlertChange     TRAP-TYPE ::= 165312
--  lparAimSysCPUThresholdChange TRAP-TYPE ::= 165313
--  lparAimSysMEMThresholdChange TRAP-TYPE ::= 165314
--  lparAimSysCpuStateChange     TRAP-TYPE ::= 165318

--  lparAimLPStateChange         TRAP-TYPE ::= 165320
--  lparAimLPMonitor             TRAP-TYPE ::= 165323
--  lparAimLPAlert               TRAP-TYPE ::= 165324
--  lparAimLPCPUMonitor          TRAP-TYPE ::= 165325
--  lparAimLPMemoryMonitor       TRAP-TYPE ::= 165326
--  lparAimLPCPULagSetting       TRAP-TYPE ::= 165327
--  lparAimLPCPUWarnThreshold    TRAP-TYPE ::= 165328
--  lparAimLPCPUCritThreshold    TRAP-TYPE ::= 165329
--  lparAimLPMemoryWarnThreshold TRAP-TYPE ::= 165330
--  lparAimLPMemoryCritThreshold TRAP-TYPE ::= 165331
--  lparAimLPMemoryState         TRAP-TYPE ::= 165332
--  lparAimLPCPUState            TRAP-TYPE ::= 165333

--  lparAimSlotMonitorChange     TRAP-TYPE ::= 165343


------------------------------------------------------------
-- The System TRAP-TYPE Definitions
------------------------------------------------------------

lparAimSysDown NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName,
        lparAimStatHMCName,
        lparAimStatSysType
    }
    STATUS current
    DESCRIPTION
        "The system down trap."
::= { sysmgmt 165315 }

lparAimSysDelete NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName,
        lparAimStatHMCName,
        lparAimStatSysType
    }
    STATUS current
    DESCRIPTION
        "The system delete trap."
::= { sysmgmt 165316 }

lparAimSysAdd NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName,
        lparAimStatHMCName,
        lparAimStatSysType
    }
    STATUS current
    DESCRIPTION
        "The system added trap."
::= { sysmgmt 165317 }


