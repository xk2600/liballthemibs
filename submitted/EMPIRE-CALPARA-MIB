    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The currently installed version of the plugin."
::= { lparAimCfgGenGroup 2 }

lparAimCfgGenPollInterval OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "The time (in seconds) between the polling by the Agent.
        Valid values are greater than or equal to 300 seconds."
    DEFVAL { 300 }
::= { lparAimCfgGenGroup 3 }

lparAimCfgGenPluginHostName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The host name on which this LPAR plugin is running."
::= { lparAimCfgGenGroup 4 }


------------------------------------------------------------
-- The LPAR Statistic System Table
------------------------------------------------------------

lparAimStatSysTable OBJECT-TYPE
    SYNTAX SEQUENCE OF LparAimStatSysRow
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table containing the servers the LPAR Plugin monitors."
::= { lparAimStatSysGroup 1 }

lparAimStatSysEntry OBJECT-TYPE
    SYNTAX LparAimStatSysRow
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The table entry of servers the LPAR Plugin monitors."
    INDEX { lparAimStatSysInstanceUID }
::= { lparAimStatSysTable 1 }

LparAimStatSysRow ::= SEQUENCE {
    lparAimStatSysInstanceUID      Integer32,
    lparAimStatSysInstanceName     DisplayString,
    lparAimStatSysName             DisplayString,
    lparAimStatSysType             DisplayString,
    lparAimStatSysStatus           Integer32,
    lparAimStatSysSrNum            DisplayString,
    lparAimStatSysModelType        DisplayString,
    lparAimStatSysProdLine         DisplayString,
    lparAimStatSysSPVer            DisplayString,
    lparAimStatSysMaxPartition     Integer32,
    lparAimStatSysProcInstalled    Integer32,
    lparAimStatSysMemInstalled     Integer32,
    lparAimStatSysFreeMem          Integer32,
    lparAimStatSysCpuCODCapable    Integer32,
    lparAimStatSysMemCODCapable    Integer32,
    lparAimStatSysOS400Capable     Integer32,
    lparAimStatSysMicroLPCapable   Integer32,
    lparAimStatSysVirtualIOCapable Integer32,
    lparAimStatSysLMBSize          Integer32,
    lparAimStatSysLoad             Integer32,
    lparAimStatSysFiveLoad         Integer32,
    lparAimStatSysFifteenLoad      Integer32,
    lparAimStatSysPhyProcConsumed  Integer32,
    lparAimStatSysCPU              Integer32,
    lparAimStatSysCPUAverage       Integer32,
    lparAimStatSysMem              Integer32,
    lparAimStatSysMemAverage       Integer32,
    lparAimStatSysUnavailable      Integer32,
    lparAimStatSysEntC             DisplayString,
    lparAimStatHMCName             DisplayString,
    lparAimStatSysCpuUtilization   DisplayString,
    lparAimStatSysIP               DisplayString,
    lparAimStatSysAllocMem         Integer32,
    lparAimStatSysLparMem          Integer32,
    lparAimStatSysAvailCPU         Integer32,
    lparAimStatSysOpPanel          DisplayString
}

lparAimStatSysInstanceUID OBJECT-TYPE
    SYNTAX Integer32 (1..2147483647)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "LPAR Instance unique identifier."
::= { lparAimStatSysEntry 1 }
        
