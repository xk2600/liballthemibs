EMPIRE-CALPARA-MIB DEFINITIONS ::= BEGIN
    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE, Integer32, Gauge32, Counter64, enterprises
            FROM SNMPv2-SMI
        RowStatus, DisplayString
            FROM SNMPv2-TC
        SnmpAdminString
            FROM SNMP-FRAMEWORK-MIB;

------------------------------------------------------------
--  LPAR MIB root OID is: 1.3.6.1.4.1.546.16.53
------------------------------------------------------------
calpara MODULE-IDENTITY
    LAST-UPDATED "201009140000Z"
    ORGANIZATION "CA Technologies"
    CONTACT-INFO    
        "One CA Plaza, Islandia, NY 11749, USA, www.ca.com/support"
    DESCRIPTION
        "This MIB is for IBM Logical Partition management.
        It is implemented by the SystemEDGE AIM for LPAR Monitoring (calpara)."

    REVISION "201009140000Z"
    DESCRIPTION
        "Deprecated status monitoring related attributes and trap definitions (see below)
        Changed MIB to conform to the SMIv2
        Changed lparAimLPAdd trap varbind (lparAimStatLPStatus instead lparAimStatLPState)
        Added new attribute lparAimStatSysCpuUtilization
        Using Counter64 for lparAimStatvDiskKBRead and lparAimStatvDiskKBWritten
        Using SnmpAdminString for several attributes (I18N support)"

    REVISION "201002240000Z"
    DESCRIPTION
        "Initial version of this MIB module (based on 11.7)."
    ::= { applications 53 }

empire       OBJECT IDENTIFIER ::= { enterprises 546 }
unix         OBJECT IDENTIFIER ::= { empire 1 }
traps        OBJECT IDENTIFIER ::= { empire 9 }
applications OBJECT IDENTIFIER ::= { empire 16 }
sysmgmt      OBJECT IDENTIFIER ::= { unix 1 }


