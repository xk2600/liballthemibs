    lparAimStatLPDesc             SnmpAdminString,
    lparAimStatLPProfile          SnmpAdminString,
    lparAimStatLPStatus           Integer32,
    lparAimStatLPCapable          Integer32,
    lparAimStatLPProcMin          Integer32,
    lparAimStatLPProcMax          Integer32,
    lparAimStatLPProcCur          Integer32,
    lparAimStatLPMemMin           Integer32,
    lparAimStatLPMemMax           Integer32,
    lparAimStatLPMemCur           Integer32,
    lparAimStatLPLoad             Integer32,
    lparAimStatLPFiveLoad         Integer32,
    lparAimStatLPFifteenLoad      Integer32,
    lparAimStatLPSharedProcPoolId DisplayString,
    lparAimStatLPProcMode         DisplayString,
    lparAimStatLPSharingMode      DisplayString,
    lparAimStatLPIP               DisplayString,
    lparAimStatLPNumCPUs          Integer32,
    lparAimStatLPPhyProcConsumed  Integer32,
    lparAimStatLPCPUPhyBusy       Integer32,
    lparAimStatLPCPUIdle          Integer32,
    lparAimStatLPCPUSysTime       Integer32,
    lparAimStatLPCPUUserTime      Integer32,
    lparAimStatLPCPUIOWait        Integer32,
