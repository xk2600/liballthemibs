------------------------------------------------------------
--  The LPAR System Instance Table
------------------------------------------------------------

lparAimInstanceCount OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Number of LPAR Instances in lparAimInstanceTable."
::= {lparAimGroup 1}

lparAimInstanceTable OBJECT-TYPE
    SYNTAX SEQUENCE OF LparAimInstanceRow
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "A table containing the LPAR Instances."
::= { lparAimGroup 2 }

lparAimInstanceEntry OBJECT-TYPE
    SYNTAX LparAimInstanceRow
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
        "The LPAR Instances Table entry."
    INDEX { lparAimInstanceUID }
::= { lparAimInstanceTable 1 }

LparAimInstanceRow ::= SEQUENCE {
    lparAimInstanceUID        Integer32,
    lparAimInstanceName       DisplayString,
    lparAimInstanceSysName    DisplayString,
    lparAimInstanceHMCName    DisplayString,
    lparAimInstanceUserName   SnmpAdminString,
    lparAimInstancePassword   SnmpAdminString,
    lparAimInstanceRowStatus  Integer32,
    lparAimInstanceDataStatus Integer32
}

lparAimInstanceUID OBJECT-TYPE
    SYNTAX Integer32 (1..2147483647)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "LPAR Instance unique identifier."
::= { lparAimInstanceEntry  1}

lparAimInstanceName OBJECT-TYPE
    SYNTAX DisplayString (SIZE(255))
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
       "Name of the LPAR Instance."
::= { lparAimInstanceEntry  2}

lparAimInstanceSysName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The name of the System."
::= { lparAimInstanceEntry  3}

lparAimInstanceHMCName OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Host name of HMC console."
::= { lparAimInstanceEntry  4}

lparAimInstanceUserName OBJECT-TYPE
    SYNTAX SnmpAdminString
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "User name for connecting to the HMC console."
::= { lparAimInstanceEntry  5}

lparAimInstancePassword OBJECT-TYPE
    SYNTAX SnmpAdminString
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "Password for connecting to the HMC console."
::= { lparAimInstanceEntry  6}

lparAimInstanceRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
        "The row status according to the Textual Conventions for SMIv2 (see RFC 2579).
        active(1)        - the row is available for usage
        notInService(2)  - setting to this value disables usage of the row
        notReady(3)      - an error occured during usage of the row
        createAndGo(4)   - setting to this value creates the row with row status set to active(1)
        createAndWait(5) - setting to this value creates the row with row status set to notInService(2)
        destroy(6)       - setting to this value deletes the row"
    DEFVAL { 5 }
::= { lparAimInstanceEntry  7 }

lparAimInstanceDataStatus OBJECT-TYPE
    SYNTAX INTEGER {
        ready(1),
        notReady(2),
        hostUnreachable(3),
        authenticationFailed(4),
        unknownError(5)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The status of data of the LPAR Instance (Server), i.e. status of Aim caches for the instance.
        ready(1)                - data are ready in Aim cache, caches were populated
        notReady(2)             - data are Not ready, caches were not populated yet
        hostUnreachable(3)      - HMC host unreachable, data are kept in Aim caches
        authenticationFailed(4) - authentication failed for HMC host, data are kept in Aim caches
        unknownError(5)         - unknow error, data are kept in Aim caches"
::= { lparAimInstanceEntry  8 }


