    lparAimStatLPCPUUsage         Gauge32,
    lparAimStatLPLMB              Integer32,
    lparAimStatLPMemoryUsage      Gauge32,
    lparAimStatLPUnavailable      Integer32,
    lparAimStatLPEntC             DisplayString,
    lparAimStatLPNumDiskDrives    Integer32,
    lparAimStatLPNumDrivePaths    Integer32,
    lparAimStatLPNumVDisks        Integer32,
    lparAimStatLPHostname         DisplayString,
    lparAimStatLPOs               DisplayString,
    lparAimStatLPMAC              DisplayString,
    lparAimStatLPCurVProc         Integer32,
    lparAimStatLPMaxVProc         Integer32,
    lparAimStatLPMinVProc         Integer32,
    lparAimStatLPOpPanel          DisplayString
}

