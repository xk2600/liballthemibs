    DESCRIPTION
        "The VIO Server SCSI deleted trap."
::= { sysmgmt 165371 }

lparAimVIOvEthernetAdd NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName, 
        lparAimStatvEthernetLPAR,
        lparAimStatvEthernetLPARID,
        lparAimStatvEthernetSlotNum,
        lparAimStatvEthernetIEEE,
        lparAimStatvEthernetPortVLAN,
        lparAimStatvEthernetAdditionalVLAN,
        lparAimStatvEthernetIsTrunk,
        lparAimStatvEthernetIsRequired,
        lparAimStatvEthernetMACAddress,
        lparAimStatvEthernetUID
    }
    STATUS current
    DESCRIPTION
        "The VIO Server Ethernet added trap."
::= { sysmgmt 165373 }

lparAimVIOvEthernetDelete NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName, 
        lparAimStatvEthernetLPAR,        
        lparAimStatvEthernetSlotNum,
        lparAimStatvEthernetUID
    }
    STATUS current
    DESCRIPTION
        "The VIO Server Ethernet deleted trap."
::= { sysmgmt 165374 }

lparAimVIOvSerialAdd NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName, 
        lparAimStatvSerialLPAR,
        lparAimStatvSerialLPARID,
        lparAimStatvSerialSlotNum,
        lparAimStatvSerialAdapterType,
        lparAimStatvSerialSupportsHMC,
        lparAimStatvSerialRemoteLPARID,
        lparAimStatvSerialRemoteLPAR,
        lparAimStatvSerialRemoteSlotNum,
        lparAimStatvSerialIsRequired
    }
    STATUS current
    DESCRIPTION
        "The VIO Server Serial added trap."
::= { sysmgmt 165375 }

lparAimVIOvSerialDelete NOTIFICATION-TYPE
    OBJECTS { 
        lparAimStatSysName, 
        lparAimStatvSerialLPAR,
        lparAimStatvSerialAdapterType,
        lparAimStatvSerialSlotNum
    }
    STATUS current
    DESCRIPTION
        "The VIO Server Serial deleted trap."
::= { sysmgmt 165376 }


------------------------------------------------------------
-- The System Instance NOTIFICATION-TYPE Definitions
------------------------------------------------------------

lparAimInstanceAddTrap NOTIFICATION-TYPE
    OBJECTS {
        lparAimInstanceUID,
        lparAimInstanceName,
        lparAimInstanceSysName,
        lparAimInstanceHMCName,
        lparAimInstanceDataStatus
    } 
    STATUS current
    DESCRIPTION
        "LPAR Instance added trap. This trap will be sent when a new LPAR Instance is added,
        and data in Aim caches were updated according to that change."
::= { sysmgmt 165380 }

lparAimInstanceDeleteTrap NOTIFICATION-TYPE
    OBJECTS {
        lparAimInstanceUID,
        lparAimInstanceName,
        lparAimInstanceSysName,
        lparAimInstanceHMCName,
        lparAimInstanceDataStatus
    } 
    STATUS current
    DESCRIPTION
        "LPAR Instance deleted trap. This trap will be sent when a LPAR Instance is deleted,
        and data in Aim caches were updated according to that change."
::= { sysmgmt 165381 }

lparAimInstanceDataStatusChangeTrap NOTIFICATION-TYPE
    OBJECTS {
        lparAimInstanceUID,
        lparAimInstanceName,
        lparAimInstanceSysName,
        lparAimInstanceHMCName,
        lparAimInstanceDataStatus
    } 
    STATUS current
    DESCRIPTION
        "LPAR Instance data status change trap. This trap will be sent when data status of
        LPAR Instance is changed, and data in Aim caches were updated according to that change.
        The first DataStatus var is an old value, the second one holds a new value."
::= { sysmgmt 165382 }


END