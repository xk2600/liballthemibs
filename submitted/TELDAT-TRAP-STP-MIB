stpNotification10 NOTIFICATION-TYPE
    OBJECTS { 
              stp10_bridge_type, 
              stp10_bridge_instance,
              stp10_bridge_port,
              stp10_interface,
              stp10_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STP.10

        Message: 'Sending Configuration BPDU on $1-$2 port $3, interface $4$5'

        Parameters list:
              $1: stp10_bridge_type
              $2: stp10_bridge_instance
              $3: stp10_bridge_port
              $4: stp10_interface
              $5: stp10_circuit_name

        A Configuration BPDU will be sent on the specified port.
        This is done normally on a periodic basis as part of the
        Spanning Tree Protocol.
        The flags field in this BPDU is zero, e.g., neither the Topology Change
        or the Topology Change Acknowledgement bits are set."
    ::= { stpNotificationsPrefix 10 }

stpNotification11 NOTIFICATION-TYPE
    OBJECTS { 
              stp11_TC, 
              stp11_TCA,
