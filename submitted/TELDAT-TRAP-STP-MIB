stpNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              stp1_source_address, 
              stp1_bridge_type,
              stp1_bridge_instance,
              stp1_bridge_port,
              stp1_interface,
              stp1_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STP.1

        Message: 'Configuration BPDU received frm $1 on $2-$3 port $4, interface $5$6'

        Parameters list:
              $1: stp1_source_address
              $2: stp1_bridge_type
              $3: stp1_bridge_instance
              $4: stp1_bridge_port
              $5: stp1_interface
              $6: stp1_circuit_name

        A configuration BPDU has been received from the specified MAC address."
    ::= { stpNotificationsPrefix 1 }

stpNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              stp2_source_address, 
              stp2_bridge_type,
              stp2_bridge_instance,
              stp2_bridge_port,
              stp2_interface,
              stp2_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STP.2

        Message: 'Topology change notification BPDU received frm $1 on $2-$3 port $4, interface $5$6'

        Parameters list:
              $1: stp2_source_address
              $2: stp2_bridge_type
              $3: stp2_bridge_instance
              $4: stp2_bridge_port
              $5: stp2_interface
              $6: stp2_circuit_name

        A topology change notification BPDU has been received from
        the specified MAC address."
    ::= { stpNotificationsPrefix 2 }

stpNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              stp3_BDPU_type, 
              stp3_source_address,
              stp3_bridge_type,
              stp3_bridge_instance,
              stp3_bridge_port,
              stp3_interface,
              stp3_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STP.3

        Message: 'Unkown BPDU type $1 received frm $2 on $3-$4 port $5, interface $6$7'

        Parameters list:
              $1: stp3_BDPU_type
              $2: stp3_source_address
              $3: stp3_bridge_type
              $4: stp3_bridge_instance
              $5: stp3_bridge_port
              $6: stp3_interface
              $7: stp3_circuit_name

        A BPDU with an undefined value in the BPDU Type field was received
        from the specified host. It will be ignored."
    ::= { stpNotificationsPrefix 3 }

stpNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              stp4_Protocol_Identifier, 
              stp4_source_address,
              stp4_bridge_type,
              stp4_bridge_instance,
              stp4_bridge_port,
              stp4_interface,
              stp4_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STP.4

        Message: 'BPDU bad protocol identifier $1 frm $2 on $3-$4 port $5, interface $6$7'

        Parameters list:
              $1: stp4_Protocol_Identifier
              $2: stp4_source_address
              $3: stp4_bridge_type
              $4: stp4_bridge_instance
              $5: stp4_bridge_port
              $6: stp4_interface
              $7: stp4_circuit_name

        A configuration BPDU has been received with a Protocol Identifier
        that is not 0000. It will be ignored."
    ::= { stpNotificationsPrefix 4 }

stpNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              stp5_Protocol_Version_Identifier, 
              stp5_source_address,
