TELDAT-TRAP-POLR-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


polrTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the POLR event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 162 }


polrEventsGroup	OBJECT IDENTIFIER	::= { polrTrapsMIB 1 }

polrNotifications	OBJECT IDENTIFIER	::= { polrTrapsMIB 2 }
polrNotificationsPrefix	OBJECT IDENTIFIER	::= { polrNotifications 0 }

polrEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event POLR.1
        A packet was generated from specified address for specified address.
        Packet length and protocol are also shown. This packet will be tested
        for policy routing."
