polrNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              polr1_srcaddr, 
              polr1_dstaddr,
              polr1_length,
              polr1_protocol 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event POLR.1

        Message: 'test local packet from $1 for $2 of length $3 protocol $4'

        Parameters list:
              $1: polr1_srcaddr
              $2: polr1_dstaddr
              $3: polr1_length
              $4: polr1_protocol

        A packet was generated from specified address for specified address.
        Packet length and protocol are also shown. This packet will be tested
        for policy routing."
    ::= { polrNotificationsPrefix 1 }

polrNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              polr2_srcaddr, 
              polr2_dstaddr,
              polr2_length,
              polr2_protocol,
              polr2_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event POLR.2

        Message: 'test packet from $1 for $2 of length $3 protocol $4 from interface $5'

        Parameters list:
              $1: polr2_srcaddr
              $2: polr2_dstaddr
              $3: polr2_length
              $4: polr2_protocol
              $5: polr2_interface

        A packet was received from specified address for specified address, on
        specified interface. Packet length and protocol are also shown. This
        packet will be tested for policy routing."
    ::= { polrNotificationsPrefix 2 }

polrNotification3 NOTIFICATION-TYPE
    STATUS current
    DESCRIPTION
        "Notification of Event POLR.3

        Message: 'local policy routing disabled'

        No route map associated to locally generated packets."
    ::= { polrNotificationsPrefix 3 }

polrNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              polr4_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event POLR.4

        Message: 'policy routing disabled on interface $1'

        Parameters list:
              $1: polr4_interface

        No route map associated to specified interface."
    ::= { polrNotificationsPrefix 4 }

polrNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              polr5_srcaddr, 
              polr5_dstaddr,
              polr5_length,
              polr5_protocol,
              polr5_routemap 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event POLR.5

        Message: 'missed local packet from $1 for $2 of length $3 protocol $4 with route map $5'

        Parameters list:
              $1: polr5_srcaddr
              $2: polr5_dstaddr
              $3: polr5_length
              $4: polr5_protocol
              $5: polr5_routemap

        A packet was generated from specified address for specified address.
        Packet length and protocol are also shown. This packet did not match the
        specified route map."
    ::= { polrNotificationsPrefix 5 }

