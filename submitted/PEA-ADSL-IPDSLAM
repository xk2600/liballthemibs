       FROM PEA-EQUIPMENT-IPDSLAM

    -- standar X.733 header for the traps       
    managedObjectClass, 
    managedObjectInstance, 
    sequenceNumber, 
    perceivedSeverity, 
    eventTime, 
    eventTypePem, 
    probableCause  
       FROM PEM-STANDARD 

    MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
    Gauge32,Integer32,Unsigned32, Counter32
        FROM SNMPv2-SMI
        
    TEXTUAL-CONVENTION,		
    TruthValue
       FROM SNMPv2-TC
       
    OBJECT-GROUP, NOTIFICATION-GROUP   
             FROM SNMPv2-CONF
        
    DisplayString
        FROM RFC1213-MIB
        
    adslAtucIntervalNumber,
    adslAturIntervalNumber,
    adslAtucChanIntervalNumber,
    adslAturChanIntervalNumber,
    adslLineConfProfileEntry,
    adslLineAlarmConfProfileEntry,
