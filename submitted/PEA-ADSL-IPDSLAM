      SYNTAX AdslProprietaryPerfDataXEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in adslProprietaryPerfDataXTable."
      INDEX { lineIndex }
      ::= { adslProprietaryPerfDataXTable 1 }

AdslProprietaryPerfDataXEntry ::=
      SEQUENCE {
        adslProprietaryPerfDataXReset                    INTEGER,
        adslProprietaryPerfDataXSinceResetTimeElapsed    Unsigned32,
        adslProprietaryPerfDataXFailedInits              Counter32
        }

adslProprietaryPerfDataXReset OBJECT-TYPE
    SYNTAX      INTEGER {reset(1)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When set to 'reset' the ADSL line performance total counters
    are reset to 0."
    ::= { adslProprietaryPerfDataXEntry 1 }    
    
adslProprietaryPerfDataXSinceResetTimeElapsed OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "seconds"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Elapsed time in seconds since the last reset of
    the ADSL line performance total counters."
    ::= { adslProprietaryPerfDataXEntry 2 }    

