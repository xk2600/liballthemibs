arpNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              arp1_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.1

        Message: 'Queue overflow on interface $1'

        Parameters list:
              $1: arp1_interface

        A packet was discarded because the queue of pending ARP packets
        to process is too long. This means the number of received ARP packets
        is so high that they cannot be processed."
    ::= { arpNotificationsPrefix 1 }

arpNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              arp2_operation_type, 
              arp2_hardware_type,
              arp2_protocol_type,
              arp2_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.2

        Message: 'Packet received $1 $2 $3 interface $4'

        Parameters list:
              $1: arp2_operation_type
              $2: arp2_hardware_type
              $3: arp2_protocol_type
              $4: arp2_interface

        An ARP packet of the type indicated has just arrived for processing."
    ::= { arpNotificationsPrefix 2 }

arpNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              arp3_hardware_type, 
              arp3_protocol_type,
