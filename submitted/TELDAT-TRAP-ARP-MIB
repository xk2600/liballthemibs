arpNotification14 NOTIFICATION-TYPE
    OBJECTS { 
              arp14_mac_address, 
              arp14_ip_address,
              arp14_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.14

        Message: 'Refresh request sent for mac $1 associated with IP $2 interface $3'

        Parameters list:
              $1: arp14_mac_address
              $2: arp14_ip_address
              $3: arp14_interface

        An ARP translation request is being sent as the result of the 
        auto-refresh mechanism for this IP and mac address."
    ::= { arpNotificationsPrefix 14 }

arpNotification15 NOTIFICATION-TYPE
    OBJECTS { 
              arp15_mac_address, 
              arp15_ip_address,
              arp15_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.15

        Message: 'Deleting translation entry with mac $1 and IP $2 interface $3'

        Parameters list:
              $1: arp15_mac_address
              $2: arp15_ip_address
              $3: arp15_interface

        A translation cache entry timed out with this IP and mac address has 
        been deleted."
    ::= { arpNotificationsPrefix 15 }

arpNotification16 NOTIFICATION-TYPE
    OBJECTS { 
              arp16_mac_address, 
              arp16_ip_address,
              arp16_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.16

        Message: 'New entry mac $1 associated with IP $2 interface $3'

        Parameters list:
              $1: arp16_mac_address
              $2: arp16_ip_address
              $3: arp16_interface

        An ARP translation is being setup this IP and mac address."
    ::= { arpNotificationsPrefix 16 }

arpNotification17 NOTIFICATION-TYPE
    OBJECTS { 
              arp17_mac_address_old, 
              arp17_mac_address_new,
              arp17_ip_address,
              arp17_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.17

        Message: 'Change entry mac $1 to $2 associated with IP $3 interface $4'

        Parameters list:
              $1: arp17_mac_address_old
              $2: arp17_mac_address_new
              $3: arp17_ip_address
              $4: arp17_interface

        An ARP translation mac address is being changed for this IP."
    ::= { arpNotificationsPrefix 17 }

arpNotification18 NOTIFICATION-TYPE
    OBJECTS { 
              arp18_mac_address, 
              arp18_ip_address,
              arp18_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ARP.18

        Message: 'Duplicated address received from mac $1 with IP $2 interface $3'

        Parameters list:
              $1: arp18_mac_address
              $2: arp18_ip_address
              $3: arp18_interface

        An ARP packet was received with a local address. The received mac and IP
        addresses are shown, as well as the interface."
    ::= { arpNotificationsPrefix 18 }


END
