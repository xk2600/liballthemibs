IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


arpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the ARP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 5 }


arpEventsGroup	OBJECT IDENTIFIER	::= { arpTrapsMIB 1 }

arpNotifications	OBJECT IDENTIFIER	::= { arpTrapsMIB 2 }
arpNotificationsPrefix	OBJECT IDENTIFIER	::= { arpNotifications 0 }

arpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ARP.1
        A packet was discarded because the queue of pending ARP packets
        to process is too long. This means the number of received ARP packets
        is so high that they cannot be processed."
    ::= { arpEventsGroup 1 }

