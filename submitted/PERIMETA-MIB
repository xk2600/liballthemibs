-- /**********************************************************/
-- /* Per-adjacency per-method information about SIP         */
-- /* responses                                              */
-- /**********************************************************/

  perimetaAdjacencyMethods OBJECT IDENTIFIER ::= { perimeta 5 }

  perimetaAdjacencyMethodStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF PerimetaAdjacencyMethodStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table contains statistics from Perimeta at
                 per-adjacency per-method scope, indexed by
                 adjacency name and SIP method."
    ::= { perimetaAdjacencyMethods 1 }

  perimetaAdjacencyMethodStatsEntry OBJECT-TYPE
    SYNTAX      PerimetaAdjacencyMethodStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The entry in the Perimeta adjacency stats table
                 contains all the per-adjacency per-method
                 statistics, indexed on adjacency name and SIP
                 method."
    INDEX      { adjMthdStatAdjacencyName,
                 adjMthdStatSIPMethod,
                 adjMthdStatSummaryPeriod }
    ::= { perimetaAdjacencyMethodStatsTable 1 }

  PerimetaAdjacencyMethodStatsEntry ::= SEQUENCE
  {
    adjMthdStatAdjacencyName                      DisplayString,
    adjMthdStatSIPMethod                          INTEGER,
    adjMthdStatSummaryPeriod                      INTEGER,
    adjMthdStatRequestsRcvdRate                   CounterBasedGauge64,
    adjMthdStatRequestsRcvdCount                  CounterBasedGauge64,
    adjMthdStatRequestsSentRate                   CounterBasedGauge64,
    adjMthdStatRequestsSentCount                  CounterBasedGauge64,
    adjMthdStat1XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat1XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat1XXSentRate                        CounterBasedGauge64,
    adjMthdStat1XXSentCount                       CounterBasedGauge64,
    adjMthdStat2XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat2XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat2XXSentRate                        CounterBasedGauge64,
    adjMthdStat2XXSentCount                       CounterBasedGauge64,
    adjMthdStat3XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat3XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat3XXSentRate                        CounterBasedGauge64,
    adjMthdStat3XXSentCount                       CounterBasedGauge64,
    adjMthdStat4XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat4XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat4XXSentRate                        CounterBasedGauge64,
    adjMthdStat4XXSentCount                       CounterBasedGauge64,
    adjMthdStat5XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat5XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat5XXSentRate                        CounterBasedGauge64,
    adjMthdStat5XXSentCount                       CounterBasedGauge64,
    adjMthdStat6XXRcvdRate                        CounterBasedGauge64,
    adjMthdStat6XXRcvdCount                       CounterBasedGauge64,
    adjMthdStat6XXSentRate                        CounterBasedGauge64,
    adjMthdStat6XXSentCount                       CounterBasedGauge64,
    adjMthdStatOtherRcvdRate                      CounterBasedGauge64,
    adjMthdStatOtherRcvdCount                     CounterBasedGauge64,
    adjMthdStatOtherSentRate                      CounterBasedGauge64,
    adjMthdStatOtherSentCount                     CounterBasedGauge64,
    adjMthdStatISARcvdRate                        CounterBasedGauge64,
    adjMthdStatISARcvdCount                       CounterBasedGauge64,
    adjMthdStatISARcvdPerc                        CounterBasedGauge64,
    adjMthdStatISASentRate                        CounterBasedGauge64,
    adjMthdStatISASentCount                       CounterBasedGauge64,
    adjMthdStatISASentPerc                        CounterBasedGauge64,
    adjMthdStatESERcvdRate                        CounterBasedGauge64,
    adjMthdStatESERcvdCount                       CounterBasedGauge64,
    adjMthdStatESESentRate                        CounterBasedGauge64,
    adjMthdStatESESentCount                       CounterBasedGauge64,
    adjMthdStatNon3xxRcvdCount                    CounterBasedGauge64,
    adjMthdStatNon3xxSentCount                    CounterBasedGauge64,
    adjMthdStatSERRcvdPerc                        CounterBasedGauge64,
    adjMthdStatSERSentPerc                        CounterBasedGauge64,
    adjMthdStatSEERRcvdPerc                       CounterBasedGauge64,
    adjMthdStatSEERSentPerc                       CounterBasedGauge64
  }

  adjMthdStatAdjacencyName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(1..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The name of the adjacency. This an index field
                 for adjacency method statistics."
    ::= { perimetaAdjacencyMethodStatsEntry 1 }

  adjMthdStatSIPMethod OBJECT-TYPE
    SYNTAX       INTEGER {
                   unknown(1),
                   ack(2),
                   bye(3),
                   cancel(4),
                   info(5),
                   invite(6),
                   message(7),
                   notify(8),
                   options(9),
                   prack(10),
                   refer(11),
                   register(12),
                   subscribe(13),
                   update(14)
                 }
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The SIP method that these statistics apply to.
                  This is an index field for adjacency method
                  statistics."
    ::= { perimetaAdjacencyMethodStatsEntry 2 }

  adjMthdStatSummaryPeriod OBJECT-TYPE
    SYNTAX       INTEGER {
                   summaryPeriod1CurrentClctnIntv(1)
                 }
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "The period over which the statistics were collected.
                  This is an index field."
    ::= { perimetaAdjacencyMethodStatsEntry 3 }

