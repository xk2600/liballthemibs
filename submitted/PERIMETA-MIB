PERIMETA-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, Unsigned32
                                            FROM SNMPv2-SMI
        DisplayString
                                            FROM SNMPv2-TC
        CounterBasedGauge64
                                            FROM HCNUM-TC;

  perimeta MODULE-IDENTITY
        LAST-UPDATED "201712180000Z"
        ORGANIZATION "Metaswitch"
        CONTACT-INFO
          "Metaswitch
           100 Church Street
           Enfield
           EN2 6BQ
           United Kingdom
           Tel: +44 20 8366 1177"
        DESCRIPTION  "This MIB module defines the perimeta module
                      and children object types, including tables
                      for global statistics and per-object
                      statistics."

        REVISION      "201712180000Z" -- 18 Dec 2017
        DESCRIPTION   "New statistics for Perimeta V4.3.20"

        REVISION      "201711210000Z" -- 21 Nov 2017
        DESCRIPTION   "New statistics for Perimeta V4.3.00"

        REVISION      "201612010000Z" -- 01 Dec 2016
        DESCRIPTION   "Registration of new per-adjacency
                       per-method statistics table for V4.2."

        REVISION      "201504301200Z" -- 30 April 2015
        DESCRIPTION   "New statistics for Perimeta V3.8."

        REVISION      "201411131200Z" -- 13 Nov 2014
        DESCRIPTION   "New statistics for Perimeta V3.7 and a
                       couple of small syntax fixes."

        REVISION      "201401081200Z" -- 08 Jan 2014
        DESCRIPTION   "Per-account stats and Gauge stats for
                       Perimeta V3.6."

        REVISION      "201311271200Z" -- 27 Nov 2013
        DESCRIPTION   "New statistics for Perimeta V3.5."

        REVISION      "201305290854Z" -- 29 May 2013
        DESCRIPTION   "Registration of new global statistics and
                       the new per-adjacency statistics table for
                       V3.4."

        REVISION      "201107220000Z" -- 22 July 2011
        DESCRIPTION   "Registration of initial global statistics
                       and the Perimeta Info table."

        ::= { dataConnection 8 }

-- /**********************************************************/
-- /* The object identifier subtree for the Perimeta MIBs.   */
-- /**********************************************************/

memberBody      OBJECT IDENTIFIER ::= { iso 2 }
gb              OBJECT IDENTIFIER ::= { memberBody 826 }
gbNational      OBJECT IDENTIFIER ::= { gb 0 }
gbNatCompany    OBJECT IDENTIFIER ::= { gbNational 1 }
dataConnection  OBJECT IDENTIFIER ::= { gbNatCompany 1578918 }

-- /**********************************************************/
-- /* MIB tables.                                            */
-- /**********************************************************/

-- /**********************************************************/
-- /* MIB Info Section                                       */
-- /**********************************************************/

  perimetaInfo   OBJECT-TYPE
    SYNTAX SEQUENCE OF PerimetaInfo
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "This table holds general information about 'Perimeta'.
    It is neither creatable nor deletable."
    ::= { perimeta 1 }

  PerimetaInfo ::= SEQUENCE
  {
    infoPerimetaName DisplayString,
    infoVersion DisplayString,
    infoPlatform DisplayString,
    infoEquipmentIdentifier DisplayString
  }

  infoPerimetaName OBJECT-TYPE
    SYNTAX       DisplayString (SIZE(1..32))
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "Corresponds to the name of the Perimeta.
    This is an index field."
    ::= { perimetaInfo 1 }

  infoVersion OBJECT-TYPE
    SYNTAX       DisplayString
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Perimeta MIB Version"
    ::= { perimetaInfo 2 }

  infoPlatform OBJECT-TYPE
    SYNTAX       DisplayString (SIZE(1..32))
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Perimeta Hardware Platform"
    ::= { perimetaInfo 3 }

  infoEquipmentIdentifier OBJECT-TYPE
    SYNTAX       DisplayString (SIZE(1..64))
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Unique identifier for this Perimeta instance"
    ::= { perimetaInfo 4 }

-- /**********************************************************/
-- /* MIB table for 'Perimeta Global Statistics'             */
-- /*                                                        */
-- /* These statistics may be cached by Perimeta, and may be */
-- /* up to five minutes old when retrieved.  The time that  */
-- /* they were retrieved is given by                        */
-- /* globalStatsCollectionTime.                             */
-- /*                                                        */
-- /* See "Statistics monitored by Perimeta" in the          */
-- /* "Operations And Maintenance Guide" for more            */
-- /* information.                                           */
-- /**********************************************************/

  perimetaGlobalStatsTable OBJECT-TYPE
    SYNTAX       SEQUENCE OF PerimetaGlobalStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "This table is a child of 'Perimeta'.
                  This table is neither creatable nor deletable."
    ::= { perimeta  2 }

  perimetaGlobalStatsEntry OBJECT-TYPE
    SYNTAX       PerimetaGlobalStatsEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION  "An entry in the table of global statistics."
    INDEX  { globalStatsSummaryPeriod }
    ::= {  perimetaGlobalStatsTable  1  }

  PerimetaGlobalStatsEntry  ::=  SEQUENCE
  {
    globalStatsSummaryPeriod                      INTEGER,
    globalStatsCollectionTime                     Unsigned32,
