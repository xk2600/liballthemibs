    STATUS       current
    DESCRIPTION  "Number of call attempts with signing
                  unavailable (in the last period)"
    ::= { perimetaGlobalStatsEntry 1319 }

  globalStatSigningUnavailableRate OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Rate of call attempts with signing
                  unavailable (calls/s)"
    ::= { perimetaGlobalStatsEntry 1320 }

  globalStatFailedRecordedCallsSetup OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Number of recorded calls failing due to
                  recording call setup failure (in the
                  last period)"
    ::= { perimetaGlobalStatsEntry 1321 }

  globalStatFailedRecordedCallsDrop OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Number of recorded calls failing due to
                  recording call drop (in the last period)"
    ::= { perimetaGlobalStatsEntry 1322 }

  globalStatFailedRecordedCallsSetupPercent OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    UNITS        "/ 10000 %"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Percentage of recorded calls failing due
                  to recording call setup failure (in the
                  last period)"
    ::= { perimetaGlobalStatsEntry 1323 }

  globalStatFailedRecordedCallsDropPercent OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    UNITS        "/ 10000 %"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Percentage of recorded calls failing due
                  to recording call drop (in the last
                  period)"
    ::= { perimetaGlobalStatsEntry 1324 }

  globalStatNoPermitCount OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Number of packets dropped on restricted
                  service interfaces (in the last period)"
    ::= { perimetaGlobalStatsEntry 1325 }

  globalStatNoPermitRate OBJECT-TYPE
    SYNTAX       CounterBasedGauge64
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "Rate of packets dropped on restricted
                  service interfaces (pkts/s)"
    ::= { perimetaGlobalStatsEntry 1326 }

-- /**********************************************************/
-- /* Information about Perimeta Adjacencies.                */
-- /**********************************************************/

  perimetaAdjacencies OBJECT IDENTIFIER ::= { perimeta 3 }

  perimetaAdjacencyNumber OBJECT-TYPE
    SYNTAX       Unsigned32
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "This is the number of adjacencies configured on Perimeta"
    ::= { perimetaAdjacencies 1 }

  perimetaAdjacencyStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF PerimetaAdjacencyStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table contains statistics from Perimeta at
                 per-adjacency scope, indexed by adjacency name."
    ::= { perimetaAdjacencies 2 }

  perimetaAdjacencyStatsEntry OBJECT-TYPE
    SYNTAX      PerimetaAdjacencyStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The entry in the Perimeta adjacency stats table
                 contains all the per-adjacency statistics,
                 indexed on adjacency name."
    INDEX      { adjStatAdjacencyName, adjStatSummaryPeriod }
    ::= { perimetaAdjacencyStatsTable 1 }

