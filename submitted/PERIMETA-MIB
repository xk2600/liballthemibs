-- /**********************************************************/
-- /* Information about Perimeta Accounts.                   */
-- /**********************************************************/

  perimetaAccounts OBJECT IDENTIFIER ::= { perimeta 4 }

  perimetaAccountNumber OBJECT-TYPE
    SYNTAX       Unsigned32
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION  "This is the number of accounts configured on Perimeta"
    ::= { perimetaAccounts 1 }

  perimetaAccountStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF PerimetaAccountStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "This table contains statistics from Perimeta at
                 per-account scope, indexed by account name."
    ::= { perimetaAccounts 2 }

  perimetaAccountStatsEntry OBJECT-TYPE
    SYNTAX      PerimetaAccountStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The entry in the Perimeta account stats table
                 contains all the per-account statistics,
                 indexed on account name."
    INDEX      { accStatAccountName, accStatSummaryPeriod }
    ::= { perimetaAccountStatsTable 1 }

  PerimetaAccountStatsEntry ::= SEQUENCE
  {
    accStatAccountName                          DisplayString,
    accStatSummaryPeriod                        INTEGER,
    accStatNumCallsHwm                            CounterBasedGauge64,
    accStatMedBwHwm                               CounterBasedGauge64,
    accStatMedStreamsHwm                          CounterBasedGauge64,
    accStatNumRegsHwm                             CounterBasedGauge64,
    accStatCallBurstRateHwm                       CounterBasedGauge64,
    accStatCallSustainRateHwm                     CounterBasedGauge64,
    accStatInCallBurstRateHwm                     CounterBasedGauge64,
    accStatInCallSustainRateHwm                   CounterBasedGauge64,
    accStatOOCBurstRateHwm                        CounterBasedGauge64,
    accStatOOCSustainRateHwm                      CounterBasedGauge64,
    accStatRegsRateHwm                            CounterBasedGauge64,
    accStatRegsSustainRateHwm                     CounterBasedGauge64,
    accStatNumCallsPerc                           CounterBasedGauge64,
    accStatMedBwPerc                              CounterBasedGauge64,
    accStatMedStrPerc                             CounterBasedGauge64,
    accStatNumRegsPerc                            CounterBasedGauge64,
    accStatCallBurstRatePerc                      CounterBasedGauge64,
    accStatCallSustainRatePerc                    CounterBasedGauge64,
    accStatInCallBurstRatePerc                    CounterBasedGauge64,
    accStatInCallSustainRatePerc                  CounterBasedGauge64,
    accStatOOCBurstRatePerc                       CounterBasedGauge64,
    accStatOOCSustainRatePerc                     CounterBasedGauge64,
    accStatRegsBurstRatePerc                      CounterBasedGauge64,
    accStatRegsSustainRatePerc                    CounterBasedGauge64,
    accStatSrcNumCallsHwm                         CounterBasedGauge64,
    accStatSrcMedBwHwm                            CounterBasedGauge64,
    accStatSrcMedStreamsHwm                       CounterBasedGauge64,
    accStatSrcNumRegsHwm                          CounterBasedGauge64,
    accStatSrcCallBurstRateHwm                    CounterBasedGauge64,
    accStatSrcCallSustainRateHwm                  CounterBasedGauge64,
    accStatSrcInCallBurstRateHwm                  CounterBasedGauge64,
    accStatSrcInCallSustainRateHwm                CounterBasedGauge64,
    accStatSrcOOCBurstRateHwm                     CounterBasedGauge64,
    accStatSrcOOCSustainRateHwm                   CounterBasedGauge64,
    accStatSrcRegsRateHwm                         CounterBasedGauge64,
    accStatSrcRegsSustainRateHwm                  CounterBasedGauge64,
    accStatSrcNumCallsPerc                        CounterBasedGauge64,
    accStatSrcMedBwPerc                           CounterBasedGauge64,
    accStatSrcMedStrPerc                          CounterBasedGauge64,
    accStatSrcNumRegsPerc                         CounterBasedGauge64,
    accStatSrcCallBurstRatePerc                   CounterBasedGauge64,
    accStatSrcCallSustainRatePerc                 CounterBasedGauge64,
    accStatSrcInCallBurstRatePerc                 CounterBasedGauge64,
    accStatSrcInCallSustainRatePerc               CounterBasedGauge64,
    accStatSrcOOCBurstRatePerc                    CounterBasedGauge64,
    accStatSrcOOCSustainRatePerc                  CounterBasedGauge64,
    accStatSrcRegsBurstRatePerc                   CounterBasedGauge64,
    accStatSrcRegsSustainRatePerc                 CounterBasedGauge64,
    accStatDstNumCallsHwm                         CounterBasedGauge64,
    accStatDstMedBwHwm                            CounterBasedGauge64,
    accStatDstMedStreamsHwm                       CounterBasedGauge64,
    accStatDstNumRegsHwm                          CounterBasedGauge64,
    accStatDstCallBurstRateHwm                    CounterBasedGauge64,
    accStatDstCallSustainRateHwm                  CounterBasedGauge64,
    accStatDstInCallBurstRateHwm                  CounterBasedGauge64,
    accStatDstInCallSustainRateHwm                CounterBasedGauge64,
