US-SRX-SERVICE-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Counter32
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION 
        FROM SNMPv2-TC
    surpassRtp
	FROM SURPASS-RTP-MIB
    hiq8000Admin
        FROM SURPASS-RTP-ADMIN-MIB
    SrxRowCountType
        FROM HIQ-ADMIN-MIB;

--########################################################
-- SRX Services Measurements MIB MODULE                  #
--########################################################

usSrxService MODULE-IDENTITY
    LAST-UPDATED "200806111500Z"
    ORGANIZATION "Siemens Network Convergence LLC"
    CONTACT-INFO " "
    DESCRIPTION
        " SRX Service Measurements MIB module. " 
        
    REVISION "200806111500Z"    
    DESCRIPTION
            "Updated the MIB to be fully compatible with SNMPv2"
    ::= { hiq8000Admin 4 }

--####################
-- OBJECT IDENTIFIER #
--####################

usSrxServiceObjects                   OBJECT IDENTIFIER      ::= { usSrxService 1 }

usSrxServicePerformData               OBJECT IDENTIFIER      ::= { usSrxServiceObjects 1 }

usSrxServicePerfCallForwardingData    OBJECT IDENTIFIER      ::= { usSrxServicePerformData 1 }

usSrxServicePerfCallingIDData         OBJECT IDENTIFIER      ::= { usSrxServicePerformData 2 }

usSrxServicePerfACRData               OBJECT IDENTIFIER      ::= { usSrxServicePerformData 3 }

usSrxServicePerfSpeedCallingData      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 4 }

usSrxServicePerfScrnListEditData      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 5 }

usSrxServicePerfSelCallAcceptData     OBJECT IDENTIFIER      ::= { usSrxServicePerformData 6 }

usSrxServicePerfSelCallRejectData     OBJECT IDENTIFIER      ::= { usSrxServicePerformData 7 }

usSrxServicePerfMalCallTraceData      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 8 }

usSrxServicePerfThreeWayCallData      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 9 }

usSrxServicePerfIntercomCallData      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 10 }

usSrxServicePerfVoiceMailData         OBJECT IDENTIFIER      ::= { usSrxServicePerformData 11 }

usSrxServiceDNDData     	      OBJECT IDENTIFIER      ::= { usSrxServicePerformData 12 }


-- *****************************************************
-- Textual Conventions used in the Service Translations MIB
-- *****************************************************

AccountCodeType ::= TEXTUAL-CONVENTION
    STATUS  current
    DESCRIPTION "
          Indicates the type of Account Code.
       'cdrAccountCode'        - CDRAccountCode
       'postDialAccountCode'   - PostDialAccountCode"
    SYNTAX INTEGER { cdrAccountCode(1),
                     postDialAccountCode(2) }

-- *****************************************************************
-- Services related Traffic and Maintenance Measurements/Statistics 
--
-- Every statistics counter will be shared by all instances of the
-- Services process.
-- *****************************************************************

-- *****************************************************************
-- Call Forwarding (CF) Traffic Measurements
-- *****************************************************************

usSrxServiceCFActToAllowedNumberCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a 
          subscriber of the feature attempts to activate call
          forwarding. An activation attempt is defined as dialing
          the activation code plus a complete legitimate telephone
          number. 
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 1  }

usSrxServiceCFActToNonAllowedNumberCounter   OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          subscriber of the feature attempts to activate call
          forwarding but dials a non-allowed number. 
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 2  }

usSrxServiceCFFirstAttemptSuccessCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of activation 
          successfully completed by answer of remote station. 
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 3  }

usSrxServiceCFSecondAttemptSuccessCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of activation 
          successfully completed by a second activation procedure.
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 4  }

usSrxServiceCFCFVOccuredCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times Call 
          Forwarding occurs for the Call Forwarding Unconditional (CFU)
          Feature. 
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 5  }

usSrxServiceCFCFBLOccuredCounter      OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times Call
          Forwarding occurs for the Call Forwarding Busy
          (CFB) Feature. 
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 6  }

usSrxServiceCFCFDAOccuredCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times Call
          Forwarding occurs for the Call Forwarding No Reply 
          (CFNR) Feature. 
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 7  }

usSrxServiceSCFServAccCdDialedCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the Call
          Forwarding Access Code is dialed.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 8  }

usSrxServiceSCFFeatActivatedCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the Call
          Forwarding Feature is activated to a remote DN.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 9  }

usSrxServiceSCFOccurredCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of forwardings that
          occur due to the Call Forwarding feature.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 10 }

usSrxServiceSCFFailedCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of forwarding 
          attempts that fail.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 11 }

usSrxServiceRCFCallsOfferedCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times calls are
          'offered' to be forwarded to the RCF remote DN. Before
          forwarding a call to the RCF remote DN, a check is made
          to see if the customer-specified limit of simultaneous
          calls has been reached. If this limit has been reached,
          the calling party shall be given busy tone from the base
          switching office.
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 12 }

usSrxServiceRCFCallsForwardedCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times calls are
          forwarded to the RCF remote DN.
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 13 }

usSrxServiceRCFCallsOverflowCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the calling
          parties are given busy tone from the base switching office
          in response to exceeding the customer-specified limit of
          simultaneous calls.
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 14 }

usSrxServiceSCFScrnEditCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the Selective
          Call Forwarding Screen Editing Service is accessed.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 15 }

usSrxServiceSCFCallsFwdedCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of forwardings that
          occur due to the Selective Call Forwarding feature.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 16 }

usSrxServiceSCFActvRemoteDNCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the
          Selective Call Forwarding feature activates the Remote DN.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 17 }

usSrxServiceSCFResUnavilCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          customer could not activate SCF due to unavailable
          resources on the hiQ.
          This counter is supported in ANSI, ITU and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 18 }

usSrxServiceCFVoiceMailCounter  OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          customer's  call is forwarded to Voice Mail.
          This counter is supported in ANSI and Japanese variants."
    ::= { usSrxServicePerfCallForwardingData 19 }

-- *****************************************************************
-- Calling Identity Delivery (CID) Traffic Measurements
-- *****************************************************************

usSrxServiceCIDDNToCalledCPECounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          calling party's directory number is delivered to a 
          subscriber with the Calling Number Delivery Service.
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 1  }

usSrxServiceCIDAnonymousDNToCalledCPECounter   OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          Private Number indication is delivered to a subscriber 
          with the Calling Number Delivery Service.
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 2  }

usSrxServiceCIDUnavailDNToCalledCPECounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times an
          out of area/unavailable indication is delivered to a 
          subscriber with the Calling Number Delivery Service.
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 3  }

usSrxServiceCIDCNDActivateBySubscriberCounter   OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the
          Calling Number Delivery (CND) feature is activated by    
          a subscriber with the Usage Sensitive version of the  
          feature. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 4  }


usSrxServiceCIDCNDDeactivateBySubscriberCounter   OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times the
          Calling Number Delivery feature is deactivated by a
          subscriber with the Usage Sensitive version of the
          feature. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 5  }

usSrxServiceCIDCallingNameToCalledCPECounter   OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          calling party's name is delivered to a subscriber
          with the Calling Name Delivery Service. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 6  }

usSrxServiceCIDAnonymousNameToCalledCPECounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times an
          Anonymous Name indication is delivered to a subscriber
          with the Calling Name Delivery Service. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 7  }

usSrxServiceCIDUnavailNameToCalledCPECounter OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times an
          out of area/unavailable name indication is delivered to 
          a subscriber with the Calling Name Delivery Service.
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 8  }

usSrxServiceCIDTCAPNameQueryCounter     OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of times a
          TCAP Name Query is sent to the SCP. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 9  }

usSrxServiceCIDTCAPNameQueryTimeoutCounter OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the total number of timeouts that 
          occured while the SRX was waiting for the response from 
          the SCP for a Calling Name Query. 
          This counter is supported in ANSI and Japanese variants. "
    ::= { usSrxServicePerfCallingIDData 10  }

