    ::= { tftpEventObjects6 1 }


tftpNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              tftp1_conn, 
              tftp1_ip_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.1

        Message: 'TFTP connection 0x$1 created, ip destination $2'

        Parameters list:
              $1: tftp1_conn
              $2: tftp1_ip_address

        TFTP connection created."
    ::= { tftpNotificationsPrefix 1 }

tftpNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              tftp2_string, 
              tftp2_conn 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.2

        Message: 'Sending $1 conn 0x$2'

        Parameters list:
              $1: tftp2_string
              $2: tftp2_conn

        TFTP packet sent."
    ::= { tftpNotificationsPrefix 2 }

tftpNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              tftp3_string, 
              tftp3_conn 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.3

        Message: 'Received $1 conn 0x$2'

        Parameters list:
              $1: tftp3_string
              $2: tftp3_conn

        TFTP packet sent."
    ::= { tftpNotificationsPrefix 3 }

tftpNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              tftp4_n_retry, 
              tftp4_conn 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.4

        Message: 'retrying $1 last packet sent, conn 0x$2'

        Parameters list:
              $1: tftp4_n_retry
              $2: tftp4_conn

        TFTP packet sent retry."
    ::= { tftpNotificationsPrefix 4 }

tftpNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              tftp5_conn 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.5

        Message: 'max retries reached, conn 0x$1'

        Parameters list:
              $1: tftp5_conn

        Maximum TFTP packet retries reached. The connection will be closed."
    ::= { tftpNotificationsPrefix 5 }

tftpNotification6 NOTIFICATION-TYPE
    OBJECTS { 
              tftp6_conn 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event TFTP.6

        Message: 'TFTP connection 0x$1 destroyed'

        Parameters list:
              $1: tftp6_conn

        TFTP connection destroyed."
    ::= { tftpNotificationsPrefix 6 }


