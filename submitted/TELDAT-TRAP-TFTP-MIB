tftpEventObjects6 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event TFTP.6
        TFTP connection destroyed."
    ::= { tftpEventsGroup 6 }

tftp6_conn OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
