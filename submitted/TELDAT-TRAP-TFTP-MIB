    ::= { tftpEventObjects3 2 }

tftpEventObjects4 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event TFTP.4
        TFTP packet sent retry."
    ::= { tftpEventsGroup 4 }

tftp4_n_retry OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'n_retry' in event TFTP.4"
    ::= { tftpEventObjects4 1 }

tftp4_conn OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'conn' in event TFTP.4"
    ::= { tftpEventObjects4 2 }

tftpEventObjects5 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event TFTP.5
        Maximum TFTP packet retries reached. The connection will be closed."
    ::= { tftpEventsGroup 5 }

tftp5_conn OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
