-- file automatically created


TELDAT-TRAP-TFTP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


tftpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the TFTP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 19 }


tftpEventsGroup	OBJECT IDENTIFIER	::= { tftpTrapsMIB 1 }

tftpNotifications	OBJECT IDENTIFIER	::= { tftpTrapsMIB 2 }
tftpNotificationsPrefix	OBJECT IDENTIFIER	::= { tftpNotifications 0 }

tftpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event TFTP.1
        TFTP connection created."
    ::= { tftpEventsGroup 1 }

tftp1_conn OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'conn' in event TFTP.1"
    ::= { tftpEventObjects1 1 }

tftp1_ip_address OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'ip_address' in event TFTP.1"
    ::= { tftpEventObjects1 2 }

tftpEventObjects2 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event TFTP.2
        TFTP packet sent."
    ::= { tftpEventsGroup 2 }

tftp2_string OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'string' in event TFTP.2"
    ::= { tftpEventObjects2 1 }

