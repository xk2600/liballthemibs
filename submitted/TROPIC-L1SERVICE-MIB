        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The router priority of the link."
        DEFVAL         { 1 }
        ::= { tnControlNetworkLinkEntry 33 }

    tnCNLinkEquipOperStatus OBJECT-TYPE
        SYNTAX         TropicLinkOperationalStateType
