        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    "The OSPF interface type of the link."
        DEFVAL         { broadcast }
        ::= { tnControlNetworkLinkEntry 32 }

    tnCNLinkConfigRtrPriority OBJECT-TYPE
