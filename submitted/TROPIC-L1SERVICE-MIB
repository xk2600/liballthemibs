    tnCNLinkSubnetMask OBJECT-TYPE
        SYNTAX         IpAddress
        MAX-ACCESS     read-create
        STATUS         current
        DESCRIPTION    ""
        ::= { tnControlNetworkLinkEntry 8 }

--    obsolete
--    tnCNLinkAreaId OBJECT-TYPE ::= { tnControlNetworkLinkEntry 9 }
