srtNotification62 NOTIFICATION-TYPE
    OBJECTS { 
              srt62_source_mac, 
              srt62_dest_mac,
              srt62_reformatted_length,
              srt62_output_maximum,
              srt62_port,
              srt62_interface,
              srt62_circuit_name 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event SRT.62

        Message: 'Source-routed frame converted to transparent frame from $1 to $2 is too big (reformatted length $3 bytes > output maximum size $4 bytes) from port $5 interface $6$7, may get dropped'

        Parameters list:
              $1: srt62_source_mac
              $2: srt62_dest_mac
              $3: srt62_reformatted_length
              $4: srt62_output_maximum
              $5: srt62_port
              $6: srt62_interface
              $7: srt62_circuit_name

        The specified source-routed frame is larger than that is allowed
        by LF-BIT configuration for the transparent bridge domain. After mapping
        to the MAC headers of the outgoing port, the packet may get dropped
        if it exceeds the MSDU limit of the port."
    ::= { srtNotificationsPrefix 62 }

srtNotification63 NOTIFICATION-TYPE
    OBJECTS { 
              srt63_routing_type, 
              srt63_source_mac,
              srt63_dest_mac,
