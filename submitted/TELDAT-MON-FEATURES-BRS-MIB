-- Tabla que muestra los estadçsticos que describen el trﬂfico de Reserva de
-- Ancho de Banda para el interfaz FR y circuito.

telProdNpMonFeatBrsFrCirCountTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TelProdNpMonFeatBrsFrCirCountEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Displays statistics describing bandwidth reservation traffic for the
    selected Frame Relay interface and circuit."
    ::= { telProdNpMonFeatBrs 3 }

telProdNpMonFeatBrsFrCirCountEntry OBJECT-TYPE
    SYNTAX TelProdNpMonFeatBrsFrCirCountEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Each entry contains statistics describing bandwidth reservation traffic for
    the selected Frame Relay interface and circuit. The table index are: the
    interface number, the Frame Relay circuit number (DLCI) and the class order
    within that circuit in this Frame Relay interface."
    INDEX { telProdNpMonFeatBrsFrCirCountIfc,
            telProdNpMonFeatBrsFrCirCountCir,
            telProdNpMonFeatBrsFrCirCountIndex }
    ::= { telProdNpMonFeatBrsFrCirCountTable 1 }

TelProdNpMonFeatBrsFrCirCountEntry ::=
    SEQUENCE{
        telProdNpMonFeatBrsFrCirCountIfc
            INTEGER,
        telProdNpMonFeatBrsFrCirCountCir
            INTEGER,
        telProdNpMonFeatBrsFrCirCountIndex
            INTEGER,
        telProdNpMonFeatBrsFrCirCountClear
            INTEGER,
        telProdNpMonFeatBrsFrCirCountClass
            DisplayString,
        telProdNpMonFeatBrsFrCirCountTxPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountTxByt
            Counter,
        telProdNpMonFeatBrsFrCirCountDiscByt
            Counter,
        telProdNpMonFeatBrsFrCirCountDiscPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountInPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountInByt
            Counter,
        telProdNpMonFeatBrsFrCirCountDiscRLPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountDiscRLByt
            Counter,
        telProdNpMonFeatBrsFrCirCountRemarkPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountRemarkByt
            Counter,
        telProdNpMonFeatBrsFrCirCountReassPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountReassByt
            Counter,
        telProdNpMonFeatBrsFrCirCountLoopPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountLoopByt
            Counter,
        telProdNpMonFeatBrsFrCirCountQueueOvflPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountQueueOvflByt
            Counter,
        telProdNpMonFeatBrsFrCirCountWredPkt
            Counter,
        telProdNpMonFeatBrsFrCirCountWredByt
            Counter
        }

telProdNpMonFeatBrsFrCirCountIfc OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "First index: interface number."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 1 }

telProdNpMonFeatBrsFrCirCountCir OBJECT-TYPE
    SYNTAX INTEGER(16..1007)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Second index: Frame Relay circuit number (DLCI)."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 2 }

telProdNpMonFeatBrsFrCirCountIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Third index: class order within that circuit in this Frame Relay
    interface."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 3 }

telProdNpMonFeatBrsFrCirCountClear OBJECT-TYPE
    SYNTAX INTEGER{
        delete(1),
        ok(2),
        error(3),
        undefined(4)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
    "Clear command to delete statistics. Enter 1 (delete) to clear
    the counters. In case the counters were correctly deleted, it will
    appear 2 (ok); otherwise it will appear 3 (error). Finally, if the
    counter has been never cleared it will appear 4 (undefined)."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 4 }

telProdNpMonFeatBrsFrCirCountClass OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..9))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Class name."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 5 }

telProdNpMonFeatBrsFrCirCountTxPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of packets transmitted."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 6 }

telProdNpMonFeatBrsFrCirCountTxByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of bytes transmitted."
    ::= { telProdNpMonFeatBrsFrCirCountEntry 7 }

telProdNpMonFeatBrsFrCirCountDiscByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of packets lost due to overflow."
