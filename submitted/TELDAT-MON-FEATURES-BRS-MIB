-- Tabla que contiene la informacin relativa a traffic-shaping para cada clase
-- dentro de cada interfaz configurado en el equipo con Reserva de Ancho de
-- Banda.

telProdNpMonFeatBrsDevTSTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TelProdNpMonFeatBrsDevTSEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "This table contains information relative to traffic-shaping for each class
    and for the whole interface ('Global' row)."
    ::= { telProdNpMonFeatBrs 7 }

telProdNpMonFeatBrsDevTSEntry OBJECT-TYPE
    SYNTAX TelProdNpMonFeatBrsDevTSEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Each entry is comprised of several variables to store information relative
    to traffic-shaping for one class within an interface or for the whole
    interface ('Global' row)."
    INDEX { telProdNpMonFeatBrsDevTSIfc,
            telProdNpMonFeatBrsDevTSClassNum }
    ::= { telProdNpMonFeatBrsDevTSTable 1 }

TelProdNpMonFeatBrsDevTSEntry ::=
    SEQUENCE{
        telProdNpMonFeatBrsDevTSIfc
            INTEGER,
        telProdNpMonFeatBrsDevTSClassNum
            INTEGER,
        telProdNpMonFeatBrsDevTSClear
            INTEGER,
        telProdNpMonFeatBrsDevTSClassName
            DisplayString,
        telProdNpMonFeatBrsDevTSCir
            INTEGER,
        telProdNpMonFeatBrsDevTSBurst
            INTEGER,
        telProdNpMonFeatBrsDevTSState
            INTEGER,
        telProdNpMonFeatBrsDevTSQueued
            INTEGER,
        telProdNpMonFeatBrsDevTSLastTx
            INTEGER,
        telProdNpMonFeatBrsDevTSThroughput
            INTEGER
        }

telProdNpMonFeatBrsDevTSIfc OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "First index: interface number."
    ::= { telProdNpMonFeatBrsDevTSEntry 1 }

telProdNpMonFeatBrsDevTSClassNum OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Second index: class order within the interface."
    ::= { telProdNpMonFeatBrsDevTSEntry 2 }

telProdNpMonFeatBrsDevTSClear OBJECT-TYPE
    SYNTAX INTEGER{
        delete(1),
        ok(2),
        error(3),
        undefined(4)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
    "This variable is used to make a clear operation: when it is set to 1
    (delete), a clear command is executed, starting the measure of the
    throughput in this moment. The throughput measuring will only take into
    account those packets which are transmitted after this instant.
    In reading operations (GET) this variable will show the result of last clear
    operation attempt: the value is 2 (ok) if clear operation was performed
    succesfully, 3 (error) if an error ocurred and 4 (undefined) if no clear
    operations have been made yet."
    ::= { telProdNpMonFeatBrsDevTSEntry 3 }

telProdNpMonFeatBrsDevTSClassName OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..9))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Class name."
    ::= { telProdNpMonFeatBrsDevTSEntry 4 }

telProdNpMonFeatBrsDevTSCir OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Maximum average permitted throughput (0 indicates there is no limit)."
    ::= { telProdNpMonFeatBrsDevTSEntry 5 }

telProdNpMonFeatBrsDevTSBurst OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Currently used burst size (in bits)."
    ::= { telProdNpMonFeatBrsDevTSEntry 6 }

telProdNpMonFeatBrsDevTSState OBJECT-TYPE
    SYNTAX INTEGER{
        congested(1),
        bursting(2),
        ready(3)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "There are three possible states:  (1) congested,  (2) bursting, (3) ready.
    Transmission cannot be executed when a class or an interface is congested;
    transmission is possible in any other state."
    ::= { telProdNpMonFeatBrsDevTSEntry 7 }

telProdNpMonFeatBrsDevTSQueued OBJECT-TYPE
    SYNTAX INTEGER{
        yes(1),
        no(2)
        }
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Indicates if this class is queued: when a class is queued, there is
    traffic to be transmitted."
    ::= { telProdNpMonFeatBrsDevTSEntry 8 }

telProdNpMonFeatBrsDevTSLastTx OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Last instant where a packet for this class was transmitted.
    (This parameter is for Teldat personnel use only)."
    ::= { telProdNpMonFeatBrsDevTSEntry 9 }

telProdNpMonFeatBrsDevTSThroughput OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Average throughput for a class."
    ::= { telProdNpMonFeatBrsDevTSEntry 10 }

-- Tabla que contiene la informacin relativa a traffic-shaping para cada clase
-- dentro de cada uno de los DLCIs de los interfaces Frame Relay configurados
-- en el equipo con Reserva de Ancho de Banda.

telProdNpMonFeatBrsFRCirTSTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TelProdNpMonFeatBrsFRCirTSEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "This table contains information relative to traffic-shaping for each class
    within a Frame Relay circuit (DLCI) and for the whole circuit
    ('Global' row)."
    ::= { telProdNpMonFeatBrs 8 }

telProdNpMonFeatBrsFRCirTSEntry OBJECT-TYPE
    SYNTAX TelProdNpMonFeatBrsFRCirTSEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Each entry is comprised of several variables to store information relative
    to traffic-shaping for one class within a Frame Relay circuit (DLCI) or for
    the whole circuit ('Global' row)."
    INDEX { telProdNpMonFeatBrsFRCirTSIfc,
            telProdNpMonFeatBrsFRCirTSDlci,
            telProdNpMonFeatBrsFRCirTSClassNum }
    ::= { telProdNpMonFeatBrsFRCirTSTable 1 }

TelProdNpMonFeatBrsFRCirTSEntry ::=
    SEQUENCE{
        telProdNpMonFeatBrsFRCirTSIfc
            INTEGER,
        telProdNpMonFeatBrsFRCirTSDlci
            INTEGER,
        telProdNpMonFeatBrsFRCirTSClassNum
            INTEGER,
        telProdNpMonFeatBrsFRCirTSClear
            INTEGER,
        telProdNpMonFeatBrsFRCirTSClassName
            DisplayString,
        telProdNpMonFeatBrsFRCirTSCir
            INTEGER,
        telProdNpMonFeatBrsFRCirTSBurst
            INTEGER,
        telProdNpMonFeatBrsFRCirTSState
            INTEGER,
        telProdNpMonFeatBrsFRCirTSQueued
            INTEGER,
        telProdNpMonFeatBrsFRCirTSLastTx
            INTEGER,
        telProdNpMonFeatBrsFRCirTSThroughput
            INTEGER
        }

telProdNpMonFeatBrsFRCirTSIfc OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "First index: interface number."
    ::= { telProdNpMonFeatBrsFRCirTSEntry 1 }

telProdNpMonFeatBrsFRCirTSDlci OBJECT-TYPE
    SYNTAX INTEGER(16..1007)
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Second index: Frame Relay circuit number (DLCI)."
    ::= { telProdNpMonFeatBrsFRCirTSEntry 2 }

telProdNpMonFeatBrsFRCirTSClassNum OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Third index: class order within the Frame Relay circuit (DLCI)."
    ::= { telProdNpMonFeatBrsFRCirTSEntry 3 }

telProdNpMonFeatBrsFRCirTSClear OBJECT-TYPE
    SYNTAX INTEGER{
        delete(1),
        ok(2),
        error(3),
        undefined(4)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
    "This variable is used to make a clear operation: when it is set to 1
    (delete), a clear command is executed, starting the measure of the
    throughput in this moment. The throughput measuring will only take into
    account those packets which are transmitted after this instant.
    In reading operations (GET) this variable will show the result of last clear
    operation attempt: the value is 2 (ok) if clear operation was performed
    succesfully, 3 (error) if an error ocurred and 4 (undefined) if no clear
    operations have been made yet."
    ::= { telProdNpMonFeatBrsFRCirTSEntry 4 }

telProdNpMonFeatBrsFRCirTSClassName OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..9))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Class name."
    ::= { telProdNpMonFeatBrsFRCirTSEntry 5 }

