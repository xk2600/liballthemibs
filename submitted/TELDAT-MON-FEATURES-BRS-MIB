TELDAT-MON-FEATURES-BRS-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        Counter, Gauge
            FROM RFC1155-SMI
        OBJECT-TYPE
            FROM RFC-1212
        DisplayString
            FROM RFC1213-MIB
        telProdNpMonFeatures
            FROM TELDAT-SW-STRUCTURE-MIB;


-- Facilidad de Reserva de Ancho de Banda (BRS)
telProdNpMonFeatBrs     OBJECT IDENTIFIER ::= { telProdNpMonFeatures 1 }


-- Tabla que recoge los estadçsticos que describen el trﬂfico de Reserva de
-- Ancho de Banda para el interfaz seleccionado.

telProdNpMonFeatBrsDevCountTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TelProdNpMonFeatBrsDevCountEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Displays the last statistics describing bandwidth reservation traffic
    for the selected interface. For the Frame Relay interface displays the last
    statistics describing bandwidth reservation traffic for all circuit classes
    of the interface."
    ::= { telProdNpMonFeatBrs 1 }

telProdNpMonFeatBrsDevCountEntry OBJECT-TYPE
    SYNTAX TelProdNpMonFeatBrsDevCountEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "The table index are: the interface number and the order of the selected
    class within the interface."
    INDEX { telProdNpMonFeatBrsDevCountIfc, telProdNpMonFeatBrsDevCountIndex }
    ::= { telProdNpMonFeatBrsDevCountTable 1 }

TelProdNpMonFeatBrsDevCountEntry ::=
    SEQUENCE{
        telProdNpMonFeatBrsDevCountIfc
            INTEGER,
        telProdNpMonFeatBrsDevCountIndex
            INTEGER,
        telProdNpMonFeatBrsDevCountClear
            INTEGER,
        telProdNpMonFeatBrsDevCountClass
            DisplayString,
        telProdNpMonFeatBrsDevCountTxPkt
            Counter,
        telProdNpMonFeatBrsDevCountTxByt
            Counter,
        telProdNpMonFeatBrsDevCountDiscByt
            Counter,
        telProdNpMonFeatBrsDevCountDiscPkt
            Counter,
        telProdNpMonFeatBrsDevCountInPkt
            Counter,
        telProdNpMonFeatBrsDevCountInByt
            Counter,
        telProdNpMonFeatBrsDevCountDiscRLPkt
            Counter,
        telProdNpMonFeatBrsDevCountDiscRLByt
            Counter,
        telProdNpMonFeatBrsDevCountRemarkPkt
            Counter,
        telProdNpMonFeatBrsDevCountRemarkByt
            Counter,
        telProdNpMonFeatBrsDevCountReassPkt
            Counter,
        telProdNpMonFeatBrsDevCountReassByt
            Counter,
        telProdNpMonFeatBrsDevCountLoopPkt
            Counter,
        telProdNpMonFeatBrsDevCountLoopByt
            Counter,
        telProdNpMonFeatBrsDevCountQueueOvflPkt
            Counter,
        telProdNpMonFeatBrsDevCountQueueOvflByt
            Counter,
    telProdNpMonFeatBrsDevCountWredPkt
        Counter,
    telProdNpMonFeatBrsDevCountWredByt
        Counter
        }

telProdNpMonFeatBrsDevCountIfc OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "First index: interface number."
    ::= { telProdNpMonFeatBrsDevCountEntry 1 }

telProdNpMonFeatBrsDevCountIndex OBJECT-TYPE
    SYNTAX INTEGER
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Second index: class order within the interface."
    ::= { telProdNpMonFeatBrsDevCountEntry 2 }

telProdNpMonFeatBrsDevCountClear OBJECT-TYPE
    SYNTAX INTEGER{
        delete(1),
        ok(2),
        error(3),
        undefined(4)
        }
    ACCESS read-write
    STATUS mandatory
    DESCRIPTION
    "Clear command to delete statistics. Enter 1 (delete) to clear
    the counters. In case the counters were correctly deleted, it will
    appear 2 (ok); otherwise it will appear 3 (error). Finally, if the
    counter has been never cleared it will appear 4 (undefined)."
    ::= { telProdNpMonFeatBrsDevCountEntry 3 }

telProdNpMonFeatBrsDevCountClass OBJECT-TYPE
    SYNTAX DisplayString(SIZE(0..9))
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Class name."
    ::= { telProdNpMonFeatBrsDevCountEntry 4 }

telProdNpMonFeatBrsDevCountTxPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of packets transmitted."
    ::= { telProdNpMonFeatBrsDevCountEntry 5 }

telProdNpMonFeatBrsDevCountTxByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of bytes transmitted."
    ::= { telProdNpMonFeatBrsDevCountEntry 6 }

telProdNpMonFeatBrsDevCountDiscByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of bytes lost due to overflow."
    ::= { telProdNpMonFeatBrsDevCountEntry 7 }

telProdNpMonFeatBrsDevCountDiscPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded packets."
    ::= { telProdNpMonFeatBrsDevCountEntry 8 }

telProdNpMonFeatBrsDevCountInPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of input packets: packets offered to a class before the application
    of any QoS action."
    ::= { telProdNpMonFeatBrsDevCountEntry 9 }

telProdNpMonFeatBrsDevCountInByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of input bytes: bytes offered to a class before the application
    of any QoS action."
    ::= { telProdNpMonFeatBrsDevCountEntry 10 }

telProdNpMonFeatBrsDevCountDiscRLPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded packets due to rate-limit."
    ::= { telProdNpMonFeatBrsDevCountEntry 11 }

telProdNpMonFeatBrsDevCountDiscRLByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded bytes due to rate-limit."
    ::= { telProdNpMonFeatBrsDevCountEntry 12 }

telProdNpMonFeatBrsDevCountRemarkPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of exceeded packets which are dscp-marked."
    ::= { telProdNpMonFeatBrsDevCountEntry 13 }

telProdNpMonFeatBrsDevCountRemarkByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of exceeded bytes which are dscp-marked."
    ::= { telProdNpMonFeatBrsDevCountEntry 14 }

telProdNpMonFeatBrsDevCountReassPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of exceeded packets which are re-classified (reassigned to another
    BRS class)."
    ::= { telProdNpMonFeatBrsDevCountEntry 15 }

telProdNpMonFeatBrsDevCountReassByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of exceeded bytes which are re-classified (reassigned to another
    BRS class)."
    ::= { telProdNpMonFeatBrsDevCountEntry 16 }

telProdNpMonFeatBrsDevCountLoopPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded packets because they were re-classified and reassigned
    to a class to which they were previously assigned."
    ::= { telProdNpMonFeatBrsDevCountEntry 17 }

telProdNpMonFeatBrsDevCountLoopByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded bytes because they were re-classified and reassigned
    to a class to which they were previously assigned."
    ::= { telProdNpMonFeatBrsDevCountEntry 18 }

telProdNpMonFeatBrsDevCountQueueOvflPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded packets due to queue overflow."
    ::= { telProdNpMonFeatBrsDevCountEntry 19 }

telProdNpMonFeatBrsDevCountQueueOvflByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded bytes due to queue overflow."
    ::= { telProdNpMonFeatBrsDevCountEntry 20 }

telProdNpMonFeatBrsDevCountWredPkt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded packets due to WRED."
    ::= { telProdNpMonFeatBrsDevCountEntry 21 }

telProdNpMonFeatBrsDevCountWredByt OBJECT-TYPE
    SYNTAX Counter
    ACCESS read-only
    STATUS mandatory
    DESCRIPTION
    "Number of discarded bytes due to WRED."
    ::= { telProdNpMonFeatBrsDevCountEntry 22 }

-- Tabla que muestra las îltimas estadçsticas de Reserva de Ancho de Banda
-- salvadas de los interfaces.

telProdNpMonFeatBrsDevLastTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TelProdNpMonFeatBrsDevLastEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "Displays the last statistics of bandwidth reservation saved for
    the interfaces."
    ::= { telProdNpMonFeatBrs 2 }

telProdNpMonFeatBrsDevLastEntry OBJECT-TYPE
    SYNTAX TelProdNpMonFeatBrsDevLastEntry
    ACCESS not-accessible
    STATUS mandatory
    DESCRIPTION
    "The table index are: the interface number and the order of the selected
    class within the interface."
    INDEX { telProdNpMonFeatBrsDevLastIfc, telProdNpMonFeatBrsDevLastIndex }
    ::= { telProdNpMonFeatBrsDevLastTable 1 }

TelProdNpMonFeatBrsDevLastEntry ::=
    SEQUENCE{
        telProdNpMonFeatBrsDevLastIfc
            INTEGER,
        telProdNpMonFeatBrsDevLastIndex
            INTEGER,
        telProdNpMonFeatBrsDevLastClass
            DisplayString,
        telProdNpMonFeatBrsDevLastTxPkt
            Counter,
        telProdNpMonFeatBrsDevLastTxByt
            Counter,
        telProdNpMonFeatBrsDevLastDiscByt
            Counter,
        telProdNpMonFeatBrsDevLastDiscPkt
            Counter,
        telProdNpMonFeatBrsDevLastInPkt
            Counter,
        telProdNpMonFeatBrsDevLastInByt
            Counter,
        telProdNpMonFeatBrsDevLastDiscRLPkt
            Counter,
        telProdNpMonFeatBrsDevLastDiscRLByt
            Counter,
        telProdNpMonFeatBrsDevLastRemarkPkt
            Counter,
        telProdNpMonFeatBrsDevLastRemarkByt
            Counter,
        telProdNpMonFeatBrsDevLastReassPkt
            Counter,
        telProdNpMonFeatBrsDevLastReassByt
            Counter,
        telProdNpMonFeatBrsDevLastLoopPkt
            Counter,
        telProdNpMonFeatBrsDevLastLoopByt
            Counter,
        telProdNpMonFeatBrsDevLastQueueOvflPkt
            Counter,
        telProdNpMonFeatBrsDevLastQueueOvflByt
            Counter,
        telProdNpMonFeatBrsDevLastWredPkt
            Counter,
        telProdNpMonFeatBrsDevLastWredByt
            Counter
        }

telProdNpMonFeatBrsDevLastIfc OBJECT-TYPE
