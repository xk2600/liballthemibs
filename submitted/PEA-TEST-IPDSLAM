--************************************************************************  
-- LOOP DIAGNOSTICS QLN TABLE
--************************************************************************  
loopDiagQlnTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LoopDiagQlnEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the per subcarrier Qln
     results of the ADSL2 Loop Diagnostics. QLN
     is calculated from the given values as follows:
     QLN[i] = -23 - (n[i]/2)(dBm/Hz), where n designates
     the QlnNE and QlnFE respectively.
     Compliant to G.997.1:
     7.5.1.19.2 (QLNpsds) and
     7.5.1.19.4 (QLNpsus)"
    ::= { peaTestIpDslamObjects  27 }   
    
loopDiagQlnEntry OBJECT-TYPE
    SYNTAX      LoopDiagQlnEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains the per subcarrier Qln results
     of the ADSL2 Loop Diagnostics for the subscriber
     line identified by lineIndex.
     NOTE: The partId specifies the section of the far-end (US)
     QLN table to return. The QLN values returned correspond
     to tones partId * 256 to (partId * 256 + 255). PartId range
     is [1..0]. Low designates the first 128 tones and High is
     the upper 128 tones for that particular partId."
    INDEX  {lineIndex }
    ::= { loopDiagQlnTable 1 }

LoopDiagQlnEntry ::= 
    SEQUENCE {
        loopDiagQlnNE                   OCTET STRING,
        loopDiagQlnFEPart0Low           OCTET STRING,
        loopDiagQlnFEPart0High          OCTET STRING,
        loopDiagQlnFEPart1Low           OCTET STRING,
        loopDiagQlnFEPart1High          OCTET STRING
        }  

loopDiagQlnNE OBJECT-TYPE
    SYNTAX      OCTET STRING  (SIZE (64))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The near-end quiet line noise PSD. Range is [255..0].
     The value 255 indicates no measurement available."
    ::= { loopDiagQlnEntry 1 } 
    
loopDiagQlnFEPart0Low OBJECT-TYPE
    SYNTAX      OCTET STRING  (SIZE (128))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The far-end quiet line noise PSD. Range is [255..0].
     The value 255 indicates no measurement available."
    ::= { loopDiagQlnEntry 2 } 
    
loopDiagQlnFEPart0High OBJECT-TYPE
    SYNTAX      OCTET STRING  (SIZE (128))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The far-end quiet line noise PSD. Range is [255..0].
     The value 255 indicates no measurement available."
    ::= { loopDiagQlnEntry 3 } 
    
