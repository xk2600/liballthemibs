--seltNoiseInputFe OBJECT-TYPE
--  SYNTAX      OCTET STRING (SIZE (512))
--    UNITS     "Tenth dBm/Hz"
--  MAX-ACCESS  read-write
--    STATUS      current
--    DESCRIPTION
--    "Pre-defined noise vector for Far-end (ATU-R) downstream band."
--    ::= { seltNoiseInputEntry 2 }    

--************************************************************************  
-- SELT TEST BASIC OUTPUT TABLE
--************************************************************************  
seltTestBasicOutputTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SeltTestBasicOutputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the basic output parameters
     for the SELT test."
    ::= { peaTestIpDslamObjects  3 }   
    
seltTestBasicOutputEntry OBJECT-TYPE
    SYNTAX      SeltTestBasicOutputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains the basic output parameters
     for the SELT test for the subscriber line 
     identified by lineIndex."
    INDEX  {lineIndex }
    ::= { seltTestBasicOutputTable 1 }

SeltTestBasicOutputEntry ::= 
    SEQUENCE {
        seltLength                      Unsigned32,
        seltSingleAttVal                Unsigned32,
        seltAdslDeployment              INTEGER,
        seltTermination                 INTEGER,
        seltCapacityUs                  Unsigned32,
        seltCapacityDs                  Unsigned32,
        --seltBridgetap
        --seltLoadedCoil
        --seltImpedenceAGnd
        --seltImpedanceBGnd
        --seltImpedanceAB
        --seltWaterIng
        seltBasicReliability            INTEGER,  
        seltUSMeanNoiseLevel            Integer32,
        seltDSMeanNoiseLevel            Integer32,
        seltAttenLimit                  Unsigned32 
        }  

seltLength OBJECT-TYPE
    SYNTAX      Unsigned32 (0..65535)
    UNITS       "meter"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Loop length to termination."
    ::= { seltTestBasicOutputEntry 1 } 

seltSingleAttVal OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1400)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Attenuation at 300kHz."
    ::= { seltTestBasicOutputEntry 2 } 
    
seltAdslDeployment OBJECT-TYPE
    SYNTAX      INTEGER {possible (0), uncertain(1), notPossible(2)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Parameter indicating the possibility for ADSL deployment. 
     A value of Possible(0) indicates that the tested loop is 
     possible for ADSL Deployment, Uncertain(1) indicates that 
     it might be possible but some uncertainties exist. A value 
     of NotPossible(2) indicates that ADSL deployment is not 
     possible on the tested loop. Decision is based on parameters 
     calculated in the selt algorithm."
    ::= { seltTestBasicOutputEntry 3 }   

seltCapacityUs OBJECT-TYPE
    SYNTAX      Unsigned32 
    UNITS       "bit/sec"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Total loop capacity in the upstream direction."
    ::= { seltTestBasicOutputEntry 4 } 
  
seltCapacityDs OBJECT-TYPE
    SYNTAX      Unsigned32 
    UNITS       "bit/sec"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Total loop capacity in the downstream direction."
    ::= { seltTestBasicOutputEntry 5 } 
 
seltTermination OBJECT-TYPE
    SYNTAX      INTEGER {open (0), short(1), matched(2), cpePowerOnAnnexA(3),
                         cpePowerOnAnnexB(4), cpePowerOnAnnexC(5), unknown(255)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Loop termination at customer side.
     The values between 6 and 254 are to be defined."
    ::= { seltTestBasicOutputEntry 6 }  
            
seltBasicReliability OBJECT-TYPE
    SYNTAX      INTEGER {reliable (0), modReliable(1), notRealiable(2), 
                         newMeasReq(3), unsupportedHw(4), shortLineReliable(5), 
                         cpeDetected(6)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Reliability of the parameters in the seltTestBasicOutputTable.
    The values between 7 and 255 are to be defined."
    ::= { seltTestBasicOutputEntry 7 }     

seltUSMeanNoiseLevel OBJECT-TYPE
    SYNTAX      Integer32 (-1600..0) 
    UNITS       "tenth dBm/Hz"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The mean US noise level present on the line seen from IPDSLAM."
    ::= { seltTestBasicOutputEntry 8 }   
  
seltDSMeanNoiseLevel OBJECT-TYPE
    SYNTAX      Integer32 (-1600..0) 
    UNITS       "tenth dBm/Hz"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The mean DS noise level present on the line seen from IPDSLAM."
    ::= { seltTestBasicOutputEntry 9 } 

seltAttenLimit OBJECT-TYPE
    SYNTAX      Unsigned32 (0..100)
    UNITS       "dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Reported maximum measurable/reliable attenuation @ 300 kHz."
    ::= { seltTestBasicOutputEntry 10 }  

--************************************************************************  
-- SELT TEST ADVANCED OUTPUT TABLE
--************************************************************************  
seltTestAdvOutputTable OBJECT-TYPE
  SYNTAX      SEQUENCE OF SeltTestAdvOutputEntry
  MAX-ACCESS  not-accessible
  STATUS      current
  DESCRIPTION
  "This table provides the advanced output parameters
   for the SELT test."
  ::= { peaTestIpDslamObjects  4 }   
    
seltTestAdvOutputEntry OBJECT-TYPE
  SYNTAX      SeltTestAdvOutputEntry
  MAX-ACCESS  not-accessible
  STATUS      current
  DESCRIPTION
  "An entry contains the advanced output parameters
   for the SELT test for the subscriber line 
   identified by lineIndex."
  INDEX  {lineIndex }
  ::= { seltTestAdvOutputTable 1 }

SeltTestAdvOutputEntry ::= 
    SEQUENCE {
        --seltAdvReliability
        --seltDcNoiseActFlag
        seltEcho0                             OCTET STRING,
        seltEcho1                             OCTET STRING,
        seltEcho2                             OCTET STRING,
        seltEcho3                             OCTET STRING,
        seltInsertionLoss0                    OCTET STRING,
        seltInsertionLoss1                    OCTET STRING,
        seltInsertionLoss2                    OCTET STRING,
        seltInsertionLoss3                    OCTET STRING,
        seltNoiseNe0                          OCTET STRING,
        seltNoiseNe1                          OCTET STRING,
        seltNoiseNe2                          OCTET STRING,
        seltNoiseNe3                          OCTET STRING,
        seltNoiseFe0                          OCTET STRING,
        seltNoiseFe1                          OCTET STRING,
        seltNoiseFe2                          OCTET STRING,
        seltNoiseFe3                          OCTET STRING,
        seltSnrNe0                            OCTET STRING,
        seltSnrNe1                            OCTET STRING,
        seltSnrNe2                            OCTET STRING,
        seltSnrNe3                            OCTET STRING,
        seltSnrFe0                            OCTET STRING,
        seltSnrFe1                            OCTET STRING,
        seltSnrFe2                            OCTET STRING,
        seltSnrFe3                            OCTET STRING,
        seltBitLoadingEstNe0                  OCTET STRING,
        seltBitLoadingEstNe1                  OCTET STRING,
        seltBitLoadingEstFe0                  OCTET STRING,
        seltBitLoadingEstFe1                  OCTET STRING,
        seltInsertionLoss4                    OCTET STRING,
        seltInsertionLoss5                    OCTET STRING,
        seltInsertionLoss6                    OCTET STRING,
        seltInsertionLoss7                    OCTET STRING,
        seltNoiseFe4                          OCTET STRING,
        seltNoiseFe5                          OCTET STRING,
        seltNoiseFe6                          OCTET STRING,
        seltNoiseFe7                          OCTET STRING,
        seltSnrFe4                            OCTET STRING,
        seltSnrFe5                            OCTET STRING,
        seltSnrFe6                            OCTET STRING,
        seltSnrFe7                            OCTET STRING,
        seltBitLoadingEstFe2                  OCTET STRING,
        seltBitLoadingEstFe3                  OCTET STRING,
        seltTDR0                              OCTET STRING,
        seltTDR1                              OCTET STRING,
        seltTDR2                              OCTET STRING,
        seltTDR3                              OCTET STRING,
        seltTDR4                              OCTET STRING,
        seltTDR5                              OCTET STRING,
        seltTDR6                              OCTET STRING,
        seltTDR7                              OCTET STRING,
        seltInputImpedance0                   OCTET STRING,
        seltInputImpedance1                   OCTET STRING,
        seltInputImpedance2                   OCTET STRING,
        seltInputImpedance3                   OCTET STRING
        --seltDcNoiseNe
        --seltDcNoiseFe
        }  
 
seltEcho0 OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (128))
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Magnitude of the Echo Frequency Response of the line. Part 0.
     Consecutive values are spaced 4.3125 kHz in frequency.
     If seltAdvOutputTableAct is deactivated the values should be 
     ignored."
    ::= { seltTestAdvOutputEntry 1 }  
    
seltEcho1 OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (128))
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Magnitude of the Echo Frequency Response of the line. Part 1.
     Consecutive values are spaced 4.3125 kHz in frequency.
     If seltAdvOutputTableAct is deactivated the values should be 
     ignored."
    ::= { seltTestAdvOutputEntry 2 } 

seltEcho2 OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (128))
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Magnitude of the Echo Frequency Response of the line. Part 2.
     Consecutive values are spaced 4.3125 kHz in frequency.
     If seltAdvOutputTableAct is deactivated the values should be 
     ignored."
    ::= { seltTestAdvOutputEntry 3 } 

seltEcho3 OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (128))
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Magnitude of the Echo Frequency Response of the line. Part 3.
     Consecutive values are spaced 4.3125 kHz in frequency.
     If seltAdvOutputTableAct is deactivated the values should be 
     ignored."
    ::= { seltTestAdvOutputEntry 4 } 

seltInsertionLoss0 OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (128))
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Line Insertion loss (attenuation) per tone. Part 0. 
     Consecutive values are spaced 4.3125 kHz in frequency.
     If seltAdvOutputTableAct is deactivated 
     the values should be ignored."
    ::= { seltTestAdvOutputEntry 5 }                               

