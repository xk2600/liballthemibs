			loopDiagHlinFeScalePart4, loopDiagHlinFeScalePart5, loopDiagHlinFeScalePart6, loopDiagHlinFeScalePart7, 
			loopDiagHlinFePart0Low, loopDiagHlinFePart0High, loopDiagHlinFePart1Low, loopDiagHlinFePart1High,   
			loopDiagHlinFePart2Low, loopDiagHlinFePart2High, loopDiagHlinFePart3Low, loopDiagHlinFePart3High,  
			loopDiagHlinFePart4Low, loopDiagHlinFePart4High, loopDiagHlinFePart5Low, loopDiagHlinFePart5High,  
			loopDiagHlinFePart6Low, loopDiagHlinFePart6High, loopDiagHlinFePart7Low, loopDiagHlinFePart7High,
		loopDiagHlogNE,  loopDiagHlogFEPart0Low, loopDiagHlogFEPart0High, loopDiagHlogFEPart1Low,   
			loopDiagHlogFEPart1High, loopDiagHlogFEPart2Low, loopDiagHlogFEPart2High, loopDiagHlogFEPart3Low,   
			loopDiagHlogFEPart3High,
		loopDiagQlnNE, loopDiagQlnFEPart0Low, loopDiagQlnFEPart0High, loopDiagQlnFEPart1Low, loopDiagQlnFEPart1High,  
		loopDiagSnrNE, loopDiagSnrFEPart0Low, loopDiagSnrFEPart0High, loopDiagSnrFEPart1Low, loopDiagSnrFEPart1High
              }
    STATUS      current
    DESCRIPTION
        "Objects for the fssServiceLine Group"
    ::= { peaTestIpDslamObjectsGroups 1 }

