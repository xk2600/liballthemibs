PEA-TEST-IPDSLAM DEFINITIONS ::= BEGIN


IMPORTS
    peaIpDslam
       FROM DIESEL-SMI
    
   lineIndex,
   pvcIndex
     FROM PEA-BRIDGE-IPDSLAM 

      -- standar X.733 header for the traps       
    managedObjectClass, 
    managedObjectInstance, 
    sequenceNumber, 
    perceivedSeverity, 
    eventTime, 
    eventTypePem, 
    probableCause  
       FROM PEM-STANDARD 

       adslIfTestResult, adslIfTestType, adslIfTestError
       FROM PEA-IF-IPDSLAM

    DisplayString  
       FROM SNMPv2-TC

    MODULE-IDENTITY, 
    Integer32,
    Unsigned32, 
    Counter32,
    NOTIFICATION-TYPE,
    OBJECT-TYPE
        FROM SNMPv2-SMI

   OBJECT-GROUP, NOTIFICATION-GROUP
          FROM SNMPv2-CONF
        ;

peaTestIpDslam MODULE-IDENTITY
    LAST-UPDATED 
         "200405031200Z"  
    ORGANIZATION
         "Ericsson Diax A/S - Denmark"
    CONTACT-INFO
         "
         Ericsson Diax A/S
         Faelledvej 17
         DK-7600 Struer
         Denmark
         
         E-mail: diax.support.xu@ericsson.com

         "       
   DESCRIPTION
         "
         In this module are defined the control entries for input 
         and output data for SELT (loop qualification) and NOISE tests.
         "                                                           

    ::= { peaIpDslam  20 }
    
peaTestIpDslamObjects           OBJECT IDENTIFIER ::= { peaTestIpDslam 1 }
peaTestIpDslamTraps             OBJECT IDENTIFIER ::= { peaTestIpDslam 2 }
peaTestIpDslamObjectsGroups     OBJECT IDENTIFIER ::= { peaTestIpDslam 3 }
peaTestIpDslamTrapsGroups       OBJECT IDENTIFIER ::= { peaTestIpDslam 4 }

--************************************************************************  
-- SELT TEST INPUT TABLE
--************************************************************************  
seltTestInputTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SeltTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the input parameters
     for the SELT test."
    ::= { peaTestIpDslamObjects  1 }   

seltTestInputEntry OBJECT-TYPE
    SYNTAX      SeltTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains input parameters for 
     the SELT test for the subscriber line 
     identified by lineIndex."
    INDEX  {lineIndex }
    ::= { seltTestInputTable 1 }

SeltTestInputEntry ::= 
    SEQUENCE {
        seltNoiseNeAct                  INTEGER,
        seltNoiseFeAct                  INTEGER,
        seltAdvOutputTableAct           INTEGER,
        --seltDcNoiseAct
        --seltImpedanceAct
        --seltWaterIngAct
        --seltNoiseDuration
        seltToneProfile                 INTEGER,
        seltVop                         Unsigned32,
        seltUsToneMask                  OCTET STRING,
        seltDsToneMask                  OCTET STRING,
        seltUsTransmitLevel             Integer32,
        seltDsTransmitLevel             Integer32,
        seltExtSplitterType             INTEGER
        }  

seltNoiseNeAct OBJECT-TYPE
    SYNTAX      INTEGER {activated (1), deactivated(0)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Value that indicates if Near-end noise should be 
     measured or pre-selected. If the noise should be 
     measured this should be Activated(1). If the noise 
     should be pre-selected seltNoiseNeAct should be 
     set to Deactivated(0)."
    ::= { seltTestInputEntry 1 }  
    
seltNoiseFeAct OBJECT-TYPE
    SYNTAX      INTEGER {activated (1), deactivated(0)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Value that indicates if Far-end noise should be 
     measured or pre-selected. If the noise should be 
     measured this should be Activated(1). If the noise 
     should be pre-selected seltNoiseFeAct should be 
     set to Deactivated(0)."
    ::= { seltTestInputEntry 2 } 
 
seltAdvOutputTableAct OBJECT-TYPE
    SYNTAX      INTEGER {activated (1), deactivated(0)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Switch that activates presentation of tone 
     information or deactivates it."
    ::= { seltTestInputEntry 3 } 
 
seltToneProfile OBJECT-TYPE
    SYNTAX      INTEGER {
                         ansit1413(0), 
                         etsi(1),
                         g9921PotsNonOverlapped(2),
                         g9921PotsOverlapped(3),
                         g9921IsdnNonOverlapped (4),
                         g9922PotsNonOverlapped (8),
                         g9923AnnexA (13),
                         g9923AnnexB (14),
                         g9923AnnexJ (15),
                         g9923ReAnnexAandL (16), 
                         g9923ReNarrowPsdAnnexAandL (17),
                         g9925AnnexA(18),
                         g9925AnnexB(19),
                         g9925AnnexM(20),
                         sadsl64(128)
                         }      
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Tone profile for different system configurations.
     The unused values for this parameter are reserved for 
     comming standards or future proprietary techniques."
    ::= { seltTestInputEntry 8 }  

seltVop OBJECT-TYPE
    SYNTAX      Unsigned32 (0..65535)
    UNITS       "10 km per sec"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Velocity of propagation is the speed at which an 
     electric signal passes through a cable."
    ::= { seltTestInputEntry 9 } 

seltUsToneMask OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Upstream active tones assumed in capacity estimation. 
     Using single bits in integer set specific tones on /off. 
     Total number of tones is 256."
    ::= { seltTestInputEntry 10 } 

seltDsToneMask OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Downstream  active tones assumed in capacity estimation. 
     Using single bits in integer set specific tones on /off. 
     Total number of tones is 512."
    ::= { seltTestInputEntry 11 }   

seltUsTransmitLevel OBJECT-TYPE
    SYNTAX      Integer32 (-128..127)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Upstream transmit level."
    ::= { seltTestInputEntry 12 } 

seltDsTransmitLevel OBJECT-TYPE
    SYNTAX      Integer32 (-128..127)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Downstream transmit level."
    ::= { seltTestInputEntry 13 } 

seltExtSplitterType OBJECT-TYPE
    SYNTAX      INTEGER {noneOrPOTS (0), edf110i(1), edf324Ncd90127R1a(2), edf324EtsiNcd90130R1a(3), kroneIsdn(4)}      
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "External splitter filter type. 
     This parameter is used to inform the SELT software about 
     any installed external splitter filter. The SELT performance 
     is affected by this parameter. 
     The unused values for this parameter are reserved for 
     coming external splitter types or future proprietary options."
    ::= { seltTestInputEntry 14 }  
                                          
