--************************************************************************
-- ATM PVC TEST INPUT TABLE
--************************************************************************
atmPvcTestInputTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF AtmPvcTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the input parameters
     for the ATM PVC test."
    ::= { peaTestIpDslamObjects 10 }

atmPvcTestInputEntry OBJECT-TYPE
    SYNTAX      AtmPvcTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains input parameters for
     the ATM PVC test for the subscriber line
     identified by lineIndex."
    INDEX  {lineIndex }
    ::= { atmPvcTestInputTable 1 }

AtmPvcTestInputEntry ::=
    SEQUENCE {
        atmF5EndToEndLoopbackInterval     INTEGER,
        atmF5EndToEndLoopbackTimeout      INTEGER,
        atmF5EndToEndLoopbackIntervalFine INTEGER,
        atmF5EndToEndBatchSize            INTEGER,
        atmF5EndToEndBatchCount           INTEGER
        }

atmF5EndToEndLoopbackInterval OBJECT-TYPE
    SYNTAX      INTEGER (0..3600)
    UNITS       "sec"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "A number of 'ATM OAM F5 end-to-end loopback' cell(s) are sent
     with this interval on each PVC created for the subscriber line
     identified by lineIndex. The CPE must return a reply
     within atmF5EndToEndLoopbackTimeout seconds.
     The value zero indicates that this test is disabled.
     The first cell will be send when a non-zero value is written
     into this variable - or when the line is ready.
	 
     If a resolution in msec is needed, use atmF5EndToEndLoopbackIntervalFine.

     Note: In order to restart a batch (if atmF5EndToEndBatchCount != 0) this
     parameter should be set to 0 and then set to the new (or possible) same interval.
    "
    ::= { atmPvcTestInputEntry 1 }

atmF5EndToEndLoopbackTimeout OBJECT-TYPE
    SYNTAX      INTEGER (1..10)
    UNITS       "sec"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Specifies the the maximum amount of time to wait
     for F5 replies for a batch.
     (The batch size is set in atmF5EndToEndBatchSize)
    "
    ::= { atmPvcTestInputEntry 2 }

atmF5EndToEndLoopbackIntervalFine OBJECT-TYPE
    SYNTAX      INTEGER (100..3600000)
    UNITS       "msec"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "A number of 'ATM OAM F5 end-to-end loopback' cell(s) are sent
     with this interval on each PVC created for the subscriber line
     identified by lineIndex. The CPE must return a reply
     within atmF5EndToEndLoopbackTimeout seconds.
     The value zero indicates that this test is disabled.
     The first cell will be send when a non-zero value is written
     into this variable - or when the line is ready.
	 
     This parameter follows atmF5EndToEndLoopbackInterval with a
     finer resolution. 

     Note: In order to restart a batch (if atmF5EndToEndBatchCount != 0) this
     parameter should be set to 0 and then set to the new (or possible) same interval.
    "
    ::= { atmPvcTestInputEntry 3 }

atmF5EndToEndBatchSize OBJECT-TYPE
    SYNTAX      INTEGER (1..20)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Specifies how many F5 cells that is be sent in an
     interval given by atmF5EndToEndLoopbackIntervalFine and
     atmF5EndToEndLoopbackInterval.
    "
    ::= { atmPvcTestInputEntry 5 }

atmF5EndToEndBatchCount OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Specifies how many batches that should be run before automatically
     stopping the test.
     Eg. A BatchCount of 4 and an interval of 3 seconds would run for 12
     seconds and send a total of 20 F5 cells if BatchSize equals 5.
	 
     Note: A BatchCount of 0 indicates that the test should run until it
     is stopped by setting interval to 0.
    "
    ::= { atmPvcTestInputEntry 6 }

--************************************************************************
-- ATM PVC TEST OUTPUT TABLE
--************************************************************************
atmPvcTestOutputTables		OBJECT IDENTIFIER ::= { peaTestIpDslamObjects 12 }

atmPvcTestOutputTableVpiIndex OBJECT-TYPE
	SYNTAX      Unsigned32 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "The vpi index in atmPvcTestOutputTablePvc."
    ::= { atmPvcTestOutputTables 1 }   
    
atmPvcTestOutputTableVciIndex OBJECT-TYPE
	SYNTAX      Unsigned32 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "The vci index in atmPvcTestOutputTablePvc."
    ::= { atmPvcTestOutputTables 2 }   
    
atmPvcTestOutputTableLine OBJECT-TYPE
	SYNTAX      SEQUENCE OF AtmPvcTestOutputEntryLine
  	MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the output parameters
     for the ATM PVC test (per line parameters).
    "
    ::= { atmPvcTestOutputTables 4 }

atmPvcTestOutputEntryLine OBJECT-TYPE
	SYNTAX      AtmPvcTestOutputEntryLine
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains output parameters for
     the ATM PVC test for the subscriber line
     identified by lineIndex.
    "
    INDEX  {lineIndex}
    ::= { atmPvcTestOutputTableLine 1 }     
    
AtmPvcTestOutputEntryLine ::= 
	SEQUENCE {
        atmF5EndToEndBatchesLeft      Counter32
        } 
                
atmF5EndToEndBatchesLeft OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "If batches are used (defined with atmF5EndToEndBatchCount != 0)
     This counter states how many batches are left before all batches
     have been run.
    "
    ::= { atmPvcTestOutputEntryLine 1 }

atmPvcTestOutputTablePvc OBJECT-TYPE
    SYNTAX      SEQUENCE OF AtmPvcTestOutputEntryPvc
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the output parameters
     for the ATM PVC test (per pvc parameters).
    "
    ::= { atmPvcTestOutputTables 6 }

atmPvcTestOutputEntryPvc OBJECT-TYPE
    SYNTAX      AtmPvcTestOutputEntryPvc
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains output parameters for the ATM PVC
    test for the subscriber PVC identified by lineIndex,
    atmPvcTestOutputTableVpiIndex, and atmPvcTestOutputTableVciIndex.
    "
    INDEX  {lineIndex, atmPvcTestOutputTableVpiIndex, atmPvcTestOutputTableVciIndex }
    ::= { atmPvcTestOutputTablePvc 1 }

AtmPvcTestOutputEntryPvc ::=
    SEQUENCE {
        atmF5EndToEndTransmitted   Counter32,
        atmF5EndToEndReceived      Counter32
        }

atmF5EndToEndTransmitted OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of F5 cells transmitted on the PVC since the
     test was started with atmF5EndToEndLoopbackIntervalFine or
     atmF5EndToEndLoopbackInterval.
    "
    ::= { atmPvcTestOutputEntryPvc 1 }

atmF5EndToEndReceived OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number of F5 cells received on the PVC since the test was
     started with atmF5EndToEndLoopbackIntervalFine or
     atmF5EndToEndLoopbackInterval.

     Note: F5 cells from a previous batch that are received
     are droped and are not counted here. (ie. very late answers
     arriving later than atmF5EndToEndLoopbackInterval(Fine) or
     atmF5EndToEndLoopbackTimeout - whichever are smallest - after
     transmission of the requests)
    "
    ::= { atmPvcTestOutputEntryPvc 2 }

--************************************************************************
-- PPPoE TEST INPUT TABLE
--************************************************************************
pppoeTestInputTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF PppoeTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the input parameters for the 'PPP-over-Ethernet' test.
     Note: This OID has to be specified in 'adslIfTestType' in order to activate the test.
     The test is a PADI packet broadcasted on the Ethernet (through the Bridge/Filter for this line).
     The test succeds when at least one 'Access Concentrator' server replies with a PADO packet.
     See description of result in pppoeTestOutputTable.
     The returned AC-Name & Service-Names from each PADO packet will be stored in pppoeTestOutputTable.
     Abbreviations:
     - PADI: PPPoE Active Discovery Initiation
     - PADO: PPPoE Active Discovery Offer
     - AC-Name: Access Concentrator Name
     See also: 'peaIfIpDslam.adslIffObjects.adslIfTestTable.adslIfTestEntry.adslIfTestType'"
    ::= { peaTestIpDslamObjects 20 }

pppoeTestInputEntry OBJECT-TYPE
    SYNTAX      PppoeTestInputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains input parameters for
     the 'PPP-over-Ethernet' test for the subscriber line
     identified by lineIndex.
     See also summary in pppoeTestInputTable."
    INDEX  {lineIndex }
    ::= { pppoeTestInputTable 1 }

PppoeTestInputEntry ::=
    SEQUENCE {
        pppoeServiceRequested           DisplayString,
        pppoeVlanId                     INTEGER,
        pppoeTimeout                    INTEGER,
        pppoeInnerVlanId                INTEGER
        }

pppoeServiceRequested OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The ServiceName used in the PADI packets (PPPoE Active Discovery Initiation)
     during PPPoE tests (default empty)."
    ::= { pppoeTestInputEntry 1 }

pppoeVlanId OBJECT-TYPE
    SYNTAX      INTEGER (0..4095)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This is the VLANID used for the pppoeTest
     (accordingly to 802.1Q). The VLANID must
     have been configured for this line in the pvcTable.
     See also 'peaBridgeIpDslam.pvcTable.pvcEntry.pvcStatus.vlanID'"
    ::= { pppoeTestInputEntry 2 }

pppoeTimeout OBJECT-TYPE
    SYNTAX      INTEGER (1..10)
    UNITS       "sec"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Specifies the the maximum amount of time to wait
    from the PADI packet is broadcasted until all replies (PADI packets) must have arrived."
    ::= { pppoeTestInputEntry 3 }

pppoeInnerVlanId OBJECT-TYPE
    SYNTAX          INTEGER (0..4095)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "This is the InnerVlanId used for the pppoeTest
     (accordingly to 802.1Q). This object is valid only if
     vlanTagType value is QinQ (Double Vlan). The innerVlanId must
     have been configured for this line in the pvcTable.
     See also 'peaBridgeIpDslam.pvcTable.pvcEntry.pvcStatus.innerVlanId'"
    ::= { pppoeTestInputEntry 4 }
    

--************************************************************************
-- PPPoE TEST OUTPUT TABLE
--************************************************************************
pppoeTestOutputTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF PppoeTestOutputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the **additional** output parameters
     for the 'PPP-over-Ethernet' test.
     The test result can be read in the generic 'adslIfTestResult' variable,
     which can have the following values:
     - InProgress
     - UnableToRun, details in adslIfTestError:
     +- lanError: The specified VLAN-id is not used by any ATM PVCs that belongs to specified line (or line down).
     +- configurationOrCustomisationError: Bridge does now allow PPPoE frames for the PVC with specified VLAN-id.
     +- softwareError: Internal error.
     - TestFailed, details in adslIfTestError:
     +- timeoutExpired: No PADO reply received
     See also: 'peaIfIpDslam.adslIfObjects.adslIfTestTable'"
    ::= { peaTestIpDslamObjects 21 }

pppoeTestOutputEntry OBJECT-TYPE
    SYNTAX      PppoeTestOutputEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains **additional** output parameters for
     the 'PPP-over-Ethernet' test for the subscriber line
     identified by lineIndex. Zero or more entries may exists for each line."
    INDEX  {lineIndex, resultIndex }
    ::= { pppoeTestOutputTable 1 }

PppoeTestOutputEntry ::=
    SEQUENCE {
        resultIndex             Unsigned32,
        pppoeAccessConcentrator DisplayString,
        pppoeServiceOffered     DisplayString 
        }

pppoeAccessConcentrator OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The name of the Access Concentrator reported in a PADO packet."
    ::= { pppoeTestOutputEntry 1 }

pppoeServiceOffered OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The name of a Service reported in a PADO packet from the Access Concentrator."
    ::= { pppoeTestOutputEntry 2 }  
    
resultIndex OBJECT-TYPE
    SYNTAX      Unsigned32 
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "Indicates the entry number for a line."
    ::= { pppoeTestOutputEntry 3 }                    

 -- resultIndex is not included in the pppoeTestOutputEntry table. Thats why SMIC giving error. 
 -- { peaTestIpDslamObjects 22 }  

--************************************************************************  
-- LOOP DIAGNOSTICS RESULT TABLE
--************************************************************************  
loopDiagResultTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LoopDiagResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the per line results of
     the ADSL2 Loop Diagnostics."
    ::= { peaTestIpDslamObjects  23 }   
    
loopDiagResultEntry OBJECT-TYPE
    SYNTAX      LoopDiagResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains the per line results of the
     ADSL2 Loop Diagnostics for the subscriber line
     identified by lineIndex."
    INDEX  {lineIndex }
    ::= { loopDiagResultTable 1 }

LoopDiagResultEntry ::= 
    SEQUENCE {
        loopDiagStatus                  INTEGER,
        loopDiagAttainableBitRateNE     Unsigned32,
        loopDiagLoopAttenuationNE       Unsigned32,
        loopDiagSignalAttenuationNE     Unsigned32,
        loopDiagSnrMarginNE             Integer32,
        loopDiagActualTxPowerFarEndNE   Integer32,
        loopDiagAttainableBitRateFE     Unsigned32,
        loopDiagLoopAttenuationFE       Unsigned32,
        loopDiagSignalAttenuationFE     Unsigned32,
        loopDiagSnrMarginFE             Integer32,
        loopDiagActualTxPowerFarEndFE   Integer32
        }  

loopDiagStatus OBJECT-TYPE
    SYNTAX      INTEGER    {loopDiagNotStarted  (0),
                            loopDiagInProgress  (1),
                            loopDiagMeasOk      (2),
                            loopDiagSuccess     (3),
                            loopDiagFailed      (4)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Status of current Loop Diagnostics.
     NOTE that Loop Diagnostics data are only
     valid in case of status LoopDiagSuccess."
    ::= { loopDiagResultEntry 1 } 

loopDiagAttainableBitRateNE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..32768000)
    UNITS       "bps"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Near-end attainable bit rate in bit per second (bps).
    Compliant to G.997.1: 7.5.1.13 (ATTNDRus)."
    ::= { loopDiagResultEntry 2 } 
    
loopDiagLoopAttenuationNE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1023)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Near-end loop attenuation. Range is [1023..0],
     unit is 0.1 dB. LoopAttenuation = 1023 is a
     special value indicating that the loop attenuation
     is outside the represented range.
     Compliant to G.997.1: 7.5.1.7 (LATNus)."
    ::= { loopDiagResultEntry 3 }
    
loopDiagSignalAttenuationNE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1023)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Near-end signal attenuation. Range is [1023..0],
     unit is 0.1 dB. SignalAttenuation = 1023 is a
     special value indicating that the attenuation
     is outside the represented range.
     Compliant to G.997.1: 7.5.1.9 (SATNus)."
    ::= { loopDiagResultEntry 4 } 
    
loopDiagSnrMarginNE OBJECT-TYPE
    SYNTAX      Integer32 (-512..511)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Near-end signal-to-noise ratio margin. Range
     is [511..-512], unit is 0.1 dB. snrMargin = -512
     is a special value indicating that the signal-to-noise
     ratio margin is outside the represented range.
     Compliant to G.997.1: 7.5.1.11 (SNRMus)."
    ::= { loopDiagResultEntry 5 } 
    
loopDiagActualTxPowerFarEndNE OBJECT-TYPE
    SYNTAX      Integer32 (-512..511)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The far-end actual aggregate transmit power as
     determined by the near-end receive PMD function.
     Range is [511..-512], unit is 0.1 dBm. actualTxPowerFE =
     -512 is a special value indicating that the actual
     aggregate transmit power is outside the represented range.
     Compliant to G.997.1: 7.5.1.17 (ACTATPus)."
    ::= { loopDiagResultEntry 6 } 
    
loopDiagAttainableBitRateFE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..32768000)
    UNITS       "bps"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Far-end attainable bit rate in bit per second (bps).
    Compliant to G.997.1: 7.5.1.12 (ATTNDRds)."
    ::= { loopDiagResultEntry 7 } 
    
loopDiagLoopAttenuationFE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1023)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Far-end loop attenuation. Range is [1023..0],
     unit is 0.1 dB. LoopAttenuation = 1023 is a
     special value indicating that the attenuation
     is outside the represented range.
     Compliant to G.997.1: 7.5.1.6 (LATNds)."
    ::= { loopDiagResultEntry 8 }
    
loopDiagSignalAttenuationFE OBJECT-TYPE
    SYNTAX      Unsigned32 (0..1023)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Far-end signal attenuation. Range is [1023..0],
     unit is 0.1 dB. SignalAttenuation = 1023
     is a special value indicating that the attenuation
     is outside the represented range.
     Compliant to G.997.1: 7.5.1.8 (SATNds)."
    ::= { loopDiagResultEntry 9 } 
    
loopDiagSnrMarginFE OBJECT-TYPE
    SYNTAX      Integer32 (-512..511)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Far-end signal-to-noise ratio margin. Range is
     [511..-512], unit is 0.1 dB. SnrMargin = -512
     is a special value indicating that the signal-to-noise
     ratio margin is outside the represented range.
     Compliant to G.997.1: 7.5.1.10 (SNRMds)."
    ::= { loopDiagResultEntry 10 } 
    
loopDiagActualTxPowerFarEndFE OBJECT-TYPE
    SYNTAX      Integer32 (-512..511)
    UNITS       "tenth dB"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The near-end actual aggregate transmit power as
     determined by the far-end receive PMD function. Range is
     [511..-512], unit is 0.1 dBm. ActualTxPowerFE = -512
     is a special value indicating that the actual aggregate
     transmit power is outside the represented range.
     Compliant to G.997.1: 7.5.1.16 (ACTATPds)."
    ::= { loopDiagResultEntry 11 } 
    
--************************************************************************  
-- LOOP DIAGNOSTICS HLIN NE (US) TABLE
--************************************************************************  
loopDiagHlinNeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LoopDiagHlinNeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the per subcarrier Hlin
     results of the ADSL2 Loop Diagnostics.
     The linear complex channel transfer function values
     for the tone range specified by the partId. 
     Hlin is calculated from the given values as follows:
     Hlin[i] = (HlinScale/215)*(Hlin[i].real + j*Hlin[i].imag)/215)
     Compliant to G.997.1:
     7.5.1.18.5 (HLINSCus) and
     7.5.1.18.6 (HLINpsus)."
    ::= { peaTestIpDslamObjects  24 }   
    
loopDiagHlinNeEntry OBJECT-TYPE
    SYNTAX      LoopDiagHlinNeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains the per subcarrier Hlin results
     of the ADSL2 Loop Diagnostics for the subscriber
     line identified by lineIndex.
     NOTE Low designates the first 32 tones and High is
     the upper 32 tones for that particular partId."
    INDEX  {lineIndex }
    ::= { loopDiagHlinNeTable 1 }

LoopDiagHlinNeEntry ::= 
    SEQUENCE {
        loopDiagHlinNeScale             Unsigned32,
        loopDiagHlinNeLow               OCTET STRING,
        loopDiagHlinNeHigh              OCTET STRING 
        }
        
loopDiagHlinNeScale OBJECT-TYPE
    SYNTAX      Unsigned32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Scale factor for the normalized complex numbers Hlin."
    ::= { loopDiagHlinNeEntry 1 } 

loopDiagHlinNeLow OBJECT-TYPE
    SYNTAX      OCTET STRING  (SIZE (128))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The linear complex channel transfer function values for the
     tone range specified by the partId.
     Bytes n*4 + [0..1]: Real part (int16)
     Bytes n*4 + [2..3]: Imaginary part (int16)
     real[n] = imag[n] = -32768 indicates no measurement."
    ::= { loopDiagHlinNeEntry 2 } 
    
loopDiagHlinNeHigh OBJECT-TYPE
    SYNTAX      OCTET STRING  (SIZE (128))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The linear complex channel transfer function values for the
     tone range specified by the partId.
     Bytes n*4 + [0..1]: Real part (int16)
     Bytes n*4 + [2..3]: Imaginary part (int16)
     real[n] = imag[n] = -32768 indicates no measurement."
    ::= { loopDiagHlinNeEntry 3 } 
    
--************************************************************************  
-- LOOP DIAGNOSTICS HLIN FE TABLE
--************************************************************************  
loopDiagHlinFeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LoopDiagHlinFeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table provides the per subcarrier Hlin
     results of the ADSL2 Loop Diagnostics.
     The linear complex channel transfer function values
     for the tone range specified by the partId. 
     Hlin is calculated from the given values as follows:
     Hlin[i] = (HlinScale/2^15)*(Hlin[i].real + j*Hlin[i].imag)/2^15)
     Compliant to G.997.1:
     7.5.1.18.1 (HLINSCds) and
     7.5.1.18.2 (HLINpsds)."
    ::= { peaTestIpDslamObjects  25 }   
    
loopDiagHlinFeEntry OBJECT-TYPE
    SYNTAX      LoopDiagHlinFeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "An entry contains the per subcarrier Hlin results
     of the ADSL2 Loop Diagnostics for the subscriber
     line identified by lineIndex.
     NOTE: The partId specifies the section of the Hlin
     table to return. The Hlin values returned correspond to
     tones: partId * 64 to (partId * 64 + 63). PartId range is [0..7].
     Low designates the first 32 tones and High is the upper 32
     tones for that particular partId."
    INDEX  {lineIndex }
    ::= { loopDiagHlinFeTable 1 }

LoopDiagHlinFeEntry ::= 
    SEQUENCE {
        loopDiagHlinFeScalePart0        Unsigned32,
        loopDiagHlinFeScalePart1        Unsigned32,
        loopDiagHlinFeScalePart2        Unsigned32,
        loopDiagHlinFeScalePart3        Unsigned32,
        loopDiagHlinFeScalePart4        Unsigned32,
        loopDiagHlinFeScalePart5        Unsigned32,
        loopDiagHlinFeScalePart6        Unsigned32,
        loopDiagHlinFeScalePart7        Unsigned32,
        loopDiagHlinFePart0Low          OCTET STRING,
        loopDiagHlinFePart0High         OCTET STRING,
        loopDiagHlinFePart1Low          OCTET STRING,
        loopDiagHlinFePart1High         OCTET STRING,
        loopDiagHlinFePart2Low          OCTET STRING,
        loopDiagHlinFePart2High         OCTET STRING,
        loopDiagHlinFePart3Low          OCTET STRING,
        loopDiagHlinFePart3High         OCTET STRING,
        loopDiagHlinFePart4Low          OCTET STRING,
        loopDiagHlinFePart4High         OCTET STRING,
        loopDiagHlinFePart5Low          OCTET STRING,
        loopDiagHlinFePart5High         OCTET STRING,
        loopDiagHlinFePart6Low          OCTET STRING,
        loopDiagHlinFePart6High         OCTET STRING,
        loopDiagHlinFePart7Low          OCTET STRING,
        loopDiagHlinFePart7High         OCTET STRING
        }
        
loopDiagHlinFeScalePart0 OBJECT-TYPE
    SYNTAX      Unsigned32 (0..65535)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Scale factor for the normalized complex numbers Hlin."
    ::= { loopDiagHlinFeEntry 1 } 

