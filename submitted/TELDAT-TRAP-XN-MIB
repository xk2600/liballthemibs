xnNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              xn1_protocol, 
              xn1_source_net,
              xn1_source_node,
              xn1_length,
              xn1_length 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.1

        Message: '$1 truncated packet from $2/$3; xns $4, physical $5'

        Parameters list:
              $1: xn1_protocol
              $2: xn1_source_net
              $3: xn1_source_node
              $4: xn1_length
              $5: xn1_length

        This message is generated when a packet has an XNS packet length
        greater then the packets physical length."
    ::= { xnNotificationsPrefix 1 }

xnNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              xn2_protocol, 
              xn2_source_net,
              xn2_source_node,
              xn2_TC 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.2

        Message: '$1 non-zero transport control from $2/$3, $4'

        Parameters list:
              $1: xn2_protocol
              $2: xn2_source_net
              $3: xn2_source_node
              $4: xn2_TC

        The reserved bits in the Transport Control field of the
        header were not zero. An Error (checksum) packet will be sent."
    ::= { xnNotificationsPrefix 2 }

xnNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              xn3_protocol, 
              xn3_source_net,
              xn3_source_node,
              xn3_cksum,
              xn3_cksum 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.3

        Message: '$1 bad routing checksum from $2/$3; received $4, compared $5'

        Parameters list:
              $1: xn3_protocol
              $2: xn3_source_net
              $3: xn3_source_node
              $4: xn3_cksum
              $5: xn3_cksum

        This message is generated when the checksum in a packet being
        forwarded does not match the calculated checksum for the packet.
        An Error (checksum) packet will be sent."
    ::= { xnNotificationsPrefix 3 }

xnNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              xn4_protocol, 
              xn4_source_net,
              xn4_source_node,
              xn4_destination_net 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.4

        Message: '$1 hop count overflow from $2/$3 to $4'

        Parameters list:
              $1: xn4_protocol
              $2: xn4_source_net
              $3: xn4_source_node
              $4: xn4_destination_net

        This message is generated when a packet's hop count counts up
        past 15 and overflows. An Error (hop count) packet will be sent."
    ::= { xnNotificationsPrefix 4 }

xnNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              xn5_protocol, 
              xn5_source_net,
              xn5_source_node,
              xn5_destination_net 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.5

        Message: '$1 no gateway from $2/$3 to $4'

        Parameters list:
              $1: xn5_protocol
              $2: xn5_source_net
              $3: xn5_source_node
              $4: xn5_destination_net

        This message is generated when a packet cannot be forwarded
        because there is no gateway to the destination network.
        An Error (unreachable) packet will be sent."
    ::= { xnNotificationsPrefix 5 }

xnNotification6 NOTIFICATION-TYPE
    OBJECTS { 
              xn6_protocol, 
              xn6_source_net,
              xn6_source_node,
              xn6_destination_net 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.6

        Message: '$1 wasted hop from $2/$3 to $4'

        Parameters list:
              $1: xn6_protocol
              $2: xn6_source_net
              $3: xn6_source_node
              $4: xn6_destination_net

        This packet is generated when a packet is being sent out
        the same network interface it arrived on. This router is
        not the best path off that network to the destination network."
    ::= { xnNotificationsPrefix 6 }

xnNotification7 NOTIFICATION-TYPE
    OBJECTS { 
              xn7_protocol, 
              xn7_source_net,
              xn7_source_node,
              xn7_destination_net 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event XN.7

        Message: '$1 destination host 0 from $2/$3 to $4'

        Parameters list:
              $1: xn7_protocol
              $2: xn7_source_net
              $3: xn7_source_node
              $4: xn7_destination_net

        This message is generated when a packet is addressed to
        node 000000000000. This is an illegal host address.
        An Error (checksum) packet will be sent."
    ::= { xnNotificationsPrefix 7 }

