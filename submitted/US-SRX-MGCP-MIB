-- the following 43 counters are added for 05.02.01.13 Patch xxx

usSrxMgcpMTATransientErrorCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the counter of MTA Transient Errors with ranges from 400-499 "
    ::= { usSrxMgcpPerformData  21 }

usSrxMgcp500ErrorCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the counter of MGCP return code 500:The 
          transaction could not be executed, because the endpoint is
          unknown."
    ::= { usSrxMgcpPerformData  22 }

usSrxMgcp501ErrorCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the counter of MGCP return code 501:The 
          transaction could not be executed, because the endpoint is
          not ready.  This includes the case where the endpoint is 
          out-of-service."
    ::= { usSrxMgcpPerformData  23 }

usSrxMgcp502ErrorCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        " This object refers to the counter of MGCP return code 502: The
          transaction could not be executed, because the endpoint does
          not have sufficient resources (permanent condition)."
    ::= { usSrxMgcpPerformData  24 }

usSrxMgcp503ErrorCounter       OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
