US-SRX-MGCP-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY, OBJECT-TYPE, Counter32
        FROM SNMPv2-SMI 
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    surpassRtp
	FROM SURPASS-RTP-MIB
    hiq8000Admin
        FROM SURPASS-RTP-ADMIN-MIB
    SrxRowCountType,SrxOnOffType,IkeKeyGenMethod,IkeKeyType,IkeKey,IkeKeyLength,NameType
        FROM HIQ-ADMIN-MIB
    Boolean, TimeStampSeconds
        FROM FSC-RTP-MIB;

-- ########################################################

usSrxMgcp MODULE-IDENTITY
    LAST-UPDATED "200806111500Z"
    ORGANIZATION "Siemens Network Convergence LLC"
    CONTACT-INFO " "
    DESCRIPTION
        "SRX MGCP Connection Control Manager MIB Module which 
         includes management of PSTN Gateway and PSTN Circuits." 
    
    REVISION "200806111500Z"    
    DESCRIPTION
            "Updated the MIB to be fully compatible with SNMPv2"

    ::= { hiq8000Admin 3 }

-- *****************************************************
-- Textual Conventions used in the Zone Translations MIB
-- *****************************************************

MediaServerCircuitType ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION "Indicates the usage of the Media Server Circuit..
		     The following ussages apply
		     None --- Is used by Trunking and Residential Gateways
		     Any--- can be used for any circuit
		     Announcement--- is to be used for annoucement only
		     Surveillance--- is to be used for surveillance only
		     Conference  --- is to be used for conference only
		     ConferenceAdhoc  --- is to be used for ad hoc 
					  conference only
                     Audit --- is offered in the list of circuit types 
                               when creating a circuit"
        SYNTAX INTEGER { none(0),
			 any(5),
			 announcement(6), 
			 surveillance(7), 
			 conference(8),
			 conferenceadhoc(9),
                         audit(10)}

MwiStatus ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION "Indicates the status of Message Waiting Indicator
		     notSubscriber --- The customer has not subscribed to this service
		     subscribedOff --- The customer has subscribed but no messages are present.
		     subscribedOn  --- The customer has subscribed and messages are present "  
        SYNTAX      INTEGER {notSubscribed(1),	
	                     subscribedOff(2),
			     subscribedOn(3)}

MgcpLineLineType ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION "Indicates the type of line belonging 
		     belonging to this circuit
		     Pots Line (several types exist)
		     Data Line (typically pots line connected to Data equipment)
		     CLASS Line (enhanced POTS with Display capabilities)
		     Coin Line (Pay phones)
		     BRI Line  (ISDN)"
	SYNTAX INTEGER {potsLine(1),
			dataLine(2),
			classLine(3),
			coinLine(4),
			briLine(5)}

MgcpGatewayUsageType ::= TEXTUAL-CONVENTION
	STATUS current
	DESCRIPTION "Indicates the type of gateway that is supported."
	SYNTAX INTEGER { trunking(1),
			residential(2),
			mediaServer(3)}

MgcpProtocolType ::= TEXTUAL-CONVENTION
	STATUS current
    	DESCRIPTION "MG supported protocol versions. The protocol versions
                     must be one of the protocol names defined in the MGCP
                     configuration parameter file.
                     For now, there are 6 supported protocol names:
                     0. mgcp 0.0
                     1. mgcp 0.1
                     2. mgcp 1.0 ncs 1.0 (Applicable for cable labs and in  
				          conjunction with Residential 
					  gateways and media server only.)
                     3. mgcp 1.0  
                     4. mgcp 0.5
                     5. mgcp 1.0 tgcp 1.0 (Applicable only for Trunking gateway)
		     
		     We are reserving values upto 10 for MGCP
		     For Megaco this value is 
		     11. MEGACO
		     All Values from 11-20 are reserved for MEGACO

		     mgcp 1.0 ncs 1.0  is used for packet cable 
		     The mgcp 1.0 tgcp 1.0 is used in conjunction
		     with trunking gateways only."
	SYNTAX INTEGER {  mgcp00(0),
                          mgcp01(1), 
			  mgcp10ncs10(2),
			  mgcp10(3),
                          mgcp05(4),
			  mgcp10tgcp10(5),
			  megaco(11)}


MgcpMediaGatewayType ::= TEXTUAL-CONVENTION
	STATUS current
    	DESCRIPTION "An integer variable indicating the gateway type. 
                     For now, the gateway type is SMX2100,hig1100,hig1400 and others. 
                     More gateway types can be added in the future. "
	SYNTAX INTEGER {  smx2100(1), 
			  other(2),
			  hig1100(3),
              		  hig1400(4),
			  ata(5)}	

MgcpLineCurrentDialTonePattern ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION "Indicates the type of dialtone belonging to this 
                     Line belonging to the circuit
		     dl -- Dial Tone
       		     sdl -- Stutter Dial Tone"
        SYNTAX INTEGER {dl(1),sdl(2)}

MgcpLineAdminStatus ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION "Indicates the Administration Status of the
                     Line belonging to the circuit
		     blocked -- Indicates that the line is blocked
		     active -- Indicates that the line is active
		     blockAndRelease -- It indicates that the line is blocked
                     and in addition it will release any existing call. This
                     value is only used for setting."
        SYNTAX INTEGER {active(1),blocked(2), blockAndRelease(3)}

MgcpLineOperStatus ::= TEXTUAL-CONVENTION
        STATUS current
        DESCRIPTION "Indicates the Operational Status of the
                     virtual lines belonging to the circuit.
                     
                     First line and second line are legs of an MTA call
                     and second leg would be a Call Waiting call or a TWC 
                     or a second call while first leg is on hold.

		     blocked -- Indicates that the lines are blocked
		     active -- Indicates that the lines are idle 
                     auditBlocked -- Indicates that the lines are blocked
                            and being audited

                     In addition, it indicates the following call
                     status:

                     OperStatus        1st line         2nd line
                     -----------------------------------------------
                     busyOut-idle      busy outgoing    idle
                     busyIn-idle       busy incoming    idle
                     idle-busy0ut      idle             busy outgoing
                     idle-busyIn       idle             busy incoming
                     busyIn-busyIn     busy incoming    busy incoming
                     busyIn-busyOut    busy incoming    busy outgoing
                     busyOut-busyIn    busy outgoing    busy incoming
                     busyOut-busyOut   busy outgoing    busy outgoing
                     "
        SYNTAX INTEGER {active(1),
                        blocked(2),
                        busyOut-idle(4),
                        busyIn-idle(5),
                        audit-blocked(512),
                        idle-busyOut(1024),
                        busyOut-busyOut(1028),
                        busyIn-busyOut(1029),
                        idle-busyIn(5120),
                        busyOut-busyIn(5124),
                        busyIn-busyIn(5125)
                       }

SrxGatewayOperStatus ::= TEXTUAL-CONVENTION
        STATUS   current
        DESCRIPTION "Indicates the Operational Status of the Network element.
                     Currently only the media gateway is supported. For future
                     releases cmts and other network elements will be supported
	             For CMTS Objects the values used will be 
			 ADMIN ACTIVE(1),
			 ADMIN BLOCKED(2),
			 ADMIN ACTIVE OVERLOAD(7),
			 ADMIN BLOCKED OVERLOAD(8),
		     For release 08.01.US.04 onwards the values for Media Gateway 
		     will be Admin Active (1) , Admin Blocked(2), Admin Active Overload(7),
		     Admin Blocked Overload(8), the values 3 to 6 are obsolete."
        SYNTAX INTEGER { active(1),
                         blocked(2),
                         mgActiveSecurityEnabled(3),
                         mgBlockedSecurityEnabled(4),
                         mgActiveSecurityDisabled(5),
                         mgBlockedSecurityDisabled(6),
			 activeoverload(7),
                         blockedoverload(8)}

SrxGatewayAdminStatus ::= TEXTUAL-CONVENTION
           STATUS   current
           DESCRIPTION "Indicates the Administrational Status of
                         the Network element.Currently only the media
                         gateway is supported. For future releases cmts and
                         other network elements will be supported"
           SYNTAX INTEGER {active(1),blocked(2),precut(3)}



