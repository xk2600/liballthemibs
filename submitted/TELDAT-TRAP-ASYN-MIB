    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event ASYN.4"
    ::= { asynEventObjects4 2 }

asynEventObjects5 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ASYN.5
        Incorrect packet format: last character of frame is escape character."
    ::= { asynEventsGroup 5 }

asyn5_frame_size OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'frame_size' in event ASYN.5"
    ::= { asynEventObjects5 1 }

asyn5_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event ASYN.5"
    ::= { asynEventObjects5 2 }


asynNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              asyn1_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ASYN.1

        Message: 'Serial port came up sucessfully on interface $1'

        Parameters list:
              $1: asyn1_interface

        Selftest completed successfully."
    ::= { asynNotificationsPrefix 1 }

asynNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              asyn2_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ASYN.2

        Message: 'Maintenance failure on interface $1'

        Parameters list:
              $1: asyn2_interface

        Maintenance failure."
    ::= { asynNotificationsPrefix 2 }

asynNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              asyn3_frame_size, 
              asyn3_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ASYN.3

        Message: 'Received packed with bad crc, frame size $1, on interface $2'

        Parameters list:
              $1: asyn3_frame_size
              $2: asyn3_interface

        An IP packet has been received with a CRC error."
    ::= { asynNotificationsPrefix 3 }

asynNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              asyn4_frame_size, 
              asyn4_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ASYN.4

        Message: 'Not received end flag on frame with size $1, on interface $2'

        Parameters list:
              $1: asyn4_frame_size
              $2: asyn4_interface

        End flag is not found on received frame."
    ::= { asynNotificationsPrefix 4 }

asynNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              asyn5_frame_size, 
              asyn5_interface 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event ASYN.5

        Message: 'Frame ended with escape character with size $1, on interface $2'

        Parameters list:
              $1: asyn5_frame_size
              $2: asyn5_interface

        Incorrect packet format: last character of frame is escape character."
    ::= { asynNotificationsPrefix 5 }


