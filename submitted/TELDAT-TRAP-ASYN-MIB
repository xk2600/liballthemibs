TELDAT-TRAP-ASYN-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


asynTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the ASYN event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 150 }


asynEventsGroup	OBJECT IDENTIFIER	::= { asynTrapsMIB 1 }

asynNotifications	OBJECT IDENTIFIER	::= { asynTrapsMIB 2 }
asynNotificationsPrefix	OBJECT IDENTIFIER	::= { asynNotifications 0 }

asynEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ASYN.1
        Selftest completed successfully."
    ::= { asynEventsGroup 1 }

asyn1_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event ASYN.1"
    ::= { asynEventObjects1 1 }

asynEventObjects2 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ASYN.2
        Maintenance failure."
    ::= { asynEventsGroup 2 }

asyn2_interface OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'interface' in event ASYN.2"
    ::= { asynEventObjects2 1 }

asynEventObjects3 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event ASYN.3
        An IP packet has been received with a CRC error."
    ::= { asynEventsGroup 3 }

asyn3_frame_size OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'frame_size' in event ASYN.3"
    ::= { asynEventObjects3 1 }

