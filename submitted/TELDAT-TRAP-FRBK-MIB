TELDAT-TRAP-FRBK-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


frbkTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the FRBK event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 114 }


frbkEventsGroup	OBJECT IDENTIFIER	::= { frbkTrapsMIB 1 }

frbkNotifications	OBJECT IDENTIFIER	::= { frbkTrapsMIB 2 }
frbkNotificationsPrefix	OBJECT IDENTIFIER	::= { frbkNotifications 0 }

frbkEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event FRBK.1
        A PVC switched to backup state."
    ::= { frbkEventsGroup 1 }

frbk1_circuit OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
