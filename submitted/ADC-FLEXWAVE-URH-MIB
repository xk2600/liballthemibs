	fwuRmtDARTFPGAProgramVer  DisplayString,
	fwuRmtDARTRIADCPartNumber  DisplayString,
	fwuRmtDARTRISerialNumber  DisplayString,
	fwuRmtDARTRIDateCode  DisplayString,
	fwuRmtDARTRIHWVer  DisplayString,
	fwuRmtDARTGeneralTableRowStatus RowStatus,	
