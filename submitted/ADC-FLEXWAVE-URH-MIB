-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Snmp Trap Dest Table  [Rev-7] (.15.2.1.3.5.7) --
-- Rearranged the OIDs in order to minimize MIB compatibility issues
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
fwuSnmpTrapMgrConfigTable	OBJECT-TYPE
	SYNTAX			SEQUENCE OF FwuSnmpTrapMgrConfigEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Table containing information about the trap destinations.  For changes
					to take effect, a (Host) reboot is	required."
	::= { fwuSnmpSettings 7}

fwuSnmpTrapMgrConfigEntry	OBJECT-TYPE
	SYNTAX			FwuSnmpTrapMgrConfigEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"An entry in the fwuSnmpTrapMgrConfigTable."
	INDEX			{ fwuTrapMgrRowId }
	::= { fwuSnmpTrapMgrConfigTable 1 }

FwuSnmpTrapMgrConfigEntry ::= SEQUENCE {
	fwuTrapMgrRowId 	INTEGER,
	fwuTrapMgrIpAddress 	OCTET STRING,
	fwuTrapMgrListeningPort	Unsigned32,
	fwuTrapMgrCommunity	DisplayString,
	fwuTrapVersion 		INTEGER,
	fwuTrapMgrRowStatus 	RowStatus	}

fwuTrapMgrRowId		OBJECT-TYPE
	SYNTAX			INTEGER (1..16)
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"The unique index of this trap destination."		
	::= {fwuSnmpTrapMgrConfigEntry 1}

fwuTrapMgrIpAddress OBJECT-TYPE
	SYNTAX 			OCTET STRING ( SIZE (0..16) )
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"Provision to set the IP address of the trap manager. Use Internet standard dot-decimal notation."
	DEFVAL  { "127.0.0.1" } 	
	::= {fwuSnmpTrapMgrConfigEntry 2}

fwuTrapMgrListeningPort	OBJECT-TYPE	
	SYNTAX			Unsigned32
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"Provision to set the port at which the trap manager is listening for the traps. The default port for SNMP traps is 162."
	DEFVAL			{ 162 }
	::= {fwuSnmpTrapMgrConfigEntry 3}
	
fwuTrapMgrCommunity	OBJECT-TYPE	
	SYNTAX			DisplayString ( SIZE(6..20))
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"The community used by the agent to send traps or informs to this manager."
	DEFVAL			{ "public" }
	::=  {  fwuSnmpTrapMgrConfigEntry  4 }	

fwuTrapVersion		OBJECT-TYPE
	SYNTAX			INTEGER  { v1(0),  v2c(1), inform(2) }
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"The SNMP version used for this trap destination. The available versions are v1(0),  v2c(1), inform(2) with v2c being set as default."
	DEFVAL			{ 1 }
	::= {fwuSnmpTrapMgrConfigEntry 5}

fwuTrapMgrRowStatus OBJECT-TYPE
	SYNTAX 			RowStatus		
	MAX-ACCESS 		read-create
	STATUS 			current
	DESCRIPTION		"The status of this conceptual row.
					This is used to manage the creation and deletion of this trap destination table."
	DEFVAL			{ notInService }	
	::= {fwuSnmpTrapMgrConfigEntry 6}
	
fwuEnableNotifHysteresis OBJECT-TYPE
    SYNTAX      INTEGER {disable(0), enable(1)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Provision to enable(1) or disable(0) event hysteresis. If disabled notification is sent out on occurance of the event. If enabled set the hysteresis value using fwuEventNotifHysteresis."
    DEFVAL          { 0 }
    ::= { fwuSnmpSettings 8 }

fwuEventNotifHysteresis OBJECT-TYPE
    SYNTAX      Integer32 (10..600)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Time period, in seconds, for which an event has to persist before a notification is sent. For this value to be useable, ensure that fwuEnableNotifHysteresis is enabled."
    ::= { fwuSnmpSettings 9 }

fwuEnableNotifInterval OBJECT-TYPE
    SYNTAX      INTEGER {disable(0), enable(1)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Provision to enable(1) or disable(0) a time lag before sending out an event notification. If disabled, 
         notification is sent out on the occurance of the event. If enabled, use fwuEventNotifInterval to set the lag."
    DEFVAL          { 0 }
    ::= { fwuSnmpSettings 10 }

fwuEventNotifInterval OBJECT-TYPE
    SYNTAX      Integer32 (5..60)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Provision to set the time lag in seconds between the successive event notifications. For this value to be useable, ensure that fwuEnableNotifInterval is enabled."
    ::= { fwuSnmpSettings 11 }

-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Scheduler Table objects (.15.2.1.3.5.12) --
-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
fwuSchedulerTable	OBJECT-TYPE
	SYNTAX			SEQUENCE OF FwuSchedulerEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Table containing information about the trap destinations.  For changes
					to take effect, a (Host) reboot is required."
	::= { fwuSnmpSettings 12}

fwuSchedulerEntry	OBJECT-TYPE
	SYNTAX			FwuSchedulerEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"An entry in the fwuSchedulerTable."
	INDEX			{ fwuTaskId }
	::= { fwuSchedulerTable 1 }

FwuSchedulerEntry ::= SEQUENCE {
    fwuTaskId  INTEGER,
    fwuTaskType  INTEGER,
    fwuScheduleStatus  INTEGER,
    fwuTaskStartTime  DisplayString,
    fwuOccurrences  INTEGER,
    fwuRecurrence  DisplayString,
    fwuFunctionArgs  DisplayString,
    fwuRowStatus 	RowStatus}

fwuTaskId		    OBJECT-TYPE
	SYNTAX			INTEGER (0 .. 2147483647) 
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"The unique index of the Scheduler table. Range limited to
	                 machine integer size(32 bit) restriction."		
	::= {fwuSchedulerEntry 1}

fwuTaskType		    OBJECT-TYPE
	SYNTAX			INTEGER { systemTest(2), iamAliveTrap(5) }
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"The task to be scheduled. If System Test is the task being scheduled it requires some more arguments. Use fwuFunctionArgs to provide them. After specifying a task, enable the task using fwuScheduleStatus."
	::= {fwuSchedulerEntry 2}

fwuScheduleStatus	OBJECT-TYPE
	SYNTAX			INTEGER { notEnabled(0), enabled(1) }
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"Provision to enable(1) or disable(0) the scheduled task (defined by fwuTaskType)."
	DEFVAL			{ 0 }
	::= {fwuSchedulerEntry 3}

	
fwuTaskStartTime	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"The start time of the scheduled activity."
	::=  {fwuSchedulerEntry  4}	

fwuOccurrences		OBJECT-TYPE
	SYNTAX			INTEGER
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"Provision to set the number of times the task is to be executed. Set a value greater than 0 for a finite number of executions. 
	                 To keep executing forever, set value to -1."
	DEFVAL			{ -1 }
	::= {fwuSchedulerEntry 5}

fwuRecurrence		OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS 		read-create
	STATUS			current
	DESCRIPTION		"Provision to set the periodicity of the scheduled task. Eg. S5 is 5 Sec, H6 is 6 Hours, M8 is 8 Min. 
	                 Note: This is also used for the time interval for sending I am alive traps."
	DEFVAL			{ "0" }
	::= {fwuSchedulerEntry 6}

fwuFunctionArgs	    OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"Colon separated string of arguments required for the given task type. 
	                 Can be empty depending on the task type."
	::=  { fwuSchedulerEntry  7}
	
fwuRowStatus        OBJECT-TYPE
	SYNTAX 			RowStatus		
	MAX-ACCESS 		read-create
	STATUS 			current
	DESCRIPTION		"The status of the conceptual row.
					This is used to manage the creation and deletion of the rows in scheduler table."
	DEFVAL			{ notInService }	
	::= {fwuSchedulerEntry 8}	
	
-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Contact Alarm Objects  [Rev-7] (.15.2.1.3.6)  fwuContactAlarmTraps --
fwuContactAlarmObjects OBJECT IDENTIFIER::= {fwuGeneralObjects 6}	
-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

fwuContactAlarmInfo	OBJECT IDENTIFIER::=  {  fwuContactAlarmObjects  1  }

fwuSystemContactAlarmMgmtTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuSystemContactAlarmMgmtEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Entities in this table are applicable to all Prism and URH remote units. 
				For Host and DRU, entities are applicable only for 2nd generation system 
				cards and onwards."
	::=  { fwuContactAlarmInfo  1 }

fwuSystemContactAlarmMgmtEntry OBJECT-TYPE
	SYNTAX			FwuSystemContactAlarmMgmtEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"An entry (conceptual row) in the fwuSnmpTrapMgrConfigTable."
	INDEX			{  fwuUnitIndex, fwuSYstemAlarmCode}
	::= { fwuSystemContactAlarmMgmtTable 1 }

FwuSystemContactAlarmMgmtEntry ::= SEQUENCE {
	fwuUnitIndex UnitID2,
	fwuSYstemAlarmCode	Unsigned32,
	fwuContactAlarm	DisplayString,
	fwuContactAlarmMode	INTEGER, 
	fwuContactAlarmName	DisplayString,
	fwuContactAlarmEnable	ALARMEnable,
	fwuContactAlarmSeverity INTEGER }

fwuUnitIndex		OBJECT-TYPE
	SYNTAX			UnitID2
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The number of the connected unit."	
	::= {fwuSystemContactAlarmMgmtEntry 1}

fwuSYstemAlarmCode	OBJECT-TYPE
	SYNTAX			Unsigned32
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION	
		"A code that uniquely identifies an alarm."
	::=  {fwuSystemContactAlarmMgmtEntry 2}

fwuContactAlarm OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Hardware Assigned Name."	
	::= {fwuSystemContactAlarmMgmtEntry 3}

fwuContactAlarmMode OBJECT-TYPE
	SYNTAX			INTEGER { normallyClose(1) ,  normallyOpen(2) } 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Provision to set the mode of the alarm as normally closed (1) or normally open  (2)."	
	::= {fwuSystemContactAlarmMgmtEntry 4}

fwuContactAlarmName	OBJECT-TYPE
	SYNTAX			DisplayString (SIZE(0..40))
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"User defined name of the host unit. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 0 character  (It may be empty.). Maximum length of the name is 64 characters."	
	::= {fwuSystemContactAlarmMgmtEntry 5}

fwuContactAlarmEnable OBJECT-TYPE
	SYNTAX			ALARMEnable 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Provision to enable (1) or disable (0) the contact alarm. The alarm is enabled by default."
	DEFVAL 			{1}
	::= {fwuSystemContactAlarmMgmtEntry 6}

fwuContactAlarmSeverity OBJECT-TYPE
	SYNTAX			INTEGER { minor (2) ,  major(3) } 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Severity of the alarm."	
	::= {fwuSystemContactAlarmMgmtEntry 7}

