	SYNTAX			SEQUENCE  OF  FwuRmtGeneralEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table maintains Information of remote."
	::=  { fwuRmtGeneral  1 }
						
fwuRmtGeneralEntry	OBJECT-TYPE
	SYNTAX			FwuRmtGeneralEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Each entry in this table refers to remote objects."
	INDEX			{  fwuRmtNumber  }
	::=  { fwuRmtGeneralTable 1 }
						
FwuRmtGeneralEntry  ::=  SEQUENCE {
	fwuRmtNumber	UnitID2,	
	fwuRmtCatalogState	CATALOGType,
	fwuRmtName	DisplayString,
	fwuRmtAlarmStatusSummary	ALARMType,
	fwuRmtTempMeasurement	DisplayString,
	fwuRmtGeneralTableRowStatus	RowStatus,
	fwuRmtType	UnitType,
	fwuDruIfCombinerReboot 	Unsigned32, 
	fwuUsrCommLinkTestIFBr	INTEGER,
	fwuUsrCommLinkTestIFBrDType	BANDType,
	fwuUsrCommLinkTestDartId	Unsigned32,
	fwuDruBackPlaneRev	OCTET STRING,
	fwuPrismUnitReset 	INTEGER
	}
						
fwuRmtNumber		OBJECT-TYPE	
	SYNTAX			UnitID2
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The Node address of connected remote."	
	::=  {  fwuRmtGeneralEntry  1  }

fwuRmtCatalogState	OBJECT-TYPE	
	SYNTAX			CATALOGType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Catalog state of remote. If fwuRmtCatalogState is commission(2),
					then start commissioning on remote, else if it in normal, do not commission the remote."
	::=  {  fwuRmtGeneralEntry  2  }
						
fwuRmtName	OBJECT-TYPE
	SYNTAX			DisplayString  ( SIZE (0 .. 64) ) 
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"User defined name of the remote unit. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL 			{ "Remote" }	
	::=  {  fwuRmtGeneralEntry  3  }	 

fwuRmtAlarmStatusSummary	OBJECT-TYPE	
	SYNTAX			ALARMType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Summary of alarm status of all connected remote units."
	::=  {  fwuRmtGeneralEntry  4  }

fwuRmtTempMeasurement	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Temperature of each remote unit in Celsius. This value comes from the RSI."
	::=  {  fwuRmtGeneralEntry  5  }

fwuRmtGeneralTableRowStatus OBJECT-TYPE
	SYNTAX			RowStatus
	MAX-ACCESS   	read-create
	STATUS       	current
	DESCRIPTION 	"The status of this conceptual row. It may be active or inactive."
	::= { fwuRmtGeneralEntry 6 }

fwuRmtType	OBJECT-TYPE	
	SYNTAX			UnitType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The type of Remote. e.g. Prism, DRU, HEU, IRU."
	::=  {  fwuRmtGeneralEntry  7  }

fwuDruIfCombinerReboot 	OBJECT-TYPE	
	SYNTAX			Unsigned32 (0..1)
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Reboot an IF-combiner and all the units connected to it. Only 
					applicable to DRU-IFs Change Value from 0 to 1 to reboot."
	::=  {  fwuRmtGeneralEntry  8  }

fwuUsrCommLinkTestIFBr	OBJECT-TYPE	
	SYNTAX			INTEGER  { inactive(0), start(1) , inprogress(2) } 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Provision to perform system test on an IF branch. Change from inactive(0) to start(1) to initiate the test. For the duration of the test inprogress(2) is displayed. On completion, inactive is displayed."
	DEFVAL 			{ 0 }
	::=  {  fwuRmtGeneralEntry  9  }

fwuUsrCommLinkTestIFBrDType	OBJECT-TYPE	
	SYNTAX			BANDType
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Selected Bands i.e. RF DARTs on units when performing system test."
	::=  {  fwuRmtGeneralEntry  10  }

fwuUsrCommLinkTestDartId	OBJECT-TYPE	
	SYNTAX			Unsigned32
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"To identify the DART ."
	::=  {  fwuRmtGeneralEntry  11  }

fwuDruBackPlaneRev	OBJECT-TYPE	
	SYNTAX			OCTET STRING (SIZE (1..40))
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Provides the back plane revision of the DRU (Three pin version Number of the Chassis back plane)"
	::=  {  fwuRmtGeneralEntry  12  }

fwuPrismUnitReset	OBJECT-TYPE	
	SYNTAX			INTEGER { normal(0), reset(1)}
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Provides a way to reset the Remote SERF card. SET this to reset(1) to trigger a remote unit reset."
	::=  {  fwuRmtGeneralEntry  13  }
	
	
