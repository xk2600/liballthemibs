	SYNTAX			Integer32 (1..8)
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"Identifier of DART on the unit. Since there is a maximum of 8 DARTs, the valid values are one to eight."
	::=  {  fwuDARTMappingEntry  3  }
	
fwuUnitSFPID	OBJECT-TYPE	
	SYNTAX			Integer32 (1..8)
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"Identifier of unit SFP connected to the mate DART (outgoing; away from the Host). Since there is a maximum of 8 SFPs, the valid values are one to eight."
	::=  {  fwuDARTMappingEntry  4  }

fwuUnitDartPassBand	OBJECT-TYPE	
	SYNTAX			OCTET STRING (SIZE (0..40)) 
	MAX-ACCESS		read-only
	STATUS			current
