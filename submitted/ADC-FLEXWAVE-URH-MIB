fwuHstDARTObjects	OBJECT IDENTIFIER	::=  {  fwuHostUnitObjects  6  }
fwuHstDARTInfo		OBJECT IDENTIFIER	::=  {  fwuHstDARTObjects  1  }

fwuHstDARTGeneral	OBJECT IDENTIFIER	::=  {  fwuHstDARTInfo  1  }

fwuHstDARTGeneralTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuHstDARTGeneralEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table maintains Information of Host DART Card."
	::=  { fwuHstDARTGeneral  1 }

fwuHstDARTGeneralEntry	OBJECT-TYPE
	SYNTAX			FwuHstDARTGeneralEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Each entry in this table refers to host DART Card objects."
	INDEX			{  fwuHstDARTNumber  }
	::=  { fwuHstDARTGeneralTable 1 }

FwuHstDARTGeneralEntry  ::=  SEQUENCE {
	fwuHstDARTNumber  Integer32,
	fwuHstDARTName  DisplayString,
	fwuHstDARTBandType  BANDType,
	fwuHstDARTPassBand  OCTET STRING,
	fwuHstDARTOperatingMode  MODEType,
	fwuHstDARTDiversityStatus  INTEGER,
	fwuHstDARTForwardGain  GAINType,
	fwuHstDARTReverseGain  GAINType,
	fwuHstDARTFPGAStatus INTEGER,
	fwuHstDARTFPGAProgramVer  DisplayString,
	fwuHstDARTRIADCPartNumber  DisplayString,
	fwuHstDARTRISerialNumber  DisplayString,
	fwuHstDARTRIDateCode  DisplayString,
	fwuHstDARTRIHWVer  DisplayString,
	fwuHstDARTRowStatus RowStatus, 
	fwuHstDARTPeakInputPwrLvl1 DisplayString,
	fwuHstDARTPeakInputPwrLvl2 DisplayString,
	fwuHstDARTPeakAvgInputPwrLvl1 DisplayString,
	fwuHstDARTPeakAvgInputPwrLvl2 DisplayString,
	fwuHstDARTMinAvgInputPwrLvl1 DisplayString,
	fwuHstDARTMinAvgInputPwrLvl2 DisplayString,
	fwuHstDARTInputPowerMode INTEGER,
	fwuHstDARTPwrLevelMaxHoldReset INTEGER, 
	fwuHstDARTLastMaxHoldResetTime OCTET STRING,
	fwuHstDARTInputSrc INTEGER	}

fwuHstDARTNumber	OBJECT-TYPE	
	SYNTAX			Integer32  (1 .. 8) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A unique identifier for each DART. As there are eight DART slots, the number may be any of 1 to 8."
	::=  {  fwuHstDARTGeneralEntry  1  }

fwuHstDARTName	OBJECT-TYPE	
	SYNTAX			DisplayString  (SIZE(0..64))
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"A user defined name for each DART Card object of host. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL 			{ "DART" }	 
	::=  {  fwuHstDARTGeneralEntry  2  }

fwuHstDARTBandType	OBJECT-TYPE	
	SYNTAX			BANDType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Band type of host DART card."
	DEFVAL 			{ -1 }	 	
	::=  {  fwuHstDARTGeneralEntry  3  }

fwuHstDARTPassBand	OBJECT-TYPE	
	SYNTAX			OCTET STRING (SIZE (0..40))
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"This is for configuration of Pass-band type for host DART card. 
					Following input formats are supported for Classic and SuperDart
					passband configuration. 
					1. For Classic Darts only, 
						User input string would be enumeration value ( i.e., 'x'), ranging from 1 to 134. 
						See below in section 3. for valid values. 
					2. For SuperDarts only,
						a. BandType_SubBand1+SubBand2 
							This is applicable only for contiguous passband selection,
							applicable only for PCS & AWS. (e.g., 'PCS_A1+A2+A3')
						b. BandType_SubBand1+SubBand2:SubBand5+SubBand6
							This is applicable for noncontiguous passband selection,
							applicable only for PCS & AWS. (e.g., 'PCS_A1+A2:C2+C1')
						c. BandType_StartFrequency-StartFrequency
							This is applicable only for contiguous passband frequency range. (e.g.,, PCS_1930-1945) 
						d. BandType_StartFrequency1-StartFrequency1:StartFrequency2-StartFrequency2
							This is applicable only for noncontiguous passband frequency ranges.
							(e.g., PCS_1930-1940.5:1960.7-1970.6). Where BandType is 'PCS', 'AWS', 'UMTS','EGSM900','GSM1800', 'UC700', 'LABC700'.
					3. Valid enumeration value for classic dart:- undefined(-1), cellularA2A(1),cellularA(2),cellularAA1A2(3),cellularA1(4),
						cellularB(5),cellularB1(6),cellularBB1(7), pcsA1(8),pcsA2(9),pcsA3(10),pcsB1(11),pcsB2(12),pcsB3(13),
						pcsC3(14),pcsC4(15),pcsC5(16),pcsD(17),pcsE(18),pcsF(19), pcsG(20),awsH(21),pcsC2(22), pcsC1(23), 								
						pcsA1A2(24),pcsA2A3(25),pcsA3D(26),pcsDB1(27),pcsB1B2(28),pcsB2B3(29), pcsB3E(30),pcsEF(31),pcsFC3(32),
						pcsC3C4(33),pcsC4C5(34), pcsC5G(35),pcsGawsH(36),pcsFC2(37),pcsC1G(38),pcsA(39), pcsA2A3D(40),pcsA3DB1(41),
						pcsDB1B2(42),pcsB(43),pcsB2B3E(44), pcsB3EF(45),pcsEFC3(46),pcsFC3C4(47),pcsC(48), pcsC4C5G(49),
						pcsC5GawsH(50),pcsEFC2(51),pcsC1GawsH(52), pcsAD(53),pcsA2A3DB1(54),pcsA3DB1B2(55),pcsDB(56),pcsBE(57), 
						pcsB2B3EF(58),pcsB3EFC3(59),pcsEFC3C4(60),pcsFC(61), pcsCG(62),pcsC4C5GawsH(63),pcsB3EFC2(64), 
						pcsADB1(65),pcsA2A3DB1B2(66),pcsA3DB(67),pcsDBE(68), pcsBEF(69),pcsB2B3EFC3(70),pcsB3EFC3C4(71),pcsEFC(72),
						pcsFCG(73),pcsCGawsH(74),pcsB2B3EFC2(75), pcsADB1B2(76),pcsA2A3DB(77),pcsA3DBE(78),pcsDBEF(79),
						pcsBEFC3(80),pcsB2B3EFC3C4(81),pcsB3EFC(82), pcsEFCG(83),pcsFCGawsH(84),pcsADB(85), pcsA2A3DBE(86),pcsA3DBEF(87),
						pcsDBEFC3(88), pcsBEFC3C4(89),pcsB2B3EFC(90),pcsB3EFCG(91), pcsEFCGawsH(92),smrLow(93) , smrHigh( 94),
						awsA(95), awsAB(96), awsABC(97), awsABCD(98), awsABCDE(99), awsB(100), awsBC(101), awsBCD(102), awsBCDE(103), 
						awsBCDEF(104), awsC(105), awsCD(106), awsCDE(107), awsCDEF(108), awsD(109), awsDE(110), awsDEF(111), 
						awsE(112), awsEF(113), awsF(114), smrLowWide(115), cellularA2ABA1B1(116), GSM1800Low(117), GSM1800Middle(118), 
						GSM1800High(119), umtsLow(120), umtsMiddle(121), umtsHigh(122), cellularAB(133), apac800iDEN(134)."
	::=  {  fwuHstDARTGeneralEntry  4  }

