	fwuRmtDARTPeakAvgInputPwrLvl2 DisplayString,
	fwuRmtDARTMinAvgInputPwrLvl1 DisplayString,
	fwuRmtDARTMinAvgInputPwrLvl2 DisplayString,
	fwuRmtDARTInputPowerMode INTEGER,
	fwuRmtDARTPwrLevelMaxHoldReset INTEGER, 
	fwuRmtDARTLastMaxHoldResetTime OCTET STRING,
	fwuRmtDARTReverseGainMode  INTEGER,
	fwuRmtDARTTxRIADCPartNumber DisplayString,
	fwuRmtDARTTxRISerialNumber DisplayString,
	fwuRmtDARTTxRIDateCode DisplayString,
	fwuRmtDARTTxRIHWVer DisplayString,
        fwuRmtDARTLinkedStatus INTEGER,
        fwuRmtDARTAvailable INTEGER,
        fwuRmtDARTUsedSFP INTEGER }

fwuRmtDARTNumber	OBJECT-TYPE	
	SYNTAX			Integer32  (1 .. 8) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A unique identifier for each DART Card Object of remote. Since there are eight DART slots the valid values are 1 to 8."
	::=  {  fwuRmtDARTGeneralEntry  1  }

fwuRmtDARTName	OBJECT-TYPE	
	SYNTAX			DisplayString (SIZE(0..64))
	MAX-ACCESS		read-create
	STATUS			current
	DESCRIPTION		"A user defined name for each DART Card object of remote. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL 			{ "DART" }
	::=  {  fwuRmtDARTGeneralEntry  2  }

