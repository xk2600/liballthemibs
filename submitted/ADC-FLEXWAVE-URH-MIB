-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
-- Static Alarms Table [Rev-7] (.15.2.1.3.7)  fwuStaticAlarmsTable --
fwuSystemStaticAlarms OBJECT IDENTIFIER ::= { fwuGeneralObjects 7 }	
-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

fwuSystemStaticAlarmsTable OBJECT-TYPE
	SYNTAX			SEQUENCE OF FwuSystemStaticAlarmsEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This is a static alarm table containing the statistics of units in the system.
					The major and minor alarms will be grouped separately for each of the units."
	::=  { fwuSystemStaticAlarms  1 }

fwuSystemStaticAlarmsEntry OBJECT-TYPE
	SYNTAX			FwuSystemStaticAlarmsEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Entries for units which can be either alarmed or not alarmed or not-discovered."
	INDEX			{ fwuSystemUnitID, fwuSystemUnitType }
	::=  { fwuSystemStaticAlarmsTable 1 }

FwuSystemStaticAlarmsEntry  ::=  SEQUENCE {		
	fwuSystemUnitID		UnitID2,
	fwuSystemUnitType	UnitType,
	fwuUnitMajorAlarms	DisplayString,
	fwuUnitMinorAlarms	DisplayString	}

fwuSystemUnitID 	OBJECT-TYPE
	SYNTAX			UnitID2
	MAX-ACCESS		read-only	
	STATUS			current
	DESCRIPTION		"The ID of the unit from which the alarms has been raised."
	::=  {  fwuSystemStaticAlarmsEntry  1  }

fwuSystemUnitType 	OBJECT-TYPE	
	SYNTAX			UnitType	
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The type of the from which the alarm has been raised."
	::=  {  fwuSystemStaticAlarmsEntry  2  }

fwuUnitMajorAlarms 	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The major alarm name of the unit."
	::=  {  fwuSystemStaticAlarmsEntry  3  }

fwuUnitMinorAlarms	OBJECT-TYPE	
	SYNTAX			DisplayString		
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The minor alarm name of the unit."
	::=  {  fwuSystemStaticAlarmsEntry  4  }


