	fwuRmtDARTForwardLowerboundDelay  Integer32,
	fwuRmtDARTForwardUpperboundDelay  Integer32,
	fwuRmtDARTActualReverseDelay  Integer32,
	fwuRmtDARTReverseLowerboundDelay  Integer32,
	fwuRmtDARTReverseUpperboundDelay  Integer32,
