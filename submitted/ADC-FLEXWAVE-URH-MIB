fwuSystemActiveAlarms		OBJECT IDENTIFIER	::=  {  fwuHstSystemLevelInfo  3 }

fwuSystemActiveAlarmTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuSystemActiveAlarmEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table lists all the outstanding (active) alarms in the system. 
				Alarms are removed from this table when the alarm condition is cleared (i.e. When CLEAR is sent out)."
					
	::=  { fwuSystemActiveAlarms  1 }
	
fwuSystemActiveAlarmEntry	OBJECT-TYPE
	SYNTAX			FwuSystemActiveAlarmEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Entries for active alarms."
	INDEX			{  fwuSystemAlarmSequenceNumber  }
	::=  { fwuSystemActiveAlarmTable 1 }

FwuSystemActiveAlarmEntry	::=  SEQUENCE {
	fwuSystemAlarmSequenceNumber	Unsigned32,
	fwuSystemAlarmTrapNodeAddress	UnitID2,
	fwuSystemlabel				DisplayString,
	fwuSystemAlarmRmtName		DisplayString,
	fwuSystemAlarmRmtType		UnitType,
	fwuSystemAlarmModuleType	MODULEType,
	fwuSystemAlarmModuleNumber	Unsigned32,
	fwuSystemAlarmModuleName	DisplayString,
	fwuSystemAlarmSeverity		ALARMType,
	fwuSystemAlarmRFBand		DisplayString,
	fwuSystemAlarmTimeStamp		DateAndTime,
	fwuSystemAlarmCode			Unsigned32,
	fwuSystemAlarmHstNbr		Unsigned32,
	fwuSystemAlarmAnalogAddress	UnitID3	}

fwuSystemAlarmSequenceNumber	OBJECT-TYPE	
	SYNTAX			Unsigned32 (0..4294967295)
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"The number of the alarm table entry which is defined by this row."
	::=  {  fwuSystemActiveAlarmEntry  1  }

fwuSystemAlarmTrapNodeAddress	OBJECT-TYPE	
	SYNTAX			UnitID2
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION	    "System Node Address, where the alarm originated."
	::=  {  fwuSystemActiveAlarmEntry  2  }
	
fwuSystemlabel		OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The name used to identify the system. Since the Host name is used to identify the system, the Host name is reflected here."
	::=  {  fwuSystemActiveAlarmEntry  3  }

fwuSystemAlarmRmtName	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Name of the unit from which the alarm originated."
	::=  {  fwuSystemActiveAlarmEntry  4  }

fwuSystemAlarmRmtType	OBJECT-TYPE
	SYNTAX			UnitType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Type of the unit from which the alarm originated. fwuSystemAlarmRmtName is the user provided name of this unit."
	::=  {  fwuSystemActiveAlarmEntry  5  }

fwuSystemAlarmModuleType	OBJECT-TYPE	
	SYNTAX			MODULEType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Type of the Module from which the alarm originated. e.g SeRF, DART, SFP. To be read with fwuSystemAlarmRmtType. Then, it identifies the module on that (fwuSystemAlarmRmtType) unit."
	::=  {  fwuSystemActiveAlarmEntry  6  }

fwuSystemAlarmModuleNumber	OBJECT-TYPE	
	SYNTAX			Unsigned32 (1..8)
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Number of the module from which the alarm originated. To be used in conjuction with the object fwuSystemAlarmModuleType. So, if module type indicated DART and module number reported 3, then it means that the alarm originated from the third DART."
	::=  {  fwuSystemActiveAlarmEntry  7  }

fwuSystemAlarmModuleName	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Name of the Module from which the alarm originated. The user provided name of the module identified by fwuSystemAlarmModuleNumber. "
	::=  {  fwuSystemActiveAlarmEntry  8  }

fwuSystemAlarmSeverity	OBJECT-TYPE	
	SYNTAX			ALARMType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Severity of the alarm."
	::=  {  fwuSystemActiveAlarmEntry  9  }

fwuSystemAlarmRFBand  OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION	        "The RF band affected by this alarm condition."
	::=  {  fwuSystemActiveAlarmEntry  10  }

fwuSystemAlarmTimeStamp	OBJECT-TYPE	
	SYNTAX			DateAndTime
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Time when the alarm is raised."
	::=  {  fwuSystemActiveAlarmEntry  11  }

fwuSystemAlarmCode	OBJECT-TYPE	
	SYNTAX			Unsigned32
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A positive number which uniquely identifies an alarm."
	::=  {  fwuSystemActiveAlarmEntry  12  }

fwuSystemAlarmHstNbr	OBJECT-TYPE	
	SYNTAX			Unsigned32
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Number identifying the host in the system."
	::=  {  fwuSystemActiveAlarmEntry  13  }

fwuSystemAlarmAnalogAddress		OBJECT-TYPE	
	SYNTAX			UnitID3
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Analog Portion of the node address."
	::=  {  fwuSystemActiveAlarmEntry  14  }

