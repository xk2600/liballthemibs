fwuHstSERFOptObjects		OBJECT IDENTIFIER 		::=  {  fwuHstSERFInfo  2 }

fwuHstSERFOptTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuHstSERFOptEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table maintains Information of Host SERF Optical Objects."
	::=  { fwuHstSERFOptObjects  1 }

fwuHstSERFOptEntry	OBJECT-TYPE
	SYNTAX			FwuHstSERFOptEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Each entry in this table refers to Host SERF Optical objects."
	INDEX			{  fwuHstSERFOptSFPNumber  }
	::=  { fwuHstSERFOptTable 1 }

FwuHstSERFOptEntry  ::=  SEQUENCE {
	fwuHstSERFOptSFPNumber		Integer32,
	fwuHstSERFOptSFPName 	 	DisplayString,
	fwuHstSERFOptSFPType  		OPTICSType,
	fwuHstSERFOptSFPTxColor		Integer32,
	fwuHstSERFOptRevLaunchPowerMeas	DisplayString,
	fwuHstSERFOptFwdLaunchPowerMeas DisplayString,
	fwuHstSERFOptMateName 		DisplayString,
	fwuHstSERFIPEnable		INTEGER,
	fwuHstSERFOptMateId		UnitID2,
	fwuHstSERFOptMateSfpId	Integer32 }

fwuHstSERFOptSFPNumber OBJECT-TYPE	
	SYNTAX			Integer32 (1 .. 8) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"SFP Number - Optical Port number."
	::=  {  fwuHstSERFOptEntry  1  }

fwuHstSERFOptSFPName OBJECT-TYPE	
	SYNTAX			DisplayString ( SIZE(0 .. 64) ) 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"User defined name of each Host SFP. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL  		{ "SFP" }
	::=  {  fwuHstSERFOptEntry  2  }

fwuHstSERFOptSFPType OBJECT-TYPE	
	SYNTAX			OPTICSType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"SFP Type - Optical Port Type. Identifies whether the SFP is short(1)/intermediate(2)/long(3) range. It may also be unidentified (-1)."
	::=  {  fwuHstSERFOptEntry  3  }

fwuHstSERFOptSFPTxColor	OBJECT-TYPE	
	SYNTAX			Integer32
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Wavelength of light used in the SFP (in nanometers)."
	::=  {  fwuHstSERFOptEntry  4  }

fwuHstSERFOptRevLaunchPowerMeas	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Value of the optical power, in dBm received at the host."
	::=  {  fwuHstSERFOptEntry  5 }

fwuHstSERFOptFwdLaunchPowerMeas	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Value of the optical power, in dBm transmitted by the host."
	::=  {  fwuHstSERFOptEntry  6  }

fwuHstSERFOptMateName	OBJECT-TYPE
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Name of Host Mates."
	::=  {  fwuHstSERFOptEntry  7  }

