fwuNotifObjects      OBJECT IDENTIFIER  ::=  {  fwuGeneralObjects  1  }

fwuTrapSequenceNumber   OBJECT-TYPE
   SYNTAX         Counter32
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "Count of the total number of traps (including this trap), raised by the agent."
   ::=  {  fwuNotifObjects  1  }

fwuTrapTimeStamp  OBJECT-TYPE
   SYNTAX         DateAndTime
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "The trap time stamp."
   ::=  {  fwuNotifObjects  2  }

fwuNotificationStatus   OBJECT-TYPE
   SYNTAX         ALARMType
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "The severity of the fault."
   ::=  {  fwuNotifObjects  3  }

fwuModuleType  OBJECT-TYPE
   SYNTAX         MODULEType
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "The module type reporting alarm."
   ::=  {  fwuNotifObjects  4  }

fwuModuleNumber   OBJECT-TYPE
   SYNTAX         Integer32
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "The numerical Id of module, for the above module type, reporting the alarm."
   ::=  {  fwuNotifObjects  5  }

fwuTrapCode     OBJECT-TYPE
   SYNTAX         INTEGER 
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "The unique ID of the trap."
   ::= { fwuNotifObjects 6 }

fwuTrapNodeAddress   OBJECT-TYPE
   SYNTAX         Integer32 (1..2147483647)
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "Unit from which the trap originated."
   ::= { fwuNotifObjects 7 }

fwuAlarmedModuleName  OBJECT-TYPE
   SYNTAX         DisplayString 
   MAX-ACCESS     accessible-for-notify
   STATUS         current
   DESCRIPTION    "Name of the hardware module where the trap originated."
   ::= { fwuNotifObjects 8 }

