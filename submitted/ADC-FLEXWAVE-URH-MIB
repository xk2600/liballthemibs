fwuSnmpSettings OBJECT IDENTIFIER::= {fwuGeneralObjects 5}	
-- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
fwuV1TrapAgentAddrOverrideMode OBJECT-TYPE
	SYNTAX 			INTEGER  { disable(0), enable(1) }
	MAX-ACCESS 		read-write
  	STATUS 			current
  	DESCRIPTION		"Provision to overide the default (1) or use default (0) agent address in v1 trap PDU. Use fwuV1TrapAgentAddrOverrideIPAddr to set the overriding agent address."
	DEFVAL 			{ 0 }
	::= { fwuSnmpSettings 1 }

fwuV1TrapAgentAddrOverrideIPAddr	OBJECT-TYPE
	SYNTAX 			OCTET STRING ( SIZE (0..16) )
	MAX-ACCESS 		read-write
	STATUS 			current
	DESCRIPTION		"The overriding agent IP address in v1 trap PDU. Use Internet standard dot-decimal notation. Enable its use by setting fwuV1TrapAgentAddrOverrideMode."
	::= { fwuSnmpSettings 2 }

fwuSnmpTrapResendInterval OBJECT-TYPE
  	SYNTAX 		Integer32 (0..600)
  	MAX-ACCESS 	read-write
  	STATUS 		current
  	DESCRIPTION	"Time, in seconds, to resend unacknowledged traps. The trap will not be
		resent if this is set to zero(0). Acknowledgement is not needed if resend is
		not required, therefore, no trap entry will be entered into fwuSystemActiveAlarmTable."
    DEFVAL          { 0 }	
	::= { fwuSnmpSettings 3 }

fwuSnmpTrapResendMaximum OBJECT-TYPE
	SYNTAX 		Integer32 (0..10)
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"Maximum number of times traps will be resent before removal if not acknowledged. If set to zero, it will not be resent."
    DEFVAL          { 2 }		
	::= { fwuSnmpSettings 4 }

