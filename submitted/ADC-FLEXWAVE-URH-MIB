-- In Rel 6.0 this attribute is removed, as GPS is never supported. 
-- While modification take care of this OID.
--fwuRmtGPSObjects		OBJECT IDENTIFIER		::=  {  fwuRemoteUnitObjects  7  }
--fwuRmtGPSInfo			OBJECT IDENTIFIER		::=  {  fwuRmtGPSObjects  1  }
--fwuRmtGPSTraps		OBJECT IDENTIFIER		::=  {  fwuRmtGPSObjects  2  }

fwuRmtLPAObjects		OBJECT IDENTIFIER		::=  {  fwuRemoteUnitObjects  8  }
fwuRmtLPAInfo			OBJECT IDENTIFIER		::=  {  fwuRmtLPAObjects  1  }

fwuRmtLPAInfoTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuRmtLPAInfoEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table maintains Information of remote LPA objects."
	::=  { fwuRmtLPAInfo  1 }

fwuRmtLPAInfoEntry	OBJECT-TYPE
	SYNTAX			FwuRmtLPAInfoEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Each entry in this table refers to LPA objects."
	INDEX			{  fwuRmtNumber, fwuRmtLPANumber  }
	::=  { fwuRmtLPAInfoTable 1 }

FwuRmtLPAInfoEntry  ::=  SEQUENCE {
	fwuRmtLPANumber  Integer32,
	fwuRmtLPAReset  INTEGER,
	fwuRmtLPAOpState  INTEGER,
	fwuRmtLPAPartNum DisplayString,
	fwuRmtLPASerialNum DisplayString,
	fwuRmtLPAHWVer DisplayString,
	fwuRmtLPASWVer DisplayString,
	fwuRmtLPADescr DisplayString,
	fwuRmtLPAControl INTEGER,
	fwuRmtLPAMaxOpPwr DisplayString,
	fwuRmtLPABandType DuplexerType }

fwuRmtLPANumber	OBJECT-TYPE	
	SYNTAX			Integer32  (1 .. 4) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A unique identifier for an LPA."
