--	Added Clock Priority Level Object for Multi Host URH
fwuHstSysCardCPLevel	OBJECT-TYPE	
	SYNTAX			Integer32   (1 .. 14) 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"System Card Master Clock Priority Level prioritizes the system card clock using values 1 through 14 (Lower value is higher priority). This is primarily used in multihost systems. In such system, all hosts should have different priority levels, and the system inherently assures this. This entity allows the user to change the system set prioritization. Changeing values in a single host system has no change."
	::=  {  fwuHstSystemCardInfo  6  }

fwuHstSysCardOutputRefClock		OBJECT-TYPE	
	SYNTAX			INTEGER  { off (0), clock10mhz(1) , clock30mhz(2) } 
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"System Card Reference clock. System Board I supports only 10 MHz and 
					System Board II supports both 10 MHz and 30 MHz Clock output. Default value is 1 for Host and 2 for DRU."
	DEFVAL 			{ 1 }
	::=  {  fwuHstSystemCardInfo  7  }

fwuHstSysCardRIHWGen OBJECT-TYPE	
	SYNTAX			INTEGER  { firstGeneration(0), secondGeneration(1) }
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Host System Card Hardware Generation. Presently, there are two generations; first Generation(0), second Generation(1)"
	::=  {  fwuHstSystemCardInfo  8  }
	
