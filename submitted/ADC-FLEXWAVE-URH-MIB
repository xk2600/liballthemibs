	fwuRmtSERFOptMateId		UnitID2,
	fwuRmtSERFOptMateSfpId	Integer32,	
	fwuRmtSERFOptMateSfpDir	INTEGER,
        fwuRmtSERFOptHostIP     DisplayString } 

fwuRmtSERFOptSFPNumber	OBJECT-TYPE	
	SYNTAX			Integer32  (1 .. 8) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A unique identifier of each SFP. The identifier is the SFP slot number. There are eight slots. Therefore, the valid values are 1 to 8."
	::=  {  fwuRmtSERFOptEntry  1  }

fwuRmtSERFOptSFPName	OBJECT-TYPE	
	SYNTAX			DisplayString (SIZE(0..64))
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"User defined name of each Remote SFP. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL  		{ "SFP" }
	::=  {  fwuRmtSERFOptEntry  2  }

