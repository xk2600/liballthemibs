fwuHstSERFEthPortObjects	OBJECT IDENTIFIER	::=  {  fwuHostUnitObjects  3  }
fwuHstSERFEthPortInfo		OBJECT IDENTIFIER	::=  {  fwuHstSERFEthPortObjects  1  }

fwuHstSERFEthPortTable	OBJECT-TYPE
	SYNTAX			SEQUENCE  OF  FwuHstSERFEthPortEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"This table maintains information of Host Ethernet Ports."
	::=  { fwuHstSERFEthPortInfo  1 }

fwuHstSERFEthPortEntry	OBJECT-TYPE
	SYNTAX			FwuHstSERFEthPortEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION		"Each entry in this table refers to Host SERF Ethernet port objects."
	INDEX			{  fwuHstSERFEthPortNumber  }
	::=  { fwuHstSERFEthPortTable 1 }

FwuHstSERFEthPortEntry  ::=  SEQUENCE {
	fwuHstSERFEthPortNumber  Unsigned32,
	fwuHstSERFEthPortType  ENETType,
	fwuHstSERFEthPortRxBytes  Counter64,
	fwuHstSERFEthPortRxPkts  Counter64,
	fwuHstSERFEthPortRxFcsErrors  Counter64,
	fwuHstSERFEthPortRxBroadcastPkts  Counter64,
	fwuHstSERFEthPortRxMulticastPkts  Counter64,
	fwuHstSERFEthPortRxFragmtdFrames  Counter64,
	fwuHstSERFEthPortRxJabbersFrames  Counter64,
	fwuHstSERFEthPortTxByteCounter  Counter64,
	fwuHstSERFEthPortTxPkts  Counter64,
	fwuHstSERFEthPortTxBroadcastPkts  Counter64,
	fwuHstSERFEthPortTxMulticastPkts  Counter64,
	fwuHstSERFEthPortSFPId  Integer32	}

fwuHstSERFEthPortNumber	OBJECT-TYPE	
	SYNTAX			Unsigned32  (1 .. 12) 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"A unique identifier for each Ethernet port."
	::=  {  fwuHstSERFEthPortEntry  1  }

fwuHstSERFEthPortType	OBJECT-TYPE	
	SYNTAX			ENETType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"An identifier for the type of Ethernet port. The port types are network(1), craft(2) , management(3) , switch(4) , SFP(5). It may be unknown (-1)."
	::=  {  fwuHstSERFEthPortEntry  2  }

fwuHstSERFEthPortRxBytes	OBJECT-TYPE	
	SYNTAX			Counter64
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Count of the number of recieved bytes at this ethernet port."
	::=  {  fwuHstSERFEthPortEntry  3  }

fwuHstSERFEthPortRxPkts	OBJECT-TYPE	
