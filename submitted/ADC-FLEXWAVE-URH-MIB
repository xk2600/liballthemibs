fwuHstGeneral				OBJECT IDENTIFIER		::=  { fwuHstSystemLevelInfo  1 }
	-- .1.1.1.1 
fwuHstNumber		OBJECT-TYPE
	SYNTAX			INTEGER (1)
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Host Number."
	DEFVAL			{ 1 }
	::=  {  fwuHstGeneral  1  }
	
	-- .1.1.1.2
fwuHstName			OBJECT-TYPE	
	SYNTAX			DisplayString  (SIZE(0 .. 64))
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"User defined name of the host unit. All characters except angular brackets, single and double quotes are allowed. Minimum length of the name is 1 character. Maximum length of the name 64 characters."
	DEFVAL  		{ "Host" }
	::=  {  fwuHstGeneral  2  }
	
	-- .1.1.1.3 
fwuHstUnitReset		OBJECT-TYPE
	SYNTAX			INTEGER { normal(0), reset(1) }
	MAX-ACCESS		read-write
	STATUS			current
	DESCRIPTION		"Use to reset the Host. Do this by changing from normal(0) to reset(1). After the operation has completed the state automatically changes to normal."
	DEFVAL  		{ 0 }
	::=  {  fwuHstGeneral  3  }
	
	-- .1.1.1.4
fwuHstBackPlaneRev	OBJECT-TYPE	
	SYNTAX			DisplayString
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"The back plane revision of the Host (It is the three pin version number of the chassis back plane)."
	::=  {  fwuHstGeneral  4  }

	-- .1.1.1.5
fwuHstAlarmStatusSummary	OBJECT-TYPE	
	SYNTAX			ALARMType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Summary of alarm status on the Host."
	::=  {  fwuHstGeneral  5  }
	
	-- .1.1.1.6
fwuHstTempMeas		OBJECT-TYPE	
	SYNTAX			DisplayString 
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Host system temperature value in Celsius."
	::=  {  fwuHstGeneral  6  }
	
	-- .1.1.1.7
fwuSystemAlarmStatusSummary		OBJECT-TYPE	
	SYNTAX			ALARMType
	MAX-ACCESS		read-only
	STATUS			current
	DESCRIPTION		"Summary of alarm status on the System. If there a unit in the system (Host and all remotes) with a fault condition, it will be reflected here. Severity is summarised by the highest severity of the alarms in the system. i.e. If there is at least one Major alarm, the summary will indicate Major Alarm. The severities supported are Major, Minor and OK."
	::=  {  fwuHstGeneral  7  }
	
