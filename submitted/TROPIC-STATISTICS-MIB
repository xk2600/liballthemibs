            tnOsnrStatsStartTime     DateAndTime,
            tnOsnrStatMinOSNR        Integer32,
            tnOsnrStatMaxOSNR        Integer32,
            tnOsnrStatAverageOSNR    Integer32,
            tnOsnrStatStatusOSNR     INTEGER
        }

