        SYNTAX      INTEGER {
                      update(1),
                      noUpdate(2)
                    }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "OSNR updating Status."
        ::= { tnOsnrStatsEntry 7 }

