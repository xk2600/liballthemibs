		WmadPrivacyEntry ::=
			SEQUENCE { 
				wmadPrivWEPKeyType
					INTEGER,
				wmadPrivDynamicRekeyFrequency
					Integer32,
				wmadPrivWEPKeyLength
					Integer32,
				wmadPrivWPA1Enabled
					TruthValue,
				wmadPrivUseSharedKey
					TruthValue,
				wmadPrivWPASharedKey
					DisplayString,
				wmadPrivWPA2Enabled
					TruthValue
			 }

		-- 1.3.6.1.4.1.1916.5.3.3.1.8.1.1
		wmadPrivWEPKeyType OBJECT-TYPE
			SYNTAX INTEGER
				{
				none(1),
				wepstatic(2),
				wpapsk(3),
				dynamic(4),
				wpa(5)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Type of key in use. 
				none(1) = not cofigured, 
				wepstatic(2) = static WEP, 
				wpapsk(3) = WPA Pre-Shared Key,
				dynamic(4) = dynamically assigned,
				wpa(5) = WPA."
			::= { wmadPrivacyEntry 1 }

		
		-- 1.3.6.1.4.1.1916.5.3.3.1.8.1.2
		wmadPrivDynamicRekeyFrequency OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Dynamic WEP re-keying frequency, in seconds.  Setting this 
				value to 0 disables rekeying.
				
				This value is only meaningful if wmadPrivWEPKeyType == dynamic.
				For any other values of wmadPrivWEPKeyType, reading or setting 
				this value will return an unsuccessful status and will return 
				a value of null or zero."
			::= { wmadPrivacyEntry 2 }

		
		-- 1.3.6.1.4.1.1916.5.3.3.1.8.1.3
		wmadPrivWEPKeyLength OBJECT-TYPE
			SYNTAX Integer32
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"WEP key length, either 40 or 104 bit.  If wmadPrivWEPKeyType
				is none, reading or setting this value will return an 
				unsuccessful status and will return a value of null or zero."
			::= { wmadPrivacyEntry 3 }

		
		-- 1.3.6.1.4.1.1916.5.3.3.1.8.1.4
