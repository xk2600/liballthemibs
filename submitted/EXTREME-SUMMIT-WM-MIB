			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Contains the component which sent the most
				recently trapped summitWMLogAlarm notification."
			::= { logNotifications 4 }

		
		-- 1.3.6.1.4.1.1916.5.3.9.5
		logEventDescription OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the description of the most recently trapped 
				summitWMLogAlarm."
			::= { logNotifications 5 }

		
		-- 1.3.6.1.4.1.1916.5.3.9.6
