			STATUS current
			DESCRIPTION 
				"Components of an alarm."
			::= { logNotifications 6 }

		
		-- 1.3.6.1.4.1.1916.5.3.19
		altitudeNotifications OBJECT IDENTIFIER ::= { summitWMMgmt 19 }

		
		-- 1.3.6.1.4.1.1916.5.3.19.1
		altitudeEventId OBJECT-TYPE
			SYNTAX INTEGER { altitudePollTimeout(1) }
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Identifies event associated with Altitude or Altitude tunnel:
				
				altitudePollTimeout - an event triggered when Altitude disconnects from WM"
			::= { altitudeNotifications 1 }

		
		-- 1.3.6.1.4.1.1916.5.3.19.2
		altitudeEventDescription OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Contains the description of the most recently triggered event."
			::= { altitudeNotifications 2 }

		
		-- 1.3.6.1.4.1.1916.5.3.19.3
		altitudeEventSerialNumber OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (16))
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"16-character serial number of the Altitude."
			::= { altitudeNotifications 3 }

		
		-- 1.3.6.1.4.1.1916.5.3.19.4
		altitudeTunnelAlarm NOTIFICATION-TYPE
			OBJECTS { altitudeEventId, altitudeEventDescription, altitudeEventSerialNumber }
			STATUS current
			DESCRIPTION 
				"alarm associated with Altitude and Altitude interface."
			::= { altitudeNotifications 4 }

		
		-- 1.3.6.1.4.1.1916.5.3.30
		summitWMConformance OBJECT IDENTIFIER ::= { summitWMMgmt 30 }

		
--  this module
		-- 1.3.6.1.4.1.1916.5.3.30.1
		summitWMModule MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"Conformance definition for the WM MIB."
			MODULE -- this module
				MANDATORY-GROUPS { summitWMGroup }
			::= { summitWMConformance 1 }

		
		-- 1.3.6.1.4.1.1916.5.3.30.2
		summitWMGroup OBJECT-GROUP
			OBJECTS { sysSoftwareVersion, sysLogLevel, logEventSeverityThreshold, logEventSeverity, logEventComponent, 
				logEventDescription, altitudeEventId, altitudeEventDescription, altitudeEventSerialNumber, assocTxPackets, 
				assocRxPackets, assocTxOctets, assocRxOctets, assocDuration, assocCount, 
				assocMUMacAddress, assocStartSysUpTime, muTxPackets, muRxPackets, muTxOctets, 
				muRxOctets, muDuration, mobileUnitCount, muMACAddress, wmadIfIndex, 
				radioWMADRowStatus, altitudeSpecific, altitudeRowStatus, radioIfIndex, altitudeDesc, 
				altitudeSerialNumber, altitudePortifIndex, wmadFilterRulePortLow, wmadFilterRulePortHigh, altitudeInUcastPkts, 
				altitudeInNUcastPkts, altitudeInOctets, altitudeInErrors, altitudeInDiscards, altitudeOutUcastPkts, 
				altitudeOutNUcastPkts, altitudeOutOctets, altitudeOutErrors, altitudeOutDiscards, cpPasswordLabel, 
				cpLoginLabel, cpURL, wmadDHCPRangeIndex, wmad3rdPartyAP, wmadUseDHCPRelay, 
				wmadMgmtTrafficEnable, wmadAuthModel, physicalPortCount, altitudeSoftwareVersion, mgmtPortIfIndex, 
				mgmtPortHostname, mgmtPortPrimaryDNS, mgmtPortSecondaryDNS, mgmtPortDomain, portMgmtTrafficEnable, 
				portDuplexMode, portFunction, adRole, adIfIndex, adHeartBeatInterval, 
				ntpEnabled, ntpTimezone, ntpTimeServer1, ntpTimeServer2, ntpTimeServer3, 
				tunnelCount, wmadCount, wmadDescription, wmadAssignmentMode, wmadMUSessionTimeout, 
				wmadAllowMulticast, wmadSSID, wmadDomain, wmadDNSServers, wmadWINSServers, 
				wmadParentIfIndex, wmadStatus, wmadDHCPRangeStart, wmadDHCPRangeEnd, wmadDHCPRangeType, 
				wmadDHCPRangeStatus, wmadRadiusServerIP, wmadRadiusServerPort, wmadRadiusServerRetries, wmadRadiusServerTimeout, 
				wmadRadiusServerSharedSecret, wmadRadiusServerNASIdentifier, wmadRadiusServerAuthType, wmadRadiusServerRowStatus, wmadFilterID, 
				wmadFilterIDStatus, wmadFilterRuleOrder, wmadFilterRuleDirection, wmadFilterRuleAction, wmadFilterRuleIPAddress, 
				wmadFilterRuleProtocol, wmadFilterRuleEtherType, wmadFilterRuleStatus, wmadPrivWEPKeyType, wmadPrivDynamicRekeyFrequency, 
				wmadPrivWEPKeyLength, wmadPrivWPA1Enabled, wmadPrivUseSharedKey, wmadPrivWPASharedKey, wmadWEPKeyIndex, 
				wmadWEPKeyValue, activeWMADSessionCount, altitudeCount, altitudeIndex, cpHeaderURL, 
				cpFooterURL, cpMessage, apMacAddress, apIpAddress, summitWMAppLogFacility, 
				serviceLogFacility, sysLogServerIP, sysLogServerPort, sysLogServerRowStatus, sysLogServerIndex, 
				altitudeActiveCount, altitudeWiredIfIndex, sysLogServerEnabled, altitudeUpTime, assocWmadIfIndex, 
				muIPAddress, muUser, muState, muAltitudeSerialNo, muWmadSSID, 
				altitudeBroadcastDisassociate, altitudeName, sysSerialNo, wmadPrivWPA2Enabled, 
				altitudeRadioFrequency, includeAllServiceMessages, 
				tunnelStartIP, tunnelStartWM, tunnelEndIP, tunnelEndWM, tunnelStatus
				 }
			STATUS current
			DESCRIPTION 
				"Conformance groups."
			::= { summitWMConformance 2 }

		
		-- 1.3.6.1.4.1.1916.5.3.30.3
		summitWMAlarms NOTIFICATION-GROUP
			NOTIFICATIONS { summitWMLogAlarm, altitudeTunnelAlarm }
			STATUS current
			DESCRIPTION 
				"Conformance information for the alarm groups."
			::= { summitWMConformance 3 }

		
	
	END

