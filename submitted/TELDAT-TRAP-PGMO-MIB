TELDAT-TRAP-PGMO-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


pgmoTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the PGMO event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 158 }


pgmoEventsGroup	OBJECT IDENTIFIER	::= { pgmoTrapsMIB 1 }

pgmoNotifications	OBJECT IDENTIFIER	::= { pgmoTrapsMIB 2 }
pgmoNotificationsPrefix	OBJECT IDENTIFIER	::= { pgmoNotifications 0 }

pgmoEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event PGMO.1
        POS Gateway don't answer to monitoring ping."
    ::= { pgmoEventsGroup 1 }

pgmo1_virtual_ip OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'virtual_ip' in event PGMO.1"
    ::= { pgmoEventObjects1 1 }

