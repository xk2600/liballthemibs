TELDAT-TRAP-WWAN-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


wwanTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the WWAN event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 211 }


wwanEventsGroup	OBJECT IDENTIFIER	::= { wwanTrapsMIB 1 }

wwanNotifications	OBJECT IDENTIFIER	::= { wwanTrapsMIB 2 }
wwanNotificationsPrefix	OBJECT IDENTIFIER	::= { wwanNotifications 0 }

wwanEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event WWAN.1
        Generic informational event."
    ::= { wwanEventsGroup 1 }

wwan1_info_string OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS accessible-for-notify
    STATUS current
    DESCRIPTION
        "Parameter 'info_string' in event WWAN.1"
    ::= { wwanEventObjects1 1 }

