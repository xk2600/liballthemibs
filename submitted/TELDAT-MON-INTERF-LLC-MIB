    -- Tabla de informacin por sesiones

    telProdNpMonInterfLLCListSessTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TelProdNpMonInterfLLCListSessEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "Displays information for the specific LLC session opened on the
        interface."
        ::={ telProdNpMonInterfLLC 4 }

     telProdNpMonInterfLLCListSessEntry OBJECT-TYPE
        SYNTAX  TelProdNpMonInterfLLCListSessEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "The table index is by order of session appearance."
        INDEX { telProdNpMonInterfLLCListSessInd }
        ::={ telProdNpMonInterfLLCListSessTable 1 }

     TelProdNpMonInterfLLCListSessEntry ::=
        SEQUENCE {
            telProdNpMonInterfLLCListSessInd
                INTEGER,
            telProdNpMonInterfLLCListSessIdSess
                DisplayString,
            telProdNpMonInterfLLCListSessRemoteMAC
                DisplayString,
            telProdNpMonInterfLLCListSessSourceMAC
                DisplayString,
            telProdNpMonInterfLLCListSessRemoteSAP
                INTEGER,
            telProdNpMonInterfLLCListSessLocalSAP
                INTEGER,
            telProdNpMonInterfLLCListSessRIF
                DisplayString,
            telProdNpMonInterfLLCListSessPriority
                INTEGER,
            telProdNpMonInterfLLCListSessState
                INTEGER,
            telProdNpMonInterfLLCListSessT1
                INTEGER,
            telProdNpMonInterfLLCListSessT2
                INTEGER,
            telProdNpMonInterfLLCListSessTi
                INTEGER,
            telProdNpMonInterfLLCListSessN1
                INTEGER,
            telProdNpMonInterfLLCListSessN2
                INTEGER,
            telProdNpMonInterfLLCListSessN3
                INTEGER,
            telProdNpMonInterfLLCListSessTw
                INTEGER,
            telProdNpMonInterfLLCListSessWw
                INTEGER,
            telProdNpMonInterfLLCListSessNw
                INTEGER,
            telProdNpMonInterfLLCListSessVs
                INTEGER,
            telProdNpMonInterfLLCListSessVr
                INTEGER,
            telProdNpMonInterfLLCListSessVa
                INTEGER,
            telProdNpMonInterfLLCListSessACKpend
                INTEGER,
            telProdNpMonInterfLLCListSessTXpend
                INTEGER,
            telProdNpMonInterfLLCListSessLocalBusy
                INTEGER,
            telProdNpMonInterfLLCListSessRemoteBusy
                INTEGER,
            telProdNpMonInterfLLCListSessRetryCount
                INTEGER,
            telProdNpMonInterfLLCListSessApOuFlStop
                INTEGER,
            telProdNpMonInterfLLCListSessSeProRunning
                INTEGER,
            telProdNpMonInterfLLCListSessXmtI
                Counter,
            telProdNpMonInterfLLCListSessRcvdI
                Counter,
            telProdNpMonInterfLLCListSessXmtRR
                Counter,
            telProdNpMonInterfLLCListSessRcvdRR
                Counter,
            telProdNpMonInterfLLCListSessXmtRNR
                Counter,
            telProdNpMonInterfLLCListSessRcvdRNR
                Counter,
            telProdNpMonInterfLLCListSessXmtREJ
                Counter,
            telProdNpMonInterfLLCListSessRcvdREJ
                Counter,
            telProdNpMonInterfLLCListSessDiscarded
                Counter,
            telProdNpMonInterfLLCListSessRefused
                Counter
            }

    telProdNpMonInterfLLCListSessInd OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Entry number. Table index."
        ::={ telProdNpMonInterfLLCListSessEntry 1 }

