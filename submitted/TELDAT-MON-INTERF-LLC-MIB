TELDAT-MON-INTERF-LLC-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        Counter
            FROM RFC1155-SMI
        OBJECT-TYPE
            FROM RFC-1212
        DisplayString
            FROM RFC1213-MIB
        telProdNpMonInterfRouter
            FROM TELDAT-SW-STRUCTURE-MIB;


    -- Grupo telProdNpMonInterfLLC

    telProdNpMonInterfLLC   OBJECT IDENTIFIER ::= { telProdNpMonInterfRouter 8 }

    -- Tabla de borrado de contadores

    telProdNpMonInterfLLCClearCountersTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TelProdNpMonInterfLLCClearCountersEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "This table allows to delete statistics of a LLC interfaz."
        ::={ telProdNpMonInterfLLC 1 }

    telProdNpMonInterfLLCClearCountersEntry OBJECT-TYPE
        SYNTAX  TelProdNpMonInterfLLCClearCountersEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        ""
        INDEX { telProdNpMonInterfLLCClearCountersIfc }
        ::={ telProdNpMonInterfLLCClearCountersTable 1 }

    TelProdNpMonInterfLLCClearCountersEntry ::=
        SEQUENCE {
            telProdNpMonInterfLLCClearCountersIfc
                INTEGER,
            telProdNpMonInterfLLCClearCountersCmd
                INTEGER
            }

    telProdNpMonInterfLLCClearCountersIfc OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Interface number. Table index"
        ::={ telProdNpMonInterfLLCClearCountersEntry 1 }

    telProdNpMonInterfLLCClearCountersCmd OBJECT-TYPE
        SYNTAX  INTEGER{
            delete(1),
            ok(2),
            error(3),
            undefined(4)
            }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "Deletes the counters containing LLC statistics. In case the delete
        operation were correct, the variable will be ok(2). Otherwise,
        the variable will be error(3)."
        ::={ telProdNpMonInterfLLCClearCountersEntry 2 }

    -- Tabla de informacin de SAPs y sesiones abiertas por interfaz

    telProdNpMonInterfLLCListInterfTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TelProdNpMonInterfLLCListInterfEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "Displays all SAPs opened on this interface, providing also the
        number of open sessions on each interface."
        ::={ telProdNpMonInterfLLC 2 }

    telProdNpMonInterfLLCListInterfEntry OBJECT-TYPE
        SYNTAX  TelProdNpMonInterfLLCListInterfEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "The table index are: Interface and SAP number."
        INDEX { telProdNpMonInterfLLCListInterfNumIfc,telProdNpMonInterfLLCListInterfNumSAP }
        ::={ telProdNpMonInterfLLCListInterfTable 1 }

    TelProdNpMonInterfLLCListInterfEntry ::=
        SEQUENCE {
            telProdNpMonInterfLLCListInterfNumIfc
                INTEGER,
            telProdNpMonInterfLLCListInterfNumSAP
                INTEGER,
            telProdNpMonInterfLLCListInterfNumSess
                INTEGER
            }

    telProdNpMonInterfLLCListInterfNumIfc OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Interface number. First table index."
        ::={ telProdNpMonInterfLLCListInterfEntry 1 }

    telProdNpMonInterfLLCListInterfNumSAP OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "SAP Number. Second table index."
        ::={ telProdNpMonInterfLLCListInterfEntry 2 }

    telProdNpMonInterfLLCListInterfNumSess OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Number of active sessions."
        ::={ telProdNpMonInterfLLCListInterfEntry 3 }

    -- Tabla de informacin por SAP

    telProdNpMonInterfLLCListSAPTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TelProdNpMonInterfLLCListSAPEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "Display information for the specified SAP on the interface."
        ::={ telProdNpMonInterfLLC 3 }

    telProdNpMonInterfLLCListSAPEntry OBJECT-TYPE
        SYNTAX  TelProdNpMonInterfLLCListSAPEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
