                INTEGER,
            telProdNpMonInterfLLCListSAPNumActivSess
                INTEGER,
            telProdNpMonInterfLLCListSAPIdSess
                DisplayString
            }

    telProdNpMonInterfLLCListSAPNumIfc OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Interface number. First table index."
        ::={ telProdNpMonInterfLLCListSAPEntry 1 }

    telProdNpMonInterfLLCListSAPNumSAP OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "SAP number. Second table index."
        ::={ telProdNpMonInterfLLCListSAPEntry 2 }

    telProdNpMonInterfLLCListSAPTypeIfc OBJECT-TYPE
        SYNTAX  INTEGER{
            vi(0),
            pn(1),
            pri(2),
            xeth(3),
            arpa(4),
            chp(5),
            astm(6),
            eth(7),
            sl(8),
            x28(9),
            dmr(10),
            tkr(11),
            snk(12),
            x25(13),
            man(14),
            atr(15),
            fddi(16),
            srly(17),
            ippn(18),
            fr(19),
            ppp(20),
            bdg(21),
            null(22),
            gwt_ntype_b_isdn(23),
            sdlc(24),
            v25b(25),
            r_n(26),
            n_r(27),
            rdsid(28),
            xot(29),
            a270(30),
            tnip(31),
            mppp(32),
            atm(33),
            subatm(34)
            }
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Interface type.
        VI (0)                  VI LNI
        PN (1)                  ProNET
        PRI (2)                 PRI IFC
        XETH (3)                Experimental EtherNet (3 MBit)
        ARPA (4)                ARPANet
        CHP (5)                 CHAOS protocol net
        ASTM (6)                ASTM line interface
        Eth (7)                 EtherNet (10 MBit)
        SL (8)                  New serial line link
        X28 (9)                 X.28
        DMR (10)                DEC DMR interface
        TKR (11)                802.5 token ring
        SNK (12)                Sink network (not a real network)
        X25 (13)                X25 network
        MAN (14)                802.6 metro area network
        ATR (15)                Apollo Token Ring
        FDDI (16)               FDDI
        SRLY (17)               SDLC relay int
        IPPN (18)               IP Protocol Net
        FR (19)                 Frame Relay network
        PPP (20)                PPP network
        BDG (21)                Bridging pseudo-handler
        NULL (22)               Null device
        GWT_NTYPE_B_ISDN (23)   ISDN network channel B
        SDLC (24)               TRUE SDLC int
        V25B (25)               V.25bis base network
        R->N (26)               Teldat Micro-node
        N->R (27)               Teldat Router
        RDSID (28)              ISDN D Channel
        XOT (29)
        270 (30)
        TNIP (31)               Interface real IP tunnel
        MPPP (32)               Interface Multilink PPP
        ATM (33)                Interface ATM
        SUBATM (34)             Subinterface ATM."
        ::={ telProdNpMonInterfLLCListSAPEntry 3 }

    telProdNpMonInterfLLCListSAPT1 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Time it takes for this timer to expire when the LLC fails to
        receive an acknowledgment or response from the other LLC
        station. In case it were 0, it woeld not be active."
        ::={ telProdNpMonInterfLLCListSAPEntry 4 }

    telProdNpMonInterfLLCListSAPT2 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Time delay the LLC uses before sending an ACK for a received
        I-frame. In case it were 0, it would not be active."
        ::={ telProdNpMonInterfLLCListSAPEntry 5 }

    telProdNpMonInterfLLCListSAPTi OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Time the LLC waits during inactivity before issuing an RR.
        In case it were 0, it would not be active."
        ::={ telProdNpMonInterfLLCListSAPEntry 6 }

    telProdNpMonInterfLLCListSAPN2 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Maximum number of retries by the LLC protocol."
        ::={ telProdNpMonInterfLLCListSAPEntry 7 }

    telProdNpMonInterfLLCListSAPN1 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Data (in bytes) allowed in the I-field of an LLC2 frame."
        ::={ telProdNpMonInterfLLCListSAPEntry 8 }

    telProdNpMonInterfLLCListSAPN3 OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Value that is used with T2 timer to reduce acknowledgment traffic
        for received I-frames."
        ::={ telProdNpMonInterfLLCListSAPEntry 9 }

    telProdNpMonInterfLLCListSAPTw OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "I-frames that can be sent before receiving an RR."
        ::={ telProdNpMonInterfLLCListSAPEntry 10 }

    telProdNpMonInterfLLCListSAPNw OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "I-frames that the LLC must receive before incrementing Ww by 1."
        ::={ telProdNpMonInterfLLCListSAPEntry 11 }

    telProdNpMonInterfLLCListSAPXmtUI OBJECT-TYPE
