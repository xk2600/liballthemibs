    -- Tabla de set de parametros para protocolo LLC

    telProdNpMonInterfLLCSetTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TelProdNpMonInterfLLCSetEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        "Dynamically configures the LLC parameters on a current LLC session.
        Changes that you make to the parameters are effective for the life
        of session."
        ::={ telProdNpMonInterfLLC 5 }

    telProdNpMonInterfLLCSetEntry OBJECT-TYPE
        SYNTAX  TelProdNpMonInterfLLCSetEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
        ""
        INDEX { telProdNpMonInterfLLCSetInd}
        ::={ telProdNpMonInterfLLCSetTable 1 }

    TelProdNpMonInterfLLCSetEntry ::=
        SEQUENCE {
            telProdNpMonInterfLLCSetInd
                INTEGER,
            telProdNpMonInterfLLCSetIdSess
                DisplayString,
            telProdNpMonInterfLLCSetN2
                INTEGER,
            telProdNpMonInterfLLCSetN3
                INTEGER,
            telProdNpMonInterfLLCSetNw
                INTEGER,
            telProdNpMonInterfLLCSetT1
                INTEGER,
            telProdNpMonInterfLLCSetT2
                INTEGER,
            telProdNpMonInterfLLCSetTi
                INTEGER,
            telProdNpMonInterfLLCSetTw
                INTEGER
            }

    telProdNpMonInterfLLCSetInd OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Entry number. It is the index."
        ::={ telProdNpMonInterfLLCSetEntry 1 }

    telProdNpMonInterfLLCSetIdSess OBJECT-TYPE
        SYNTAX  DisplayString(SIZE(0..10))
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
        "Session identifier."
        ::={ telProdNpMonInterfLLCSetEntry 2 }

    telProdNpMonInterfLLCSetN2 OBJECT-TYPE
        SYNTAX  INTEGER(1..127)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "The maximum number of times the LLC protocol transmits an RR without
        receiving an acknowledgment when the inactivity timer expires. The range
        is 1 to 127. Default is 8."
        DEFVAL { 8 }
        ::={ telProdNpMonInterfLLCSetEntry 3 }

    telProdNpMonInterfLLCSetN3 OBJECT-TYPE
        SYNTAX  INTEGER(1..255)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "This value is used with the T2 timer to reduce acknowledgment traffic
        for received I-frames. Set this counter to a specified value. Each time
        an I-frame is received, this value is decremented. When this counter
        reaches 0, or the T2 timer expires, an acknowledgment is sent. The
        range is 1 to 255. Default is 1."
        DEFVAL { 1 }
        ::={ telProdNpMonInterfLLCSetEntry 4 }

    telProdNpMonInterfLLCSetNw OBJECT-TYPE
        SYNTAX  INTEGER(1..127)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "When the ability to send I-frames is not working, the LLC protocol goes
        into a mode where the working window (Ww) is set back to 1, and is
        then slowly increased back to its normal size (Tw). This is known as the
        dynamic window algorithm. This value is the number of I-frames that the
        LLC must receive before incrementing Ww by 1. The  range is 1 to 127.
        Default is 1."
        DEFVAL { 1 }
        ::={ telProdNpMonInterfLLCSetEntry 5 }

    telProdNpMonInterfLLCSetT1 OBJECT-TYPE
        SYNTAX  INTEGER(1..256)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "This timer expires when the LLC fails to receive a required
        acknowledgment or response from the other LLC station. When this timer
        expires, an RR is sent with the poll bit set and T1 is started again.
        If the LLC receives no response after the configured maximum number
        of retries (N2), the link underneath is declared inoperative. The range
        is 1 to 256.  Default is 1."
        DEFVAL { 1 }
        ::={ telProdNpMonInterfLLCSetEntry 6 }

    telProdNpMonInterfLLCSetT2 OBJECT-TYPE
        SYNTAX  INTEGER(1..2560)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "This timer is used to delay sending of an acknowledgment for a
        received I-format frame. This timer is started when an I-frame is
        received and reset when an acknowledgment is sent. If this timer
        expires, LLC2 sends an acknowledgment as soon as possible. Set
        this value so that it is less than that of T1.  This insures that the
        remote LLC2 peer receives the delayed acknowledgment before the
        T1 timer expires. The range is 1 to 2,560. Default is 1 (100 ms)."
        DEFVAL { 1 }
        ::={ telProdNpMonInterfLLCSetEntry 7 }

    telProdNpMonInterfLLCSetTi OBJECT-TYPE
        SYNTAX  INTEGER(1..256)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "This timer expires when the LLC does not receive a frame for a
        specified time period.  When this timer expires the LLC transmits
        an RR until the other LLC responds or the N2 timer expires. The
        range is 1 to 256. Default is 30 seconds."
        DEFVAL { 30 }
        ::={ telProdNpMonInterfLLCSetEntry 8 }

    telProdNpMonInterfLLCSetTw OBJECT-TYPE
        SYNTAX  INTEGER(1..127)
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
        "Sets the maximum number of I-frames that can be sent before
        receiving an RR. Assuming that the other end of the LLC session
        can actually receive this many consecutive I-frames, and the router
        has enough heap memory to keep copies of these frames until an
        acknowledgment is received, increasing this value may increase the
        throughput. The range is 1 to 127. Default is 2."
        DEFVAL { 2 }
        ::={ telProdNpMonInterfLLCSetEntry 9 }

END
