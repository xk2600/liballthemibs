                INTEGER,
            telProdNpMonInterfLLCListSAPT2
                INTEGER,
            telProdNpMonInterfLLCListSAPTi
                INTEGER,
            telProdNpMonInterfLLCListSAPN2
                INTEGER,
            telProdNpMonInterfLLCListSAPN1
                INTEGER,
            telProdNpMonInterfLLCListSAPN3
                INTEGER,
            telProdNpMonInterfLLCListSAPTw
                INTEGER,
            telProdNpMonInterfLLCListSAPNw
                INTEGER,
            telProdNpMonInterfLLCListSAPXmtUI
                Counter,
            telProdNpMonInterfLLCListSAPRcvdUI
                Counter,
            telProdNpMonInterfLLCListSAPXmtTEST
                Counter,
            telProdNpMonInterfLLCListSAPRcvdTEST
                Counter,
            telProdNpMonInterfLLCListSAPXmtXID
                Counter,
            telProdNpMonInterfLLCListSAPRcvdXID
                Counter,
            telProdNpMonInterfLLCListSAPXmtI
                Counter,
            telProdNpMonInterfLLCListSAPRcvdI
                Counter,
            telProdNpMonInterfLLCListSAPXmtRR
                Counter,
            telProdNpMonInterfLLCListSAPRcvdRR
                Counter,
            telProdNpMonInterfLLCListSAPXmtRNR
                Counter,
            telProdNpMonInterfLLCListSAPRcvdRNR
                Counter,
            telProdNpMonInterfLLCListSAPXmtREJ
                Counter,
            telProdNpMonInterfLLCListSAPRcvdREJ
                Counter,
            telProdNpMonInterfLLCListSAPXmtSABME
                Counter,
            telProdNpMonInterfLLCListSAPRcvdSABME
                Counter,
            telProdNpMonInterfLLCListSAPXmtUA
                Counter,
            telProdNpMonInterfLLCListSAPRcvdUA
                Counter,
            telProdNpMonInterfLLCListSAPXmtDISC
                Counter,
            telProdNpMonInterfLLCListSAPRcvdDISC
                Counter,
            telProdNpMonInterfLLCListSAPXmtDM
                Counter,
            telProdNpMonInterfLLCListSAPRcvdDM
                Counter,
            telProdNpMonInterfLLCListSAPXmtFRMR
                Counter,
            telProdNpMonInterfLLCListSAPRcvdFRMR
                Counter,
            telProdNpMonInterfLLCListSAPDiscarded
                Counter,
