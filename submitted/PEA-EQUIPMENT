PEA-EQUIPMENT DEFINITIONS ::= BEGIN 

IMPORTS
    pea                                                       
        FROM PEA-SMI 

    -- standar X.733 header for the traps       
    managedObjectClass, 
    managedObjectInstance, 
    sequenceNumber, 
    perceivedSeverity, 
    eventTime, 
    eventTypePem, 
    probableCause  
        FROM PEM-STANDARD 
    
    TruthValue,
    DisplayString,
    MacAddress,
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC

    InterfaceIndex,
    ifIndex         
        FROM IF-MIB
        
    MODULE-IDENTITY,  
    OBJECT-TYPE, 
    NOTIFICATION-TYPE,
    Integer32,
    Unsigned32,
    IpAddress
        FROM SNMPv2-SMI

--     pethMainPseConsumptionPower,
--     pethPsePortDetectionStatus
--        FROM POWER-ETHERNET-MIB

--     alarmIndex, 
--     alarmVariable, 
--     alarmSampleType, 
--     alarmValue,  
--     alarmRisingThreshold,
--     alarmFallingThreshold
--         FROM RMON-MIB
        
    OBJECT-GROUP, NOTIFICATION-GROUP
        FROM SNMPv2-CONF;
           
peaEquipment  MODULE-IDENTITY
    LAST-UPDATED 
         "200508161200Z"
    ORGANIZATION
         "Ericsson Danmark A/S"
    CONTACT-INFO
         "
         Ericsson Diax
         Faelledvej 17
         DK-7600 Struer
         Denmark
         
         E-mail: diax.support@ericsson.com
         "       
   DESCRIPTION
         "
         The public ethernet access software supports a number of standard 
         and proprietary MIBs. This MIB module defines objects for reading/writing
         common equipment information.           
         "                                                           
         
    ::= { pea 1 }
    
commonObjects             OBJECT IDENTIFIER ::= { peaEquipment  1 }
commonTraps               OBJECT IDENTIFIER ::= { peaEquipment  2 }

traps                     OBJECT IDENTIFIER ::= { commonTraps  0 }
trapsParameters           OBJECT IDENTIFIER ::= { commonTraps  1 }

versions                  OBJECT IDENTIFIER ::= { commonObjects 1 }
mainProcessor             OBJECT IDENTIFIER ::= { commonObjects 2 }
temperature               OBJECT IDENTIFIER ::= { commonObjects 3 }
fansStatus                OBJECT IDENTIFIER ::= { commonObjects 4 }
alarmFilter               OBJECT IDENTIFIER ::= { commonObjects 5 }
configurationData         OBJECT IDENTIFIER ::= { commonObjects 6 }
upgradeSW                 OBJECT IDENTIFIER ::= { commonObjects 7 }


-- commonObjects between 8 and 14 are used below
identificationData        OBJECT IDENTIFIER ::= { commonObjects 15 }
-- commonObjects between 16 and 19 are used below
peaMultilink              OBJECT IDENTIFIER ::= { commonObjects 20 }
-- commonObjects 15 used below

deviceIdData            OBJECT IDENTIFIER ::= { identificationData 1 }
microProcessorIdData    OBJECT IDENTIFIER ::= { identificationData 2 }
analogueFrontEndIdData  OBJECT IDENTIFIER ::= { identificationData 3 }
dcDcConverterIdData     OBJECT IDENTIFIER ::= { identificationData 4 }


--pethObjects             OBJECT IDENTIFIER ::= { peaEquipment  3 }
--pethNotifications       OBJECT IDENTIFIER ::= { peaEquipment  4 }
--pethNotificationTypes   OBJECT IDENTIFIER ::= { pethNotifications 0 }


ifObjects               OBJECT IDENTIFIER ::= { peaEquipment 5  }
ifNotifications         OBJECT IDENTIFIER ::= { peaEquipment 6  }
ifNotificationTypes     OBJECT IDENTIFIER ::= { ifNotifications 0   }

-- Mib Groups ...   
peaEquipmentMibGroups   OBJECT IDENTIFIER ::= { peaEquipment 7  } 

-- SNMP v2 notification
snmpv2Notifications       OBJECT IDENTIFIER ::= { peaEquipment 8  }   
snmpv2NotificationsTypes  OBJECT IDENTIFIER ::= { snmpv2Notifications 0  }
-- Bridge notifications
bridgeNotifications       OBJECT IDENTIFIER ::= { peaEquipment 9  } 
bridgeNotificationsTypes  OBJECT IDENTIFIER ::= { bridgeNotifications 0  }
--RMON notifications
rmonNotifications         OBJECT IDENTIFIER ::= { peaEquipment 10  } 
rmonNotificationsTypes    OBJECT IDENTIFIER ::= { rmonNotifications 0  }

PhyModeType ::= TEXTUAL-CONVENTION
    DISPLAY-HINT "1x"
    STATUS       current
    DESCRIPTION
            "Represent the Ethernet PHY Mode."
    SYNTAX       OCTET STRING (SIZE (1))

--************************************************************************
-- Equipment Information
--************************************************************************

sysId OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "This value includes the full name and version
     identification of the node software."
    ::= { versions 1 }    
 
bootSwId OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "This value includes the full name and version
     identification of the BOOT software."
    ::= { versions 2 } 

cfgVer OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The CfgVer will initially be zero, but is set to
     the version number defined in the configuration file 
     received from the tftp server at system start-up. The 
     management system may use this version number to verify 
     that the unit contains the correct configuration 
     parameter values. The management system may update the 
     version number when configuration parameters are modified."
    ::= { versions 3 }

activeSwInfo OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "This value represents the software version of the active software part of the node."
    ::= { versions 4 }

passiveSwInfo OBJECT-TYPE
    SYNTAX      DisplayString 
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "This value represents the software version of the passive software part of the node."
    ::= { versions 5 } 

--************************************************************************
-- common performance statistics
--************************************************************************

processorLoad OBJECT-TYPE
    SYNTAX      Unsigned32 (0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The percentage of time that the main processor was not idle.
     Average over 10 seconds. (almost like hrProcessorLoad in RFC1514)."
    ::= { mainProcessor 1 }

memFree OBJECT-TYPE
    SYNTAX      Unsigned32 (0..100)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The amount of free memory on main processor in percent."
    ::= { mainProcessor 2 }
    
memFreeBytes OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The amount of free memory on main processor in bytes."
    ::= { mainProcessor 3 }
    
memBuffersUsed OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The total number of memory buffers currently used (Ethernet, ATM & local IP-stack)."
    ::= { mainProcessor 4 }    

--************************************************************************
-- temperature and state of fans
--************************************************************************

tempControlState OBJECT-TYPE
    SYNTAX      INTEGER {low(0), normal(1), high(2), tooHigh(3)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The state of the temperature control can be:
     0 - Low
     1 - Normal
     2 - High
     3 - TooHigh"
    ::= { temperature 1 }

tempValue OBJECT-TYPE
    SYNTAX      Integer32 (1..150)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The temperature value in the node in deg. Celsius."
    ::= { temperature 2 }
          
greenFanStatus OBJECT-TYPE
    SYNTAX      INTEGER {failed(0), working(1)}
    MAX-ACCESS  read-only
    STATUS      deprecated
    DESCRIPTION
    "The status of the fan which is placed closest to the green LED.
     For ESN310/ECN320/ECN330, green fan is the fan placed near the front.
     Made deprecated. Use the greenFanPerfStatus parameter instead."
    ::= { fansStatus 1 }

redFanStatus OBJECT-TYPE
    SYNTAX      INTEGER {failed(0), working(1)}
    MAX-ACCESS  read-only
    STATUS      deprecated
    DESCRIPTION
    "The status of the fan which is placed closest to the red LED.
     For ESN310/ECN320/ECN330, red fan is the fan placed near the rear.
     Made deprecated. Use the redFanPerfStatus parameter instead."
    ::= { fansStatus 2 }

greenFanSpeed OBJECT-TYPE
    SYNTAX      Unsigned32 
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The speed of the fan, which is placed closest to the
     green LED, measured in rpm (revolutions per minute).
     For ESN310/ECN320/ECN330, green fan is the fan placed near the front." 
    ::= { fansStatus 3 } 
    
redFanSpeed OBJECT-TYPE
    SYNTAX      Unsigned32 
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The speed of the fan, which is placed closest to the
     red LED, measured in rpm (revolutions per minute).
     For ESN310/ECN320/ECN330, red fan is the fan placed near the rear." 
    ::= { fansStatus 4 }

greenFanPerfStatus OBJECT-TYPE
    SYNTAX      INTEGER {failed(1), poor(2), good (3)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The performance status of the fan which is placed closest to the green LED.
     For ESN310/ECN320/ECN330, green fan is the fan placed near the front."
    ::= { fansStatus 5 }

redFanPerfStatus OBJECT-TYPE
    SYNTAX      INTEGER {failed(1), poor(2), good (3)}
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The performance status of the fan which is placed closest to the red LED.
     For ESN310/ECN320/ECN330, red fan is the fan placed near the rear."
    ::= { fansStatus 6 }
    

--************************************************************************
-- Actions
--************************************************************************

hideInternalInterfaces OBJECT-TYPE
    SYNTAX      INTEGER { off (0), on (1) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The management system can chose to hide all interfaces (RFC1233/RFC2233)
    that are irrelevant in the PEM network view (HP OpenView issue).
     If 0, Don't hide any interfaces.
     If 1, Hide internal interfaces (for example loopback and ATM interfaces)."  
    ::= { commonObjects 8} 

restart OBJECT-TYPE
    SYNTAX      INTEGER {restart(1)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When set to 'restart' the node restarts, which includes 
    loading the SW image and configuration data from the server. 
    In this way a new SW version can be loaded and/or the configu-
    ration data can be updated." 
    ::= { commonObjects 9 }    

ledsAltBlink OBJECT-TYPE
    SYNTAX      INTEGER {on(1), off(0)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When the ledsAltBlink is set to ON(1) the
     LEDs will start to blink alternatively."
    ::= { commonObjects 10 }

startCfgVerification OBJECT-TYPE
    SYNTAX      INTEGER {success(0), verify(1), verificationInProgress(2), noAnswerFromDHCP(3), noAnswerFromTFTP(4)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When startCfgVerification is set to verify(1) the node makes a DHCP renew and retrieves
    the configuration file from the address of the server just received from DHCP.
    The parameters contained in the configuration file overwrites the current settings.
    The status of the last issued verification can be read out by reading this MIB variable." 
    ::= { commonObjects 11 }

saveCfgToNvm OBJECT-TYPE
    SYNTAX      INTEGER { success (0), start(1), saveInProgress (2), failure (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "When saveCfgToNvm is set to start(1) the device saves the configuration data parameters to NVM.
     Using GET command the current (inProgress) or last known state (success or failure) are 
     displayed." 
    ::= { commonObjects 12 }    

cfgVerificationInterval OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This is the time in minutes between configuration file verification.
     If the value is zero the configuration verification is disabled." 
    ::= { commonObjects 13 }

notifyType OBJECT-TYPE
    SYNTAX      INTEGER { trap(1), inform (2)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Set the type of the notify sent by the node:
     1. Trap
     2. Inform" 
    ::= { commonObjects 14 }

sntpServerIP OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The IP address of SNTP server used for time synchronization." 
    ::= { commonObjects 16 }                        

snmpKeepAliveTimerTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The interval between SNMP KeepAlive checks." 
    ::= { commonObjects 17 }                        
    
allowBroadcastToDux OBJECT-TYPE
    SYNTAX      INTEGER { false(0), true(1) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Enables/disables broadcast to DUX (broadcast beacons).
     When the IP DSLAM is restarted one broadcast to DUX is always sent, hereafter
     allowBroadcastToDux determines whether broadcast to DUX are sent on a regular
     basis. In default settings the broadcast to DUX is enabled.
     
     Note: It is not recommended to disable broadcast to DUX because it might result in reboot of
           EDA nodes due to expire of the 'Keep-alive Timer'. For more details consult the Users
           Guide."
    ::= { commonObjects 18 }

comboConnector OBJECT-TYPE
    SYNTAX      INTEGER { mounted (1), notMounted (2) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Tell if a combo connector is mounted for this board."  
    ::= { commonObjects 21} 

blockUnit OBJECT-TYPE
    SYNTAX      INTEGER { block (1), unblock (2) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Block this unit. When blocked:
      - no user traffic is carried out
      - no alarms are issued
      - it does not allow any configuration

     Getting a card out of blocked state can be done by either
     sending a restart request to it (MIB or Reset-On-Lan) or
     by dismouting and mounting it again.
     Setting state to unblock will be ignored.
    "  
    ::= { commonObjects 22} 
    
--************************************************************************
-- alarm filter parameters
--************************************************************************
comAlarmFilter  OBJECT-TYPE
    SYNTAX      INTEGER { warning (0), minor (1), major (2), critical (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This alarm filter will filter out all communication 
     alarms with lower severity than the selected.
     For example: Selecting minor (1) would filter out all
     alarms with warning (0) severity."
    ::= { alarmFilter 1 }

envAlarmFilter  OBJECT-TYPE
    SYNTAX      INTEGER { warning (0), minor (1), major (2), critical (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This alarm filter will filter out all environment 
     alarms with lower severity than the selected.
     For example: Selecting minor (1) would filter out all
     alarms with warning (0) severity."
    ::= { alarmFilter 2 }

equipAlarmFilter  OBJECT-TYPE
    SYNTAX      INTEGER { warning (0), minor (1), major (2), critical (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This alarm filter will filter out all equipment 
     alarms with lower severity than the selected.
     For example: Selecting minor (1) would filter out all
     alarms with warning (0) severity."
    ::= { alarmFilter 3 }
    
processAlarmFilter  OBJECT-TYPE
    SYNTAX      INTEGER { warning (0), minor (1), major (2), critical (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This alarm filter will filter out all processing 
     alarms with lower severity than the selected.
     For example: Selecting minor (1) would filter out all
     alarms with warning (0) severity."
    DEFVAL { major }
    ::= { alarmFilter 4 }
    
qosAlarmFilter  OBJECT-TYPE
    SYNTAX      INTEGER { warning (0), minor (1), major (2), critical (3)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "This alarm filter will filter out all quality 
     alarms with lower severity than the selected.
     For example: Selecting minor (1) would filter out all
     alarms with warning (0) severity."
    DEFVAL { major }
    ::= { alarmFilter 5 }      

alarmFilterThres OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      obsolete
    DESCRIPTION
    "The threshold value indicates how many alarms that can be filtered by the
     node in the time interval indicated by the alarmFilterInterval parameter
     before an alarm is sent. If set to 0, the functionality is disabled."
    ::= { alarmFilter 6 } 

alarmFilterInterval OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS   "sec"
    MAX-ACCESS  read-write
    STATUS      obsolete
    DESCRIPTION
    "The value of this parameter indicates the time interval in which the node
     can filter a number of alarms indicated by the alarmFilterThres parameter
     before an alarm is sent. If set to 0, the functionality is disabled."
    ::= { alarmFilter 7 } 

--************************************************************************
-- Configuration Data
--************************************************************************

localMacAddr OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Unique MAC address of the node."
    ::= { configurationData 1 }

phyMode OBJECT-TYPE
    SYNTAX      PhyModeType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Ethernet PHY Mode setting.
     The Phy Mode is coded as a bitfield:
     *  Bit 0: Auto-negotiation. If set, auto-negotiation is OFF else ON.
     *  Bit 2: Full Duplex control. If set, full duplex operation is NOT allowed.
     *  Bit 3: 100 Mbit/s control. If set, 100 Mbit/s operation is NOT allowed.
     *  Bit 4: Half Duplex control. If set, half duplex operation is NOT allowed
     *  Bit 5: 10 Mbit/s control. If set, 10 Mbit/s operation is NOT allowed.
     Examples of PHY Mode values:
     *  0x00: Full auto-negotiation.
     *  0x20: 100 Mbit/s only. Auto-negotiation between half & full duplex.
     *  0x31: Fixed 100 Mbit/s, full duplex only operation.
     *  0x25: Fixed 100 Mbit/s, half duplex only operation
    "
    ::= { configurationData 2 }
 
watchDogEnable OBJECT-TYPE
    SYNTAX      INTEGER { off (0), on (1) }
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Enabling of the HW Watchdog.
     If 0 (zero), the watchdog is OFF, 
     else the watchdog is ON."
    ::= { configurationData 3 }

vlanId OBJECT-TYPE
    SYNTAX      Unsigned32 (0..4095)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The VLAN ID setting for the management VLAN."  
    ::= { configurationData 5 }     
    
vlanPrio OBJECT-TYPE
    SYNTAX      Unsigned32 (0..7)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Specifies the IEEE 802.1Q priority used for the management VLAN."  
    ::= { configurationData 6 }     
      
vlanTagging OBJECT-TYPE
    SYNTAX      INTEGER { off (0), on (1) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "If 0, VLAN tagging is off, meaning that VLAN is not used.
     If 1, VLAN tagging is used, with the given ID and Prio."  
    ::= { configurationData 7 } 
    
bootString OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (0..255))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Text commands for the bootloader."
    ::= { configurationData 8 }

    
--************************************************************************
-- Upgrade Software
--************************************************************************

swFileName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The filename to upload to the node.
     The path to this file is in the root directory that is specified in the FTP server"
    ::= { upgradeSW 1 }    

uploadSW OBJECT-TYPE
    SYNTAX      INTEGER {  success (0), uploadSwWithRestart (1), uploadSwWithoutRestart (2),
                           uploadInProgress (3), failure (4) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Upload new application software to device.
    The file which is loaded is indicated by the swFileName parameter.
    1 - upload SW to the device and restart afterwards.
    2 - upload SW to the device without restarting.
    Using GET command the current (inProgress) or last known state (success or failure) are 
    displayed."
    ::= { upgradeSW 2 }    

bootSwFileName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The filename of a boot sw image located on the domain file server. This file
     name is used by the uploadBootSw."
    ::= { upgradeSW 3 }

uploadBootSw OBJECT-TYPE
    SYNTAX      INTEGER { success (0), start (1), uploadInProgress (2), failure (3) }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Upload new boot software to this device. The boot sw image must be named by the
     bootSwFileName parameter and located on the domain file server, which is the next-server
     in the DHCP offer.
     Using GET command the current (inProgress) or last known state (success or failure) are 
     displayed."
    ::= { upgradeSW 4 }    
  
    
--************************************************************************
-- Device identification data
--************************************************************************

productNumber OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (0..24))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Index number that identifies the node."
    ::= { deviceIdData 1 }    
          
revisionState OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (0..7))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Revision State or 'R-state' declaring minor versions. 
     For example: different types of components or new boot SW."
    ::= { deviceIdData 2 }    
    
functionDesignation OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (0..12))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Number with reference to the handling and use of the product."
    ::= { deviceIdData 3 }    

dateOfProduction OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (4))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Manufacturing date, describing when the unit has been manufactured,
     in terms of a year, month and a day, as described below:
        Year code(0000 ..9999) - 2 bytes (LSB,MSB)
        Month (1..12)- 1 byte
        Day (1 ..31)- 1 byte. 
     For example, Wednesday June 12, 2002 would be displayed as: 
     h'02, h'14, h'06, h'0C."
    ::= { deviceIdData 4 }
    
dateOfRepair OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (4))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Repair date, declaring the year, month and day of the latest repair.
     The format is described below:
        Year code(0000 ..9999) - 2 bytes (LSB,MSB)
        Month (1..12)- 1 byte
        Day (1 ..31)- 1 byte. 
     For example, Wednesday June 12, 2002 would be displayed as: 
     h'02, h'14, h'06, h'0C."
   ::= { deviceIdData 5 }

productionSerialNumber OBJECT-TYPE
    SYNTAX      DisplayString (SIZE (13))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Unique serial number, describing the product in terms of a manufacturing
     site code and a sequence number."
    ::= { deviceIdData 6 }
    
layoutVersionCode OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (4))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "The data layout version that allows the possibility
     to interpret stored data differently."
    ::= { deviceIdData 7 }  
    
idCheckSum OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE (4))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "CRC32 Checksum of identification data structure 
     of the node."
    ::= { deviceIdData 8 }
     
