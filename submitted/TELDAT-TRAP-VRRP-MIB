vrrpNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              vrrp1_generic_info 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event VRRP.1

        Message: '$1'

        Parameters list:
              $1: vrrp1_generic_info

        Generic info."
    ::= { vrrpNotificationsPrefix 1 }

vrrpNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              vrrp2_interface, 
              vrrp2_vrid,
              vrrp2_state,
              vrrp2_event 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event VRRP.2

        Message: 'VR[$1, vrId $2] State $3, Event $4'

        Parameters list:
              $1: vrrp2_interface
              $2: vrrp2_vrid
              $3: vrrp2_state
              $4: vrrp2_event

        Virtual router event descriptor."
    ::= { vrrpNotificationsPrefix 2 }

