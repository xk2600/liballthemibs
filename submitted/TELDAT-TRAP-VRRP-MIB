TELDAT-TRAP-VRRP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


vrrpTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the VRRP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 179 }


vrrpEventsGroup	OBJECT IDENTIFIER	::= { vrrpTrapsMIB 1 }

vrrpNotifications	OBJECT IDENTIFIER	::= { vrrpTrapsMIB 2 }
vrrpNotificationsPrefix	OBJECT IDENTIFIER	::= { vrrpNotifications 0 }

vrrpEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event VRRP.1
        Generic info."
    ::= { vrrpEventsGroup 1 }

