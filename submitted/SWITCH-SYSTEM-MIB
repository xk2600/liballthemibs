       rcUspIfToUspMappingUnitIndex      INTEGER,
       rcUspIfToUspMappingSlotIndex      INTEGER,
       rcUspIfToUspMappingPortIndex      INTEGER,
	   rcUspIfType                       INTEGER,
	   rcUspIfState                      INTEGER
       }  
       
   rcUspIfToUspMappingUnitIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The unit index which IF is mapped to."
       ::= { rcUspIfToUspMappingEntry 1 } 
       
   rcUspIfToUspMappingSlotIndex    OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The slot index which IF is mapped to."
       ::= { rcUspIfToUspMappingEntry 2 }
       
   rcUspIfToUspMappingPortIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The port index which IF is mapped to."
       ::= { rcUspIfToUspMappingEntry 3 }             

   rcUspIfType   OBJECT-TYPE
       SYNTAX     INTEGER
        {
       		none(0),  
       		port(1),
       		line(2), 
       		client(3), 
       		tdm(4), 
       		ethernet(5),
       		fastethernet(6),
       		gigaethernet(7),
			tengigabitethernet(8),
			aggregator (9), 
			ip (10),
			loopback(11),
			outband(12),
			max(13)
        }
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The IF type which IF is mapped to."
       ::= { rcUspIfToUspMappingEntry 4 }  
   
   rcUspIfState   OBJECT-TYPE
       SYNTAX     INTEGER {
        		active(1),
        		empty(2)
        }
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The IF State."
       ::= { rcUspIfToUspMappingEntry 5 } 
	      
    -- USP to port mapping table
   rcUspUspToIfMappingTable  OBJECT-TYPE
       SYNTAX     SEQUENCE OF RcUspUspToIfMappingEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "The table describes how the USP map to the IF."
       ::= { rcPortMappingInfoConfig 7 }
       
   rcUspUspToIfMappingEntry OBJECT-TYPE
       SYNTAX      RcUspUspToIfMappingEntry
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION 
           "Defines an entry in the rcUspUspToIfMappingTable.The indexes are
           rcUspUspToIfMappingUnitIndex,rcUspUspToIfMappingSlotIndex,rcUspUspToIfMappingPortIndex and rcUspIfTypeIndex."
       INDEX   { 
                 rcUspUspToIfMappingUnitIndex,
                 rcUspUspToIfMappingSlotIndex,
                 rcUspUspToIfMappingPortIndex,
				 rcUspIfTypeIndex				 
               }
       ::= { rcUspUspToIfMappingTable 1 }

   RcUspUspToIfMappingEntry ::=
       SEQUENCE {  
       rcUspUspToIfMappingUnitIndex      INTEGER,
       rcUspUspToIfMappingSlotIndex      INTEGER,
       rcUspUspToIfMappingPortIndex      INTEGER,
	   rcUspIfTypeIndex                          INTEGER,
       rcUspIfIndex                      INTEGER
       }  

   rcUspUspToIfMappingUnitIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "The unit index which is mapped to the IF."
       ::= { rcUspUspToIfMappingEntry 1 }
              
   rcUspUspToIfMappingSlotIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "The slot index which is mapped to the IF."
       ::= { rcUspUspToIfMappingEntry 2 }
       
   rcUspUspToIfMappingPortIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "The port index which is mapped to the IF."
       ::= { rcUspUspToIfMappingEntry 3 }
	   
   rcUspIfTypeIndex   OBJECT-TYPE
       SYNTAX     INTEGER
        {
       		none(0),  
       		port(1),
       		line(2), 
       		client(3), 
       		tdm(4), 
       		ethernet(5),
       		fastethernet(6),
       		gigaethernet(7),
			tengigabitethernet(8),
			aggregator (9), 
			ip (10),
			loopback(11),
			outband(12),
			max(13)
        }
       MAX-ACCESS  not-accessible
       STATUS      current
       DESCRIPTION
           "The port type which is mapped to the IF."
       ::= { rcUspUspToIfMappingEntry 4 }
	   
   rcUspIfIndex   OBJECT-TYPE
       SYNTAX     INTEGER
       MAX-ACCESS  read-only
       STATUS      current
       DESCRIPTION
           "The If Index which is mapped to the IF."
       ::= { rcUspUspToIfMappingEntry 5 }       
