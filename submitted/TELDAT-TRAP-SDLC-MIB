TELDAT-TRAP-SDLC-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


sdlcTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the SDLC event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 90 }


sdlcEventsGroup	OBJECT IDENTIFIER	::= { sdlcTrapsMIB 1 }

sdlcNotifications	OBJECT IDENTIFIER	::= { sdlcTrapsMIB 2 }
sdlcNotificationsPrefix	OBJECT IDENTIFIER	::= { sdlcNotifications 0 }

sdlcEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event SDLC.1
        DLSw SDLC has been initialized for operation over the serial
        device and is now available for use in the router."
    ::= { sdlcEventsGroup 1 }

