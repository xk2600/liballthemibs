    ::= { stunEventObjects3 4 }


stunNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              stun1_state 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STUN.1

        Message: 'Client $1'

        Parameters list:
              $1: stun1_state

        client state changed"
    ::= { stunNotificationsPrefix 1 }

stunNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              stun2_ip, 
              stun2_port,
              stun2_id 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STUN.2

        Message: 'Request sent to $1:$2 id $3'

        Parameters list:
              $1: stun2_ip
              $2: stun2_port
              $3: stun2_id

        stun request sent"
    ::= { stunNotificationsPrefix 2 }

stunNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              stun3_type, 
              stun3_id,
              stun3_ip,
              stun3_port 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event STUN.3

        Message: 'Msg $1 rcvd, id $2 from $3:$4'

        Parameters list:
              $1: stun3_type
              $2: stun3_id
              $3: stun3_ip
              $4: stun3_port

        stun message received"
    ::= { stunNotificationsPrefix 3 }


