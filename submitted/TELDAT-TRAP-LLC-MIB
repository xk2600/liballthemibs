TELDAT-TRAP-LLC-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


llcTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the LLC event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 103 }


llcEventsGroup	OBJECT IDENTIFIER	::= { llcTrapsMIB 1 }

llcNotifications	OBJECT IDENTIFIER	::= { llcTrapsMIB 2 }
llcNotificationsPrefix	OBJECT IDENTIFIER	::= { llcNotifications 0 }

llcEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event LLC.1
        LLC is sending a frame. Possible frame types are:
        SABME_C0 or SABME_C1 (Set Asynchronous Balanced Mode Extended),
        DM_R0 or DM_R1 (Disconnected Mode), DISC_C0 or DISC_C1 (Disconnect),
        RR_C0 or RR_C1 or RR_R0 or RR_R1 (Receiver Ready),
        RNR_C0 or RNR_C1 or RNR_R0 or RNR_R1 (Receiver Not Ready),
        REJ_C0 or REJ_C1 or REJ_R0 or REJ_R1 (Reject),
        UA_R0 or UA_R1 (Unnumbered Acknowledgement),
        FRMR_R0 or FRMR_R1 (Frame Reject), and
        I_C0 or I_C1 or I_R0 or I_R1 (Information Frame).
        The abbrevation suffixes are C0 (command, poll bit off),
        C1 (command, poll bit on), R0 (response, final bit off) and
        R1 (response, final bit on)."
    ::= { llcEventsGroup 1 }

