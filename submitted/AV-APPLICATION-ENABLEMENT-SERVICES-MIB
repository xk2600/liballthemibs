AV-APPLICATION-ENABLEMENT-SERVICES-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Integer32, 
    enterprises 
                    FROM SNMPv2-SMI

    DisplayString,
    TEXTUAL-CONVENTION,
    TruthValue
                    FROM SNMPv2-TC 

    InetAddress,     
    InetAddressType, 
    InetPortNumber
                    FROM INET-ADDRESS-MIB

    MODULE-COMPLIANCE, 
    OBJECT-GROUP
                    FROM SNMPv2-CONF;

avAesMIB MODULE-IDENTITY       
    LAST-UPDATED "201303050000Z" -- March 05, 2013
