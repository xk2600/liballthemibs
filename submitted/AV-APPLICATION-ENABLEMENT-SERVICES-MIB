        avAesTsapiTsdiMsgBytesAllocTlink        Integer32,
        avAesTsapiTsdiMsgBytesAllocTsrv         Integer32,
        avAesTsapiTsdiBuffSpaceAlloc            Integer32,
        avAesTsapiTsdiBuffSize                  Integer32,
        avAesTsapiTsdiBuffHighWaterMark         Integer32
}

