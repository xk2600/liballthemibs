-- Trap definitions
--
    
        mscsAimInstanceAddedTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { mscsAimInstanceIndex, mscsAimInstanceName, mscsAimInstanceRowStatus, mscsAimInstanceDataStatus }
            DESCRIPTION 
                "This trap is sent when a new Instance/Server is added.
                
                Parameters:
                mscsAimInstanceIndex
                mscsAimInstanceName
                mscsAimInstanceRowStatus
                mscsAimInstanceDataStatus"
            ::=  165100

        mscsAimInstanceRemovedTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { mscsAimInstanceIndex, mscsAimInstanceName, mscsAimInstanceRowStatus, mscsAimInstanceDataStatus }
            DESCRIPTION 
                "This trap is sent when an Instance/Server is removed.
                
                Parameters:
                mscsAimInstanceIndex
                mscsAimInstanceName
                mscsAimInstanceRowStatus
                mscsAimInstanceDataStatus"
            ::=  165101

        mscsAimInstanceDataStatusChanged TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { mscsAimInstanceIndex, mscsAimInstanceName, mscsAimInstanceRowStatus, mscsAimInstanceDataStatus }
            DESCRIPTION 
                "This trap is sent when Instance/Server Data Status is changed.
                
                Parameters:
                mscsAimInstanceIndex
                mscsAimInstanceName
                mscsAimInstanceRowStatus
                mscsAimInstanceDataStatus"
            ::=  165102

        mscsAimResourceGroupMigration TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { mscsAimStatResourceGroupIndex, mscsAimStatResourceGroupName, mscsAimStatResourceGroupNodeIndex, mscsAimStatResourceGroupNodeName, mscsAimStatResourceGroupPreviousNodeIndex, 
                mscsAimStatResourceGroupPreviousNodeName, mscsAimStatResourceGroupInstanceName, mscsAimStatResourceGroupClusterIndex }
            DESCRIPTION 
                "This trap is sent when a resource group is migrated.
                
                Parameters:
                mscsAimStatResourceGroupIndex
                mscsAimStatResourceGroupName
                mscsAimStatResourceGroupNodeIndex
                mscsAimStatResourceGroupNodeName
                mscsAimStatResourceGroupPreviousNodeIndex
                mscsAimStatResourceGroupPreviousNodeName
                mscsAimStatResourceGroupInstanceName
                mscsAimStatResourceGroupClusterIndex
                
                         
                "
            ::=  165103

    
    END

