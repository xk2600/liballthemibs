--       1.3.6.1.4.1.546.16.51.3.1.1
        mscsAimCfgname OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The agent's name."
            ::= { mscsAimCfgGeneralGroup 1 }

        
--       1.3.6.1.4.1.546.16.51.3.1.2
        mscsAimCfgversion OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Version of the agent."
            ::= { mscsAimCfgGeneralGroup 2 }

        
--       1.3.6.1.4.1.546.16.51.3.1.3
        mscsAimCfgstarttime OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Time the agent was started."
            ::= { mscsAimCfgGeneralGroup 3 }

        
