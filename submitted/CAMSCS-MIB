    CAMSCS-MIB DEFINITIONS ::= BEGIN
 
        IMPORTS
            OBJECT-TYPE            
                FROM RFC-1212            
            TRAP-TYPE            
                FROM RFC-1215            
            enterprises, Counter, Gauge
                FROM RFC1155-SMI            
            DisplayString            
                FROM RFC1213-MIB            
            SnmpAdminString            
                FROM SNMP-FRAMEWORK-MIB;
    
    
    
--
-- Node definitions
--
    
-- *****************************************************************************
-- OIDs of mscs agent start with 1.3.6.1.4.1.546.16.51
-- *****************************************************************************
-- 
        empire OBJECT IDENTIFIER ::= { enterprises 546 }

        
--     1.3.6.1.4.1.546.1
        unix OBJECT IDENTIFIER ::= { empire 1 }

        
--     1.3.6.1.4.1.546.1.1
        sysmgmt OBJECT IDENTIFIER ::= { unix 1 }

        
--     1.3.6.1.4.1.546.1.1.9
        traps OBJECT IDENTIFIER ::= { empire 9 }

        
--       1.3.6.1.4.1.546.16
        applications OBJECT IDENTIFIER ::= { empire 16 }

        
--       1.3.6.1.4.1.546.16.56
        camscsaim OBJECT IDENTIFIER ::= { applications 51 }

        
--       1.3.6.1.4.1.546.16.51.1
        mscsAimGroup OBJECT IDENTIFIER ::= { camscsaim 1 }

        
--       1.3.6.1.4.1.546.16.56.1.1
        mscsAimInstanceCount OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of instances in mscsAimInstanceTable."
            ::= { mscsAimGroup 1 }

        
--       1.3.6.1.4.1.546.16.56.1.2
        mscsAimInstanceTable OBJECT-TYPE
            SYNTAX SEQUENCE OF MscsAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A table containing the MSCS instances."
            ::= { mscsAimGroup 2 }

        
--       1.3.6.1.4.1.546.16.56.1.2.1
        mscsAimInstanceEntry OBJECT-TYPE
            SYNTAX MscsAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The MSCS instances table entry."
            INDEX { mscsAimInstanceIndex }
            ::= { mscsAimInstanceTable 1 }

        
        MscsAimInstanceEntry ::=
            SEQUENCE { 
                mscsAimInstanceIndex
                    INTEGER,
                mscsAimInstanceName
                    DisplayString,
                mscsAimInstanceUserName
                    DisplayString,
                mscsAimInstancePassword
                    DisplayString,
                mscsAimInstanceRowStatus
                    INTEGER,
                mscsAimInstanceDataStatus
                    INTEGER
             }

--       1.3.6.1.4.1.546.16.51.1.2.1.1
        mscsAimInstanceIndex OBJECT-TYPE
            SYNTAX INTEGER (1..2147483647)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                " The unique index of the instance."
            ::= { mscsAimInstanceEntry 1 }

        
--       1.3.6.1.4.1.546.16.51.1.2.1.2
        mscsAimInstanceName OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Name of the instance"
            ::= { mscsAimInstanceEntry 2 }

        
--       1.3.6.1.4.1.546.16.51.1.2.1.3
        mscsAimInstanceUserName OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "User name for connecting to the instance."
            ::= { mscsAimInstanceEntry 3 }

        
--       1.3.6.1.4.1.546.16.51.1.2.1.4
        mscsAimInstancePassword OBJECT-TYPE
            SYNTAX DisplayString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Password for the user name provided to connect to the instance."
            ::= { mscsAimInstanceEntry 4 }

        
--       1.3.6.1.4.1.546.16.51.1.2.1.5
        mscsAimInstanceRowStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                active(1),
                notInService(2),
                notReady(3),
                createAndGo(4),
                createAndWait(5),
                destroy(6)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The status of this monitor entry according to the Textual Conventions for SNMPv2 (see RFC 1443).
                active(1) - the row is available for usage
                notInService(2) - setting to this value disables usage of the row
                notReady(3) - an error occurred during usage of the row
                createAndGo(4) - setting to this value creates the row with row status set to active(1)
                createAndWait(5) - setting to this value creates the row with row status set to notInService(2)
                destroy(6) - setting to this value deletes the row"
            DEFVAL { 5 }
            ::= { mscsAimInstanceEntry 5 }

        
--       1.3.6.1.4.1.546.16.51.1.2.1.6
        mscsAimInstanceDataStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                ready(1),
                notReady(2),
                hostUnreachable(3),
                hostIsSlave(4),
                authenticationFailed(5),
                unknownError(6)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The status of data of the  MSCS Instance (Server), i.e. status of Aim caches for the instance.
                ready(1) - data are ready in Aim cache, caches were populated
                notReady(2)  - data are Not ready, caches were not populated yet
                hostUnreachable(3) - MSCS host unreachable, data are kept in Aim caches
                hostIsSlave(4) -  MSCS host is a slave server, data are kept in Aim caches
                authenticationFailed(5) - authentication failed for host, data are kept in Aim caches
                unknownError(6) - unknow error, data are kept in Aim caches"
            ::= { mscsAimInstanceEntry 6 }

        
        mscsAimInstanceUnusedIndex OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "An SNMP GET of this attribute returns an unused Instance UID. This
                is normally used to expedite safe table row creation."
            ::= { mscsAimGroup 3 }

        
--       1.3.6.1.4.1.546.16.51.2
        mscsAimStatGroup OBJECT IDENTIFIER ::= { camscsaim 2 }

        
