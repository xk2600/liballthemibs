--******************************************************************
-- RfiNotches
--******************************************************************
vdslLineConfPhysVdslRfiNotchesTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF VdslLineConfPhysVdslRfiNotchesEntry
      MAX-ACCESS not-accessible
      STATUS     current
      DESCRIPTION
      "This table contains information of RfiNotches
       configuration"
      ::= { vdslObjects 6 }

vdslLineConfPhysVdslRfiNotchesEntry OBJECT-TYPE
      SYNTAX VdslLineConfPhysVdslRfiNotchesEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in vdslLineConfPhysVdslRfiNotchesTable."
      INDEX { lineIndex }
      ::= { vdslLineConfPhysVdslRfiNotchesTable 1 }

VdslLineConfPhysVdslRfiNotchesEntry ::=
      SEQUENCE {
        rfiNotchesNoOfToneGroups			INTEGER,
        rfiNotchesToneGroups0StartTone		INTEGER,
        rfiNotchesToneGroups0EndTone 		INTEGER,
        rfiNotchesToneGroups1StartTone		INTEGER,
        rfiNotchesToneGroups1EndTone 		INTEGER,
        rfiNotchesToneGroups2StartTone		INTEGER,
        rfiNotchesToneGroups2EndTone 		INTEGER,
        rfiNotchesToneGroups3StartTone		INTEGER,
        rfiNotchesToneGroups3EndTone 		INTEGER,
        rfiNotchesToneGroups4StartTone		INTEGER,
        rfiNotchesToneGroups4EndTone 		INTEGER,
        rfiNotchesToneGroups5StartTone		INTEGER,
        rfiNotchesToneGroups5EndTone 		INTEGER,
        rfiNotchesToneGroups6StartTone		INTEGER,
        rfiNotchesToneGroups6EndTone 		INTEGER,
        rfiNotchesToneGroups7StartTone		INTEGER,
        rfiNotchesToneGroups7EndTone 		INTEGER,
        rfiNotchesToneGroups8StartTone		INTEGER,
        rfiNotchesToneGroups8EndTone 		INTEGER,
        rfiNotchesToneGroups9StartTone		INTEGER,
        rfiNotchesToneGroups9EndTone 		INTEGER,
        rfiNotchesToneGroups10StartTone		INTEGER,
        rfiNotchesToneGroups10EndTone 		INTEGER,
        rfiNotchesToneGroups11StartTone		INTEGER,
        rfiNotchesToneGroups11EndTone 		INTEGER,
        rfiNotchesToneGroups12StartTone		INTEGER,
        rfiNotchesToneGroups12EndTone 		INTEGER,
        rfiNotchesToneGroups13StartTone		INTEGER,
        rfiNotchesToneGroups13EndTone 		INTEGER,
        rfiNotchesToneGroups14StartTone		INTEGER,
        rfiNotchesToneGroups14EndTone 		INTEGER,
        rfiNotchesToneGroups15StartTone		INTEGER,
        rfiNotchesToneGroups15EndTone 		INTEGER
        }

