      SYNTAX      OCTET STRING  (SIZE (32))
      UNITS       "ToneIndex"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Toneindexes for 16 breakpoints. Each ToneIndex is
       2 octets. The octet string has the format:
       [Bp0_Hi, Bp0_Low....Bp15_Hi, Bp15_Low]
       Valid range is [0, 4096].
       G.997.1 name: PSDMASKus.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMibMaskUsEntry 10 }

mibMaskUsBpPsd OBJECT-TYPE
      SYNTAX      OCTET STRING  (SIZE (32))
      UNITS       "0.1 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "PSD level in 0.1 dBm/Hz for 16 breakpoints. Each
       BpPsd has to octets. The octet string has the format:
       [Bp0_Hi, Bp0_Low....Bp15_Hi, Bp15_Low]
       Valid range is [0,-96] dBm/Hz.
       G.997.1 name: PSDMASKus.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMibMaskUsEntry 15 }





