      SEQUENCE {
        limitingMaskUpNoOfBreakPoints		INTEGER,
        limitingMaskUpBpToneIndex 			OCTET STRING,
        limitingMaskUpBpPsd			  		OCTET STRING,
        limitingMaskUpUseIsdnTypeUS0		TruthValue
        }

limitingMaskUpNoOfBreakPoints OBJECT-TYPE
      SYNTAX      INTEGER  (0..16)			
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Number of breakpoints in upstream mask"
      ::= { vdslLineConfPhysVdslLimitingMaskUpEntry 1 }

limitingMaskUpBpToneIndex OBJECT-TYPE
      SYNTAX      OCTET STRING  (SIZE (32))
      UNITS       "ToneIndex"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Toneindexes for 16 breakpoints. The octet string
       has the format: [Bp0_Hi, Bp0_Low....Bp31_Hi, Bp31_Low]
       Valid range is [0, 4096]"
      ::= { vdslLineConfPhysVdslLimitingMaskUpEntry 2 }

limitingMaskUpBpPsd OBJECT-TYPE
      SYNTAX      OCTET STRING  (SIZE (32))
      UNITS       "0.1 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "PSD level in 0.1 dBm/Hz for 16 breakpoints.
       Valid range is [0,-96] dBm/Hz. The octet string
       has the format: [Bp0_Hi, Bp0_Low....Bp31_Hi, Bp31_Low]"
      ::= { vdslLineConfPhysVdslLimitingMaskUpEntry 3 }

limitingMaskUpUseIsdnTypeUS0 OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "If set it indicates that an ISDN type US0 has to
       be negociated, else POTS type is used (default)."
      ::= { vdslLineConfPhysVdslLimitingMaskUpEntry 4 }
      
