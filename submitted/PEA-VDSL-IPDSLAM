--******************************************************************
-- PboPsdUp
--******************************************************************
vdslLineConfPhysVdslPboPsdUpTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF VdslLineConfPhysVdslPboPsdUpEntry
      MAX-ACCESS not-accessible
      STATUS     current
      DESCRIPTION
      "This table contains information of PboPsdUp
       configuration.
       Ai and Bi for three upstream bands, not counting US0.
       A and B are used by the upstream PBO algorithm as in
       G993.2, par. 7.2.4.
       The implementation is according to G.997.1
       (third revision) section 7.3.1.2.14. The
       parameters will be substituted by XDSL2 Line
       MIB parameters."
      ::= { vdslObjects 7 }

vdslLineConfPhysVdslPboPsdUpEntry OBJECT-TYPE
      SYNTAX VdslLineConfPhysVdslPboPsdUpEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in vdslLineConfPhysVdslPboPsdUpTable."
      INDEX { lineIndex }
      ::= { vdslLineConfPhysVdslPboPsdUpTable 1 }

VdslLineConfPhysVdslPboPsdUpEntry ::=
      SEQUENCE {
        vdslLineConfPhysVdslPboPsdUpNoOfToneGroups			INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup0A					INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup1A 				INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup2A					INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup0B 				INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup1B					INTEGER,
        vdslLineConfPhysVdslPboPsdUpGroup2B 				INTEGER,
        vdslLineConfPhysVdslPboPsdUpForceElectricalLength	INTEGER,
        vdslLineConfPhysVdslPboPsdUpAdvanced				TruthValue,
        vdslLineConfPhysVdslPboPsdUpDisable					TruthValue
        }

vdslLineConfPhysVdslPboPsdUpNoOfToneGroups OBJECT-TYPE
      SYNTAX      INTEGER  (0..3)
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Number of tone groups in the descriptor. Maximum is three.
       Must be equal to the number of upstream bands in the bandplan,
       not counting US0."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 1 }

vdslLineConfPhysVdslPboPsdUpGroup0A OBJECT-TYPE
      SYNTAX      INTEGER  (-20725..-10240 | 0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US1. A/256 is the actual value and should
       be in [-40, -80.96].
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 2 }
      
vdslLineConfPhysVdslPboPsdUpGroup1A OBJECT-TYPE
      SYNTAX      INTEGER  (-20725..-10240 | 0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US2. A/256 is the actual value and should
       be in [-40, -80.96].
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 3 }
      
vdslLineConfPhysVdslPboPsdUpGroup2A OBJECT-TYPE
      SYNTAX      INTEGER  (-20725..-10240 | 0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US3. A/256 is the actual value and should
       be in [-40, -80.96].
       Should be 0 if noOfToneGroups < 3.
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 4 }
      

vdslLineConfPhysVdslPboPsdUpGroup0B OBJECT-TYPE
      SYNTAX      INTEGER  (-10240..0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US1. B/256 is the actual value and should
       be in [-40, 0].
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 5 }
      
vdslLineConfPhysVdslPboPsdUpGroup1B OBJECT-TYPE
      SYNTAX      INTEGER  (-10240..0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US2. B/256 is the actual value and should
       be in [-40, 0].
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 6 }
      
vdslLineConfPhysVdslPboPsdUpGroup2B OBJECT-TYPE
      SYNTAX      INTEGER  (-10240..0)
      UNITS       "1/256 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "A value of US3. B/256 is the actual value and should
       be in [-40, 0].
       Should be 0 if noOfToneGroups < 3.
       G.997.1 name: UPBOPSD-pb (per band)."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 7 }

vdslLineConfPhysVdslPboPsdUpForceElectricalLength OBJECT-TYPE
      SYNTAX      INTEGER  (-1..1270)
      UNITS       "0.1 dBm"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "If set, this electrical length (unit in 0.1 dB)
       will be enforced by the CO on the CPE, resulting
       in a predefined US PBO. If !=0, enforce US PBO
       corresponding to a line whose attenuation at 1MHz
       equals this number. Valid range [0,127] dB. A negative
       value of -1 mean: don't force the electrical length,
       but use the measured value instead.
       G.997.1 name: UPBOKL.
       G.997.1 name: UPBOKLF."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 10 }
      
vdslLineConfPhysVdslPboPsdUpAdvanced OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Enables an alternative UPBOMASK calculation,
       which differs from the standard G.997.1
       PBO mask calculation."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 15 }
      
vdslLineConfPhysVdslPboPsdUpDisable OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Disable VDSL US power back off, forcing the CPE to use
       the max US PSD as template. Default not disabled."
      ::= { vdslLineConfPhysVdslPboPsdUpEntry 20 }
      
--******************************************************************
-- MiscVdslConf
--******************************************************************
vdslLineConfPhysVdslMiscVdslConfTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF VdslLineConfPhysVdslMiscVdslConfEntry
      MAX-ACCESS not-accessible
      STATUS     current
      DESCRIPTION
      "This table contains information of MiscVdslConf
       configuration"
      ::= { vdslObjects 8 }

vdslLineConfPhysVdslMiscVdslConfEntry OBJECT-TYPE
      SYNTAX VdslLineConfPhysVdslMiscVdslConfEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in vdslLineConfPhysVdslMiscVdslConfTable."
      INDEX { lineIndex }
      ::= { vdslLineConfPhysVdslMiscVdslConfTable 1 }

VdslLineConfPhysVdslMiscVdslConfEntry ::=
      SEQUENCE {
        miscVdslConfForceElectricalLength		INTEGER,
        miscVdslConfMaxTxPowerDn				INTEGER,
        miscVdslConfDisableBitRateLimitation	TruthValue,
        miscVdslConfDisableVdslUsPbo			TruthValue,
        miscVdslConfDynamicInterleaverUs		TruthValue,
        miscVdslConfDynamicInterleaverDs		TruthValue,
        miscVdslConfDisableProfileBitmap        INTEGER,
        miscVdslConfEnableProfileBitmap			BITS,
        miscVdslConfigStandardLimitMask			INTEGER
        }

miscVdslConfForceElectricalLength OBJECT-TYPE
      SYNTAX      INTEGER  (-1..1270)
      UNITS       "0.1 dBm"
      MAX-ACCESS  read-write
      STATUS      obsolete
      DESCRIPTION
      "If set, this electrical length (unit in 0.1 dB) will be enforced
       by the CO on the CPE, resulting in a predefined US PBO.
       If !=0, enforce US PBO corresponding to a line whose attenuation
       at 1MHz equals this number.
       Valid range [0,127] dB. A negative value of -1 mean: don't force the
       electrical length, but use the measured value instead, to be sent
       in O-UPDATE
       Substituted by: vdslLineConfPhysVdslPboPsdUpForceElectricalLength.
       Obsoleted from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 1 }

miscVdslConfMaxTxPowerDn OBJECT-TYPE
      SYNTAX      INTEGER  (-256..256)
      UNITS       "0.1 dBm"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Maximum allowed downstream transmit power. Unit is 0.1 dBm.
       Valid range [-25.6, 25.6] dBm."
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 2 }
      
miscVdslConfDisableBitRateLimitation OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Disables the rate limitation. Only to be used
       for test purposes. Note that disabling the
       limitation on more lines may lead to a crash."
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 3 }
      
miscVdslConfDisableVdslUsPbo OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      obsolete
      DESCRIPTION
      "Disable VDSL US power back off, forcing the CPE to use
       the max US PSD as template. Default not disabled.
       Substituted by: vdslLineConfPhysVdslPboPsdUpDisable.
       Obsoleted from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 4 }

miscVdslConfDynamicInterleaverUs OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Enable support of dynamic change of interleaver
       depth in the upstream direction.
       Note: This field is specific to VDSL2 mode."
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 5 }

miscVdslConfDynamicInterleaverDs OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Enable support of dynamic change of interleaver
       depth in the downstream direction.
       Note: This field is specific to VDSL2 mode."
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 6 }

miscVdslConfDisableProfileBitmap OBJECT-TYPE
      SYNTAX      INTEGER (0..256)
      MAX-ACCESS  read-write
      STATUS      obsolete
      DESCRIPTION
      "Bitmap of VDSL2 profiles to be masked
       The bits are ordered as in G994.1:
       bit0 = 8a
       bit6 = 17a
       Set the respective bit to disable the profile.
       Note: This field is specific to VDSL2 mode.
       Substituted by  miscVdslConfEnableProfileBitmap
       which is standard compliant .
       Obsoleted from:			   EDA 4.0"
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 7 }
      
miscVdslConfEnableProfileBitmap OBJECT-TYPE
      SYNTAX   BITS { profile30a (0), profile17a (1), profile12b (2), profile12a (3),
      				  profile8d (4), profile8c(5), profile8b(6), profile8a(7)}
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "VDSL2 profiles to be allowed by the near-end xTU on
       this line. It is coded in a bit map representation
       (0 if not allowed, 1 if allowed) as defined in
       G.997.1 (third revision), section 7.3.1.1.11:
	   NOTE that profile 30a is not supported in EDA.
       BIT	PROFILE
		7	G.993.2 Profile 8a.
		6	G.993.2 Profile 8b.
		5	G.993.2 Profile 8c.
		4	G.993.2 Profile 8d.
		3	G.993.2 Profile 12a.
		2	G.993.2 Profile 12b.
		1	G.993.2 Profile 17a.
		0	G.993.2 Profile 30a.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 10 }
      
miscVdslConfigStandardLimitMask OBJECT-TYPE
    SYNTAX      INTEGER {   aNUS0  (0),
							aEU32  (1),
							aEU36  (2),
							aEU40  (3),
							aEU44  (4),
							aEU48  (5),
							aEU52  (6),
							aEU56  (7),
							aEU60  (8),
							aEU64  (9),
							b81	  (10),
							b82	  (11),
							b83	  (12),
							b84	  (13),
							b85	  (14),
							b86	  (15),
							b87	  (16),
							b88	  (17),
							b89	  (18),
							b810  (19),
							b811  (20),
							b71	  (21),
							b72	  (22),
							b73	  (23),
							b74	  (24),
							b75	  (25),
							b76	  (26),		 
							custom (255)		} 
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Proprietary interface for selecting a predefined
       standard compliant Bandplan & LimitMask.
       NOTE that selecting a predefined BandPlan & LimitMask
       overrides the vdslLineConfPhysVdslLimitingMaskDn/Up
       and the vdslLineConfPhysVdslBandPlanDn/Up parameters.
       To avoid this (e.g. for using a custom BandPlan and/or
       LimitMask), you should select the value <custom> here.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMiscVdslConfEntry 15 }
      
--******************************************************************
-- VDSL2 VTU-C Proprietary Channel Extension Table
--******************************************************************
vdslVtucProprietaryChanXTable   OBJECT-TYPE
      SYNTAX          SEQUENCE OF VdslVtucProprietaryChanXEntry
      MAX-ACCESS      not-accessible
      STATUS          current
      DESCRIPTION
      "VDSL VTU-C Proprietary Channel Extension Table."
      ::= { vdslObjects 9 }

vdslVtucProprietaryChanXEntry   OBJECT-TYPE
      SYNTAX          VdslVtucProprietaryChanXEntry
      MAX-ACCESS      not-accessible
      STATUS          current
      DESCRIPTION     
      "An entry in the vdslVtucProprietaryChanXEntry."
      INDEX  { lineIndex }
      ::= { vdslVtucProprietaryChanXTable 1 }

VdslVtucProprietaryChanXEntry ::=
      SEQUENCE {
        vdslVtucProprietaryChanXI        	Unsigned32,
        vdslVtucProprietaryChanXG        	Unsigned32,
        vdslVtucProprietaryChanXU        	Unsigned32,
        vdslVtucProprietaryChanXF        	Unsigned32,
        vdslVtucProprietaryChanXTpsTcTypeB0	INTEGER,
        vdslVtucProprietaryChanXTpsTcTypeB1	INTEGER
        }

vdslVtucProprietaryChanXI     OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "I is the interleaver block length.
     Note: This parameter applies to VDSL2 protocols only."
    ::= { vdslVtucProprietaryChanXEntry 1 } 

vdslVtucProprietaryChanXG     OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "G is the total number of overhead octets in an overhead
     superframe.
     Note: This parameter applies to VDSL2 protocols"
    ::= { vdslVtucProprietaryChanXEntry 2 } 
    
vdslVtucProprietaryChanXU     OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "U is the total number of overhead subframes per overhead
     superframe.
     Note: This parameter applies to VDSL2 protocols only."
    ::= { vdslVtucProprietaryChanXEntry 3 }
    
vdslVtucProprietaryChanXF     OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "F is the number of overhead frames in the overhead
     superframe.
     Note: This parameter applies to VDSL2 protocols only."
    ::= { vdslVtucProprietaryChanXEntry 4 } 

vdslVtucProprietaryChanXTpsTcTypeB0 OBJECT-TYPE
    SYNTAX      INTEGER {inactive(1), ptm (2), atm (4)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The TPC-TC currently used for bearer channel B0.
     The two channels must use the same TPS-TC configuration,
     if enabled."
    ::= { vdslVtucProprietaryChanXEntry 10 } 

vdslVtucProprietaryChanXTpsTcTypeB1 OBJECT-TYPE
    SYNTAX      INTEGER {inactive(1), ptm (2), atm (4)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "The TPC-TC currently used for bearer channel B1.
     The two channels must use the same TPS-TC configuration,
     if enabled."
    ::= { vdslVtucProprietaryChanXEntry 15 }
    
