--******************************************************************
-- PboPsdDn
--******************************************************************
vdslLineConfPhysVdslPboPsdDnTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF VdslLineConfPhysVdslPboPsdDnEntry
      MAX-ACCESS not-accessible
      STATUS     current
      DESCRIPTION
      "This table provides a set of line configuration
       parameters to generate a modified downstream PSD
       mask for Downstream Power Back-Off - Shaped
       (DPBOSHAPED).
       The implementation is according to G.997.1
       (third revision) section 7.3.1.2.13. The
       parameters will be substituted by XDSL2 Line
       MIB parameters."
      ::= { vdslObjects 20 }

vdslLineConfPhysVdslPboPsdDnEntry OBJECT-TYPE
      SYNTAX VdslLineConfPhysVdslPboPsdDnEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in vdslLineConfPhysVdslPboPsdDnTable."
      INDEX { lineIndex }
      ::= { vdslLineConfPhysVdslPboPsdDnTable 1 }

VdslLineConfPhysVdslPboPsdDnEntry ::=
      SEQUENCE {
        vdslLineConfPhysVdslPboPsdDnEsel		INTEGER,
        vdslLineConfPhysVdslPboPsdDnEscmA		INTEGER,
        vdslLineConfPhysVdslPboPsdDnEscmB		INTEGER,
        vdslLineConfPhysVdslPboPsdDnEscmC		INTEGER,
        vdslLineConfPhysVdslPboPsdDnFmin		INTEGER,
        vdslLineConfPhysVdslPboPsdDnFmax		INTEGER,
        vdslLineConfPhysVdslPboPsdDnMus 		INTEGER,
        vdslLineConfPhysVdslPboPsdDnAdvanced	TruthValue,
        vdslLineConfPhysVdslPboPsdDnEselMin		INTEGER
        }

vdslLineConfPhysVdslPboPsdDnEsel OBJECT-TYPE
      SYNTAX      INTEGER  (0..511)
      UNITS       "0.5 dB"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "This configuration parameter defines the assumed
       electrical length of cables (E-side cables)
       connecting exchange based DSL services to a remote
       flexibility point (cabinet), that hosts the xTU-C
       that is subject to spectrally shaped downstream
       power back-off depending on this length.
       For this parameter the electrical length is defined
       as the loss (in dB) of an equivalent length of
       hypothetical cable at a reference frequency defined
       by the network operator or in spectrum management
       regulations. DPBOESEL shall be coded as an unsigned
       integer representing an electrical length from
       0 dB to 255.5 dB in steps of 0.5 dB.
       All values in the range are valid.
       If DPBOESEL is set to zero, the DPBO in this section
       shall be disabled.
       G.997.1 name: DPBOESEL.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 5 }
      
vdslLineConfPhysVdslPboPsdDnEscmA OBJECT-TYPE
      SYNTAX      INTEGER  (0..640)
      UNITS       "1/256 dB"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "This configuration parameter is used to define a cable
       model in terms of three scalars DPBOESCMA, DPBOESCMB
       and DPBOESCMC that shall be used to describe the
       frequency dependent loss of E-side cables.
       Parameters DPBOESCMA, DPBOESCMB, DPBOESCMC shall be
       coded as unsigned integers representing a scalar value
       from -1 to 1.5 in steps of 1/256. All values in the range
       are valid.
       G.997.1 name: DPBOESCMA.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 10 }

vdslLineConfPhysVdslPboPsdDnEscmB OBJECT-TYPE
      SYNTAX      INTEGER  (0..640)
      UNITS       "1/256 dB"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "This configuration parameter is used to define a cable
       model in terms of three scalars DPBOESCMA, DPBOESCMB
       and DPBOESCMC that shall be used to describe the
       frequency dependent loss of E-side cables.
       Parameters DPBOESCMA, DPBOESCMB, DPBOESCMC shall be
       coded as unsigned integers representing a scalar value
       from -1 to 1.5 in steps of 1/256. All values in the range
       are valid.
       G.997.1 name: DPBOESCMB.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 15 }
      
vdslLineConfPhysVdslPboPsdDnEscmC OBJECT-TYPE
      SYNTAX      INTEGER  (0..640)
      UNITS       "1/256 dB"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "This configuration parameter is used to define a cable
       model in terms of three scalars DPBOESCMA, DPBOESCMB
       and DPBOESCMC that shall be used to describe the
       frequency dependent loss of E-side cables.
       Parameters DPBOESCMA, DPBOESCMB, DPBOESCMC shall be
       coded as unsigned integers representing a scalar value
       from -1 to 1.5 in steps of 1/256. All values in the range
       are valid.
       G.997.1 name: DPBOESCMC.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 20 }
      
vdslLineConfPhysVdslPboPsdDnFmin OBJECT-TYPE
      SYNTAX      INTEGER  (0..2048)
      UNITS       "ToneIndex"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Defines the minimum frequency from which the DPBO shall
       be applied. It ranges from 0 kHz to 8832 kHz in steps of
       4.3125 kHz.
       G.997.1 name: DPBOFMIN.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 25 }
      
vdslLineConfPhysVdslPboPsdDnFmax OBJECT-TYPE
      SYNTAX      INTEGER  (32..6956)
      UNITS       "ToneIndex"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Defines the maximum frequency at which DPBO may be applied.
       It ranges from 138 kHz to 29997.75 kHz in steps of 4.3125 kHz.
       G.997.1 name: DPBOFMAX.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 30 }
      
vdslLineConfPhysVdslPboPsdDnMus OBJECT-TYPE
      SYNTAX      INTEGER  (-255..0)
      UNITS       "0.5 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Defines the assumed Minimum Usable receive PSD mask (in dBm/Hz)
       for exchange based services, used to modify parameter DPBOFMAX.
       It shall be coded as an integer representing a PSD mask level
       from -127.5 dBm/Hz to 0 dBm/Hz in steps of 0.5 dB. All values
       in the range are valid.
       NOTE - The PSD mask level is 3.5 dB above the signal PSD level
       G.997.1 name: DPBOFMUS.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 35 }

vdslLineConfPhysVdslPboPsdDnAdvanced OBJECT-TYPE
      SYNTAX      TruthValue
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Enables an alternative downstream Power
       Back-Off, which differs from the standard
       G.997.1 PBO calculation."
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 40 }
      
vdslLineConfPhysVdslPboPsdDnEselMin OBJECT-TYPE
      SYNTAX      INTEGER  (0..511)
      UNITS       "0.5 dB"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "This configuration parameter defines the
       minimum value of all trunk line losses of the
       cable distributors served (including the KVz at
       the DSLAM site).
       This parameter is not G.997.1 compliant and has 
       only effect when vdslLineConfPhysVdslPboPsdDnAdvanced
       is enabled.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslPboPsdDnEntry 45 }
      
--******************************************************************
-- MibMaskDs
--******************************************************************
vdslLineConfPhysVdslMibMaskDsTable OBJECT-TYPE
      SYNTAX     SEQUENCE OF VdslLineConfPhysVdslMibMaskDsEntry
      MAX-ACCESS not-accessible
      STATUS     current
      DESCRIPTION
      "This table contains information of MibMaskDs
       configuration in accordance with  to G.997.1
       section 7.3.1.2.9"
      ::= { vdslObjects 25 }

vdslLineConfPhysVdslMibMaskDsEntry OBJECT-TYPE
      SYNTAX VdslLineConfPhysVdslMibMaskDsEntry
      MAX-ACCESS  not-accessible
      STATUS      current
      DESCRIPTION
      "An entry in vdslLineConfPhysVdslMibMaskDsTable."
      INDEX { lineIndex }
      ::= { vdslLineConfPhysVdslMibMaskDsTable 1 }

VdslLineConfPhysVdslMibMaskDsEntry ::=
      SEQUENCE {
        mibMaskDsNoOfBreakPoints			INTEGER,
        mibMaskDsBpToneIndex				OCTET STRING,
        mibMaskDsBpPsd			  			OCTET STRING
        }

mibMaskDsNoOfBreakPoints OBJECT-TYPE
      SYNTAX      INTEGER  (0..32)			
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Number of breakpoints in downstream mask.
       Each breakpoint is defined by a subcarrier
       index (mibMaskDsBpToneIndex) and a PSD level
       (mibMaskDsBpPsd).
       G.997.1 name: PSDMASKds.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMibMaskDsEntry 5 }

mibMaskDsBpToneIndex OBJECT-TYPE
      SYNTAX      OCTET STRING  (SIZE (64))
      UNITS       "ToneIndex"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "Toneindexes for 32 breakpoints. Each ToneIndex is
       2 octets. The octet string has the format:
       [Bp0_Hi, Bp0_Low....Bp31_Hi, Bp31_Low]
       Valid range is [0, 4096].
       G.997.1 name: PSDMASKds.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMibMaskDsEntry 10 }

mibMaskDsBpPsd OBJECT-TYPE
      SYNTAX      OCTET STRING  (SIZE (64))
      UNITS       "0.1 dBm/Hz"
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
      "PSD level in 0.1 dBm/Hz for 32 breakpoints. Each
       BpPsd has to octets. The octet string has the format:
       [Bp0_Hi, Bp0_Low....Bp31_Hi, Bp31_Low]
       Valid range is [0,-96] dBm/Hz.
       G.997.1 name: PSDMASKds.
       Valid from:                EDA 4.0"
      ::= { vdslLineConfPhysVdslMibMaskDsEntry 15 }

