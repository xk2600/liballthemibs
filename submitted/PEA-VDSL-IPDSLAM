        vdslVtucProprietaryConfXTpsTcType					INTEGER,
        vdslVtucProprietaryConfXPreemtionAllowedB0			TruthValue,
        vdslVtucProprietaryConfXPreemtionAllowedB1			TruthValue
        }

vdslVtucProprietaryConfXTpsTcType OBJECT-TYPE
    SYNTAX      INTEGER {atm (2), ptm (4), both (6)}
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Configure allowed traffic type: ATM, PTM or both."
    ::= { vdslProprietaryLineConfProfileXEntry 5 } 

vdslVtucProprietaryConfXPreemtionAllowedB0 OBJECT-TYPE
	SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "If True the subscriber is allowed to send 
     preemptive traffic frames on bearer channel B0. 
     The value can be either True(1) or False(2)." 
    ::= { vdslProprietaryLineConfProfileXEntry 10 }

