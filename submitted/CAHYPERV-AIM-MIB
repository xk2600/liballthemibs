--   1.3.6.1.4.1.546.16.56.2.2.2.1.5
        hypervAimStatVMEnabledStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                unknown(1),
                enabled(2),
                disabled(3),
                paused(32768),
                suspended(32769),
                starting(32770),
                snapshotting(32771),
                migrating(32772),
                saving(32773),
                stopping(32774),
                deleted(32775),
                pausing(32776),
                resuming(32777)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Indicates the enabled and disabled states of an element. 
                It can also indicate the transitions between these requested states.
                Possible values are:
                        unknown(1)
                        enabled(2)
                        disabled(3)
                        paused(32768)
                        suspended(32769)
                        starting(32770)
                        snapshotting(32771)
                        migrating(32772)
                        saving(32773)
                        stopping(32774)
                        deleted(32775)
                        pausing(32776)
                        resuming(32777)
                "
            ::= { hypervAimStatVMEntry 5 }

        
