-- 1.3.6.1.4.1.546.18.1
        hypervAimStatVMAddEvent OBJECT IDENTIFIER ::= { traps 1 }

        
--   1.3.6.1.4.1.546.18.2
        hypervAimStatVMRemoveEvent OBJECT IDENTIFIER ::= { traps 2 }

        
--   1.3.6.1.4.1.546.18.3
        hypervAimStatVMEnabledEvent OBJECT IDENTIFIER ::= { traps 3 }

        
--   1.3.6.1.4.1.546.18.4
        hypervAimStatVMMigrateEvent OBJECT IDENTIFIER ::= { traps 4 }

        
    
-- 
--  Trap definitions
-- 
-- HyperV system name
-- VM index
-- VM name
-- VM GUID
-- VM DNS name
-- IPv4 address
-- IPv6 address
-- MAC address
-- 
        hypervAimStatVMAddTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hypervAimStatVMHyperVHostIndex, hypervAimStatVMHyperVHostName, hypervAimStatVMIndex, hypervAimStatVMElementName, hypervAimStatVMName, hypervAimStatVMDNSName, hypervAimStatVMNetworkAddressIPv6, 
                hypervAimStatVMNetworkAddressIPv4, hypervAimStatVMMACAddress }
            DESCRIPTION 
                "Trap is sent when a new virtual machine is added. "
            ::=  16501

--   HyperV system name
-- VM name
-- VM GUID
-- VM DNS name
        hypervAimStatVMRemoveTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hypervAimStatVMHyperVHostIndex, hypervAimStatVMHyperVHostName, hypervAimStatVMIndex, hypervAimStatVMElementName, hypervAimStatVMName, hypervAimStatVMDNSName }
            DESCRIPTION 
                "Trap is sent when a new virtual machine is removed. "
            ::=  16502

-- System name
-- VM index
-- VM name
-- VM GUID
-- VM DNS name
-- Previous EnabledState
-- Current EnabledState
        hypervAimStatVMEnabledTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hypervAimStatVMHyperVHostIndex, hypervAimStatVMHyperVHostName, hypervAimStatVMIndex, hypervAimStatVMElementName, hypervAimStatVMName, hypervAimStatVMDNSName, hypervAimStatVMPreviousEnabledStatus, 
                hypervAimStatVMEnabledStatus }
            DESCRIPTION 
                "Trap is sent when the virtual machines enabled state (hypervAimStatVMEnabledStatus) has changed."
            ::=  16504

-- VM index
-- VM name
-- VM GUID
-- VM DNS name
-- Previous HyperV system name
-- New HyperV system name
        hypervAimStatVMMigrateTrap TRAP-TYPE 
            ENTERPRISE sysmgmt
            VARIABLES { hypervAimStatVMIndex, hypervAimStatVMElementName, hypervAimStatVMName, hypervAimStatVMDNSName, hypervAimStatVMHyperVPreviousHostIndex, hypervAimStatVMHyperVPreviousHostName, hypervAimStatVMHyperVHostIndex, hypervAimStatVMHyperVHostName
                 }
            DESCRIPTION 
                "Trap is sent when a virtual machine is migrated to a new HyperV host."
            ::=  16505

    
