    CAHYPERV-AIM-MIB DEFINITIONS ::= BEGIN
 
        IMPORTS
            OBJECT-TYPE            
                FROM RFC-1212            
            TRAP-TYPE            
                FROM RFC-1215            
            enterprises            
                FROM RFC1155-SMI            
            DisplayString            
                FROM RFC1213-MIB
            SnmpAdminString
                FROM SNMP-FRAMEWORK-MIB;
    
    
-- 
-- Node definitions
-- 
-- *****************************************************************************
-- OIDs of hyperv agent start with 1.3.6.1.4.1.546.16.56
-- *****************************************************************************
-- 1.3.6.1.4.1.546
        empire       OBJECT IDENTIFIER ::= { enterprises 546 }
-- 1.3.6.1.4.1.546.1
	unix         OBJECT IDENTIFIER ::= { empire 1 }
-- 1.3.6.1.4.1.546.1.1
	sysmgmt      OBJECT IDENTIFIER ::= { unix 1 }
-- 1.3.6.1.4.1.546.1.1.9
	traps        OBJECT IDENTIFIER ::= { empire 9 }
--   1.3.6.1.4.1.546.16
        applications OBJECT IDENTIFIER ::= { empire 16 }
--   1.3.6.1.4.1.546.16.56
        cahypervaim  OBJECT IDENTIFIER ::= { applications 56 }


--   1.3.6.1.4.1.546.16.56.1
        hypervAimGroup OBJECT IDENTIFIER ::= { cahypervaim 1 }

        
--   1.3.6.1.4.1.546.16.56.1.1
        hypervAimInstanceCount OBJECT-TYPE
            SYNTAX INTEGER
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "Number of HyperV instances in hypervAimInstanceTable."
            ::= { hypervAimGroup 1 }

        
--   1.3.6.1.4.1.546.16.56.1.2
        hypervAimInstanceTable OBJECT-TYPE
            SYNTAX SEQUENCE OF HypervAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "A table containing the HyperV instances."
            ::= { hypervAimGroup 2 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1
        hypervAimInstanceEntry OBJECT-TYPE
            SYNTAX HypervAimInstanceEntry
            ACCESS not-accessible
            STATUS mandatory
            DESCRIPTION
                "The HyperV instances table entry."
            INDEX { hypervAimInstanceIndex }
            ::= { hypervAimInstanceTable 1 }

        
        HypervAimInstanceEntry ::=
            SEQUENCE { 
                hypervAimInstanceName
                    SnmpAdminString,
                hypervAimInstanceUserName
                    SnmpAdminString,
                hypervAimInstancePassword
                    SnmpAdminString,
                hypervAimInstanceRowStatus
                    INTEGER,
                hypervAimInstanceDataStatus
                    INTEGER,
                hypervAimInstanceIndex
                    INTEGER
             }

--   1.3.6.1.4.1.546.16.56.1.2.1.1
        hypervAimInstanceName OBJECT-TYPE
            SYNTAX SnmpAdminString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Name of the HyperV Instance. Here it is the HyperV Server host name."
            ::= { hypervAimInstanceEntry 1 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1.4
        hypervAimInstanceUserName OBJECT-TYPE
            SYNTAX SnmpAdminString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "User name for connecting to the HyperV Server."
            ::= { hypervAimInstanceEntry 4 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1.5
        hypervAimInstancePassword OBJECT-TYPE
            SYNTAX SnmpAdminString
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "Password for the user name provided to connect to the  HyperV Server."
            ::= { hypervAimInstanceEntry 5 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1.7
        hypervAimInstanceRowStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                active(1),
                notInService(2),
                notReady(3),
                createAndGo(4),
                createAndWait(5),
                destroy(6)
                }
            ACCESS read-write
            STATUS mandatory
            DESCRIPTION
                "The status of this monitor entry according to the Textual Conventions for SNMPv2 (see RFC 1443).
                active(1) - the row is available for usage
                notInService(2) - setting to this value disables usage of the row
                notReady(3) - an error occured during usage of the row
                createAndGo(4) - setting to this value creates the row with row status set to active(1)
                createAndWait(5) - setting to this value creates the row with row status set to notInService(2)
                destroy(6) - setting to this value deletes the row"
            DEFVAL { 5 }
            ::= { hypervAimInstanceEntry 7 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1.8
        hypervAimInstanceDataStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                ready(1),
                notReady(2),
                hostUnreachable(3),
                hostIsSlave(4),
                authenticationFailed(5),
                unknownError(6)
                }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The status of data of the  HyperV Instance (Server), i.e. status of Aim caches for the instance.
                ready(1) - data are ready in Aim cache, caches were populated
                notReady(2)  - data are Not ready, caches were not populated yet
                hostUnreachable(3) - HyperV host unreachable, data are kept in Aim caches
                hostIsSlave(4) -  HyperV host is a slave server, data are kept in Aim caches
                authenticationFailed(5) - authentication failed for HyperV host, data are kept in Aim caches
                unknownError(6) - unknow error, data are kept in Aim caches"
            ::= { hypervAimInstanceEntry 8 }

        
--   1.3.6.1.4.1.546.16.56.1.2.1.9
        hypervAimInstanceIndex OBJECT-TYPE
            SYNTAX INTEGER (1..2147483647)
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                " The unique index of the instance."
            ::= { hypervAimInstanceEntry 9 }

        
