--   1.3.6.1.4.1.546.16.56.3.1.7
        hypervAimCfgDataStatus OBJECT-TYPE
            SYNTAX INTEGER
            {
                dataReady(1),
                dataNotReady(2),
                dataError(3)
            }
            ACCESS read-only
            STATUS mandatory
            DESCRIPTION
                "The status of Hyper-V data, i.e. status of Aim caches.
                dataReady(1)     - data are ready in Aim caches, caches were populated
                dataNotReady(2)  - data are Not ready yet, Aim is retrieving data from platform, caches were not populated yet
                dataError(3)     - failed to retrieve data from platform, Aim caches were not populated"
            DEFVAL { 2 }
            ::= { hypervAimCfgGeneralGroup 7 }

        
