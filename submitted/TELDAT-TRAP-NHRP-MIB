nhrpNotification18 NOTIFICATION-TYPE
    OBJECTS { 
              nhrp18_version, 
              nhrp18_ip_address_src,
              nhrp18_ip_address_dest 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event NHRP.18

        Message: 'Wrong protocol version $1, src: $2, dest: $3'

        Parameters list:
              $1: nhrp18_version
              $2: nhrp18_ip_address_src
              $3: nhrp18_ip_address_dest

        The fixed part of the NHRP packet holds a wrong protocol version."
    ::= { nhrpNotificationsPrefix 18 }

nhrpNotification19 NOTIFICATION-TYPE
    OBJECTS { 
              nhrp19_ip_address_src, 
              nhrp19_ip_address_dest 
            }
    STATUS current
    DESCRIPTION
