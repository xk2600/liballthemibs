TELDAT-TRAP-EAP-MIB DEFINITIONS ::= BEGIN


IMPORTS
	MODULE-IDENTITY,OBJECT-TYPE,OBJECT-IDENTITY,NOTIFICATION-TYPE,Integer32,IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	telEventTraps
		FROM TELDAT-MIB;


eapTrapsMIB MODULE-IDENTITY
    LAST-UPDATED "201401200930Z"
    ORGANIZATION "Teldat, S.A."
    CONTACT-INFO 
        "Teldat, S.A.
        Customer Service

        Postal: Parque Tecnologico de Madrid
                Tres Cantos 28760
                Madrid, SPAIN

        Tel.: +34 918 076 565

        http://www.teldat.com"
    DESCRIPTION
        "This Module Information Base file is defined for managing received
        SNMP notifications generated by the EAP event subsystem."
    REVISION "201401200930Z"
    DESCRIPTION "Initial implementation"
        ::= { telEventTraps 191 }


eapEventsGroup	OBJECT IDENTIFIER	::= { eapTrapsMIB 1 }

eapNotifications	OBJECT IDENTIFIER	::= { eapTrapsMIB 2 }
eapNotificationsPrefix	OBJECT IDENTIFIER	::= { eapNotifications 0 }

eapEventObjects1 OBJECT-IDENTITY
    STATUS current
    DESCRIPTION
        "Event EAP.1
        An EAP packet of the type indicated has just arrived for processing."
    ::= { eapEventsGroup 1 }

