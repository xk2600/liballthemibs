eapNotification1 NOTIFICATION-TYPE
    OBJECTS { 
              eap1_code_string, 
              eap1_type_string,
              eap1_identificer,
              eap1_length,
              eap1_eap_ctrl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event EAP.1

        Message: 'Received EAP $1/$2 Packet, identifier $3, length $4, eap_ctrl 0x$5'

        Parameters list:
              $1: eap1_code_string
              $2: eap1_type_string
              $3: eap1_identificer
              $4: eap1_length
              $5: eap1_eap_ctrl

        An EAP packet of the type indicated has just arrived for processing."
    ::= { eapNotificationsPrefix 1 }

eapNotification2 NOTIFICATION-TYPE
    OBJECTS { 
              eap2_code_string, 
              eap2_type_string,
              eap2_identificer,
              eap2_length,
              eap2_eap_ctrl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event EAP.2

        Message: 'Transmitted EAP $1/$2 Packet, identifier $3, length $4, eap_ctrl 0x$5'

        Parameters list:
              $1: eap2_code_string
              $2: eap2_type_string
              $3: eap2_identificer
              $4: eap2_length
              $5: eap2_eap_ctrl

        An EAP packet of the type indicated has been transmitted."
    ::= { eapNotificationsPrefix 2 }

eapNotification3 NOTIFICATION-TYPE
    OBJECTS { 
              eap3_retry_count, 
              eap3_eap_ctrl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event EAP.3

        Message: 'EAP Timeout retry $1, eap_ctrl 0x$2'

        Parameters list:
              $1: eap3_retry_count
              $2: eap3_eap_ctrl

        An EAP request was sent and no response was received. The packet is
        retransmitted."
    ::= { eapNotificationsPrefix 3 }

eapNotification4 NOTIFICATION-TYPE
    OBJECTS { 
              eap4_eap_ctrl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event EAP.4

        Message: 'EAP Timeout max retries reached, eap_ctrl 0x$1'

        Parameters list:
              $1: eap4_eap_ctrl

        An EAP request has been sent with no response for the maximum configured
        retries. The EAP session is closed."
    ::= { eapNotificationsPrefix 4 }

eapNotification5 NOTIFICATION-TYPE
    OBJECTS { 
              eap5_Identity_len, 
              eap5_Available_len,
              eap5_eap_ctrl 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event EAP.5

        Message: 'Not enough space to store Identity len $1, available $2, eap_ctrl 0x$3'

        Parameters list:
              $1: eap5_Identity_len
              $2: eap5_Available_len
              $3: eap5_eap_ctrl

        The received EAP identity can not be processed because it is longer
        than the available buffer."
    ::= { eapNotificationsPrefix 5 }

