                        INTEGER,
                  gw_q931_code
                        INTEGER,
                  gw_ldc_q931_amount
                        INTEGER,
                  gw_ldc_objname
                        DisplayString
              }
          gw_object_index OBJECT-TYPE
              SYNTAX	INTEGER
              ACCESS    read-only
              STATUS    mandatory
              DESCRIPTION
                      ""
              ::= { gwLdcEntry 1 }

          gw_ldc_code OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { gwLdcEntry 2 }

          gw_q931_code OBJECT-TYPE
              SYNTAX     INTEGER
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { gwLdcEntry 3 }

          gw_ldc_q931_amount OBJECT-TYPE
              SYNTAX     INTEGER
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { gwLdcEntry 4 }

          gw_ldc_objname OBJECT-TYPE
              SYNTAX     DisplayString
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { gwLdcEntry 5 }

----------------------------------------------------------------------------------

	-- the Routes Table

        rtTable OBJECT-TYPE
           SYNTAX  SEQUENCE OF RtEntry
           ACCESS  not-accessible
           STATUS  mandatory
           DESCRIPTION
                     ""
        ::= { mvts 109 }

        rtEntry   OBJECT-TYPE
            SYNTAX  RtEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                      ""
            INDEX   { rt_index }
        ::= { rtTable 1 }

        RtEntry ::=                                  	
            SEQUENCE {
                rt_index
                      INTEGER,
                rt_name
                      DisplayString,
                rt_stat_uptime
                      DisplayString,
                rt_lastupdate
                      DisplayString,
                rt_asr
                      INTEGER,
                rt_asr_std
                      INTEGER,
                rt_acd
                      DisplayString,
                rt_average_qos
                      INTEGER,
                rt_maxload
                      INTEGER,
                rt_active_calls
                      INTEGER,
                rt_totaltime
                      DisplayString,
                rt_calls_received
                      INTEGER,
                rt_calls_normal
                      INTEGER,
                rt_calls_zero_normal
                      INTEGER,
                rt_calls_failed
                      INTEGER,
                rt_calls_zero_failed
                      INTEGER,
                rt_kb_received
                      INTEGER,
                rt_kb_sent
                      INTEGER,
                rt_status
                      DisplayString,
                rt_success_calls
                      INTEGER
              }
          rt_index OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
              ::= { rtEntry 1 }
          rt_name OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 2 }
          rt_stat_uptime OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 3 }
          rt_lastupdate OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 4 }
          rt_asr OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 5 }
          rt_asr_std OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 6 }
          rt_acd OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 7 }
          rt_average_qos OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 8 }
          rt_maxload OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 9 }
          rt_active_calls OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 10 }
          rt_totaltime OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 11 }
          rt_calls_received OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 12 }
          rt_calls_normal OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 13 }
          rt_calls_zero_normal OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 14 }
          rt_calls_failed OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 15 }
          rt_calls_zero_failed OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 16 }
          rt_kb_received OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 17 }
          rt_kb_sent OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 18 }

          rt_status OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 19 }

          rt_success_calls OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { rtEntry 20 }


