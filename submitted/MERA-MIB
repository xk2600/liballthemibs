

-- Mera SNMP parameters


MERA-MIB DEFINITIONS ::= BEGIN

    IMPORTS
        OBJECT-TYPE          FROM RFC-1212
        enterprises          FROM RFC1155-SMI
        DisplayString        FROM SNMPv2-TC
        TRAP-TYPE            FROM RFC-1215;


mera    OBJECT IDENTIFIER ::= { enterprises 999 }

    mvts    OBJECT IDENTIFIER ::= { mera 10 }

    -- Simple SNMP items 

        active_calls       OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION    
                           ""
        ::= { mvts 1 }
        avg5_calls         OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 2 }
        max_calls          OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 3 }
        uptime             OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 4 }
        stat_time          OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 5 }
        call_duration      OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 6 }
        received_calls     OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 7 }
        normal_calls       OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 8 }
        failed_calls       OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 9 }
        rejected_calls     OBJECT-TYPE
            SYNTAX         INTEGER
            ACCESS         read-only
            STATUS         mandatory
            DESCRIPTION
                           ""
        ::= { mvts 10 }


	-- the Originators Table

        srcTable OBJECT-TYPE
           SYNTAX  SEQUENCE OF SrcEntry
           ACCESS  not-accessible
           STATUS  mandatory
           DESCRIPTION
                     ""
        ::= { mvts 101 }

        srcEntry    OBJECT-TYPE
            SYNTAX  SrcEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
                      ""
            INDEX   { src_index }
        ::= { srcTable 1 }


        SrcEntry ::=                                  	
            SEQUENCE {
                src_index
                      INTEGER,
                src_name
                      DisplayString,
                src_stat_uptime
                      DisplayString,
                src_lastupdate
                      DisplayString,
                src_asr
                      INTEGER,
                src_asr_std
                      INTEGER,
                src_acd
                      DisplayString,
                src_average_qos
                      INTEGER,
                src_maxload
                      INTEGER,
                src_active_calls
                      INTEGER,
                src_totaltime
                      DisplayString,
                src_calls_received
                      INTEGER,
                src_calls_normal
                      INTEGER,
                src_calls_zero_normal
                      INTEGER,
                src_calls_failed
                      INTEGER,
                src_calls_zero_failed
                      INTEGER,
                src_kb_received
                      INTEGER,
                src_kb_sent
                      INTEGER
               }
          
          src_index   OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
              ::= { srcEntry 1 }
          src_name OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 2 }
          src_stat_uptime OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 3 }
          src_lastupdate OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 4 }
          src_asr OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 5 }
          src_asr_std OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 6 }
          src_acd OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 7 }
          src_average_qos OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 8 }
          src_maxload OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 9 }
          src_active_calls OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 10 }
          src_totaltime OBJECT-TYPE
              SYNTAX  DisplayString
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 11 }
          src_calls_received OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 12 }
          src_calls_normal OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 13 }
          src_calls_zero_normal OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 14 }
          src_calls_failed OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 15 }
          src_calls_zero_failed OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 16 }
          src_kb_received OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 17 }
          src_kb_sent OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcEntry 18 }
          
          srcLdcTable OBJECT-TYPE
              SYNTAX  SEQUENCE OF SrcLdcEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                     ""
          ::= { mvts 102 }
          
          srcLdcEntry OBJECT-TYPE
              SYNTAX  SrcLdcEntry
              ACCESS  not-accessible
              STATUS  mandatory
              DESCRIPTION
                    ""
              INDEX   { src_object_index, src_ldc_code, src_q931_code }
          ::= { srcLdcTable 1 }

          SrcLdcEntry ::=                                  	
              SEQUENCE {
                  src_object_index
                        INTEGER,
                  src_ldc_code
                        INTEGER,
                  src_q931_code
                        INTEGER,
                  src_ldc_q931_amount
                        INTEGER,
                  src_ldc_objname
                        DisplayString
              }
          src_object_index OBJECT-TYPE
              SYNTAX	INTEGER
              ACCESS    read-only
              STATUS    mandatory
              DESCRIPTION
                      ""
              ::= { srcLdcEntry 1 }

          src_ldc_code OBJECT-TYPE
              SYNTAX  INTEGER
              ACCESS  read-only
              STATUS  mandatory
              DESCRIPTION
                      ""
          ::= { srcLdcEntry 2 }

          src_q931_code OBJECT-TYPE
              SYNTAX     INTEGER
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { srcLdcEntry 3 }

          src_ldc_q931_amount OBJECT-TYPE
              SYNTAX     INTEGER
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { srcLdcEntry 4 }

          src_ldc_objname OBJECT-TYPE
              SYNTAX     DisplayString
              ACCESS     read-only
              STATUS     mandatory
              DESCRIPTION
                      ""
              ::= { srcLdcEntry 5 }

