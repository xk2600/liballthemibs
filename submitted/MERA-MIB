	-- Traps

        meraCriticalErrorTrap TRAP-TYPE
              ENTERPRISE  enterprises
              DESCRIPTION
                      "Mvts critical error trap"
        ::= 1

        meraErrorTrap TRAP-TYPE
              ENTERPRISE  enterprises
              DESCRIPTION
                      "Mvts error trap"
        ::= 2

        meraWarningTrap TRAP-TYPE
              ENTERPRISE  enterprises
              DESCRIPTION
                      "Mvts warning trap"
        ::= 3

        meraInfoTrap TRAP-TYPE
              ENTERPRISE  enterprises
              DESCRIPTION
                      "Mvts info trap"
        ::= 4

        meraNotifyTrap TRAP-TYPE
              ENTERPRISE  enterprises
              DESCRIPTION
                      "Mvts notify trap"
        ::= 5

                
