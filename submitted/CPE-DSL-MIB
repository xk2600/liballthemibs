					INTEGER,
				cpePvcAdminStatus
					INTEGER,
				cpePvcOperStatus
					INTEGER,
				cpePvcBridgeMode
					EnableAdmin,
				cpePvcMacLearnMode
					EnableAdmin,
				cpePvcIgmpMode
					EnableAdmin,
				cpePvcDhcpClientMode 
					EnableAdmin,
				cpePvcNatMode
					EnableAdmin
			 }

          	cpePvcId OBJECT-TYPE
	     		SYNTAX Integer32
