bfdNotification52 NOTIFICATION-TYPE
    OBJECTS { 
              bfd52_authentication_seqnum, 
              bfd52_peer_address,
              bfd52_last_authentication_seqnum 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event BFD.52

        Message: 'Received invalid authentication sequence number $1 from peer $2, last was $3'

        Parameters list:
              $1: bfd52_authentication_seqnum
              $2: bfd52_peer_address
              $3: bfd52_last_authentication_seqnum

        A packet with authentication has been received but authentication
        sequence number is invalid."
    ::= { bfdNotificationsPrefix 52 }

bfdNotification53 NOTIFICATION-TYPE
    OBJECTS { 
              bfd53_peer_address 
            }
    STATUS current
    DESCRIPTION
        "Notification of Event BFD.53

        Message: 'Authentication fail in packet received from peer $1'

        Parameters list:
              $1: bfd53_peer_address

        A packet with authentication has been received but authentication
        digest is invalid."
    ::= { bfdNotificationsPrefix 53 }


